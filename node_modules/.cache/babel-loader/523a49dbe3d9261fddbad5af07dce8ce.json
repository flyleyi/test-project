C;AACD;AACF,CApBD;AAsBA;AACA;AACA;AACA;;;AACA,SAAS0B,YAAT,CACEvB,IADF,EAIE;AAAA,MAFA+B,MAEA,uEAFS,eAET;AAAA,MADAC,QACA,uEADW,gBACX;AACA,MAAI,CAACZ,KAAK,CAACC,OAAN,CAAcrB,IAAd,CAAL,EAA0B,OAAO,EAAP;AAC1B,SAAOA,IAAI,CAACO,GAAL,CAAS,UAASL,IAAT,EAAe;AAC7B,QAAIA,IAAI,CAACsB,QAAL,IAAiBtB,IAAI,CAACsB,QAAL,CAAcT,MAAnC,EACEb,IAAI,CAACsB,QAAL,GAAgBD,YAAY,CAACrB,IAAI,CAACsB,QAAN,EAAgBO,MAAhB,EAAwBC,QAAxB,CAA5B;AACF,WAAO;AACL7B,MAAAA,KAAK,EAAED,IAAI,CAAC6B,MAAD,CADN;AAEL3B,MAAAA,KAAK,EAAEF,IAAI,CAAC8B,QAAD;AAFN,KAAP;AAID,GAPM,CAAP;AAQD;;AAED,eAAe;AACbC,EAAAA,UAAU,EAAE,IADC;AAEbvC,EAAAA,KAAK,EAALA,KAFa;AAGbC,EAAAA,OAAO,EAAPA,OAHa;AAIbU,EAAAA,SAAS,EAATA,SAJa;AAKbM,EAAAA,OAAO,EAAPA;AALa,CAAf","sourcesContent":["import { queryDictList } from '../../api/dict/index';\r\nimport { has } from 'lodash';\r\n\r\n// 字典数据\r\nconst state = {};\r\n\r\n// 获取方法\r\nconst getters = {\r\n  /**\r\n   * 根据type获取字典列表数据\r\n   * 如果是获取大于两层字典根据层级使用逗号隔开\r\n   * @param {String} type 字典类型\r\n   * @return {Array}\r\n   */\r\n  getList: state => type => getList(state, type),\r\n  /**\r\n   * 根据传递type和key查询字典翻译\r\n   * @param {type: String, key: String} 参数\r\n   * @return {String}\r\n   */\r\n  queryLabel: state => options => {\r\n    if (typeof options !== 'object' || options === null) return '';\r\n    var type = options.type;\r\n    var list = getList(state, type);\r\n    var key = options.key;\r\n    for (const item of list) {\r\n      if (item.value === key) return item.label;\r\n    }\r\n    return '';\r\n  }\r\n};\r\n\r\nconst mutations = {\r\n  /**\r\n   * 设置字典数据\r\n   * @param {*} state 状态\r\n   * @param {*} map map数据\r\n   */\r\n  SET_DICT_MAP(state, map) {\r\n    // 匹配有的进行赋值\r\n    Object.keys(state).forEach(key => {\r\n      has(map, key) && (state[key] = map[key]);\r\n    });\r\n  }\r\n};\r\n\r\nconst actions = {\r\n  /**\r\n   * 请求获取字典数据\r\n   * @param {*} param0 状态\r\n   * @returns {Promise}\r\n   */\r\n  ajaxGetData({ state, commit }) {\r\n    let keyList = Object.keys(state);\r\n    // 无字典\r\n    if (!keyList.length) return Promise.resolve();\r\n    return queryDictList({\r\n      keyList // 要查询的字典\r\n    }).then(({ data }) => {\r\n      // 将list转map结构便于查找赋值\r\n      let map = {};\r\n      Array.isArray(data) &&\r\n        data.forEach(\r\n          item =>\r\n            (map[item.dictionaryKey] = list2options(\r\n              item.children,\r\n              'dictionaryKey',\r\n              'dictionaryValueZh'\r\n            ))\r\n        );\r\n      // 设置值\r\n      commit('SET_DICT_MAP', map);\r\n    });\r\n  }\r\n};\r\n\r\n/**\r\n * 获取字典列表\r\n * @param {*} state 状态\r\n * @param {*} type 类型\r\n */\r\nconst getList = (state, type) => {\r\n  type = type || '';\r\n  var types = type.split(',');\r\n  if (types.length > 1) {\r\n    var list = has(state, types[0]) ? state[types[0]] : [];\r\n    for (var i = 1; i < types.length; i++) {\r\n      var val = types[i];\r\n      var res = list.find(function(value) {\r\n        return value.key == val;\r\n      });\r\n      if (res !== void 0) list = res.children || [];\r\n      else {\r\n        list = [];\r\n        break;\r\n      }\r\n    }\r\n    return list;\r\n  } else {\r\n    return has(state, type) ? state[type] : [];\r\n  }\r\n};\r\n\r\n/**\r\n * 将list转为默认的options数据\r\n * @param {*} list 源数据列表\r\n */\r\nfunction list2options(\r\n  list,\r\n  keyKey = 'dictionaryKey',\r\n  labelKey = 'dictionaryText'\r\n) {\r\n  if (!Array.isArray(list)) return [];\r\n  return list.map(function(item) {\r\n    if (item.children && item.children.length)\r\n      item.children = list2options(item.children, keyKey, labelKey);\r\n    return {\r\n      value: item[keyKey],\r\n      label: item[labelKey]\r\n    };\r\n  });\r\n}\r\n\r\nexport default {\r\n  namespaced: true,\r\n  state,\r\n  getters,\r\n  mutations,\r\n  actions\r\n};\r\n"]}]}
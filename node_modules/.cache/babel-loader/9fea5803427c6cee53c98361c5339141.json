GAAA,EAAA,KAAA,CAAA;AAJA,WADA,CAHA;AAWA,UAAA,SAAA,EAAA,mBAAA,GAAA,EAAA;AACA;AACA,gBAAA,GAAA,CAAA,EAAA,KAAA,GAAA,EAAA;AACA,cAAA,KAAA,CAAA,aAAA;AACA;AACA,WAhBA;AAiBA,UAAA,MAAA,EAAA,gBAAA,GAAA,EAAA;AACA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA;AApBA,SAAA;AAsBA,OAvBA,MAuBA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,GAAA,CAAA,UAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,KADA,CACA;;AADA,SAAA;AAGA;AACA,KAnCA;;AAoCA;AACA;AACA;AACA,IAAA,UAvCA,wBAuCA;AAAA;;AACA,WAAA,IAAA,GAAA,IAAA;AACA,MAAA,cAAA,CAAA;AAAA,QAAA,EAAA,EAAA,KAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,gBAAA;AAAA,YAAA,IAAA,QAAA,IAAA;;AACA,aAAA,IAAA,CAAA,IAAA,KAAA,CAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,CAAA,IAAA,IAAA,CAAA,CAAA,CAAA;AACA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,uBAAA,GAAA,SAAA,CACA,KAAA,CAAA,QAAA,CAAA,gBADA,EAEA,cAFA,CAAA;AAIA,QAAA,KAAA,CAAA,QAAA,CAAA,qBAAA,GAAA,SAAA,CACA,KAAA,CAAA,QAAA,CAAA,cADA,EAEA,cAFA,CAAA;;AAIA,YAAA,IAAA,CAAA,SAAA,EAAA;AACA,cAAA,UAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,cAAA,WAAA,GAAA,IAAA,CAAA,aAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,QAAA,GAAA,IAAA,CAAA,SAAA,CACA,KADA,CACA,GADA,EAEA,GAFA,CAEA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,gBAAA,GAAA,GAAA,UAAA,CAAA,KAAA,CAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,mBAAA,MAAA,CAAA,MAAA,CACA,EADA,EAEA;AACA,cAAA,IAAA,EAAA,GAAA,CAAA,GAAA,CAAA,MAAA,GAAA,CAAA,CADA;AAEA,cAAA,GAAA,EAAA,IAFA;AAGA,cAAA,IAAA,EAAA,WAAA,CAAA,KAAA;AAHA,aAFA,CAAA;AAQA,WAZA,CAAA;AAaA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,KAAA;AACA,OA/BA;AAgCA,KAzEA;;AA0EA;AACA;AACA;AACA,IAAA,aA7EA,2BA6EA;AAAA;;AACA,cAAA,OAAA,CAAA;AACA,QAAA,OAAA,EAAA,YADA;AAEA,QAAA,kBAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,iBAAA,CAAA;AAAA,UAAA,EAAA,EAAA,MAAA,CAAA;AAAA,SAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,mBAAA,OAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,WAHA,MAGA;AACA,mBAAA,IAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,SARA,EASA,KATA,CASA,UAAA,GAAA,EAAA;AACA,iBAAA,IAAA,CAAA,GAAA,CAAA,OAAA;AACA,SAXA;AAYA,OAjBA,EAkBA,KAlBA,CAkBA,YAAA;AACA,eAAA,IAAA,CAAA,OAAA;AACA,OApBA;AAqBA,KAnGA;;AAoGA;AACA;AACA;AACA,IAAA,YAvGA,0BAuGA;AAAA;;AACA,MAAA,uBAAA;AACA,QAAA,kBAAA,EAAA,KAAA;AADA,SAEA,KAAA,YAFA,EAAA,CAGA,IAHA,CAGA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,IAAA,CAAA,OAAA;AACA,OALA;AAMA;AA9GA;AAhHA,CAAA","sourcesContent":["<template>\r\n  <div class=\"task-detail\">\r\n    <van-cell-group>\r\n      <van-cell title=\"课题名称:\" :value=\"formData.topicThesisName\" />\r\n      <van-cell title=\"课题级别:\" :value=\"formData.topicThesisLeveName\" />\r\n      <van-cell title=\"课题类别:\" :value=\"formData.topicThesisTypeName\" />\r\n      <van-cell title=\"学科类别:\" :value=\"formData.subjectTypeName\" />\r\n      <van-cell title=\"学校:\" :value=\"formData.schoolName\" />\r\n      <van-cell title=\"负责人:\" :value=\"formData.leaderAuthorName\" />\r\n      <van-cell title=\"成员:\" :value=\"formData.memberName\" />\r\n      <van-cell title=\"状态:\" :value=\"formData.statusName\" />\r\n      <van-cell title=\"立项时间:\" :value=\"formData.formateProjectStartTime\" />\r\n      <van-cell title=\"预计结题时间:\" :value=\"formData.formateConclusionTime\" />\r\n      <van-cell class=\"column-flex\" title=\"课题概述:\">\r\n        <template #extra>\r\n          <p class=\"opinion\" v-html=\"formData.abstractDescription\"></p>\r\n        </template>\r\n      </van-cell>\r\n      <van-cell class=\"column-flex\" title=\"研究目标:\">\r\n        <template #extra>\r\n          <p class=\"opinion\" v-html=\"formData.researchTarget\"></p>\r\n        </template>\r\n      </van-cell>\r\n      <van-cell class=\"column-flex\" title=\"研究内容:\">\r\n        <template #extra>\r\n          <p class=\"opinion\" v-html=\"formData.researchContent\"></p>\r\n        </template>\r\n      </van-cell>\r\n      <van-cell class=\"column-flex\" title=\"研究成果:\">\r\n        <template #extra>\r\n          <div class=\"extra-conclusion\">\r\n            <h3\r\n              class=\"extra-index\"\r\n              v-if=\"formData.fileList || formData.researchResult\"\r\n            >\r\n              1、\r\n            </h3>\r\n            <table class=\"extra-table\" v-if=\"awardList.length > 0\">\r\n              <tr class=\"table-th-tr\">\r\n                <th>奖项名称</th>\r\n                <th>获奖人</th>\r\n                <th>级别</th>\r\n              </tr>\r\n              <tr class=\"table-td-tr\" v-for=\"item in awardList\" :key=\"item.id\">\r\n                <td>{{ item.issueName }}</td>\r\n                <td>{{ item.winnerName }}</td>\r\n                <td>{{ item.issueLevelName }}</td>\r\n              </tr>\r\n            </table>\r\n\r\n            <table class=\"extra-table no-data-table\" v-else>\r\n              <tr class=\"table-th-tr\">\r\n                <th>奖项名称</th>\r\n                <th>获奖人</th>\r\n                <th>级别</th>\r\n              </tr>\r\n              <div class=\"no-data-block\">\r\n                <p class=\"no-data\">暂无数据</p>\r\n              </div>\r\n            </table>\r\n\r\n            <h3 class=\"extra-index\" v-if=\"formData.fileList\">\r\n              {{ computedFj }}、<span\r\n                >(word、ppt、excel附件下载请到电脑端操作)\r\n              </span>\r\n            </h3>\r\n            <ul class=\"extra-fileList\">\r\n              <li\r\n                v-for=\"(item, index) in formData.fileList\"\r\n                @click=\"toPdfPreview(item)\"\r\n                :key=\"index\"\r\n              >\r\n                <img :src=\"item | ComputedImg\" alt=\"\" />\r\n                <p>{{ item.name }}</p>\r\n              </li>\r\n            </ul>\r\n            <h3 class=\"extra-index\" v-if=\"formData.researchResult\">\r\n              {{ computedResult }}、\r\n            </h3>\r\n            <p class=\"opinion\" v-html=\"formData.researchResult\"></p>\r\n          </div>\r\n        </template>\r\n      </van-cell>\r\n    </van-cell-group>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { parseTime } from '../../plugins/core/utils';\r\nimport { Cell, CellGroup, Dialog, Toast } from 'vant';\r\nimport {\r\n  getDataDetails,\r\n  deleteDataDetails,\r\n  getAwardAndResearchList\r\n} from '../../api/base/course-all';\r\nimport { compareAuth } from '../../plugins/common/auth';\r\nimport { toPreview } from '../../plugins/common/utils';\r\nexport default {\r\n  name: 'courseResearchTask',\r\n  components: {\r\n    [Cell.name]: Cell,\r\n    [CellGroup.name]: CellGroup,\r\n    [Dialog.name]: Dialog,\r\n    [Toast.name]: Toast\r\n  },\r\n  data() {\r\n    return {\r\n      id: this.$route.query.id,\r\n      formData: {\r\n        topicThesisName: '',\r\n        schoolName: '',\r\n        subjectTypeName: '',\r\n        topicThesisTypeName: '',\r\n        topicThesisLeveName: '',\r\n        leaderAuthorName: '',\r\n        memberName: '',\r\n        projectStartTime: '',\r\n        formateProjectStartTime: '',\r\n        conclusionTime: '',\r\n        formateConclusionTime: '',\r\n        statusName: '',\r\n        abstractDescription: '',\r\n        researchTarget: '',\r\n        researchContent: '',\r\n        researchResult: '',\r\n        fileList: []\r\n      },\r\n      pagingParams: {\r\n        //暂不分页\r\n        limit: 100,\r\n        offset: 0\r\n      },\r\n      awardList: [],\r\n      load: true,\r\n      iconDelete: require('../../assets/images/icon/icon-delete.png')\r\n    };\r\n  },\r\n  filters: {\r\n    ComputedImg(val) {\r\n      let url;\r\n      switch (val.type) {\r\n        case 'zip':\r\n          url = require('../../assets/images/zip.png');\r\n          break;\r\n        case 'avi':\r\n          url = require('../../assets/images/avi.png');\r\n          break;\r\n        case 'mp4':\r\n          url = require('../../assets/images/mp4.png');\r\n          break;\r\n        case 'doc':\r\n        case 'docx':\r\n        case 'DOC':\r\n        case 'DOCX':\r\n          url = require('../../assets/images/word.png');\r\n          break;\r\n        case 'xlsx':\r\n        case 'xls':\r\n        case 'XLSX':\r\n        case 'XLS':\r\n          url = require('../../assets/images/excel.png');\r\n          break;\r\n        case 'ppt':\r\n        case 'pptx':\r\n        case 'PPT':\r\n        case 'PPTX':\r\n          url = require('../../assets/images/ppt.png');\r\n          break;\r\n        case 'pdf':\r\n        case 'PDF':\r\n          url = require('../../assets/images/pdf.png');\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      return url;\r\n    }\r\n  },\r\n  computed: {\r\n    computedFj() {\r\n      let returnIndex = '';\r\n      if (this.formData.fileList.length > 0) {\r\n        returnIndex = 2;\r\n      }\r\n      return returnIndex;\r\n    },\r\n    computedResult() {\r\n      let returnIndex = '';\r\n      if (this.formData.fileList) {\r\n        if (this.formData.fileList.length > 0) {\r\n          returnIndex = 3;\r\n        } else {\r\n          if (this.formData.researchResult) {\r\n            returnIndex = 2;\r\n          }\r\n        }\r\n      } else {\r\n        if (this.formData.researchResult) {\r\n          returnIndex = 2;\r\n        }\r\n      }\r\n      return returnIndex;\r\n    }\r\n  },\r\n  created() {\r\n    this.init();\r\n    this.getDetails();\r\n    this.getAwardList();\r\n  },\r\n  methods: {\r\n    toPdfPreview(item) {\r\n      toPreview(item);\r\n    },\r\n    init() {\r\n      let _self = this;\r\n      let isDelete = compareAuth('task-delete');\r\n      if (isDelete) {\r\n        _self.$dd.biz.navigation.setMenu({\r\n          backgroundColor: '#20D1CD',\r\n          textColor: '#ADD8E611',\r\n          items: [\r\n            {\r\n              id: '1',\r\n              iconId: 'delete',\r\n              text: '删除',\r\n              url: _self.iconDelete\r\n            }\r\n          ],\r\n          onSuccess: function(res) {\r\n            // 调用成功时回调\r\n            if (res.id === '1') {\r\n              _self.deleteDetails();\r\n            }\r\n          },\r\n          onFail: function(err) {\r\n            // 调用失败时回调\r\n            console.log(err);\r\n          }\r\n        });\r\n      } else {\r\n        _self.$dd.biz.navigation.setRight({\r\n          show: false //控制按钮显示， true 显示， false 隐藏， 默认true\r\n        });\r\n      }\r\n    },\r\n    /**\r\n     * 编辑页面的获取详细信息\r\n     */\r\n    getDetails() {\r\n      this.load = true;\r\n      getDataDetails({ id: this.id }).then(({ data }) => {\r\n        for (let i in this.formData) {\r\n          this.formData[i] = data[i];\r\n        }\r\n        this.formData.formateProjectStartTime = parseTime(\r\n          this.formData.projectStartTime,\r\n          '{y}年{m}月{d}日'\r\n        );\r\n        this.formData.formateConclusionTime = parseTime(\r\n          this.formData.conclusionTime,\r\n          '{y}年{m}月{d}日'\r\n        );\r\n        if (data.resultUrl) {\r\n          let resultName = data.resultUrl.split(',');\r\n          let resultTitle = data.resultUrlName.split(',');\r\n          this.formData.fileList = data.resultUrl\r\n            .split(',')\r\n            .map((item, index) => {\r\n              let arr = resultName[index].split('.');\r\n              return Object.assign(\r\n                {},\r\n                {\r\n                  type: arr[arr.length - 1],\r\n                  url: item,\r\n                  name: resultTitle[index]\r\n                }\r\n              );\r\n            });\r\n        }\r\n        console.log(this.formData);\r\n        this.load = false;\r\n      });\r\n    },\r\n    /**\r\n     * 删除\r\n     */\r\n    deleteDetails() {\r\n      Dialog.confirm({\r\n        message: '确认要删除该条数据?',\r\n        confirmButtonColor: '#489EF3'\r\n      })\r\n        .then(() => {\r\n          deleteDataDetails({ id: this.id })\r\n            .then(res => {\r\n              if (res.status === 200) {\r\n                Toast.success('删除成功!');\r\n                this.$router.go(-1);\r\n              } else {\r\n                Toast.fail(res.message);\r\n              }\r\n            })\r\n            .catch(err => {\r\n              Toast.fail(err.message);\r\n            });\r\n        })\r\n        .catch(() => {\r\n          Toast.fail('已取消删除');\r\n        });\r\n    },\r\n    /**\r\n     * 获取 已关联研究成果列表\r\n     */\r\n    getAwardList() {\r\n      getAwardAndResearchList({\r\n        teachingResearchId: this.id,\r\n        ...this.pagingParams\r\n      }).then(({ data }) => {\r\n        this.awardList = data.records;\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.task-detail {\r\n  width: 100%;\r\n  height: 100%;\r\n  background: #f6f6f6;\r\n  padding-top: 0.2rem;\r\n  box-sizing: border-box;\r\n  /deep/ .van-cell {\r\n    padding: 0.4rem 0.32rem;\r\n    line-height: 0.4rem;\r\n    .opinion {\r\n      word-break: break-all;\r\n      margin-top: 0.2rem;\r\n      img {\r\n        max-width: 100%;\r\n      }\r\n    }\r\n    &::after {\r\n      border-bottom: 0.02rem solid rgba(25, 31, 37, 0.08);\r\n    }\r\n  }\r\n  /deep/ .van-cell__title,\r\n  .van-cell__value {\r\n    width: auto;\r\n    flex: auto;\r\n    -webkit-box-flex: inherit;\r\n    -webkit-flex: inherit;\r\n    padding-right: 0.2rem;\r\n  }\r\n  /deep/ .van-cell__title {\r\n    white-space: nowrap;\r\n    color: rgba(0, 0, 0, 0.45);\r\n  }\r\n  /deep/ .van-cell__value {\r\n    text-align: start;\r\n    color: rgba(0, 0, 0, 0.85);\r\n  }\r\n  .column-flex {\r\n    display: flex;\r\n    flex-direction: column;\r\n\r\n    .extra-conclusion {\r\n      margin: 0.2rem 0;\r\n      .extra-index {\r\n        font-size: 0.28rem;\r\n        padding: 0.4rem 0 0.3rem;\r\n        color: rgba(0, 0, 0, 0.85);\r\n      }\r\n      .extra-fileList {\r\n        display: flex;\r\n        width: 100%;\r\n        flex-wrap: wrap;\r\n        li {\r\n          width: 33.3%;\r\n          text-align: center;\r\n        }\r\n        img {\r\n          width: 1.6rem;\r\n          height: 1.6rem;\r\n        }\r\n      }\r\n      .extra-table {\r\n        width: 100%;\r\n        table-layout: fixed;\r\n        padding-top: 0.4rem;\r\n        .table-th-tr {\r\n          height: 0.76rem;\r\n          font-size: 0.28rem;\r\n          background: #f6f6f6;\r\n          color: rgba(0, 0, 0, 0.65);\r\n          th {\r\n            width: 27.5%;\r\n            text-align: start;\r\n            padding-left: 0.2rem;\r\n            vertical-align: middle;\r\n          }\r\n          th:first-child {\r\n            width: 45%;\r\n          }\r\n        }\r\n        .table-td-tr {\r\n          height: 0.96rem;\r\n          font-size: 0.28rem;\r\n          color: rgba(0, 0, 0, 0.85);\r\n          border-bottom: 0.02rem solid rgba(25, 31, 37, 0.08);\r\n          td {\r\n            width: 27.5%;\r\n            height: 0.96rem;\r\n            padding-left: 0.2rem;\r\n            overflow: hidden;\r\n            text-overflow: ellipsis;\r\n            white-space: nowrap;\r\n            border-right: 0.02rem solid rgba(25, 31, 37, 0.08);\r\n          }\r\n          td:first-child {\r\n            width: 45%;\r\n          }\r\n          td:last-child {\r\n            border-right: none;\r\n          }\r\n        }\r\n      }\r\n      .no-data-table {\r\n        display: block;\r\n        .table-th-tr {\r\n          width: 100%;\r\n          display: block;\r\n          line-height: 0.76rem;\r\n          th {\r\n            display: inline-block;\r\n            box-sizing: border-box;\r\n          }\r\n        }\r\n        .no-data-block {\r\n          width: 100%;\r\n          padding: 0.6rem 0;\r\n          text-align: center;\r\n          color: rgba(51, 51, 51, 0.55);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages/teaching-points-h5/views/course-research"}]}
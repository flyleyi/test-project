AM,GAAG,CAAb,EAAgB,OAAOzC,IAAI,CAAC,MAAD,EAAS,GAAT,CAAX;AACjB;AAED;AACA;AACA;;AACA,OAAO,IAAM0C,QAAQ,GAAG,SAAXA,QAAW,CAAAC,GAAG;AAAA,SAAKC,KAAK,CAAC,CAACD,GAAF,CAAL,GAAc,CAAd,GAAkB,CAACA,GAAxB;AAAA,CAApB;AAEP;AACA;AACA;AACA;;AACA,OAAO,SAASE,KAAT,CAAeC,CAAf,EAAkB;AACvB,SAAO,IAAIC,OAAJ,CAAY,UAAAC,OAAO,EAAI;AAC5BC,IAAAA,UAAU,CAACD,OAAD,EAAUF,CAAC,GAAG,IAAd,CAAV;AACD,GAFM,CAAP;AAGD;AAED;AACA;AACA;AACA;;AACA,OAAO,SAASI,oBAAT,GAAiE;AAAA,MAAnCC,KAAmC,uEAA3B,IAAIlB,IAAJ,GAAWmB,QAAX,KAAwB,CAAG;AACtE,MAAIf,SAAS,GAAG,IAAIJ,IAAJ,EAAhB;AACAI,EAAAA,SAAS,CAACgB,QAAV,CAAmBF,KAAK,GAAG,CAA3B;AACAd,EAAAA,SAAS,CAACiB,OAAV,CAAkB,CAAlB;AACAjB,EAAAA,SAAS,CAACE,QAAV,CAAmB,CAAnB,EAAsB,CAAtB,EAAyB,CAAzB;AACA,MAAID,OAAO,GAAG,IAAIL,IAAJ,CAASI,SAAS,CAACkB,OAAV,EAAT,CAAd;AACAjB,EAAAA,OAAO,CAACe,QAAR,CAAiBf,OAAO,CAACc,QAAR,KAAqB,CAAtC;AACAd,EAAAA,OAAO,CAACkB,eAAR,CAAwB,CAAC,CAAzB;AACA,SAAO,CAACnB,SAAD,EAAYC,OAAZ,CAAP;AACD;AAED;AACA;AACA;AACA;AACA;;AACA,OAAO,SAASmB,OAAT,GAA2B;AAAA,oCAAPC,KAAO;AAAPA,IAAAA,KAAO;AAAA;;AAChC,SAAOA,KAAK,CAACC,MAAN,CAAa,UAACC,CAAD,EAAIC,CAAJ;AAAA,WAAU;AAAA,aAAaD,CAAC,CAACC,CAAC,MAAD,mBAAD,CAAd;AAAA,KAAV;AAAA,GAAb,CAAP;AACD","sourcesContent":["import { ajaxQueryConjunctiveList } from '../../api/base';\r\nimport isNumber from 'lodash/isNumber';\r\nimport { dict } from '../core/dict';\r\n\r\n/**\r\n * 级联查询校区以下数据（学校、学部等）\r\n * @param {Object} params 参数 {code: //当前code, codeType: //当前code类型, childCodeType: //子code类型}\r\n * @param {boolean} extendDiabled 扩展禁用属性\r\n */\r\nexport function queryConjunctiveList(params, extendDiabled) {\r\n  return ajaxQueryConjunctiveList(params).then(({ data }) => {\r\n    extendDiabled &&\r\n      Array.isArray(data) &&\r\n      data.forEach(item => {\r\n        item.disabled = !item.isShow;\r\n      });\r\n    return data;\r\n  });\r\n}\r\n\r\n/**\r\n * base64位图片转码文件流\r\n * @param {String} dataurl base64字符串\r\n * @param {String} filename 文件名\r\n */\r\nexport function base64toFile(dataurl, filename = 'file') {\r\n  var arr = dataurl.split(',');\r\n  var mime = arr[0].match(/:(.*?);/)[1];\r\n  let suffix = mime.split('/')[1];\r\n  var bstr = atob(arr[1]);\r\n  var n = bstr.length;\r\n  var u8arr = new Uint8Array(n);\r\n  while (n--) {\r\n    u8arr[n] = bstr.charCodeAt(n);\r\n  }\r\n  let blob = new Blob([u8arr], { type: mime });\r\n  return new File([blob], `${filename}.${suffix}`, {\r\n    type: mime\r\n  });\r\n}\r\n\r\n/**\r\n * 校验日期\r\n * @param {Date} date 日期\r\n */\r\nexport function verifyDate(date) {\r\n  date = new Date(date);\r\n  return date.toString() !== 'Invalid Date';\r\n}\r\n\r\n/**\r\n * 标准化时间范围\r\n * 兼容类型\r\n *  - 时间戳\r\n *  - 字符串\r\n *  - Date对象\r\n * [\r\n *   new Date('2021-04-21'),\r\n *   new Date('2021-04-21')\r\n * ]\r\n * 处理成\r\n * [\r\n *   new Date('2021-04-21 00:00:00'),\r\n *   new Date('2021-04-21 23:59:59')\r\n * ]\r\n * @param {Array} daterange 时间范围数组\r\n * @returns {Array}\r\n */\r\nexport function normalDateRange(daterange) {\r\n  // 判断空，是否有2个日期元素\r\n  if (!daterange || daterange.length < 2) return daterange;\r\n  const startDate = new Date(daterange[0]), // 开始时间\r\n    endDate = new Date(daterange[1]); // 结束时间\r\n  // 判断日期是否有效\r\n  if (!verifyDate(startDate) || !verifyDate(endDate)) return daterange;\r\n  // 开始时间设置当天0点0分0秒0毫秒\r\n  startDate.setHours(0, 0, 0, 0);\r\n  // 结束时间设置当天23点59分59秒0毫秒\r\n  endDate.setHours(23, 59, 59, 0);\r\n  return [startDate, endDate];\r\n}\r\n\r\n/**\r\n * 格式化退补费类型\r\n * - 补退不补 3\r\n * - 补费 1\r\n * - 退费 2\r\n * @param {number} number 金额\r\n */\r\nexport function formatRefundType(number) {\r\n  // 判断是否是数字\r\n  if (!isNumber(number)) return '';\r\n  // 补退不补\r\n  if (number === 0) return dict('TBLX', '3');\r\n  // 补费\r\n  if (number > 0) return dict('TBLX', '1');\r\n  // 退费\r\n  if (number < 0) return dict('TBLX', '2');\r\n}\r\n\r\n/**\r\n * 转数字，非数字转0\r\n */\r\nexport const toNumber = num => (isNaN(+num) ? 0 : +num);\r\n\r\n/**\r\n * 延迟执行\r\n * @param {Number} s 延迟多少秒\r\n */\r\nexport function sleep(s) {\r\n  return new Promise(resolve => {\r\n    setTimeout(resolve, s * 1000);\r\n  });\r\n}\r\n\r\n/**\r\n * 获取自然月日期范围\r\n * @param {Number} month 月份,默认当前月\r\n */\r\nexport function getNaturalMonthRange(month = new Date().getMonth() + 1) {\r\n  let startDate = new Date();\r\n  startDate.setMonth(month - 1);\r\n  startDate.setDate(1);\r\n  startDate.setHours(0, 0, 0);\r\n  let endDate = new Date(startDate.getTime());\r\n  endDate.setMonth(endDate.getMonth() + 1);\r\n  endDate.setMilliseconds(-1);\r\n  return [startDate, endDate];\r\n}\r\n\r\n/**\r\n * 组合函数\r\n * @param  {...any} funcs 函数\r\n * @returns {Function}\r\n */\r\nexport function compose(...funcs) {\r\n  return funcs.reduce((a, b) => (...args) => a(b(...args)));\r\n}\r\n"]}]}
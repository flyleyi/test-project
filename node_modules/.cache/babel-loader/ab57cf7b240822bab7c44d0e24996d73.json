  this.chartData = newVal;\n        this.initEchartsBox();\n      }\n    },\n    initEchartsBox() {\n      let obj = this.chartData;\n      console.log(obj);\n      let option = {\n        title: {\n          text: obj.titleText,\n          x: 'center',\n          y: '45%',\n          textStyle: {\n            fontSize: 16\n          }\n        },\n        series: [\n          {\n            type: 'pie',\n            center: ['50%', '50%'],\n            radius: ['50%', '90%'],\n            data: obj.seriesData,\n            itemStyle: {\n              borderColor: '#fff',\n              borderWidth: obj.borderWidth\n            },\n            labelLine: {\n              length: 20,\n              length2: 10,\n              smooth: true\n            }\n          }\n        ]\n      };\n      if (this.chart !== null) this.chart.setOption(option);\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/pages/score-analysis-h5/components/echarts"}]}
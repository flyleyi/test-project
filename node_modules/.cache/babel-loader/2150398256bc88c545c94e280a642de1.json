AA,EAAA;AACA;AACA,UAAA,SAAA,EAAA,mBAAA,CAAA;AAFA;AAFA,OAAA;AAOA,KAzBA;;AA0BA;AACA;AACA;AACA;AACA,IAAA,UA9BA,sBA8BA,GA9BA,EA8BA;AACA,UAAA,GAAA,CAAA,EAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,mBADA;AAEA,UAAA,KAAA,EAAA;AACA;AACA,YAAA,EAAA,EAAA,GAAA,CAAA;AAFA;AAFA,SAAA;AAOA,OARA,MAQA;AACA,aAAA,QAAA,CAAA;AAAA,UAAA,IAAA,EAAA,SAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AACA;AACA,KA1CA;;AA2CA;AACA;AACA;AACA;AACA,IAAA,UA/CA,sBA+CA,GA/CA,EA+CA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,qBADA;AAEA,QAAA,KAAA,EAAA;AACA;AACA,UAAA,SAAA,EAAA,mBAAA,CAAA,IAFA;;AAGA;AACA,UAAA,EAAA,EAAA,GAAA,CAAA;AAJA;AAFA,OAAA;AASA,KAzDA;;AA0DA;AACA;AACA;AACA;AACA,IAAA,aA9DA,yBA8DA,GA9DA,EA8DA;AAAA;;AACA,WAAA,QAAA,CAAA,mBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA,GAAA,CAAA,OAAA,EAAA,OAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,CAAA;;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA,EAAA,IAAA;;AACA,eAAA,oBAAA,CAAA,GAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,WAAA;AACA,SAJA,EAKA,OALA,CAKA,YAAA;AACA,UAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAPA,CAAA;AAQA,OAfA;AAgBA,KA/EA;;AAgFA;AACA;AACA;AACA;AACA,IAAA,mBApFA,+BAoFA,GApFA,EAoFA;AAAA;;AACA,WAAA,QAAA,CAAA,sBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA,GAAA,CAAA,OAAA,EAAA,OAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,YAAA,CAAA;;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA,EAAA,IAAA;;AACA,eAAA,0BAAA,CAAA,GAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,WAAA;AACA,SAJA,EAKA,OALA,CAKA,YAAA;AACA,UAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAPA,CAAA;AAQA,OAfA;AAgBA;AArGA;AAxHA,CAAA","sourcesContent":["<template>\n  <!-- 版本公告列表 -->\n  <div class=\"app-container\">\n    <b-table\n      ref=\"bTable\"\n      :loading=\"true\"\n      :form-items=\"formItems\"\n      :headers=\"headers\"\n      :ajax-get-data=\"ajaxGetData\"\n      :toolbars=\"toolbars\"\n    ></b-table>\n  </div>\n</template>\n\n<script>\nimport BTable from '@/components/business/BTable';\nimport {\n  FORM_OPERATION_TYPE,\n  PUBLISH_STATUS\n} from '@/pages/sims/shared/constant';\nimport {\n  cancelPublishVersionNotice,\n  publishVersionNotice,\n  queryVersionNoticeList\n} from '@/pages/sims/api/version_notice';\nimport utils from '@/plugins/common/utils';\n\nexport default {\n  name: 'VersionNotice',\n  components: {\n    BTable\n  },\n  data() {\n    return {\n      /** 表单项 */\n      formItems: [\n        {\n          label: '版本号',\n          prop: 'version',\n          type: 'input',\n          attrs: {\n            maxlength: 30\n          }\n        },\n        {\n          label: '发布人',\n          prop: 'publishStaff',\n          type: 'input',\n          attrs: {\n            maxlength: 30\n          }\n        },\n        {\n          label: '发布状态',\n          prop: 'publishStatus',\n          type: 'select',\n          options: this.$D('publishStatus'),\n          attrs: {\n            clearable: true\n          }\n        }\n      ],\n      /** 列表列 */\n      headers: [\n        {\n          label: '版本号',\n          prop: 'version'\n        },\n        {\n          label: '公告标题',\n          prop: 'title',\n          attrs: {\n            'show-overflow-tooltip': true\n          }\n        },\n        {\n          label: '发布人',\n          prop: 'publishStaff',\n          width: '100'\n        },\n        {\n          label: '发布时间',\n          prop: 'publishTime',\n          width: '160',\n          formatter: (row, column, value) =>\n            value\n              ? utils.dateConverter('yyyy-MM-dd hh:mm:ss', new Date(value))\n              : '-'\n        },\n        {\n          label: '发布状态',\n          prop: 'publishStatus',\n          width: '100',\n          formatter: (row, column, value) => this.$D('publishStatus', value)\n        },\n        {\n          label: '操作',\n          colType: 'btns',\n          width: '200',\n          btns: [\n            {\n              label: '查看',\n              click: this.handleLook,\n              size: 'mini',\n              type: 'text'\n            },\n            {\n              label: '编辑',\n              click: this.handleEdit,\n              size: 'mini',\n              type: 'text',\n              /** 已发布隐藏 */\n              hidden: row => row.publishStatus === PUBLISH_STATUS.PUBLISH\n            },\n            {\n              label: '发布',\n              click: this.handlePublish,\n              size: 'mini',\n              type: 'text',\n              /** 已发布隐藏 */\n              hidden: row => row.publishStatus === PUBLISH_STATUS.PUBLISH\n            },\n            {\n              label: '取消发布',\n              click: this.handleCancelPublish,\n              size: 'mini',\n              type: 'text',\n              /** 未发布隐藏 */\n              hidden: row => row.publishStatus === PUBLISH_STATUS.UN_PUBLISH\n            }\n          ]\n        }\n      ],\n      /** 工具栏 */\n      toolbars: [\n        {\n          label: '新增',\n          size: 'mini',\n          type: 'primary',\n          click: this.handleNew\n        }\n      ]\n    };\n  },\n  activated() {\n    this.refreshList();\n  },\n  methods: {\n    /**\n     * 获取列表数据\n     * @param {*} params 查询参数\n     */\n    ajaxGetData(params) {\n      return queryVersionNoticeList(params);\n    },\n    /**\n     * 刷新列表\n     */\n    refreshList() {\n      this.$refs.bTable && this.$refs.bTable.onRefreshTable();\n    },\n    /**\n     * 新增\n     */\n    handleNew() {\n      this.$router.push({\n        name: 'VersionNoticeDetail',\n        query: {\n          /** 操作类型 */\n          operation: FORM_OPERATION_TYPE.NEW\n        }\n      });\n    },\n    /**\n     * 查看\n     * @param {*} row 记录\n     */\n    handleLook(row) {\n      if (row.id) {\n        this.$router.push({\n          name: 'VersionNoticeLook',\n          query: {\n            /** 版本公告id */\n            id: row.id\n          }\n        });\n      } else {\n        this.$message({ type: 'warning', message: '该数据无ID，请联系管理员' });\n      }\n    },\n    /**\n     * 编辑\n     * @param {*} row 记录\n     */\n    handleEdit(row) {\n      this.$router.push({\n        name: 'VersionNoticeDetail',\n        query: {\n          /** 操作类型 */\n          operation: FORM_OPERATION_TYPE.EDIT,\n          /** 版本公告id */\n          id: row.id\n        }\n      });\n    },\n    /**\n     * 发布\n     * @param {*} row 记录\n     */\n    handlePublish(row) {\n      this.$confirm('发布后，用户将看到该版本发布内容。', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        if (row.loading) return this.$message.warning('正在发布中...');\n        this.$set(row, 'loading', true);\n        return publishVersionNotice(row.id)\n          .then(() => {\n            this.$message.success('操作成功');\n            this.refreshList();\n          })\n          .finally(() => {\n            row.loading = false;\n          });\n      });\n    },\n    /**\n     * 取消发布\n     * @param {*} row 记录\n     */\n    handleCancelPublish(row) {\n      this.$confirm('取消发布后，用户将看不到该版本发布内容。', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        if (row.loading) return this.$message.warning('正在取消发布中...');\n        this.$set(row, 'loading', true);\n        return cancelPublishVersionNotice(row.id)\n          .then(() => {\n            this.$message.success('操作成功');\n            this.refreshList();\n          })\n          .finally(() => {\n            row.loading = false;\n          });\n      });\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/pages/sims/views/system/VersionNotice"}]}
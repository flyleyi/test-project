ons: [],\r\n    teachingTypeOptions: [],\r\n    gradeOptions: [],\r\n    sectionOptions: [],\r\n    statusOptions: [],\r\n    topicLevelOptions: [],\r\n    topicTypeOptions: []\r\n  },\r\n  getters: {\r\n    getSchoolOptions: state => state.schoolOptions,\r\n    getSubjectOptions: state => state.subjectOptions,\r\n    getAllSchoolOptions: state => state.allSchoolOptions,\r\n    getSemesterOptions: state => state.semesterOptions,\r\n    getTeachingTypeOptions: state => state.teachingTypeOptions,\r\n    getAllSubjectOptions: state => state.allSubjectOptions,\r\n    getGradeOptions: state => state.gradeOptions,\r\n    getSectionOptions: state => state.sectionOptions,\r\n    getStatusOptions: state => state.statusOptions,\r\n    getTopicLevelOptions: state => state.topicLevelOptions,\r\n    getTopicTypeOptions: state => state.topicTypeOptions\r\n  },\r\n  mutations: {\r\n    SET_SCHOOL_OPTIONS: (state, view) => {\r\n      state.schoolOptions = view;\r\n    },\r\n    SET_SUBJECT_OPTIONS: (state, view) => {\r\n      state.subjectOptions = view;\r\n    },\r\n    SET_ALL_SCHOOL_OPTIONS: (state, view) => {\r\n      state.allSchoolOptions = view;\r\n    },\r\n    SET_TEACHING_TYPE_OPTIONS: (state, view) => {\r\n      state.teachingTypeOptions = view;\r\n    },\r\n    SET_ALL_SUBJECT_OPTIONS: (state, view) => {\r\n      state.allSubjectOptions = view;\r\n    },\r\n    SET_SEMESTER_OPTIONS: (state, view) => {\r\n      state.semesterOptions = view;\r\n    },\r\n    SET_GRADE_OPTIONS: (state, view) => {\r\n      state.gradeOptions = view;\r\n    },\r\n    SET_SECTION_OPTIONS: (state, view) => {\r\n      state.sectionOptions = view;\r\n    },\r\n    SET_STATUS_OPTIONS: (state, view) => {\r\n      state.statusOptions = view;\r\n    },\r\n    SET_TOPIC_LEVEL_OPTIONS: (state, view) => {\r\n      state.topicLevelOptions = view;\r\n    },\r\n    SET_TOPIC_TYPE_OPTIONS: (state, view) => {\r\n      state.topicTypeOptions = view;\r\n    }\r\n  },\r\n  actions: {\r\n    loadSchoolOptions(context, payload) {\r\n      return axios(\r\n        {\r\n          url: '/routineCheck/getAuthSchoolList',\r\n          method: 'GET',\r\n          params: payload\r\n        },\r\n        'teaching-point-mobile-pre'\r\n      ).then(res => {\r\n        context.commit('SET_SCHOOL_OPTIONS', res.data);\r\n      });\r\n    },\r\n    loadSubjectOptions(context, payload) {\r\n      return axios(\r\n        {\r\n          url: '/routineCheck/getSubjectList',\r\n          method: 'GET',\r\n          params: payload\r\n        },\r\n        'teaching-point-mobile-pre'\r\n      ).then(res => {\r\n        context.commit('SET_SUBJECT_OPTIONS', res.data);\r\n      });\r\n    },\r\n\r\n    loadSemesterOptions(context, payload) {\r\n      return axios(\r\n        {\r\n          url: '/routineCheck/getSemesterList',\r\n          method: 'GET',\r\n          params: payload\r\n        },\r\n        'teaching-point-mobile-pre'\r\n      ).then(res => {\r\n        context.commit('SET_SEMESTER_OPTIONS', res.data);\r\n      });\r\n    },\r\n\r\n    loadChildrenOptions(context, payload) {\r\n      return axios(\r\n        {\r\n          url: '/dictionary/selectDictionaryToChildren',\r\n          method: 'GET',\r\n          params: payload.params\r\n        },\r\n        'teaching-point-mobile-pre'\r\n      ).then(res => {\r\n        context.commit(payload.types, res.data);\r\n      });\r\n    }\r\n  }\r\n};\r\n\r\nexport default options;\r\n"]}]}
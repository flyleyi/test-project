W,OAAOP,KAAP,CAhBwC,CAiBnD;;AACA,QAAI,OAAOS,IAAP,CAAYX,GAAZ,CAAJ,EAAsB;AACpBA,MAAAA,GAAG,GAAGA,GAAG,CAACQ,OAAJ,CACJI,MAAM,CAACC,EADH,EAEJ,CAACJ,IAAI,CAACK,WAAL,KAAqB,EAAtB,EAA0BC,MAA1B,CAAiC,IAAIH,MAAM,CAACC,EAAP,CAAUG,MAA/C,CAFI,CAAN;AAID;;AACD,QAAIC,CAAC,GAAG;AACN,YAAMR,IAAI,CAACS,QAAL,KAAkB,CADlB;AAEN,YAAMT,IAAI,CAACU,OAAL,EAFA;AAGN,YAAMV,IAAI,CAACW,QAAL,EAHA;AAIN,YAAMX,IAAI,CAACY,UAAL,EAJA;AAKN,YAAMZ,IAAI,CAACa,UAAL;AALA,KAAR;;AAOA,SAAK,IAAIC,CAAT,IAAcN,CAAd,EAAiB;AACf,UAAI,IAAIL,MAAJ,YAAeW,CAAf,QAAqBZ,IAArB,CAA0BX,GAA1B,CAAJ,EAAoC;AAClC,YAAII,IAAG,GAAGa,CAAC,CAACM,CAAD,CAAD,GAAO,EAAjB;;AACAvB,QAAAA,GAAG,GAAGA,GAAG,CAACQ,OAAJ,CACJI,MAAM,CAACC,EADH,EAEJD,MAAM,CAACC,EAAP,CAAUG,MAAV,KAAqB,CAArB,GAAyBZ,IAAzB,GAA+B,CAAC,OAAOA,IAAR,EAAaW,MAAb,CAAoBX,IAAG,CAACY,MAAxB,CAF3B,CAAN;AAID;AACF;;AACD,WAAOhB,GAAP;AACD;AAhDa,CAAhB,C,CAmDA;;AACA,KAAK,IAAIwB,GAAT,IAAgB3B,OAAhB,EAAyB;AACvBD,EAAAA,GAAG,CAAC6B,MAAJ,CAAWD,GAAX,EAAgB3B,OAAO,CAAC2B,GAAD,CAAvB;AACD","sourcesContent":["// vue 过滤器\r\nimport Vue from 'vue';\r\n\r\nconst filters = {\r\n  /**\r\n   * 将时间转换为指定格式\r\n   * @param {String/Date/Number} val 时间\r\n   * @param {String} fmt 转换格式 默认yyyy-MM-dd\r\n   * @param {String} noData 传值为空时，默认返回值\r\n   */\r\n  formatDateTime(val, fmt = 'yyyy-MM-dd', noData = '') {\r\n    let value = val || '';\r\n    if (!value) return noData;\r\n    // 时间戳\r\n    if (!isNaN(value)) {\r\n      let str = value.toString();\r\n      value = Number((str + '0000000000000').slice(0, 13));\r\n      value = Number(value);\r\n    }\r\n    // IOS 需要转成yyyy/MM/dd\r\n    else\r\n      value = value\r\n        .toString()\r\n        .replace(/-/g, '/')\r\n        .replace(/\\..$/g, '');\r\n    let date = new Date(value);\r\n    if (!date) return value;\r\n    // 时间格式化\r\n    if (/(y+)/.test(fmt)) {\r\n      fmt = fmt.replace(\r\n        RegExp.$1,\r\n        (date.getFullYear() + '').substr(4 - RegExp.$1.length)\r\n      );\r\n    }\r\n    let o = {\r\n      'M+': date.getMonth() + 1,\r\n      'd+': date.getDate(),\r\n      'h+': date.getHours(),\r\n      'm+': date.getMinutes(),\r\n      's+': date.getSeconds()\r\n    };\r\n    for (let k in o) {\r\n      if (new RegExp(`(${k})`).test(fmt)) {\r\n        let str = o[k] + '';\r\n        fmt = fmt.replace(\r\n          RegExp.$1,\r\n          RegExp.$1.length === 1 ? str : ('00' + str).substr(str.length)\r\n        );\r\n      }\r\n    }\r\n    return fmt;\r\n  }\r\n};\r\n\r\n// 注册全局过滤器\r\nfor (let key in filters) {\r\n  Vue.filter(key, filters[key]);\r\n}\r\n"]}]}
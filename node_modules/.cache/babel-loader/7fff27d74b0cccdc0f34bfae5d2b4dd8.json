AA,CACA,IADA,CACA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,YAAA;AACA,OAJA,EAKA,OALA,CAKA,YAAA;AACA,QAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAPA;AAQA,KAnFA;;AAoFA;AACA;AACA;AACA;AACA,IAAA,aAxFA,yBAwFA,GAxFA,EAwFA;AAAA;;AACA,WAAA,QAAA,CAAA,kBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA,GAAA,CAAA,OAAA,EAAA,OAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,YAAA,CAAA;;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA,EAAA,IAAA;;AACA,QAAA,+BAAA,CAAA,cAAA,CAAA,OAAA,EAAA,GAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,YAAA;AACA,SAJA,EAKA,OALA,CAKA,YAAA;AACA,UAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAPA;AAQA,OAfA;AAgBA,KAzGA;;AA0GA;AACA;AACA;AACA;AACA,IAAA,mBA9GA,+BA8GA,GA9GA,EA8GA;AAAA;;AACA,WAAA,QAAA,CAAA,qBAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA,GAAA,CAAA,OAAA,EAAA,OAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,YAAA,CAAA;;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA,EAAA,IAAA;;AACA,QAAA,+BAAA,CAAA,cAAA,CAAA,UAAA,EAAA,GAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,YAAA;AACA,SAJA,EAKA,OALA,CAKA,YAAA;AACA,UAAA,GAAA,CAAA,OAAA,GAAA,KAAA;AACA,SAPA;AAQA,OAfA;AAgBA,KA/HA;;AAgIA;AACA;AACA;AACA,IAAA,YAnIA,0BAmIA;AAAA;;AACA,WAAA,SAAA,GAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,cAAA;AACA,OAFA;AAGA;AAvIA;AAjJA,CAAA","sourcesContent":["<template>\n  <!-- 帮助文档列表 -->\n  <div class=\"app-container\">\n    <b-table\n      ref=\"bTable\"\n      :form-items=\"formItems\"\n      :headers=\"headers\"\n      :toolbars=\"toolbars\"\n      :ajax-get-data=\"ajaxGetData\"\n    ></b-table>\n  </div>\n</template>\n\n<script>\nimport BTable from '@/components/business/BTable';\nimport {\n  FORM_OPERATION_TYPE,\n  PUBLISH_STATUS\n} from '@/pages/sims/shared/constant';\nimport {\n  moveHelpDocument,\n  MOVE_TYPE,\n  queryHelpDocumentList,\n  updateHelpDocumentPublishStatus\n} from '@/pages/sims/api/help_document';\nimport utils from '@/plugins/common/utils';\n\nexport default {\n  name: 'HelpDocument',\n  components: {\n    BTable\n  },\n  data() {\n    return {\n      /** 表单项 */\n      formItems: [\n        {\n          label: '文档标题',\n          prop: 'title',\n          type: 'input',\n          attrs: {\n            maxlength: 30\n          }\n        },\n        {\n          label: '发布人',\n          prop: 'publishStaff',\n          type: 'input',\n          attrs: {\n            maxlength: 30\n          }\n        },\n        {\n          label: '发布状态',\n          prop: 'publishStatus',\n          type: 'select',\n          options: this.$D('publishStatus'),\n          attrs: {\n            clearable: true\n          }\n        }\n      ],\n      /** 列表列 */\n      headers: [\n        {\n          label: '文章分类',\n          prop: 'documentType',\n          width: '100',\n          formatter: (row, column, value) => this.$D('documentType', value)\n        },\n        {\n          label: '文档标题',\n          prop: 'title',\n          'min-width': '120',\n          'show-overflow-tooltip': true\n        },\n        {\n          label: '发布人',\n          prop: 'publishStaff',\n          width: '100'\n        },\n        {\n          label: '发布时间',\n          prop: 'publishTime',\n          width: '150',\n          formatter: (row, column, value) =>\n            value\n              ? utils.dateConverter('yyyy-MM-dd hh:mm:ss', new Date(value))\n              : '-'\n        },\n        {\n          label: '发布状态',\n          prop: 'publishStatus',\n          width: '80',\n          formatter: (row, column, value) => this.$D('publishStatus', value)\n        },\n        {\n          label: '操作',\n          colType: 'btns',\n          width: '210',\n          btns: [\n            {\n              label: '查看',\n              click: this.handleLook,\n              size: 'mini',\n              type: 'text'\n            },\n            {\n              label: '编辑',\n              click: this.handleEdit,\n              size: 'mini',\n              type: 'text',\n              /** 已发布隐藏 */\n              hidden: row => row.publishStatus !== PUBLISH_STATUS.UN_PUBLISH\n            },\n            {\n              label: '上移',\n              click: this.handleMoveUp,\n              size: 'mini',\n              type: 'text',\n              /** 第一项隐藏 */\n              hidden: (row, rowIndex) => {\n                if (!this.$refs.bTable) return true;\n                const len = this.$refs.bTable.tableData.length;\n                return len >= 2 && rowIndex === 0;\n              }\n            },\n            {\n              label: '下移',\n              click: this.handleMoveDown,\n              size: 'mini',\n              type: 'text',\n              /** 最后一项隐藏 */\n              hidden: (row, rowIndex) => {\n                if (!this.$refs.bTable) return true;\n                const len = this.$refs.bTable.tableData.length;\n                return len >= 2 && rowIndex === len - 1;\n              }\n            },\n            {\n              label: '发布',\n              click: this.handlePublish,\n              size: 'mini',\n              type: 'text',\n              /** 已发布隐藏 */\n              hidden: row => row.publishStatus !== PUBLISH_STATUS.UN_PUBLISH\n            },\n            {\n              label: '取消发布',\n              click: this.handleCancelPublish,\n              size: 'mini',\n              type: 'text',\n              /** 未发布隐藏 */\n              hidden: row => row.publishStatus !== PUBLISH_STATUS.PUBLISH\n            }\n          ]\n        }\n      ],\n      /** 工具栏 */\n      toolbars: [\n        {\n          label: '新增',\n          size: 'mini',\n          type: 'primary',\n          click: this.handleNew\n        }\n      ]\n    };\n  },\n  activated() {\n    this.refreshTable();\n  },\n  methods: {\n    /**\n     * 获取列表数据\n     * @param {*} params 查询参数\n     */\n    ajaxGetData(params) {\n      return queryHelpDocumentList(params);\n    },\n    /**\n     * 查看\n     * @param {*} row 记录\n     */\n    handleLook(row) {\n      if (row.id) {\n        this.$router.push({\n          name: 'HelpDocumentLook',\n          query: {\n            /** 帮助中心id */\n            id: row.id\n          }\n        });\n      } else {\n        this.$message({ type: 'warning', message: '该数据无ID，请联系管理员' });\n      }\n    },\n    /**\n     * 新增\n     */\n    handleNew() {\n      this.$router.push({\n        name: 'HelpDocumentDetail',\n        query: {\n          /** 操作类型 */\n          operation: FORM_OPERATION_TYPE.NEW\n        }\n      });\n    },\n    /**\n     * 编辑\n     * @param {*} row 记录\n     */\n    handleEdit(row) {\n      this.$router.push({\n        name: 'HelpDocumentDetail',\n        query: {\n          /** 操作类型 */\n          operation: FORM_OPERATION_TYPE.EDIT,\n          /** 版本公告id */\n          id: row.id\n        }\n      });\n    },\n    /**\n     * 上移\n     * @param {*} row 记录\n     */\n    handleMoveUp(row) {\n      if (row.loading) return this.$message.warning('数据正在操作中...');\n      this.$set(row, 'loading', true);\n      moveHelpDocument(MOVE_TYPE.UP, row.id)\n        .then(() => {\n          this.$message.success('操作成功');\n          this.refreshTable();\n        })\n        .finally(() => {\n          row.loading = false;\n        });\n    },\n    /**\n     * 下移\n     * @param {*} row 记录\n     */\n    handleMoveDown(row) {\n      if (row.loading) return this.$message.warning('数据正在操作中...');\n      this.$set(row, 'loading', true);\n      moveHelpDocument(MOVE_TYPE.DOWN, row.id)\n        .then(() => {\n          this.$message.success('操作成功');\n          this.refreshTable();\n        })\n        .finally(() => {\n          row.loading = false;\n        });\n    },\n    /**\n     * 发布\n     * @param {*} row 记录\n     */\n    handlePublish(row) {\n      this.$confirm('发布后，用户将看到该条发布内容。', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        if (row.loading) return this.$message.warning('数据正在操作中...');\n        this.$set(row, 'loading', true);\n        updateHelpDocumentPublishStatus(PUBLISH_STATUS.PUBLISH, row.id)\n          .then(() => {\n            this.$message.success('操作成功');\n            this.refreshTable();\n          })\n          .finally(() => {\n            row.loading = false;\n          });\n      });\n    },\n    /**\n     * 取消发布\n     * @param {*} row 记录\n     */\n    handleCancelPublish(row) {\n      this.$confirm('取消发布后，用户将看不到该条发布内容。', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        if (row.loading) return this.$message.warning('数据正在操作中...');\n        this.$set(row, 'loading', true);\n        updateHelpDocumentPublishStatus(PUBLISH_STATUS.UN_PUBLISH, row.id)\n          .then(() => {\n            this.$message.success('操作成功');\n            this.refreshTable();\n          })\n          .finally(() => {\n            row.loading = false;\n          });\n      });\n    },\n    /**\n     * 刷新列表\n     */\n    refreshTable() {\n      this.$nextTick().then(() => {\n        this.$refs.bTable.onRefreshTable();\n      });\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/pages/sims/views/system/HelpDocument"}]}
,EAAb;AACA,MAAMG,IAAI,GAAGL,GAAG,CAACa,KAAJ,CAAUC,QAAQ,CAACC,IAAnB,EAAyB,IAAzB,CAAb;;AACA,MAAIV,IAAI,CAACW,KAAT,EAAgB;AACdd,IAAAA,MAAM,GAAGG,IAAI,CAACW,KAAd;AACD;;AACD,SAAOd,MAAP;AACD;AAED;AACA;AACA;AACA;AACA;AACA;;AACA,OAAO,SAASe,YAAT,CAAsBC,IAAtB,EAA4BC,GAA5B,EAAiC;AACtC,MAAIC,GAAG,GAAG,CAAV;;AACA,MAAIF,IAAJ,EAAU;AACRA,IAAAA,IAAI,CAACG,gBAAL,CAAsB,QAAtB,EAAgCC,QAAQ,CAACH,GAAD,EAAM,GAAN,CAAxC;AACD,GAFD,MAEO;AACLC,IAAAA,GAAG,GAAGG,QAAQ,CAACC,eAAT,CAAyBC,SAAzB,IAAsCF,QAAQ,CAACG,IAAT,CAAcD,SAA1D;AACD;;AACD,SAAOL,GAAP;AACD;AAED;AACA;AACA;AACA;AACA;AACA;AACA;;AACA,OAAO,SAASO,QAAT,CAAkBC,GAAlB,EAAuBC,IAAvB,EAA6BC,IAA7B,EAAmC;AACxC,MAAIC,MAAM,GAAG,EAAb;AACA,MAAIC,GAAG,GAAGJ,GAAG,CAACK,MAAd;AACA,MAAIC,IAAI,GAAGF,GAAG,GAAGH,IAAjB;AACA,MAAIM,IAAI,GAAGC,QAAQ,CAACJ,GAAG,GAAGH,IAAP,CAAnB;;AACA,OAAK,IAAIQ,CAAC,GAAG,CAAb,EAAgBA,CAAC,GAAGF,IAApB,EAA0BE,CAAC,EAA3B,EAA+B;AAC7B,QAAIC,CAAC,GAAGV,GAAG,CAACW,KAAJ,CAAUF,CAAC,GAAGR,IAAd,EAAoB,CAACQ,CAAC,GAAG,CAAL,IAAUR,IAA9B,CAAR;AACA,QAAIW,CAAC,GAAGH,CAAC,KAAK,CAAN,GAAU,EAAV,GAAeP,IAAvB;AACAC,IAAAA,MAAM,GAAGA,MAAM,GAAGS,CAAT,GAAaF,CAAtB;AACD;;AACDP,EAAAA,MAAM,GAAGA,MAAM,GAAGD,IAAT,GAAgBF,GAAG,CAACW,KAAJ,CAAUP,GAAG,GAAGE,IAAhB,EAAsBF,GAAtB,CAAzB;AACA,SAAOD,MAAP;AACD;AAED;AACA;AACA;AACA;AACA;AACA;;AACA,SAAST,QAAT,CAAkBmB,EAAlB,EAAsBC,IAAtB,EAA4B;AAC1B,MAAIC,GAAG,GAAGC,IAAI,CAACC,GAAL,EAAV;AACA,SAAO,YAAW;AAChB,QAAIC,OAAO,GAAG,IAAd;AACA,QAAIC,IAAI,GAAGC,SAAX;AACA,QAAIH,GAAG,GAAGD,IAAI,CAACC,GAAL,EAAV;;AACA,QAAIA,GAAG,GAAGF,GAAN,IAAaD,IAAjB,EAAuB;AACrBD,MAAAA,EAAE,CAACQ,KAAH,CAASH,OAAT,EAAkBC,IAAlB;AACAJ,MAAAA,GAAG,GAAGC,IAAI,CAACC,GAAL,EAAN;AACD;AACF,GARD;AASD","sourcesContent":["/*\n * @Author: wangxu\n * @Date: 2021-09-18 14:23:28\n * @LastEditTime: 2021-10-18 18:09:40\n * @LastEditors: Please set LastEditors\n * @Description: In User Settings Edit\n * @FilePath: \\mutilwap\\src\\pages\\score-analysis-h5\\plugins\\common\\utils.js\n */\nimport { ajaxQueryConjunctiveList } from '../../api/base';\nimport url from 'url';\n/**\n * 级联查询校区以下数据（学校、学部等）\n * @param {Object} params 参数 {code: //当前code, codeType: //当前code类型, childCodeType: //子code类型}\n * @param {boolean} extendDiabled 扩展禁用属性\n */\nexport function queryConjunctiveList(params, extendDiabled) {\n  return ajaxQueryConjunctiveList(params).then(({ data }) => {\n    extendDiabled &&\n      Array.isArray(data) &&\n      data.forEach(item => {\n        item.disabled = !item.isShow;\n      });\n    return data;\n  });\n}\n\n/**\n * @description: 获取链接中的参数\n * @param {*}\n * @return {*}\n */\nexport function getUrlParams() {\n  let params = {};\n  const data = url.parse(location.href, true);\n  if (data.query) {\n    params = data.query;\n  }\n  return params;\n}\n\n/**\n * @description: 获得当前页滚动值\n * @param {*} node 节点元素\n * @param {*} fun 回调函数\n * @return {*}\n */\nexport function getScrollTop(node, fun) {\n  let num = 0;\n  if (node) {\n    node.addEventListener('scroll', throttle(fun, 500));\n  } else {\n    num = document.documentElement.scrollTop || document.body.scrollTop;\n  }\n  return num;\n}\n\n/**\n * @description: 字符串分割\n * @param {*} str 目标字符串\n * @param {*} step 分割间隔\n * @param {*} fill 填充内容\n * @return {*}\n */\nexport function strSlice(str, step, fill) {\n  let newStr = '';\n  let len = str.length;\n  let num1 = len % step;\n  let num2 = parseInt(len / step);\n  for (let i = 0; i < num2; i++) {\n    let o = str.slice(i * step, (i + 1) * step);\n    let m = i === 0 ? '' : fill;\n    newStr = newStr + m + o;\n  }\n  newStr = newStr + fill + str.slice(len - num1, len);\n  return newStr;\n}\n\n/**\n * @description: 节流函数，时间戳方案\n * @param {*} fn 回调函数\n * @param {*} wait 时间间隔\n * @return {*}\n */\nfunction throttle(fn, wait) {\n  let pre = Date.now();\n  return function() {\n    let context = this;\n    let args = arguments;\n    let now = Date.now();\n    if (now - pre >= wait) {\n      fn.apply(context, args);\n      pre = Date.now();\n    }\n  };\n}\n"]}]}
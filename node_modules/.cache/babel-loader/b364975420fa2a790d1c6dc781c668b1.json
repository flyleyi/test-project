n-loading v-if=\"load\" />\r\n    <div v-else>\r\n      <div class=\"lists\" v-if=\"type === 'school'\">\r\n        <van-radio-group\r\n          v-if=\"getSchoolOptions.length > 0\"\r\n          v-model=\"formData.schoolCode\"\r\n        >\r\n          <van-cell-group>\r\n            <van-cell\r\n              v-for=\"item in getSchoolOptions\"\r\n              clickable\r\n              :key=\"item.DICTIONARY_KEY\"\r\n              :title=\"item.DICTIONARY_TEXT\"\r\n              @click=\"getRadioName(item.DICTIONARY_TEXT, item.DICTIONARY_KEY)\"\r\n            >\r\n              <template #right-icon>\r\n                <van-radio :name=\"item.DICTIONARY_KEY\">\r\n                  <template #icon=\"props\">\r\n                    <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                  </template>\r\n                </van-radio>\r\n              </template>\r\n            </van-cell>\r\n          </van-cell-group>\r\n        </van-radio-group>\r\n        <div v-else class=\"no-data\">暂无数据</div>\r\n      </div>\r\n      <div class=\"lists\" v-if=\"type === 'subject'\">\r\n        <van-radio-group\r\n          v-if=\"getSubjectOptions.length > 0\"\r\n          v-model=\"formData.subject\"\r\n        >\r\n          <van-cell-group>\r\n            <van-cell\r\n              v-for=\"item in getSubjectOptions\"\r\n              clickable\r\n              :key=\"item.DICTIONARY_KEY\"\r\n              :title=\"item.DICTIONARY_TEXT\"\r\n              @click=\"getRadioName(item.DICTIONARY_TEXT, item.DICTIONARY_KEY)\"\r\n            >\r\n              <template #right-icon>\r\n                <van-radio :name=\"item.DICTIONARY_KEY\">\r\n                  <template #icon=\"props\">\r\n                    <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                  </template>\r\n                </van-radio>\r\n              </template>\r\n            </van-cell>\r\n          </van-cell-group>\r\n        </van-radio-group>\r\n        <div v-else class=\"no-data\">暂无数据</div>\r\n      </div>\r\n      <div class=\"confirm-btn\">\r\n        <van-button @click=\"cofirm\" type=\"info\">确定</van-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport {\r\n  RadioGroup,\r\n  Radio,\r\n  Button,\r\n  Cell,\r\n  CellGroup,\r\n  Icon,\r\n  Loading\r\n} from 'vant';\r\nexport default {\r\n  name: 'listSelector',\r\n  components: {\r\n    [RadioGroup.name]: RadioGroup,\r\n    [Radio.name]: Radio,\r\n    [Button.name]: Button,\r\n    [Cell.name]: Cell,\r\n    [CellGroup.name]: CellGroup,\r\n    [Icon.name]: Icon,\r\n    [Loading.name]: Loading\r\n  },\r\n  data() {\r\n    return {\r\n      id: this.$route.query.id,\r\n      type: this.$route.query.type,\r\n      load: true,\r\n      formData: {}\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['getSchoolOptions', 'getSubjectOptions', 'getGradeOptions'])\r\n  },\r\n  async created() {\r\n    this.init();\r\n    this.formData = JSON.parse(localStorage.getItem('formData'));\r\n    if (this.type === 'school') {\r\n      await this.$store.dispatch('loadSchoolOptions', {\r\n        staffCode: this.formData.teacherCode\r\n      });\r\n    } else if (this.type === 'subject') {\r\n      await this.$store.dispatch('loadSubjectOptions', {\r\n        staffCode: this.formData.teacherCode\r\n      });\r\n    }\r\n    this.load = false;\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.$dd.biz.navigation.setTitle({\r\n        title: this.type === 'school' ? '选择学校' : '选择学科'\r\n      });\r\n      this.$dd.biz.navigation.setRight({\r\n        show: false //控制按钮显示， true 显示， false 隐藏， 默认true\r\n      });\r\n    },\r\n    async getRadioName(name, code) {\r\n      if (this.type === 'school') {\r\n        this.formData.schoolName = name;\r\n        this.formData.schoolCode = code;\r\n        await this.getTypeOptions(\r\n          this.formData.schoolCode,\r\n          'SET_GRADE_OPTIONS'\r\n        );\r\n      } else if (this.type === 'subject') {\r\n        this.formData.subjectName = name;\r\n        this.formData.subject = code;\r\n      }\r\n    },\r\n    async getTypeOptions(code, type) {\r\n      let payload = {\r\n        params: { parentKey: code },\r\n        types: type\r\n      };\r\n      await this.$store.dispatch('loadChildrenOptions', payload);\r\n      if (\r\n        Array.isArray(this.getGradeOptions) &&\r\n        this.getGradeOptions.length > 0\r\n      ) {\r\n        this.formData.grade = this.getGradeOptions[0].dictionaryKey;\r\n        this.formData.gradeName = this.getGradeOptions[0].dictionaryText;\r\n      } else {\r\n        this.formData.grade = this.formData.gradeName = '';\r\n      }\r\n    },\r\n    cofirm() {\r\n      localStorage.setItem('formData', JSON.stringify(this.formData));\r\n      this.$router.go(-1); //避免返回两次 同一页面\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.list-selector {\r\n  background: #f6f6f6;\r\n  width: 100%;\r\n  height: 100%;\r\n  /deep/ .van-loading {\r\n    text-align: center;\r\n    padding-top: 4rem;\r\n    .van-loading__spinner {\r\n      width: 1rem;\r\n      height: 1rem;\r\n    }\r\n  }\r\n  /deep/ .van-radio__icon--checked .van-icon {\r\n    color: #1daacc;\r\n    background-color: transparent;\r\n    border-color: transparent;\r\n  }\r\n  /deep/ .van-radio__icon--round .van-icon {\r\n    border: none;\r\n  }\r\n  .lists {\r\n    width: 100%;\r\n    max-height: calc(100vh - 1.96rem);\r\n    overflow-y: auto;\r\n    .no-data {\r\n      color: #969799;\r\n      font-size: 0.28rem;\r\n      line-height: 1rem;\r\n      text-align: center;\r\n      padding-top: 1rem;\r\n    }\r\n  }\r\n  .confirm-btn {\r\n    width: 100%;\r\n    padding: 0 0.32rem;\r\n    box-sizing: border-box;\r\n    margin-top: 0.3rem;\r\n    button {\r\n      width: 100%;\r\n      height: 0.96rem;\r\n      font-size: 0.28rem;\r\n      border-radius: 0.08rem;\r\n      background: linear-gradient(#40adf6, #528bee);\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages/teaching-points-h5/views/normal-examine/components"}]}
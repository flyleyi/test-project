   <slot :item=\"item\" :index=\"index\"></slot>\n          </template>\n        </div>\n      </van-list>\n    </van-pull-refresh>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'refresh-list',\n  props: ['getListData', 'cardType', 'otherParams'],\n  components: {},\n  data() {\n    return {\n      list: [\n        // {\n        //   arrivalTime: 1626883200000,\n        //   audit: 'admin',\n        //   auditName: 'admin',\n        //   auditTime: 1626061863000,\n        //   cityCode: '|33|3301',\n        //   cityName: '杭州市',\n        //   contentNum: 1,\n        //   costPrice: '220.00',\n        //   createTime: 1626061177000,\n        //   creator: '88406346',\n        //   creatorName: '张晓飞',\n        //   deleted: 0,\n        //   deptId: 138,\n        //   deptName: '一年级',\n        //   districtCode: '|33|330108',\n        //   districtName: '滨江区',\n        //   editor: 'admin',\n        //   groupId: 1,\n        //   orderId: 569,\n        //   orderNo: 'CG202107120007-01',\n        //   outOfStockNum: 0,\n        //   parentDeptId: 137,\n        //   parentDeptName: '海亮小学滨江区',\n        //   parentId: 567,\n        //   provinceCode: '|33|33',\n        //   provinceName: '浙江省',\n        //   remarks: '',\n        //   sellingPrice: '0.00',\n        //   status: 30100,\n        //   submit: '88406346',\n        //   submitName: '张晓飞',\n        //   submitTime: 1626061845000,\n        //   type: 1,\n        //   updateTime: 1626061863000,\n        //   orderType: 1\n        // },\n        // {\n        //   arrivalTime: 1626883200000,\n        //   audit: 'admin',\n        //   auditName: 'admin',\n        //   auditTime: 1626061863000,\n        //   cityCode: '|33|3301',\n        //   cityName: '杭州市',\n        //   contentNum: 1,\n        //   costPrice: '220.00',\n        //   createTime: 1626061177000,\n        //   creator: '88406346',\n        //   creatorName: '张晓飞',\n        //   deleted: 0,\n        //   deptId: 138,\n        //   deptName: '一年级',\n        //   districtCode: '|33|330108',\n        //   districtName: '滨江区',\n        //   editor: 'admin',\n        //   groupId: 1,\n        //   orderId: 569,\n        //   orderNo: 'CG202107120007-01',\n        //   outOfStockNum: 0,\n        //   parentDeptId: 137,\n        //   parentDeptName: '海亮小学滨江区',\n        //   parentId: 567,\n        //   provinceCode: '|33|33',\n        //   provinceName: '浙江省',\n        //   remarks: '',\n        //   sellingPrice: '0.00',\n        //   status: 30100,\n        //   submit: '88406346',\n        //   submitName: '张晓飞',\n        //   submitTime: 1626061845000,\n        //   type: 1,\n        //   updateTime: 1626061863000,\n        //   orderType: 2\n        // },\n        // {\n        //   arrivalTime: 1626883200000,\n        //   audit: 'admin',\n        //   auditName: 'admin',\n        //   auditTime: 1626061863000,\n        //   cityCode: '|33|3301',\n        //   cityName: '杭州市',\n        //   contentNum: 1,\n        //   costPrice: '220.00',\n        //   createTime: 1626061177000,\n        //   creator: '88406346',\n        //   creatorName: '张晓飞',\n        //   deleted: 0,\n        //   deptId: 138,\n        //   deptName: '一年级',\n        //   districtCode: '|33|330108',\n        //   districtName: '滨江区',\n        //   editor: 'admin',\n        //   groupId: 1,\n        //   orderId: 570,\n        //   orderNo: 'CG202107120007-01',\n        //   outOfStockNum: 0,\n        //   parentDeptId: 137,\n        //   parentDeptName: '海亮小学滨江区',\n        //   parentId: 567,\n        //   provinceCode: '|33|33',\n        //   provinceName: '浙江省',\n        //   remarks: '',\n        //   sellingPrice: '0.00',\n        //   status: 30100,\n        //   submit: '88406346',\n        //   submitName: '张晓飞',\n        //   submitTime: 1626061845000,\n        //   type: 1,\n        //   updateTime: 1626061863000,\n        //   orderType: 3\n        // }\n      ],\n      loading: false,\n      finished: false,\n      refreshing: false,\n      pageInfo: {\n        //填报信息总页数\n        total: 0\n      },\n      pageParams: {\n        //填报分页参数\n        limit: 10,\n        offset: 0\n      }\n    };\n  },\n  methods: {\n    //点击订单跳转详情\n    clickCard(item) {\n      this.$emit('clickCard', item);\n    },\n    /**\n     * 获取列表数据\n     */\n    getData() {\n      if (this.pageParams.offset > this.pageInfo.total) return;\n      this.load = true;\n      let promiseData;\n\n      promiseData = this.getListData({\n        ...this.pageParams,\n        ...this.otherParams\n      });\n      promiseData\n        .then(({ data }) => {\n          if (data.current == 1 && data.records.length == 0) {\n            this.list = [];\n            this.finished = true;\n          }\n          this.list = this.list.concat(data.records);\n          this.pageInfo.total = data.total || 0;\n          if (this.pageInfo.total <= data.current * data.size) {\n            this.finished = true;\n          } else {\n            this.pageParams.offset =\n              (Math.floor(this.pageParams.offset / this.pageParams.limit) + 1) *\n              this.pageParams.limit;\n          }\n          this.loading = false;\n          setTimeout(() => {\n            this.load = false;\n          });\n        })\n        .catch(() => {\n          this.error = true;\n        });\n    },\n    onLoad() {\n      if (this.refreshing) {\n        this.list = [];\n        this.refreshing = false;\n      }\n      this.loading = true;\n      this.getData();\n    },\n    onRefresh() {\n      this.list = [];\n      this.finished = false;\n      // this.loading = true;\n      this.pageInfo.total = 0;\n      this.pageParams.offset = 0;\n      this.onLoad();\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.order-list {\n  padding: 12px;\n}\n</style>\n"],"sourceRoot":"src/pages/catering-h5/views/refresh-list"}]}
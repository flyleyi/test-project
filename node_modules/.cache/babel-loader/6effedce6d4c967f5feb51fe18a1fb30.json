AAA;AACA,YAAA,GAAA;AACA;AACA,SAJA;AAKA,OANA;AAOA,aAAA,GAAA;AACA;AA1BA,GAXA;AAuCA,EAAA,OAvCA,qBAuCA;AACA,SAAA,SAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA;AACA,SAAA,IAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,GAAA,EAAA;AACA,SAAA,WAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA,GACA,KAAA,MAAA,CAAA,KAAA,CAAA,WADA,GAEA,EAFA;AAGA,GA7CA;AA8CA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,gBAFA,4BAEA,KAFA,EAEA;AACA,WAAA,SAAA,CAAA,KAAA,EAAA,MAAA,GAAA,CAAA,KAAA,SAAA,CAAA,KAAA,EAAA,MAAA;AACA,KAJA;AAKA;AACA,IAAA,eANA,2BAMA,KANA,EAMA,UANA,EAMA;AACA;AACA,UAAA,QAAA,GAAA,KAAA,SAAA,CAAA,KAAA,CAAA;;AACA,UAAA,UAAA,IAAA,QAAA,CAAA,WAAA,CAAA,MAAA,EAAA;AACA,QAAA,QAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,SAAA,GAAA,IAAA;AACA,SAFA;AAGA,QAAA,QAAA,CAAA,UAAA,GAAA,QAAA,CAAA,WAAA,CAAA,MAAA;AACA,OALA,MAKA;AACA;AACA,YAAA,WAAA,GAAA,CAAA;AACA,QAAA,QAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,QAAA,EAAA;AACA,YAAA,WAAA;AACA,WAFA,MAEA;AACA,YAAA,IAAA,CAAA,SAAA,GAAA,KAAA;AACA;AACA,SANA;AAOA,QAAA,QAAA,CAAA,UAAA,GAAA,WAAA;AACA;AACA,KA1BA;AA2BA;AACA,IAAA,mBA5BA,+BA4BA,KA5BA,EA4BA,MA5BA,EA4BA;AACA;AACA,UAAA,QAAA,GAAA,KAAA,SAAA,CAAA,KAAA,CAAA,CAFA,CAGA;;AACA,UAAA,QAAA,CAAA,WAAA,CAAA,MAAA,EAAA,QAAA,EAAA;AAEA,MAAA,QAAA,CAAA,WAAA,CAAA,MAAA,EAAA,SAAA,GAAA,CAAA,QAAA,CAAA,WAAA,CAAA,MAAA,EACA,SADA;;AAEA,UAAA,QAAA,CAAA,WAAA,CAAA,MAAA,EAAA,SAAA,EAAA;AACA,QAAA,QAAA,CAAA,UAAA;AACA,OAFA,MAEA;AACA,QAAA,QAAA,CAAA,UAAA;AACA;AACA,KAzCA;AA0CA;AACA,IAAA,iBA3CA,+BA2CA;AACA,UAAA,KAAA,aAAA,IAAA,KAAA,KAAA,EAAA;AACA,aAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,UAAA,GAAA,IAAA,CAAA,WAAA,CAAA,MAAA;AACA,UAAA,IAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,WAFA;AAGA,SALA;AAMA,OAPA,MAOA;AACA;AACA,aAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,UAAA,GACA,IAAA,CAAA,WAAA,IAAA,IAAA,CAAA,WAAA,GAAA,CAAA,GAAA,IAAA,CAAA,WAAA,GAAA,CADA;AAEA,UAAA,IAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,gBAAA,CAAA,KAAA,CAAA,QAAA,EAAA;AACA,cAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA;AACA,WAJA;AAKA,SARA;AASA;AACA,KA/DA;AAgEA;AACA,IAAA,gBAjEA,8BAiEA;AACA,UAAA,KAAA,aAAA,IAAA,CAAA,EAAA;AACA,aAAA,MAAA,CAAA,SAAA;AACA,OAFA,MAEA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA,sBAAA,EAAA,KAAA,SAAA;AACA,aAAA,OAAA,CAAA,IAAA;AACA;AACA;AAxEA;AA9CA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <div class=\"app\">\n    <div class=\"send-num\" v-if=\"from == 'detail'\">\n      已发送<span style=\"color: #5ECFA8\">{{ totalSend }}</span\n      >人\n    </div>\n    <div\n      class=\"class-item\"\n      :class=\"item.isShow && index != classList.length - 1 ? 'active' : ''\"\n      v-for=\"(item, index) in classList\"\n      :key=\"item.id\"\n    >\n      <div class=\"class-info\">\n        <img\n          v-if=\"item.studentList.length == 0\"\n          src=\"@/pages/jx-questionnaire-h5/assets/images/icons/unchecked4_icon.png\"\n        />\n        <img\n          v-else\n          :src=\"\n            item.checkedNum == 0\n              ? require('@/pages/jx-questionnaire-h5/assets/images/icons/unchecked3_icon.png')\n              : item.checkedNum == item.studentList.length\n              ? require('@/pages/jx-questionnaire-h5/assets/images/icons/checked3_icon.png')\n              : require('@/pages/jx-questionnaire-h5/assets/images/icons/checked2_icon.png')\n          \"\n          @click=\"handleChooseAll(index, item.checkedNum)\"\n        />\n        <div class=\"class-name\" @click=\"openStundentList(index)\">\n          <div\n            class=\"base-info\"\n            :style=\"\n              item.studentList.length == 0 ? 'color:rgba(0, 0, 0, 0.45);' : ''\n            \"\n          >\n            <div>{{ item.name }}</div>\n            <div>（{{ item.checkedNum + '/' + item.studentList.length }}）</div>\n          </div>\n          <img\n            class=\"arrow-icon\"\n            :src=\"\n              item.isShow\n                ? require('@/pages/jx-questionnaire-h5/assets/images/icons/down_arrow_icon.png')\n                : require('@/pages/jx-questionnaire-h5/assets/images/icons/right_arrow_icon.png')\n            \"\n          />\n        </div>\n      </div>\n      <!-- 学生列表 -->\n      <div class=\"list\" v-if=\"item.isShow\">\n        <template v-if=\"item.studentList.length > 0\">\n          <div\n            class=\"item\"\n            v-for=\"(item2, index2) in item.studentList\"\n            :key=\"item2.id\"\n            @click=\"handleChooseStudent(index, index2)\"\n          >\n            <img\n              :src=\"\n                item2.isChecked\n                  ? require('@/pages/jx-questionnaire-h5/assets/images/icons/checked_icon.png')\n                  : require('@/pages/jx-questionnaire-h5/assets/images/icons/unchecked2_icon.png')\n              \"\n            />\n            <div class=\"name\">{{ item2.name }}</div>\n            <div>{{ item2.studentCode }}</div>\n            <div v-if=\"item2.disabled\" class=\"status\">\n              {{ isPublished == 0 ? '待发送' : '已发送' }}\n            </div>\n          </div>\n        </template>\n        <template v-else>\n          <div class=\"no-student\">暂无激活学生</div>\n        </template>\n      </div>\n    </div>\n    <div class=\"blank\"></div>\n    <div class=\"bottom-btn\">\n      <div class=\"check-all\">\n        <img\n          :src=\"\n            totalCheckNum == 0\n              ? require('@/pages/jx-questionnaire-h5/assets/images/icons/unchecked3_icon.png')\n              : totalCheckNum == total\n              ? require('@/pages/jx-questionnaire-h5/assets/images/icons/checked3_icon.png')\n              : require('@/pages/jx-questionnaire-h5/assets/images/icons/checked2_icon.png')\n          \"\n          @click=\"handleChooseTotal\"\n        />\n        <div class=\"all\" @click=\"handleChooseTotal\">全选</div>\n        <div>\n          已选：<span style=\"color:#5ECFA8\">{{ totalCheckNum }}</span\n          >{{ '/' + total }}\n        </div>\n      </div>\n      <div class=\"done\" @click=\"handleChooseDone\">选好了</div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'StudentList',\n  components: {},\n  data() {\n    return {\n      from: '', // 上一个页面\n      classList: [],\n      status: '', // 问卷状态 2为草稿\n      isPublished: '' // 发布状态\n    };\n  },\n  computed: {\n    // 已选总数\n    totalCheckNum() {\n      let num = 0;\n      this.classList.forEach(item => {\n        num += item.checkedNum;\n      });\n      return num;\n    },\n    total() {\n      let num = 0;\n      this.classList.forEach(item => {\n        num += item.studentList.length;\n      });\n      return num;\n    },\n    totalSend() {\n      let num = 0;\n      this.classList.forEach(item => {\n        item.studentList.forEach(item2 => {\n          if (item2.disabled) {\n            num++;\n          }\n        });\n      });\n      return num;\n    }\n  },\n  created() {\n    this.classList = JSON.parse(this.$route.query.listStr);\n    this.from = this.$route.query.from ? this.$route.query.from : '';\n    this.isPublished = this.$route.query.isPublished\n      ? this.$route.query.isPublished\n      : '';\n  },\n  methods: {\n    // 展示学生列表\n    openStundentList(index) {\n      this.classList[index].isShow = !this.classList[index].isShow;\n    },\n    // 全选/取消选中\n    handleChooseAll(index, checkedNum) {\n      // 当前操作班级\n      let curClass = this.classList[index];\n      if (checkedNum != curClass.studentList.length) {\n        curClass.studentList.forEach(item => {\n          item.isChecked = true;\n        });\n        curClass.checkedNum = curClass.studentList.length;\n      } else {\n        // 编辑状态下默认选中学生的个数\n        let disabledNum = 0;\n        curClass.studentList.forEach(item => {\n          if (item.disabled) {\n            disabledNum++;\n          } else {\n            item.isChecked = false;\n          }\n        });\n        curClass.checkedNum = disabledNum;\n      }\n    },\n    // 选择单个学生\n    handleChooseStudent(index, index2) {\n      // 当前操作班级\n      let curClass = this.classList[index];\n      // 编辑状态下，之前选中的学生不允许反选\n      if (curClass.studentList[index2].disabled) return;\n\n      curClass.studentList[index2].isChecked = !curClass.studentList[index2]\n        .isChecked;\n      if (curClass.studentList[index2].isChecked) {\n        curClass.checkedNum++;\n      } else {\n        curClass.checkedNum--;\n      }\n    },\n    // 选择所有班级学生\n    handleChooseTotal() {\n      if (this.totalCheckNum != this.total) {\n        this.classList.forEach(item => {\n          item.checkedNum = item.studentList.length;\n          item.studentList.forEach(item2 => {\n            item2.isChecked = true;\n          });\n        });\n      } else {\n        // 编辑状态下默认选中学生的个数\n        this.classList.forEach(item => {\n          item.checkedNum =\n            item.disabledNum && item.disabledNum > 0 ? item.disabledNum : 0;\n          item.studentList.forEach(item2 => {\n            if (!item2.disabled) {\n              item2.isChecked = false;\n            }\n          });\n        });\n      }\n    },\n    // 确定选择\n    handleChooseDone() {\n      if (this.totalCheckNum == 0) {\n        this.$toast('请选择发布对象');\n      } else {\n        this.$store.commit('classes/SAVE_CLASSES', this.classList);\n        this.$router.back();\n      }\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.app {\n  .send-num {\n    font-size: 0.28rem;\n    font-family: PingFangSC-Regular, PingFang SC;\n    color: rgba(0, 0, 0, 0.85);\n    padding: 0.24rem;\n  }\n  .class-item {\n    background-color: #ffffff;\n    .class-info {\n      margin: 0 0.24rem;\n      display: flex;\n      align-items: center;\n      position: relative;\n      //伪元素绝对定位\n      &::after {\n        content: ' ';\n        position: absolute;\n        left: 0;\n        bottom: 0;\n        right: 0;\n        height: 1px;\n        background: rgba(0, 0, 0, 0.08);\n        transform-origin: 0 0;\n        transform: scaleY(0.5);\n      }\n      & > img {\n        height: 0.32rem;\n        padding: 0.25rem 0.1rem 0.25rem 0;\n      }\n      .class-name {\n        padding: 0.25rem 0;\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        flex: 1;\n        .base-info {\n          font-size: 0.28rem;\n          font-family: PingFangSC-Regular, PingFang SC;\n          color: rgba(0, 0, 0, 0.85);\n          display: flex;\n        }\n        .arrow-icon {\n          height: 0.4rem;\n        }\n      }\n    }\n    .list {\n      padding: 0.26rem 0.24rem 0 0.24rem;\n      .item {\n        padding-bottom: 0.26rem;\n        display: flex;\n        align-items: center;\n        & > img {\n          height: 0.32rem;\n        }\n        & > div {\n          font-size: 0.28rem;\n          font-family: PingFangSC-Regular, PingFang SC;\n          color: rgba(0, 0, 0, 0.85);\n        }\n        .name {\n          width: 2.24rem;\n          padding: 0 0.24rem 0 0.1rem;\n          overflow: hidden;\n          text-overflow: ellipsis;\n          white-space: nowrap;\n        }\n        .status {\n          color: #5ecfa8;\n          padding-left: 0.2rem;\n        }\n      }\n      .no-student {\n        padding-bottom: 0.24rem;\n        font-size: 0.28rem;\n        font-family: PingFangSC-Regular, PingFang SC;\n        color: rgba(0, 0, 0, 0.45);\n        display: flex;\n        align-items: center;\n        justify-content: center;\n      }\n    }\n  }\n  .blank {\n    height: 1.76rem;\n  }\n  .bottom-btn {\n    width: 100%;\n    height: 1.76rem;\n    background-color: #ffffff;\n    box-shadow: 0 -0.04rem 0.1rem 0 rgba(0, 0, 0, 0.04);\n    display: flex;\n    justify-content: space-between;\n    position: fixed;\n    bottom: 0;\n    left: 0;\n    .check-all {\n      height: 0.6rem;\n      margin: 0.24rem;\n      display: flex;\n      align-items: center;\n      & > img {\n        height: 0.32rem;\n      }\n      & > div {\n        font-size: 0.28rem;\n        font-family: PingFangSC-Regular, PingFang SC;\n        color: rgba(0, 0, 0, 0.85);\n      }\n      .all {\n        padding: 0 0.24rem 0 0.1rem;\n      }\n    }\n    .done {\n      width: 1.4rem;\n      height: 0.6rem;\n      background: #5ecfa8;\n      border-radius: 0.06rem;\n      font-size: 0.26rem;\n      font-family: PingFangSC-Regular, PingFang SC;\n      color: #ffffff;\n      margin: 0.24rem;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n    }\n  }\n  .active {\n    margin-bottom: 0.2rem;\n  }\n}\n</style>\n"],"sourceRoot":"src/pages/jx-questionnaire-h5/views/student-list"}]}
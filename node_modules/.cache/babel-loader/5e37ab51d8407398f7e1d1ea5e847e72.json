AAUA;AACA;AACA;AACA;;;AACA,IAAME,kBAAkB,GAAG,SAArBA,kBAAqB,CAASC,MAAT,EAAiB;AAC1C,MAAIC,IAAJ;;AACA,MAAID,MAAM,GAAG,CAAb,EAAgB;AACdC,IAAAA,IAAI,GAAG,MAAP;AACD,GAFD,MAEO,IAAID,MAAM,GAAG,CAAb,EAAgB;AACrBC,IAAAA,IAAI,GAAG,MAAP;AACD,GAFM,MAEA;AACLA,IAAAA,IAAI,GAAG,MAAP;AACD;;AACD,mBAAUA,IAAV,mBAAkBR,iBAAiB,CAACS,IAAI,CAACC,GAAL,CAASH,MAAT,CAAD,CAAnC;AACD,CAVD;;AAYA,IAAMI,OAAO,GAAG;AACd;AACF;AACA;AACA;AACEb,EAAAA,eAAe,EAAfA,eALc;;AAMd;AACF;AACA;AACA;AACEE,EAAAA,iBAAiB,EAAjBA,iBAVc;;AAWd;AACF;AACA;AACEG,EAAAA,kBAAkB,EAAlBA,kBAdc;;AAed;AACF;AACA;AACEG,EAAAA,kBAAkB,EAAlBA;AAlBc,CAAhB,C,CAqBA;;AACA,KAAK,IAAIM,GAAT,IAAgBD,OAAhB,EAAyB;AACvBd,EAAAA,GAAG,CAACgB,MAAJ,CAAWD,GAAX,EAAgBD,OAAO,CAACC,GAAD,CAAvB;AACD;;AAED,SACEd,eADF,EAEEE,iBAFF,EAGEG,kBAHF,EAIEG,kBAJF,EAKED,iBALF","sourcesContent":["// vue 过滤器\r\nimport Vue from 'vue';\r\n\r\nconst formatNullValue = function(val) {\r\n  return !val && val !== 0 ? '-' : val;\r\n};\r\n\r\nconst formatAmountValue = function(val) {\r\n  //空值异常处理\r\n  if (val === null || val === undefined) {\r\n    return '-';\r\n  }\r\n  return val.toLocaleString();\r\n};\r\n\r\nconst formatBooleanValue = function(val) {\r\n  let res;\r\n  if (val === null || val === undefined) {\r\n    res = '-';\r\n  } else {\r\n    res = val === true ? '是' : '否';\r\n  }\r\n  return res;\r\n};\r\n\r\nconst formatStatusValue = function(val) {\r\n  let res;\r\n  if (val === null || val === undefined) {\r\n    res = '-';\r\n  } else {\r\n    res = val === true ? '启用' : '禁用';\r\n  }\r\n  return res;\r\n};\r\n\r\n/**\r\n * 格式化退补类型\r\n * @param {number} number 金额\r\n */\r\nconst formatBackFillType = function(number) {\r\n  let text;\r\n  if (number > 0) {\r\n    text = '补费金额';\r\n  } else if (number < 0) {\r\n    text = '退费金额';\r\n  } else {\r\n    text = '不退不补';\r\n  }\r\n  return `${text}：${formatAmountValue(Math.abs(number))}`;\r\n};\r\n\r\nconst filters = {\r\n  /**\r\n   * 格式化字段为空时的数据转为\"-\"\r\n   * @param {string} val\r\n   */\r\n  formatNullValue,\r\n  /**\r\n   * 格式化金额，千分位‘,’号隔开\r\n   * @param {string} val\r\n   */\r\n  formatAmountValue,\r\n  /**\r\n   * 格式化数据为true和false的字段\r\n   */\r\n  formatBooleanValue,\r\n  /**\r\n   * 格式化退补类型\r\n   */\r\n  formatBackFillType\r\n};\r\n\r\n// 注册全局过滤器\r\nfor (let key in filters) {\r\n  Vue.filter(key, filters[key]);\r\n}\r\n\r\nexport {\r\n  formatNullValue,\r\n  formatAmountValue,\r\n  formatBooleanValue,\r\n  formatBackFillType,\r\n  formatStatusValue\r\n};\r\n"]}]}
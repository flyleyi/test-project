state, data) {\r\n      localStorage.setItem('CheckArea', JSON.stringify(data));\r\n    },\r\n    setDangerItem({ state, actions }, data) {\r\n      if (data.isSelf) {\r\n        let checkList = JSON.parse(localStorage.getItem(CheckList)) || [];\r\n        for (let i = 0; i < checkList.length; i++) {\r\n          if (+checkList[i].id == +data.params.id) {\r\n            data.params.pid = data.params.id;\r\n            data.params.id = new Date().getTime();\r\n            checkList[i].children.push(data.params);\r\n          }\r\n        }\r\n        localStorage.setItem(CheckList, JSON.stringify(checkList));\r\n      } else {\r\n        let list = JSON.parse(localStorage.getItem('CheckData'));\r\n        list.forEach((item, index) => {\r\n          if (item.id == data.params.pid) {\r\n            item.subItemList.forEach((iitem, iindex) => {\r\n              if (iitem.id == data.params.id) {\r\n                data.params.lastId = new Date().getTime();\r\n                iitem.children.push(data.params);\r\n              }\r\n            });\r\n          }\r\n        });\r\n        localStorage.setItem('CheckData', JSON.stringify(list));\r\n      }\r\n    },\r\n    setCheckList(state, data) {\r\n      if (localStorage.getItem(CheckList) == null) {\r\n        localStorage.setItem(CheckList, []);\r\n      }\r\n      let id = 1;\r\n      let list = [];\r\n      if (localStorage.getItem(CheckList)) {\r\n        list = JSON.parse(localStorage.getItem(CheckList));\r\n        id = list.length + 1;\r\n      }\r\n      let obj = JSON.parse(JSON.stringify(data));\r\n      obj.id = id;\r\n      if (!obj.children) {\r\n        obj.children = [\r\n          {\r\n            dangerDesc: obj.dangerDesc,\r\n            id: new Date().getTime(),\r\n            dangerImag: obj.dangerImag,\r\n            itemName: obj.itemName,\r\n            pid: obj.id\r\n          }\r\n        ];\r\n      }\r\n      list.push(obj);\r\n      localStorage.setItem(CheckList, JSON.stringify(list));\r\n    },\r\n    setCheckData(state, data) {\r\n      localStorage.setItem(CheckData, JSON.stringify(data));\r\n    },\r\n    deleteCheckList(state, data) {\r\n      localStorage.setItem(CheckList, JSON.stringify(data));\r\n    },\r\n    deleteRuleCheckList(state, data) {\r\n      localStorage.setItem(RuleCheckList, JSON.stringify(data));\r\n    },\r\n    getCheckList(state) {\r\n      return JSON.parse(localStorage.getItem(CheckList));\r\n    }\r\n  },\r\n  actions: {\r\n    setCheckData({ state, commit }, data) {\r\n      commit('setCheckData', data);\r\n    }\r\n    // setEditDangerItem({ state, commit }, data) {\r\n    //   let flag = true;\r\n\r\n    //   if (typeof data.isSelf == 'string') {\r\n    //     if (data.isSelf == 'true') {\r\n    //       flag = true;\r\n    //     } else {\r\n    //       flag = false;\r\n    //     }\r\n    //   }\r\n    //   if (data.isSelf && flag) {\r\n    //     // let list = JSON.parse(localStorage.getItem(CheckList)) || [];\r\n    //     // list.forEach((item, index)=>{\r\n    //     //   if (item.id == data.params.pid) {\r\n    //     //     item.children.forEach((iitem, iindex)=>{\r\n    //     //       if (iitem.id == data.params.id) {\r\n    //     //         iitem.dangerDesc = data.params.dangerDesc\r\n    //     //         iitem.dangerImag = data.params.dangerImag\r\n    //     //       }\r\n    //     //     })\r\n    //     //   }\r\n    //     // })\r\n    //     // commit('deleteCheckList', list)\r\n    //     let areaList = JSON.parse(localStorage.getItem('CheckArea'));\r\n    //     for (let i = 0; i < areaList.length; i++) {\r\n    //       if (areaList[i].id == data.query.firstid) {\r\n    //         for (let j = 0; j < areaList[i].subAreaList.length; j++) {\r\n    //           if (areaList[i].subAreaList[j].id == data.query.areaId) {\r\n    //             for (\r\n    //               let w = 0;\r\n    //               w < areaList[i].subAreaList[j].selfChild.length;\r\n    //               w++\r\n    //         
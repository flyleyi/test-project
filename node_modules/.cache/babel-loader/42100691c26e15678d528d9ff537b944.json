AAN,CAAcF,WAAd,CADF,IAEEA,WAAW,CAACG,OAAZ,CAAoB,UAAAC,IAAI,EAAI;AAC1BA,MAAAA,IAAI,CAACC,QAAL,GAAgB,CAACD,IAAI,CAACE,MAAtB;AACD,KAFD,CAFF;AAKA,WAAON,WAAP;AACD,GAfM,CAAP;AAgBD;AAED;AACA;AACA;AACA;AACA;;AACA,OAAO,SAASO,eAAT,CAAyBf,MAAzB,EAAiC;AACtC,SAAOF,KAAK,CACV;AACEG,IAAAA,GAAG,EAAE,wCADP;AAEEC,IAAAA,MAAM,EAAE,KAFV;AAGEF,IAAAA,MAAM,EAAEA,MAAM,IAAI;AAHpB,GADU,EAMV,SANU,CAAZ;AAQD;AAED;AACA;AACA;AACA;AACA;;AACA,OAAO,SAASgB,mBAAT,CAA6BC,KAA7B,EAAoC;AACzC,SAAOnB,KAAK,CACV;AACEG,IAAAA,GAAG,EAAE,4CADP;AAEEC,IAAAA,MAAM,EAAE,KAFV;AAGEF,IAAAA,MAAM,EAAE;AACNkB,MAAAA,QAAQ,EAAED;AADJ;AAHV,GADU,EAQV,SARU,CAAL,CASLV,IATK,CASA;AAAA,QAAGJ,IAAH,SAAGA,IAAH;AAAA,WAAcA,IAAd;AAAA,GATA,CAAP;AAUD;AAED;AACA;AACA;AACA;;AACA,OAAO,SAASgB,0BAAT,CAAoCnB,MAApC,EAA4C;AACjD,SAAOF,KAAK,CACV;AACEG,IAAAA,GAAG,EAAE,0CADP;AAEEC,IAAAA,MAAM,EAAE,KAFV;AAGEF,IAAAA,MAAM,EAANA;AAHF,GADU,EAMV,SANU,CAAZ;AAQD;AAED;AACA;AACA;AACA;;AACA,OAAO,SAASoB,oBAAT,CAA8BC,UAA9B,EAA0C;AAC/C,SAAOvB,KAAK,CACV;AACEG,IAAAA,GAAG,EAAE,0CADP;AAEEC,IAAAA,MAAM,EAAE,KAFV;AAGEF,IAAAA,MAAM,EAAE;AACNqB,MAAAA,UAAU,EAAEZ,KAAK,CAACC,OAAN,CAAcW,UAAd,IAA4BA,UAAU,CAACC,IAAX,CAAgB,GAAhB,CAA5B,GAAmD;AADzD;AAHV,GADU,EAQV,SARU,CAAZ;AAUD","sourcesContent":["import axios from '@/plugins/axios';\n\n/**\n * 查询用户列表\n * @param {*} params 查询参数\n * @return {Promise}\n */\nexport function queryUserList(params) {\n  return axios(\n    {\n      url: '/commonData/staffInfo/manage/search',\n      method: 'POST',\n      data: params\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 保存用户设置\n * @param {*} data 数据\n * @return {Promise}\n */\nexport function saveUserSetting(data) {\n  return axios(\n    {\n      url: '/dataAuth/userDataAuthSetUp',\n      method: 'POST',\n      data: data || {}\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 查询自建组织机构叶子节点列表\n * @param {*} params\n * @param {*} extendDisabled 扩展disabled属性\n * @return {Promise}\n */\nexport function queryOrgSelfList(params, extendDisabled = false) {\n  return axios(\n    {\n      url: '/commonData/staffInfo/manage/pageInit',\n      method: 'GET',\n      params\n    },\n    'simsbff'\n  ).then(({ data: { orgSelfList } }) => {\n    // 扩展diabled属性\n    extendDisabled &&\n      Array.isArray(orgSelfList) &&\n      orgSelfList.forEach(item => {\n        item.disabled = !item.status;\n      });\n    return orgSelfList;\n  });\n}\n\n/**\n * 查询学部列表\n * @param {*} params 查询参数\n * @return {Promise}\n */\nexport function queryDepartList(params) {\n  return axios(\n    {\n      url: '/depart-api/departApi/searchDepartList',\n      method: 'GET',\n      params: params || {}\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 指定层级查询组织机构\n * @param {*} level 组织机构层级\n * @return {Promise}\n */\nexport function queryOrgSelfByLevel(level) {\n  return axios(\n    {\n      url: '/commonData/orgSelfApi/queryOrgSelfByLevel',\n      method: 'GET',\n      params: {\n        orgLevel: level\n      }\n    },\n    'simsbff'\n  ).then(({ data }) => data);\n}\n\n/**\n * 学部code集合查询班级列表\n * @param {*} params 查询参数\n */\nexport function queryAllClassByDepartCodes(params) {\n  return axios(\n    {\n      url: '/class-api/classApi/queryAllClassForAuth',\n      method: 'GET',\n      params\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 班级编辑集合查询详情\n * @param {Array} classCodes 班级code列表\n */\nexport function queryClassByCodeList(classCodes) {\n  return axios(\n    {\n      url: '/class-api/classApi/queryClassByCodeList',\n      method: 'GET',\n      params: {\n        classCodes: Array.isArray(classCodes) ? classCodes.join(',') : ''\n      }\n    },\n    'simsbff'\n  );\n}\n"]}]}
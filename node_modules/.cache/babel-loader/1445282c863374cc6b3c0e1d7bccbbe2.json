kBAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,eAAA,CAAA;AAAA,UAAA,GAAA,EAAA,MAAA,CAAA;AAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,mBAAA,OAAA,CAAA,MAAA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,WAHA,MAGA;AACA,mBAAA,IAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,SAPA;AAQA,OAbA,EAcA,KAdA,CAcA,YAAA;AACA,eAAA,IAAA,CAAA,OAAA;AACA,OAhBA;AAiBA;AAjGA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"bk-detail\">\r\n    <!-- <button @click=\"editDetails\">编辑</button> -->\r\n    <van-cell-group>\r\n      <van-cell title=\"姓名:\" :value=\"formData.teacherName\" />\r\n      <van-cell title=\"学校:\" :value=\"formData.schoolName\" />\r\n      <van-cell title=\"学科:\" :value=\"formData.subjectName\" />\r\n      <van-cell title=\"年级:\" :value=\"formData.gradeName\" />\r\n      <van-cell title=\"班级:\" :value=\"formData.classes\" />\r\n      <van-cell title=\"检查日期:\" :value=\"formData.filterCheckDate\" />\r\n      <van-cell title=\"备课内容:\" :value=\"formData.content\" />\r\n      <van-cell title=\"备课等级:\" :value=\"formData.scoreLevel\" />\r\n      <van-cell title=\"检查人:\" :value=\"formData.createrName\" />\r\n      <van-cell class=\"column-flex\" title=\"指导意见:\">\r\n        <template #extra>\r\n          <p class=\"opinion\" v-html=\"formData.opinion\"></p>\r\n        </template>\r\n      </van-cell>\r\n    </van-cell-group>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport { parseTime } from '../../plugins/core/utils';\r\nimport { Cell, CellGroup, Dialog, Toast } from 'vant';\r\nimport { getPrepareDetails, deleteCheckList } from '../../api/base/types-all';\r\nimport { compareAuth } from '../../plugins/common/auth';\r\nexport default {\r\n  name: 'bkDetail',\r\n  components: {\r\n    [Cell.name]: Cell,\r\n    [CellGroup.name]: CellGroup,\r\n    [Dialog.name]: Dialog,\r\n    [Toast.name]: Toast\r\n  },\r\n  data() {\r\n    return {\r\n      id: this.$route.query.id,\r\n      formData: {\r\n        schoolName: '',\r\n        teacherName: '',\r\n        teacherCode: '',\r\n        subjectName: '',\r\n        gradeName: '',\r\n        classes: '',\r\n        content: '',\r\n        scoreLevel: '',\r\n        checkDate: '',\r\n        filterCheckDate: '',\r\n        opinion: '',\r\n        createrName: ''\r\n      },\r\n      iconDelete: require('../../assets/images/icon/icon-delete.png'),\r\n      iconEdit: require('../../assets/images/icon/icon-edit.png')\r\n    };\r\n  },\r\n  created() {\r\n    this.getDetails();\r\n  },\r\n  computed: {\r\n    ...mapGetters(['staffCode'])\r\n  },\r\n  methods: {\r\n    init() {\r\n      let _self = this;\r\n      let item = [];\r\n      let isDelete = compareAuth('prepare-course-delete');\r\n      let isEdit = compareAuth('prepare-course-edit');\r\n      if (isDelete || isEdit) {\r\n        if (_self.staffCode !== _self.formData.teacherCode) {\r\n          if (isDelete) {\r\n            item.push({\r\n              id: '1',\r\n              iconId: 'delete',\r\n              text: '删除',\r\n              url: _self.iconDelete\r\n            });\r\n          }\r\n          if (isEdit) {\r\n            item.push({\r\n              id: '2',\r\n              iconId: 'edit',\r\n              text: '编辑',\r\n              url: _self.iconEdit\r\n            });\r\n          }\r\n          _self.$dd.biz.navigation.setMenu({\r\n            backgroundColor: '#20D1CD',\r\n            textColor: '#ADD8E611',\r\n            items: item,\r\n            onSuccess: function(res) {\r\n              // 调用成功时回调\r\n              if (res.id === '1') {\r\n                _self.deleteDetails();\r\n              } else if (res.id === '2') {\r\n                _self.editDetails();\r\n              }\r\n            },\r\n            onFail: function(err) {\r\n              // 调用失败时回调\r\n              console.log(err);\r\n            }\r\n          });\r\n        } else {\r\n          _self.$dd.biz.navigation.setRight({\r\n            show: false //控制按钮显示， true 显示， false 隐藏， 默认true\r\n          });\r\n        }\r\n      } else {\r\n        _self.$dd.biz.navigation.setRight({\r\n          show: false //控制按钮显示， true 显示， false 隐藏， 默认true\r\n        });\r\n      }\r\n    },\r\n    /**\r\n     * 编辑页面的获取详细信息\r\n     */\r\n    getDetails() {\r\n      getPrepareDetails({ id: this.id }).then(({ data }) => {\r\n        for (let i in this.formData) {\r\n          this.formData[i] = data[i];\r\n        }\r\n        this.formData.filterCheckDate = parseTime(\r\n          this.formData.checkDate,\r\n          '{y}年{m}月{d}日'\r\n        );\r\n        this.init();\r\n      });\r\n    },\r\n    /**\r\n     * 编辑\r\n     */\r\n    editDetails() {\r\n      this.$router.push({\r\n        name: 'bkEdit',\r\n        query: { id: this.id }\r\n      });\r\n    },\r\n    /**\r\n     * 删除\r\n     */\r\n    deleteDetails() {\r\n      Dialog.confirm({\r\n        message: '确认要删除该条数据?',\r\n        confirmButtonColor: '#489EF3'\r\n      })\r\n        .then(() => {\r\n          deleteCheckList({ ids: this.id }).then(res => {\r\n            if (res.status === 200) {\r\n              Toast.success('删除成功');\r\n              this.$router.go(-1);\r\n            } else {\r\n              Toast.fail(res.message);\r\n            }\r\n          });\r\n        })\r\n        .catch(() => {\r\n          Toast.fail('已取消删除');\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.bk-detail {\r\n  width: 100%;\r\n  height: 100%;\r\n  background: #f6f6f6;\r\n  padding-top: 0.2rem;\r\n  /deep/ .van-cell {\r\n    padding: 0.4rem 0.32rem;\r\n    line-height: 0.4rem;\r\n    .opinion {\r\n      word-break: break-all;\r\n      margin-top: 0.2rem;\r\n      ul,\r\n      li,\r\n      ol {\r\n        list-style: revert;\r\n        padding: 0;\r\n        margin: 0;\r\n      }\r\n      ul,\r\n      ol {\r\n        padding-left: 15px;\r\n      }\r\n    }\r\n    &::after {\r\n      border-bottom: 0.02rem solid rgba(25, 31, 37, 0.08);\r\n    }\r\n  }\r\n  .column-flex {\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n  /deep/ .van-cell__title,\r\n  .van-cell__value {\r\n    width: auto;\r\n    flex: auto;\r\n    -webkit-box-flex: inherit;\r\n    -webkit-flex: inherit;\r\n    padding-right: 0.2rem;\r\n  }\r\n  /deep/ .van-cell__title {\r\n    color: rgba(0, 0, 0, 0.45);\r\n    white-space: nowrap;\r\n  }\r\n  /deep/ .van-cell__value {\r\n    color: rgba(0, 0, 0, 0.85);\r\n    text-align: left;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages/teaching-points-h5/views/normal-examine"}]}
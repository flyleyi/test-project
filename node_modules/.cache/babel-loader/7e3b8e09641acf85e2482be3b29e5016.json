n  return axios.post(\n    '/contentComment/delete',\n    null,\n    { params },\n    'school-book-admin'\n  );\n}\nexport function replyMessage(params) {\n  return axios.post(\n    '/contentComment/answerReply',\n    null,\n    { params },\n    'school-book-admin'\n  );\n}\n\n//目录分页\nexport function getCatalogPage(params) {\n  return axios.get('/catalog/page', params, 'school-book-admin');\n}\n//所有目录\nexport function getCatalogList(params) {\n  return axios.get('/catalog/list', params, 'school-book-admin');\n}\nexport function getCatalogListAll(params) {\n  return axios.get('/catalog/v1/list', params, 'school-book-admin');\n}\n//新增下级目录\nexport function addCatalog(params) {\n  return axios.post('/catalog/add', params, null, 'school-book-admin');\n}\n//新增上级目录\nexport function addParentCatalog(params) {\n  return axios.get('/catalog/addParent', params, 'school-book-admin');\n}\n//移动编辑目录\nexport function updateCatalog(params) {\n  return axios.post('/catalog/update', params, null, 'school-book-admin');\n}\n//删除目录\nexport function deleteCatalog(params) {\n  return axios.get('/catalog/delete', params, 'school-book-admin');\n}\n//删除目录\nexport function deleteCatalogV1(params) {\n  return axios.post(\n    '/catalog/v1/delete',\n    null,\n    { params },\n    'school-book-admin'\n  );\n}\n//根据 id 查询目录日志\nexport function catalogLogDetail(params) {\n  return axios.get('/catalogLog/detail', params, 'school-book-admin');\n}\n//根据 id 查询文章日志\nexport function contentLogDetail(params) {\n  return axios.get('/content/logDetails', params, 'school-book-admin');\n}\n//新增编辑文章内容\nexport function saveContent(params) {\n  return axios.post('/content/save', params, null, 'school-book-admin');\n}\n//新增编辑文章内容v1\nexport function saveContentV1(params) {\n  return axios.post('/content/v1/save', params, null, 'school-book-admin');\n}\n//文章详情\nexport function contentDetail(params) {\n  return axios.get('/content/detail', params, 'school-book-admin');\n}\n//文章详情\nexport function contentDetailV1(params) {\n  return axios.get('/content/v1/detail', params, 'school-book-admin');\n}\n//获取最后一级目录\nexport function getLastCatalog(params) {\n  return axios.get('/catalog/lastCatalog', params, 'school-book-admin');\n}\n//删除文章\nexport function deleteContent(params) {\n  return axios.get('/content/delete', params, 'school-book-admin');\n}\nexport function deleteContentBatch(params) {\n  return axios.post(\n    '/content/v1/delete',\n    null,\n    { params },\n    'school-book-admin'\n  );\n}\n//审核文章\nexport function checkContent(params) {\n  return axios.post('/content/update', null, { params }, 'school-book-admin');\n}\n//审核文章V1\nexport function checkContentV1(params) {\n  return axios.post(\n    '/content/v1/update',\n    null,\n    { params },\n    'school-book-admin'\n  );\n}\nexport function getContentList(params) {\n  return axios.get('/content/v1/getContentPage', params, 'school-book-admin');\n}\n//文章上线\nexport function onLineContentV1(params) {\n  return axios.post(\n    '/content/v1/updateOnLine',\n    null,\n    { params },\n    'school-book-admin'\n  );\n}\n//文章上线\nexport function onLineContent(params) {\n  return axios.post(\n    '/content/updateOnLine',\n    null,\n    { params },\n    'school-book-admin'\n  );\n}\n\n/**\n * 上传单文件\n * @param {*} file 文件\n * @returns {Promise}\n */\nexport async function uploadFile(file) {\n  let formData = new FormData();\n  formData.append('file', file);\n  return axios(\n    {\n      method: 'POST',\n      url: '/uploadFile/uploadImag',\n      data: formData\n    },\n    'school-book-admin'\n  );\n}\n"]}]}
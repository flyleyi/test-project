OAAA,CAAA,MAAA,EAAA,SAAA;;AACA,QAAA,MAAA,CAAA,iBAAA;AACA,OAHA;AAIA,KA3HA;AA4HA,IAAA,OA5HA,qBA4HA;AACA,WAAA,iBAAA;AACA,KA9HA;AA+HA,IAAA,QA/HA,oBA+HA,IA/HA,EA+HA,CACA;AACA,KAjIA;AAkIA,IAAA,QAlIA,sBAkIA;AAAA;;AACA,MAAA,oBAAA,CAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,GAAA,GAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,EAAA,SAAA;AACA,OAHA,EADA,CAKA;AACA;AACA,KAzIA;AA0IA,IAAA,OA1IA,qBA0IA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,KA5IA;AA6IA,IAAA,QA7IA,oBA6IA,GA7IA,EA6IA;AACA,WAAA,iBAAA,GAAA,IAAA;AAEA,WAAA,SAAA,CAAA,YAAA;AACA,aAAA,IAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA,OAFA;AAGA;AAnJA;AAtDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"cost-center-wrapper\">\r\n    <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\r\n      <el-form-item label=\"薪酬看板成本中心\">\r\n        <el-select\r\n          v-model=\"formInline.salary\"\r\n          @change=\"changeSalary\"\r\n          clearable\r\n          placeholder=\"请选择薪酬看板成本中心\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in salaryList\"\r\n            :key=\"item\"\r\n            :label=\"item\"\r\n            :value=\"item\"\r\n          ></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"财务成本中心\">\r\n        <el-select\r\n          v-model=\"formInline.finance\"\r\n          @change=\"changeFinance\"\r\n          clearable\r\n          placeholder=\"请选择财务成本中心\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in financeList\"\r\n            :key=\"item\"\r\n            :label=\"item\"\r\n            :value=\"item\"\r\n          ></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"人资成本中心\">\r\n        <el-select\r\n          v-model=\"formInline.hummanResource\"\r\n          clearable\r\n          placeholder=\"请选择人资成本中心\"\r\n        >\r\n          <el-option\r\n            v-for=\"item in hummanResourceList\"\r\n            :key=\"item\"\r\n            :label=\"item\"\r\n            :value=\"item\"\r\n          ></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"操作时间\">\r\n        <el-date-picker\r\n          v-model=\"formInline.operaDate\"\r\n          type=\"date\"\r\n          :picker-options=\"pickerOptions\"\r\n          format=\"yyyy-MM-dd\"\r\n          value-format=\"yyyy-MM-dd\"\r\n          placeholder=\"选择日期\"\r\n        >\r\n        </el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button @click=\"addItem\" type=\"primary\" size=\"primary\"\r\n          >新增</el-button\r\n        >\r\n      </el-form-item>\r\n\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onQuery\">查询</el-button>\r\n      </el-form-item>\r\n\r\n      <el-form-item>\r\n        <el-upload\r\n          class=\"upload-demo\"\r\n          action=\"/laborcost-api/costCenterData/import\"\r\n          :headers=\"{\r\n            Authorization: token\r\n          }\"\r\n          accept=\".xls,.xlsx\"\r\n          :on-error=\"handleError\"\r\n          :on-progress=\"handleProgress\"\r\n          :on-success=\"handleSuccess\"\r\n          :show-file-list=\"false\"\r\n        >\r\n          <el-button type=\"primary\" @click=\"onImport\">导入</el-button>\r\n        </el-upload>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onExport\">导出</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div>\r\n      <el-table border :data=\"tableData\">\r\n        <el-table-column label=\"薪酬看板成本中心\" prop=\"salaryCostCenter\">\r\n        </el-table-column>\r\n        <el-table-column label=\"财务成本中心\" prop=\"financeCostCenter\">\r\n        </el-table-column>\r\n        <el-table-column label=\"人资成本中心\" prop=\"hrCostCenter\">\r\n        </el-table-column>\r\n        <el-table-column label=\"人资成本中心代码\" prop=\"hrCostCenterCode\">\r\n        </el-table-column>\r\n        <el-table-column label=\"课时工资\" prop=\"classHourSalary\">\r\n        </el-table-column>\r\n        <el-table-column label=\"班主任补贴\" prop=\"classTeacherSubsidy\">\r\n        </el-table-column>\r\n        <el-table-column label=\"学期期末考勤奖\" prop=\"termEndBonus\">\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <!-- <el-button @click=\"addItem(scope.row)\" type=\"text\" size=\"small\"\r\n              >新增</el-button\r\n            > -->\r\n            <el-button @click=\"editItem(scope.row)\" type=\"text\" size=\"small\"\r\n              >编辑</el-button\r\n            >\r\n            <el-button\r\n              @click=\"confirmDialog(scope.row)\"\r\n              type=\"text\"\r\n              size=\"small\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作时间\" prop=\"updateTime\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            {{ formatTime(scope.row.updateTime) }}\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <div>\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-sizes=\"[10, 50, 100]\"\r\n        :page-size=\"currentSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n    <el-dialog\r\n      :title=\"dialogTitle\"\r\n      @close=\"dialogCancel\"\r\n      center\r\n      :visible.sync=\"dialogFormVisible\"\r\n    >\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"roleForm\">\r\n        <el-form-item\r\n          label=\"薪酬看板成本中心\"\r\n          prop=\"salaryCostCenter\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.salaryCostCenter\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"财务成本中心\"\r\n          prop=\"financeCostCenter\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.financeCostCenter\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"人资成本中心\"\r\n          prop=\"hrCostCenter\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.hrCostCenter\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"人资成本中心代码\"\r\n          prop=\"hrCostCenterCode\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.hrCostCenterCode\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"课时工资\"\r\n          prop=\"classHourSalary\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.classHourSalary\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"班主任补贴\"\r\n          prop=\"classTeacherSubsidy\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.classTeacherSubsidy\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"学期期末考核\"\r\n          prop=\"termEndBonus\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.termEndBonus\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogCancel\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogConfirm('roleForm')\"\r\n          >确 定</el-button\r\n        >\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getSalaryCostCenterList,\r\n  getFinanceCostCenterList,\r\n  getHrCostCenterList,\r\n  getCostCenterList,\r\n  getCostCenterItem,\r\n  saveCostCenterItem,\r\n  deleteCostCenterItem,\r\n  importCostCenterData,\r\n  exportCostCenterData\r\n} from './../../api/data.js';\r\nimport { getToken } from '@/plugins/core/auth';\r\nimport laborcostMixin from '../../assets/mixins/laborcostMixin';\r\nexport default {\r\n  name: 'CoseCenter',\r\n  mixins: [laborcostMixin],\r\n  data() {\r\n    return {\r\n      formInline: {\r\n        salary: '',\r\n        finance: '',\r\n        hummanResource: '',\r\n        operaDate: ''\r\n      },\r\n      tableData: [],\r\n      salaryList: [],\r\n      financeList: [],\r\n      hummanResourceList: [],\r\n      dialogFormVisible: false,\r\n      dialogTitle: '新增',\r\n      currentPage: 1,\r\n      currentSize: 10,\r\n      token: getToken(),\r\n      total: 0,\r\n      form: {\r\n        id: null,\r\n        salaryCostCenter: '',\r\n        financeCostCenter: '',\r\n        hrCostCenter: '',\r\n        hrCostCenterCode: '',\r\n        classHourSalary: '',\r\n        classTeacherSubsidy: '',\r\n        termEndBonus: ''\r\n      },\r\n      rules: {\r\n        salaryCostCenter: [\r\n          {\r\n            required: true,\r\n            message: '请输入薪酬看板成本中心',\r\n            trigger: 'blur'\r\n          }\r\n        ],\r\n        financeCostCenter: [\r\n          { required: true, message: '请输入财务成本中心', trigger: 'blur' }\r\n        ]\r\n      },\r\n      pickerOptions: {\r\n        disabledDate(time) {\r\n          return time.getTime() > Date.now();\r\n        }\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getSalaryCostCenterList();\r\n    this.getCostCenterList();\r\n  },\r\n  methods: {\r\n    changeSalary() {\r\n      this.formInline.finance = '';\r\n      this.formInline.hummanResource = '';\r\n      this.getFinanceCostCenterList();\r\n    },\r\n    changeFinance() {\r\n      this.formInline.hummanResource = '';\r\n      this.getHrCostCenterList();\r\n    },\r\n    handleSizeChange(val) {\r\n      this.currentPage = 1;\r\n      this.currentSize = val;\r\n      this.getCostCenterList();\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n      this.getCostCenterList();\r\n    },\r\n    getSalaryCostCenterList() {\r\n      getSalaryCostCenterList({}).then(res => {\r\n        this.salaryList = res.data;\r\n      });\r\n    },\r\n    getFinanceCostCenterList() {\r\n      if (!this.formInline.salary) {\r\n        this.financeList = [];\r\n        return;\r\n      }\r\n      getFinanceCostCenterList({\r\n        salaryCostCenter: this.formInline.salary\r\n      }).then(res => {\r\n        this.financeList = res.data;\r\n      });\r\n    },\r\n    getHrCostCenterList() {\r\n      if (!this.formInline.finance) {\r\n        this.hummanResourceList = [];\r\n        return;\r\n      }\r\n      getHrCostCenterList({\r\n        salaryCostCenter: this.formInline.salary,\r\n        financeCostCenter: this.formInline.finance\r\n      }).then(res => {\r\n        this.hummanResourceList = res.data;\r\n      });\r\n    },\r\n    getCostCenterList() {\r\n      const { salary, finance, hummanResource, operaDate } = this.formInline;\r\n      let params = {\r\n        current: this.currentPage,\r\n        size: this.currentSize,\r\n        salaryCostCenter: salary,\r\n        financeCostCenter: finance,\r\n        hrCostCenter: hummanResource,\r\n        // hrCostCenterCode: null,\r\n        updateTimeStr: operaDate\r\n      };\r\n      getCostCenterList(params).then(res => {\r\n        this.tableData = res.data.records || [];\r\n        this.total = res.data.total;\r\n        if (this.currentPage != res.data.current) {\r\n          this.currentPage = res.data.current;\r\n        }\r\n      });\r\n    },\r\n    dialogConfirm(formName) {\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          this.saveCostCenterItem();\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    dialogCancel() {\r\n      this.dialogFormVisible = false;\r\n      this.$refs['roleForm'].resetFields();\r\n    },\r\n    saveCostCenterItem() {\r\n      let params = Object.assign(this.form);\r\n      saveCostCenterItem(params)\r\n        .then(res => {\r\n          this.dialogFormVisible = false;\r\n          this.getCostCenterList();\r\n          if (this.form.id) {\r\n            this.warning('修改成功', 'success');\r\n          } else {\r\n            this.warning('新增成功', 'success');\r\n          }\r\n        })\r\n        .catch(e => {\r\n          if (e.status && e.status == 511) {\r\n            this.$confirm('该条记录已存在！', '', {\r\n              confirmButtonText: '确定',\r\n              cancelButtonText: '取消',\r\n              center: true\r\n            })\r\n              .then(() => {})\r\n              .catch(() => {\r\n                // cancelFun && cancelFun();\r\n              });\r\n          }\r\n        });\r\n    },\r\n    confirmDialog(row) {\r\n      this.$confirm('是否确认删除本行？', '', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        center: true\r\n      })\r\n        .then(() => {\r\n          this.deleteItem(row);\r\n        })\r\n        .catch(() => {\r\n          // cancelFun && cancelFun();\r\n        });\r\n    },\r\n    deleteItem(row) {\r\n      deleteCostCenterItem({ id: row.id }).then(res => {\r\n        this.warning('删除成功', 'success');\r\n        this.getCostCenterList();\r\n      });\r\n    },\r\n    onQuery() {\r\n      this.getCostCenterList();\r\n    },\r\n    onImport(file) {\r\n      // importCostCenterData({ file: file }).then(res => {});\r\n    },\r\n    onExport() {\r\n      exportCostCenterData({}).then(res => {\r\n        const content = res;\r\n        this.$util.downLoadBlob(content, '成本中心对照表');\r\n      });\r\n      // const url = window.location.origin + '/laborcost/costCenterData/import';\r\n      // window.open(url);\r\n    },\r\n    addItem() {\r\n      this.dialogFormVisible = true;\r\n    },\r\n    editItem(row) {\r\n      this.dialogFormVisible = true;\r\n\r\n      this.$nextTick(function() {\r\n        this.form = Object.assign({}, row);\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.cost-center-wrapper {\r\n  padding: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages/logistics-data/views/CostCenter"}]}
C,GAA7C,CAAd;AACA,MAAMC,IAAI,GAAGR,IAAI,CACdS,OADU,GAEVH,QAFU,GAGVC,QAHU,CAGD,CAHC,EAGE,GAHF,CAAb;AAIA,MAAMG,KAAK,GAAGV,IAAI,CACfW,QADW,GAEXL,QAFW,GAGXC,QAHW,CAGF,CAHE,EAGC,GAHD,CAAd;AAIA,MAAMK,MAAM,GAAGZ,IAAI,CAChBa,UADY,GAEZP,QAFY,GAGZC,QAHY,CAGH,CAHG,EAGA,GAHA,CAAf;AAIA,MAAMO,MAAM,GAAGd,IAAI,CAChBe,UADY,GAEZT,QAFY,GAGZC,QAHY,CAGH,CAHG,EAGA,GAHA,CAAf;;AAIA,MAAIR,IAAI,IAAI,CAAZ,EAAe;AACb,WAAOG,IAAI,GAAG,GAAP,GAAaE,KAAb,GAAqB,GAArB,GAA2BI,IAA3B,GAAkC,GAAzC;AACD,GAFD,MAEO,IAAIT,IAAI,IAAI,CAAZ,EAAe;AACpB,WACEG,IAAI,GACJ,GADA,GAEAE,KAFA,GAGA,GAHA,GAIAI,IAJA,GAKA,GALA,GAMAE,KANA,GAOA,GAPA,GAQAE,MARA,GASA,GATA,GAUAE,MAXF;AAaD,GAdM,MAcA,IAAIf,IAAI,IAAI,CAAZ,EAAe;AACpB,WACEG,IAAI,GACJ,GADA,GAEAE,KAFA,GAGA,GAHA,GAIAI,IAJA,GAKA,GALA,GAMAE,KANA,GAOA,GAPA,GAQAE,MARA,GASA,GATA,GAUAE,MAXF;AAaD,GAdM,MAcA,IAAIf,IAAI,IAAI,CAAZ,EAAe;AACpB,WAAOG,IAAI,GAAG,GAAP,GAAaE,KAAb,GAAqB,GAArB,GAA2BI,IAA3B,GAAkC,GAAzC;AACD,GAFM,MAEA,IAAIT,IAAI,IAAI,CAAZ,EAAe;AACpB,WAAOG,IAAI,GAAG,GAAP,GAAaE,KAAb,GAAqB,GAArB,GAA2BI,IAA3B,GAAkC,GAAlC,GAAwCE,KAAxC,GAAgD,GAAhD,GAAsDE,MAA7D;AACD,GAFM,MAEA,IAAIb,IAAI,IAAI,CAAZ,EAAe;AACpB,WAAOG,IAAI,GAAG,GAAP,GAAaE,KAAb,GAAqB,GAArB,GAA2BI,IAA3B,GAAkC,GAAlC,GAAwCE,KAAxC,GAAgD,GAAhD,GAAsDE,MAA7D;AACD;AACF","sourcesContent":["import { ajaxQueryConjunctiveList } from '../../api/base';\r\n/**\r\n * 级联查询校区以下数据（学校、学部等）\r\n * @param {Object} params 参数 {code: //当前code, codeType: //当前code类型, childCodeType: //子code类型}\r\n * @param {boolean} extendDiabled 扩展禁用属性\r\n */\r\nexport function queryConjunctiveList(params, extendDiabled) {\r\n  return ajaxQueryConjunctiveList(params).then(({ data }) => {\r\n    extendDiabled &&\r\n      Array.isArray(data) &&\r\n      data.forEach(item => {\r\n        item.disabled = !item.isShow;\r\n      });\r\n    return data;\r\n  });\r\n}\r\n\r\nexport function formateDate(timestamp, type = 1) {\r\n  if (!timestamp) {\r\n    return '-';\r\n  }\r\n  let time = new Date(timestamp);\r\n  let year = time.getFullYear();\r\n  const month = (time.getMonth() + 1).toString().padStart(2, '0');\r\n  const date = time\r\n    .getDate()\r\n    .toString()\r\n    .padStart(2, '0');\r\n  const hours = time\r\n    .getHours()\r\n    .toString()\r\n    .padStart(2, '0');\r\n  const minute = time\r\n    .getMinutes()\r\n    .toString()\r\n    .padStart(2, '0');\r\n  const second = time\r\n    .getSeconds()\r\n    .toString()\r\n    .padStart(2, '0');\r\n  if (type == 1) {\r\n    return year + '年' + month + '月' + date + '日';\r\n  } else if (type == 2) {\r\n    return (\r\n      year +\r\n      '-' +\r\n      month +\r\n      '-' +\r\n      date +\r\n      ' ' +\r\n      hours +\r\n      ':' +\r\n      minute +\r\n      ':' +\r\n      second\r\n    );\r\n  } else if (type == 3) {\r\n    return (\r\n      year +\r\n      '_' +\r\n      month +\r\n      '_' +\r\n      date +\r\n      '_' +\r\n      hours +\r\n      '_' +\r\n      minute +\r\n      '_' +\r\n      second\r\n    );\r\n  } else if (type == 4) {\r\n    return year + '-' + month + '-' + date + ' ';\r\n  } else if (type == 5) {\r\n    return year + '-' + month + '-' + date + ' ' + hours + ':' + minute;\r\n  } else if (type == 6) {\r\n    return year + '/' + month + '/' + date + ' ' + hours + ':' + minute;\r\n  }\r\n}\r\n"]}]}
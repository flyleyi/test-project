iv>\n    <div class=\"show-text\" @click=\"handleShow\">\n      <span>{{ date }}</span>\n      <span class=\"time\">{{ selectedTime }}</span>\n    </div>\n    <van-calendar v-model=\"show\" :show-confirm=\"false\" @confirm=\"onConfirm\" />\n    <van-popup\n      v-model=\"showTime\"\n      position=\"bottom\"\n      :style=\"{ height: '50%' }\"\n      :close-on-click-overlay=\"false\"\n    >\n      <van-datetime-picker\n        v-model=\"selectedTime\"\n        type=\"time\"\n        title=\"选择时间\"\n        @confirm=\"onTimeConfirm\"\n      />\n    </van-popup>\n  </div>\n</template>\n\n<script>\nimport { formateDate } from '@/pages/visitor-h5/plugins/common/utils';\nexport default {\n  props: ['isStart'],\n  data() {\n    return {\n      date: '',\n      show: false,\n      showTime: false,\n      selectedTime: ''\n    };\n  },\n  methods: {\n    handleShow() {\n      this.show = true;\n    },\n    onConfirm(date) {\n      this.show = false;\n      this.date = formateDate(date, 4);\n      this.showTime = true;\n    },\n    onTimeConfirm(time) {\n      this.showTime = false;\n      this.selectedTime = time;\n      this.$emit('setDateTime', `${this.date} ${this.selectedTime}`);\n    },\n    onCancel() {\n      this.date = '';\n      this.selectedTime = '';\n      this.showTime = false;\n      setTimeout(() => {\n        this.selectedTime = '';\n      });\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.show-text {\n  padding-left: 12px;\n  width: 100%;\n  height: 100%;\n  .time {\n    margin-left: 3px;\n  }\n}\nspan {\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: rgba(0, 0, 0, 0.65);\n  line-height: 40px;\n}\n\n/deep/ .van-picker__cancel {\n  opacity: 0;\n}\n</style>\n"],"sourceRoot":"src/pages/visitor-h5/views/verify-visitor/components"}]}
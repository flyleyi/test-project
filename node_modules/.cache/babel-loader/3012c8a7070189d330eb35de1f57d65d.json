AGA,KAjBA;AAkBA,IAAA,WAlBA,uBAkBA,KAlBA,EAkBA;AACA,UAAA,IAAA,GAAA,KAAA,IAAA,KAAA,CAAA,IAAA;;AACA,UAAA,CAAA,IAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,aACA,IAAA,CAAA,IAAA,GAAA,iBAAA,OAAA,YAAA,iBAAA,EADA;AAGA,KA1BA;AA2BA,IAAA,WA3BA,uBA2BA,IA3BA,EA2BA;AACA;AACA,UAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,UAAA,MAAA,GAAA,YAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,aAAA,MAAA,CAAA,MAAA,CAAA;AACA,KAjCA;AAkCA,IAAA,MAlCA,kBAkCA,IAlCA,EAkCA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AAAA,UAAA,IAAA,GAAA,IAAA,CAAA,IAAA;;AACA,UAAA,QAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AACA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,KAAA,WAAA,CAAA,IAAA,CAAA;AACA,KAzCA;;AA0CA;AACA;AACA;AACA,IAAA,cA7CA,4BA6CA;AACA,UAAA,IAAA,GAAA,KAAA,MAAA,CAAA,IAAA;AACA,UAAA,QAAA,GAAA,KAAA,QAAA,CAAA,KAAA,CAAA,CAAA,CAAA,CAFA,CAEA;;AACA,UAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,GAAA,KAAA,IAAA;AAAA,OAAA,CAAA;AACA,UAAA,CAAA,IAAA,EAAA,OAAA,EAAA,CAJA,CAIA;;AACA,UAAA,OAAA,GAAA,EAAA,CALA,CAMA;;AACA,UAAA,EAAA,GAAA,SAAA,EAAA,CAAA,MAAA,EAAA;AACA,YAAA,WAAA,GAAA,QAAA,CAAA,SAAA,CAAA,UAAA,IAAA;AAAA,iBAAA,IAAA,CAAA,MAAA,KAAA,MAAA;AAAA,SAAA,CAAA;AACA,YAAA,WAAA,KAAA,CAAA,CAAA,EAAA,OAFA,CAEA;;AACA,YAAA,UAAA,GAAA,QAAA,CAAA,WAAA,CAAA;AACA,QAAA,QAAA,CAAA,MAAA,CAAA,WAAA,EAAA,CAAA,EAJA,CAIA;;AACA,QAAA,OAAA,CAAA,OAAA,CAAA,UAAA;AACA,QAAA,UAAA,CAAA,MAAA,KAAA,UAAA,CAAA,YAAA,IAAA,EAAA,CAAA,IAAA,CAAA,YAAA,CAAA,CANA,CAMA;AACA,OAPA;;AAQA,MAAA,EAAA,CAAA,IAAA,CAAA,MAAA,CAAA;AACA,aAAA,OAAA;AACA;AA9DA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <el-breadcrumb class=\"app-breadcrumb\" separator=\"/\">\r\n    <transition-group name=\"breadcrumb\">\r\n      <el-breadcrumb-item v-for=\"(item, index) in levelList\" :key=\"item.path\">\r\n        <span\r\n          v-if=\"item.redirect === 'noRedirect' || index == levelList.length - 1\"\r\n          class=\"no-redirect\"\r\n          >{{ item.meta.title }}</span\r\n        >\r\n        <a v-else @click.prevent=\"onLink(item)\">{{ item.meta.title }}</a>\r\n      </el-breadcrumb-item>\r\n    </transition-group>\r\n  </el-breadcrumb>\r\n</template>\r\n\r\n<script>\r\nimport pathToRegexp from 'path-to-regexp';\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  name: 'CBreadcrumb',\r\n  data() {\r\n    return {\r\n      levelList: null\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['menuList'])\r\n  },\r\n  watch: {\r\n    $route() {\r\n      this.getBreadcrumb();\r\n    }\r\n  },\r\n  created() {\r\n    this.getBreadcrumb();\r\n  },\r\n  methods: {\r\n    getBreadcrumb() {\r\n      // console.log(this.getMenuMatched());\r\n      // only show routes with meta.title\r\n      // let matched = this.$route.matched.filter(\r\n      //   item => item.meta && item.meta.title\r\n      // );\r\n      // 使用菜单匹配路由\r\n      let matched = this.getMenuMatched();\r\n      const first = matched[0];\r\n\r\n      if (!this.isDashboard(first)) {\r\n        matched = [{ path: '/home', meta: { title: '首页' } }].concat(matched);\r\n      }\r\n      this.levelList = matched.filter(\r\n        item => item.meta && item.meta.title && item.meta.breadcrumb !== false\r\n      );\r\n    },\r\n    isDashboard(route) {\r\n      const name = route && route.name;\r\n      if (!name) {\r\n        return false;\r\n      }\r\n      return (\r\n        name.trim().toLocaleLowerCase() === 'Dashboard'.toLocaleLowerCase()\r\n      );\r\n    },\r\n    pathCompile(path) {\r\n      // To solve this problem https://github.com/PanJiaChen/vue-element-admin/issues/561\r\n      const { params } = this.$route;\r\n      console.log(path);\r\n      var toPath = pathToRegexp.compile(path);\r\n      return toPath(params);\r\n    },\r\n    onLink(item) {\r\n      const { redirect, path } = item;\r\n      if (redirect) {\r\n        this.$router.push(redirect);\r\n        return;\r\n      }\r\n      this.$router.push(this.pathCompile(path));\r\n    },\r\n    /**\r\n     * 获取菜单访问路径\r\n     */\r\n    getMenuMatched() {\r\n      let path = this.$route.path;\r\n      let menuList = this.menuList.slice(0); // 拷贝路由\r\n      let menu = menuList.find(item => item.url === path);\r\n      if (!menu) return []; // 未找到菜单\r\n      let matched = [];\r\n      // 向上查找父级菜单\r\n      let fn = menuId => {\r\n        let parentIndex = menuList.findIndex(item => item.menuId === menuId);\r\n        if (parentIndex === -1) return; // 未找到上级菜单\r\n        let parentMenu = menuList[parentIndex];\r\n        menuList.splice(parentIndex, 1); // 防止死递归\r\n        matched.unshift(parentMenu);\r\n        parentMenu.menuId !== parentMenu.parentMenuId && fn(menu.parentMenuId); // 防止死递归\r\n      };\r\n      fn(menu.menuId);\r\n      return matched;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.app-breadcrumb.el-breadcrumb {\r\n  display: inline-block;\r\n  font-size: 14px;\r\n  line-height: 50px;\r\n  margin-left: 8px;\r\n\r\n  .no-redirect {\r\n    color: #97a8be;\r\n    cursor: text;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages/logistics-data/components/business/CBreadcrumb"}]}
A,mBAeA;AACA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,sBAAA,EAAA,EAAA,EAFA,CAGA;;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,wBAAA,EAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA,KAvBA;AAwBA;AACA,IAAA,WAzBA,uBAyBA,EAzBA,EAyBA;AACA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,sBAAA,EAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,iCAAA,EAAA;AADA,OAAA;AAGA;AA/BA;AAbA,CAAA","sourcesContent":["<!--  -->\n<template>\n  <div class=\"app\">\n    <!-- 自定义发布 -->\n    <div class=\"new\" @click=\"goPub\">\n      <div class=\"add-btn\">\n        <img\n          src=\"@/pages/jx-questionnaire-h5/assets/images/icons/add3_icon.png\"\n        />\n        <div>自定义发布</div>\n      </div>\n    </div>\n    <!-- 模板发布 -->\n    <van-list\n      v-model=\"loading\"\n      :finished=\"finished\"\n      finished-text=\"\"\n      @load=\"onLoad\"\n    >\n      <div class=\"modules\">\n        <div\n          class=\"item\"\n          v-for=\"item in list\"\n          :key=\"item.id\"\n          @click=\"goModulePub(item.id)\"\n        >\n          <div class=\"title\">{{ item.title }}</div>\n          <div class=\"introduction\">\n            {{ item.description }}\n          </div>\n          <div class=\"num\">共{{ item.questionCount }}题</div>\n        </div>\n      </div>\n    </van-list>\n  </div>\n</template>\n\n<script>\nimport { List } from 'vant';\nimport { getTemplateList } from '../../api/survey';\nexport default {\n  name: 'OptionModules',\n  components: {\n    [List.name]: List\n  },\n  data() {\n    return {\n      list: [],\n      loading: false,\n      finished: false,\n      total: 0\n    };\n  },\n  methods: {\n    onLoad() {\n      // 异步更新数据\n      getTemplateList().then(res => {\n        this.total = res.data.total;\n        this.list = this.list.concat(res.data.list);\n        // 加载状态结束\n        this.loading = false;\n        // 数据全部加载完成\n        if (this.list.length >= res.data.total) {\n          this.finished = true;\n        }\n      });\n    },\n    // 跳转自定义发布\n    goPub() {\n      // 重置班级学生列表\n      this.$store.commit('classes/SAVE_CLASSES', []);\n      // 重置问题列表\n      this.$store.commit('survey/UPDATA_QUESTION', []);\n      this.$router.push({\n        path: '/survey-add'\n      });\n    },\n    // 跳转模板发布\n    goModulePub(id) {\n      // 重置班级学生列表\n      this.$store.commit('classes/SAVE_CLASSES', []);\n      this.$router.push({\n        path: `/survey-add?moduleId=${id}`\n      });\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.app {\n  // 自定义发布\n  .new {\n    width: 100%;\n    height: 2.8rem;\n    background: #ffffff;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    .add-btn {\n      & > img {\n        height: 1.24rem;\n      }\n      & > div {\n        font-size: 0.28rem;\n        font-family: PingFangSC-Regular, PingFang SC;\n        color: rgba(0, 0, 0, 0.85);\n        text-align: center;\n        padding-top: 0.24rem;\n      }\n    }\n  }\n  // 模板发布\n  .modules {\n    padding: 0.24rem 0.24rem 0 0.24rem;\n    display: flex;\n    justify-content: space-between;\n    flex-wrap: wrap;\n    .item {\n      width: 3.4rem;\n      height: 2.82rem;\n      background: #ffffff;\n      box-shadow: 0 0 0.1rem 0 rgba(0, 0, 0, 0.1);\n      border-radius: 0.06rem;\n      margin-bottom: 0.24rem;\n      .title {\n        margin: 0 0.12rem;\n        padding: 0.24rem 0.12rem;\n        font-size: 0.3rem;\n        font-family: PingFangSC-Medium, PingFang SC;\n        font-weight: 500;\n        color: rgba(0, 0, 0, 0.85);\n        overflow: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n        position: relative;\n        //伪元素绝对定位\n        &::after {\n          content: ' ';\n          position: absolute;\n          left: 0;\n          bottom: 0;\n          right: 0;\n          height: 1px;\n          background: rgba(0, 0, 0, 0.08);\n          transform-origin: 0 0;\n          transform: scaleY(0.5);\n        }\n      }\n      .introduction {\n        height: 0.86rem;\n        margin: 0.24rem;\n        font-size: 0.24rem;\n        line-height: 0.38rem;\n        font-family: PingFangSC-Regular, PingFang SC;\n        color: rgba(0, 0, 0, 0.65);\n      }\n      .num {\n        width: 1.6rem;\n        height: 0.38rem;\n        background: rgba(0, 0, 0, 0.08);\n        border-radius: 0.2rem;\n        font-size: 0.24rem;\n        font-family: PingFangSC-Regular, PingFang SC;\n        color: rgba(0, 0, 0, 0.85);\n        margin-left: 0.9rem;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/pages/jx-questionnaire-h5/views/option-modules"}]}
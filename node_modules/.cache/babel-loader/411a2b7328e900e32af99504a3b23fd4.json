      title: '首页',\r\n        icon: 'dashboard'\r\n      },\r\n      id: '',\r\n      menuDesc: '',\r\n      menuId: 866,\r\n      menuName: '首页',\r\n      menuType: '0',\r\n      name: 'Dashboard',\r\n      orderNo: 0,\r\n      parentMenuId: -1,\r\n      path: '/home',\r\n      roleType: '',\r\n      sysCode: '',\r\n      text: '',\r\n      updateTime: null,\r\n      url: '/home'\r\n    },\r\n    {\r\n      icon: 'el-icon-location',\r\n      path: '/costCenter',\r\n      menuId: 867,\r\n      title: '成本中心对照表',\r\n      hidden: false,\r\n      meta: {\r\n        title: '成本中心对照表',\r\n        icon: 'el-icon-location'\r\n      },\r\n      url: '/costCenter'\r\n    },\r\n    {\r\n      icon: 'el-icon-menu',\r\n      path: '/humanResource',\r\n      menuId: 868,\r\n      title: '人资导入数据表',\r\n      hidden: false,\r\n      meta: {\r\n        title: '人资导入数据表',\r\n        icon: 'el-icon-menu'\r\n      },\r\n      url: '/humanResource'\r\n    },\r\n    {\r\n      icon: 'el-icon-trophy',\r\n      path: '/finance',\r\n      menuId: 869,\r\n      title: '财务导入数据表',\r\n      hidden: false,\r\n      meta: {\r\n        title: '财务导入数据表',\r\n        icon: 'el-icon-trophy'\r\n      },\r\n      url: '/finance'\r\n    },\r\n    {\r\n      icon: 'el-icon-user-solid',\r\n      path: '/boundaryStu',\r\n      menuId: 870,\r\n      title: '用户管理',\r\n      hidden: false,\r\n      meta: {\r\n        title: '用户管理',\r\n        icon: 'el-icon-user-solid'\r\n      },\r\n      url: '/boundaryStu'\r\n    }\r\n  ],\r\n  // 按钮权限列表\r\n  authList: []\r\n};\r\n\r\n/**\r\n * 菜单列表转换树列表\r\n */\r\nconst menuListConvertMenuTree = menuList => {\r\n  return utils.listConvertTree(menuList, 'menuId', 'parentMenuId', true);\r\n};\r\n\r\n/**\r\n * 排序菜单树\r\n * @param {Array} menuTree 菜单树\r\n */\r\nconst sortMenuTree = menuTree => {\r\n  let sort = list => {\r\n    list.sort((a, b) => {\r\n      return a.orderNo - b.orderNo;\r\n    });\r\n    list.forEach(\r\n      value => value.children && value.children.length && sort(value.children)\r\n    );\r\n    return list;\r\n  };\r\n  return sort(menuTree);\r\n};\r\n\r\n/**\r\n * 树型数据转列表\r\n * @param {*} tree 树型数据\r\n * @param {*} childrenKey 子节点列表键\r\n */\r\nconst treeConvertList = (tree, childrenKey = 'children') => {\r\n  if (!Array.isArray(tree)) return [];\r\n  let list = [];\r\n  let fn = data => {\r\n    list = list.concat(data.slice(0));\r\n    data.forEach(item => {\r\n      let children = item[childrenKey];\r\n      Array.isArray(children) && fn(children);\r\n    });\r\n  };\r\n  fn(tree);\r\n  return list;\r\n};\r\n\r\nconst mutations = {\r\n  SET_ROUTES(state, routes) {\r\n    state.routes = routes;\r\n  },\r\n  SET_MENU_LIST(state, menuList) {\r\n    state.menuList = menuList;\r\n  },\r\n  SET_MENU_TREE(state, menuTree) {\r\n    state.menuTree = menuTree;\r\n  },\r\n  SET_AUTH_LIST(state, authList) {\r\n    state.authList = authList;\r\n  }\r\n};\r\n\r\nconst actions = {\r\n  /**\r\n   * 获取菜单列表\r\n   */\r\n  getMenuList({ commit }) {\r\n    return getMenuList().then(({ data }) => {\r\n      // 首页路由配置\r\n      let homeRoute = routes.find(item => item.path === '/');\r\n      // 打平路由数据\r\n      let routeList = treeConvertList(\r\n        homeRoute ? asyncRoutes.concat([homeRoute]) : asyncRoutes\r\n      );\r\n      data = Array.isArray(data) ? data : [];\r\n      data = data.map(item => {\r\n        let route = routeList.find(route => item.url === route.path) || {}; // 匹配到的路由配置\r\n        route.meta = route.meta || {};\r\n        // 生成菜单配置\r\n        return {\r\n          ...item,\r\n          hidden: route.hidden,\r\n          path: item.url,\r\n          name: route.name,\r\n          meta: {\r\n            ...route.meta,\r\n            title: item.menuName || route.meta.title,\r\n            icon: item.icon || route.meta.icon\r\n          }\r\n        };\r\n      });\r\n      let menuTree = menuListConvertMenuTree(data); // 菜单列表转菜单树\r\n      menuTree = sortMenuTree(menuTree); // 菜单排序\r\n      commit('SET_MENU_LIST', data);\r\n      commit('SET_MENU_TREE', menuTree);\r\n      return data;\r\n    });\r\n  },\r\n  /**\r\n   * 获取按钮列表\r\n   */\r\n  getAuthList({ commit }) {\r\n    return getAuthList().then(({ data }) => {\r\n      let authList = data.map(item => item && item.itfCode);\r\n      commit('SET_AUTH_LIST', authList);\r\n      return data;\r\n    });\r\n  },\r\n  /**\r\n   * 获取权限数据\r\n   */\r\n  async getAuthData({ dispatch }) {\r\n    let menuList = await dispatch('getMenuList');\r\n    let authList = await dispatch('getAuthList');\r\n    return {\r\n      menuList,\r\n      authList\r\n    };\r\n  }\r\n};\r\n\r\nexport default {\r\n  namespaced: true,\r\n  state,\r\n  mutations,\r\n  actions\r\n};\r\n"]}]}
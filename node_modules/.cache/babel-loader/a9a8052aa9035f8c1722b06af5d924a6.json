FA;AAGA,MAAA,WAAA,EAAA,WAHA;AAIA,MAAA,UAAA,EAAA,OAJA;AAKA,MAAA,gBAAA,EAAA,EALA;AAMA,MAAA,sBAAA,EAAA,EANA;AAOA,MAAA,eAAA,EAAA,EAPA;AAQA,MAAA,OAAA,EAAA,IARA;AASA,MAAA,UAAA,EAAA,IATA;AAUA,MAAA,QAAA,EAAA;AACA,QAAA,KAAA,EAAA,EADA;AAEA,QAAA,WAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAVA;AAeA,MAAA,QAAA,EAAA,KAfA;AAgBA,MAAA,QAAA,EAAA,KAhBA;AAiBA,MAAA,UAAA,EAAA;AAjBA,KAAA;AAmBA,GAzBA;AA0BA,EAAA,OA1BA,qBA0BA;AACA,SAAA,OAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA;AACA,SAAA,YAAA;AACA,SAAA,oBAAA;AACA,GA9BA;AA+BA,EAAA,OA/BA,qBA+BA,CAAA,CA/BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,IAJA,kBAIA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KANA;AAQA,IAAA,YARA,0BAQA;AACA,WAAA,YAAA,CAAA,OAAA;AACA,KAVA;AAWA,IAAA,YAXA,0BAWA;AACA,UAAA,CAAA,KAAA,UAAA,CAAA,IAAA,EAAA,EAAA;AACA;AACA;;AACA,WAAA,YAAA,CAAA,QAAA;AACA,KAhBA;AAiBA,IAAA,eAjBA,2BAiBA,MAjBA,EAiBA,IAjBA,EAiBA;AACA,UAAA,MAAA,KAAA,SAAA,EAAA;AACA,YAAA,CAAA,KAAA,UAAA,CAAA,IAAA,EAAA,EAAA;AACA,iBAAA,IAAA,CAAA,KAAA,CAAA;AACA;AACA;;AACA,aAAA,IAAA,EAAA;AACA,KAxBA;AAyBA;AACA,IAAA,YA1BA,wBA0BA,IA1BA,EA0BA;AAAA;;AACA,MAAA,aAAA,CAAA;AAAA,QAAA,EAAA,EAAA,KAAA,OAAA;AAAA,QAAA,MAAA,EAAA,KAAA,UAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,MAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAGA,OANA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,CAAA;AACA,OATA;AAUA,KArCA;AAsCA,IAAA,UAtCA,wBAsCA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,KAxCA;AAyCA,IAAA,UAzCA,wBAyCA;AACA,WAAA,oBAAA;AACA,KA3CA;AA4CA,IAAA,YA5CA,0BA4CA;AAAA;;AACA,MAAA,aAAA,CAAA;AAAA,QAAA,OAAA,EAAA,KAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,aAAA,qBAAA,IAAA;AACA,OAHA;AAIA,KAjDA;AAkDA,IAAA,oBAlDA,kCAkDA;AAAA;;AACA,MAAA,qBAAA,CAAA;AACA,QAAA,OAAA,EAAA,KAAA,OADA;AAEA,QAAA,SAAA,EAAA,KAAA,YAAA,CAAA,KAAA,UAAA;AAFA,OAAA,CAAA,CAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,UAAA,IAAA,GAAA,CAAA,IAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,UAAA,GAAA,aAAA,CAAA,GAAA,EAAA;;AACA,aAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,UAAA,GAAA,aAAA,CAAA,CAAA,IAAA,CAAA,CAAA;AACA;AACA,OATA;AAUA,KA7DA;AA8DA,IAAA,WAAA,EAAA;AA9DA;AAhCA,CAAA","sourcesContent":["<template>\n  <div\n    class=\"detail-wrapper\"\n    :style=\"{ paddingBottom: initOrderInfo.status == '20100' ? '133px' : '0' }\"\n  >\n    <div class=\"detail-head\" v-if=\"initOrderInfo.status != '20100'\">\n      <div class=\"dtatil-status\">\n        <span class=\"title\">订单状态:</span>\n        <span\n          class=\"order-status\"\n          :style=\"{\n            color:\n              initOrderInfo.status == '-10200'\n                ? 'rgba(0, 0, 0, 0.45)'\n                : '#51D0BC'\n          }\"\n          ><img\n            class=\"order-status-image\"\n            :src=\"\n              initOrderInfo.status == '-10200'\n                ? require('./../../assets/images/reject.png')\n                : require('./../../assets/images/pass.png')\n            \"\n          />\n          {{ initOrderInfo.status == '-10200' ? '已驳回' : '已通过' }}</span\n        >\n      </div>\n      <div class=\"reject-reason\" v-if=\"initOrderInfo.status == '-10200'\">\n        <span class=\"title\">驳回原因: </span>{{ initOrderInfo.rejectReason }}\n      </div>\n    </div>\n    <div class=\"detail-top\">\n      <div class=\"detail-top-item\">\n        <div>\n          <span class=\"item-part item-part-top\">{{\n            initOrderInfo.cleanVegetableWeight\n          }}</span\n          ><span class=\"item-part item-part-top\">{{\n            initOrderInfo.originalVegetableWeight\n          }}</span>\n        </div>\n        <div class=\"item-bottom\">\n          <span class=\"item-part item-part-bottom\">净菜重量(kg)</span\n          ><span class=\"item-part item-part-bottom\">毛菜重量(kg)</span>\n        </div>\n      </div>\n      <div class=\"detail-top-item\">\n        <div>\n          <span class=\"item-part item-part-top\">{{\n            initOrderInfo.costPrice\n          }}</span\n          ><span class=\"item-part item-part-top\">{{\n            initOrderInfo.sellingPrice\n          }}</span>\n        </div>\n        <div class=\"item-bottom item-bottom-price\">\n          <span class=\"item-part item-part-bottom\">成本参考价(元)</span\n          ><span class=\"item-part item-part-bottom\">售价(元)</span>\n        </div>\n      </div>\n    </div>\n    <div class=\"detail-middle\">\n      <van-tabs v-model=\"activeName\" @change=\"changeTabs\">\n        <van-tab title=\"汇总采购\" name=\"total\">\n          <van-collapse w v-model=\"totalActiveNames\" class=\"collapse-content\">\n            <van-collapse-item\n              v-for=\"(item, index) in tabsData['total']\"\n              :key=\"item.orderContentDetailId\"\n              :name=\"index\"\n            >\n              <template #title>\n                <div>\n                  {{\n                    formateDate(item.arrivalDate, 4) + ' ' + item.arrivalTime\n                  }}\n                  <span style=\"float:right\">\n                    {{ totalActiveNames.includes(index) ? '收起' : '展开' }}\n                  </span>\n                </div>\n              </template>\n              <van-list>\n                <van-cell\n                  :class=\"[\n                    'detail-item',\n                    index < item.detailList.length - 1 ? 'border' : ''\n                  ]\"\n                  v-for=\"(el, index) in item.detailList\"\n                  :key=\"el.relationId\"\n                >\n                  <div>\n                    <ingredient-card :el=\"el\"></ingredient-card>\n                  </div>\n                </van-cell>\n              </van-list>\n            </van-collapse-item>\n          </van-collapse>\n        </van-tab>\n        <van-tab title=\"食材采购\" name=\"ingredients\">\n          <van-collapse\n            v-model=\"ingredientsActiveNames\"\n            class=\"collapse-content\"\n          >\n            <van-collapse-item\n              v-for=\"(item, index) in tabsData['ingredients']\"\n              :key=\"item.orderContentDetailId\"\n              :name=\"index\"\n            >\n              <template #title>\n                <div>\n                  {{\n                    formateDate(item.arrivalDate, 4) + ' ' + item.arrivalTime\n                  }}\n                  <span style=\"float:right\">{{\n                    ingredientsActiveNames.includes(index) ? '收起' : '展开'\n                  }}</span>\n                </div>\n              </template>\n              <van-list>\n                <van-cell\n                  :class=\"[\n                    'detail-item',\n                    index < item.detailList.length - 1 ? 'border' : ''\n                  ]\"\n                  v-for=\"(el, index) in item.detailList\"\n                  :key=\"el.relationId\"\n                >\n                  <div>\n                    <ingredient-card :el=\"el\"></ingredient-card>\n                  </div>\n                </van-cell>\n              </van-list>\n            </van-collapse-item>\n          </van-collapse>\n        </van-tab>\n        <van-tab title=\"菜品采购\" name=\"dish\">\n          <van-collapse v-model=\"dishActiveNames\" class=\"collapse-content\">\n            <van-collapse-item\n              v-for=\"(item, index) in tabsData['dish']\"\n              :key=\"item.arrivalDate + item.arrivalTime\"\n              :name=\"index\"\n            >\n              <template #title>\n                <div>\n                  {{\n                    formateDate(item.arrivalDate, 4) + ' ' + item.arrivalTime\n                  }}\n                  <span style=\"float:right\">{{\n                    dishActiveNames.includes(index) ? '收起' : '展开'\n                  }}</span>\n                </div>\n              </template>\n              <van-list>\n                <van-cell\n                  :class=\"[\n                    'detail-item',\n                    index < item.detailList.length - 1 ? 'border' : ''\n                  ]\"\n                  v-for=\"(el, index) in item.detailList\"\n                  :key=\"el.relationId\"\n                >\n                  <div>\n                    <div class=\"item-head\">\n                      <div class=\"name\">\n                        <!-- <i :class=\"classObj(el.status)\"></i> -->\n                        <span>{{ el.relationName }}</span>\n                      </div>\n                      <!-- <div class=\"operation\" :class=\"classObj(el.status)\">\n                          {{ statusObj[el.status + ''] }}\n                        </div> -->\n                    </div>\n                    <ul class=\"item-content\">\n                      <li>\n                        <span>餐别:</span>\n                        <div>{{ mealTypeObj[el.mealType] }}</div>\n                      </li>\n                      <li>\n                        <span>数量(份):</span>\n                        <div>{{ el.quantity }}</div>\n                      </li>\n                      <li>\n                        <span>成本参考价(元):</span>\n                        <div>{{ el.costPrice }}</div>\n                      </li>\n                      <li>\n                        <span>售价(元):</span>\n                        <div>{{ el.sellingPrice }}</div>\n                      </li>\n                    </ul>\n                  </div>\n                </van-cell>\n              </van-list>\n            </van-collapse-item>\n          </van-collapse>\n        </van-tab>\n      </van-tabs>\n    </div>\n    <div class=\"order-remark\" v-if=\"initOrderInfo.remarks\">\n      <div class=\"item-head\">\n        <div class=\"name\">\n          备注\n        </div>\n        <div class=\"remark-info\">\n          {{ initOrderInfo.remarks }}\n        </div>\n      </div>\n    </div>\n    <div class=\"detail-bottom\" v-if=\"initOrderInfo.status == '20100'\">\n      <span class=\"button\" @click=\"deny\">打回修改</span>\n      <span class=\"button\" @click=\"pass\">审核通过</span>\n    </div>\n    <van-dialog\n      v-model=\"showPass\"\n      message=\"你确定要通过审批吗？\"\n      show-cancel-button\n      confirm-button-color=\"#51D0BC\"\n      @confirm=\"passApproval\"\n    >\n    </van-dialog>\n    <van-dialog\n      v-model=\"showDeny\"\n      title=\"驳回原因\"\n      show-cancel-button\n      :confirm-button-color=\"\n        denyReason.trim() ? '#51D0BC' : 'rgba(81, 208, 188,0.5)'\n      \"\n      :before-close=\"dialogStopClose\"\n      @confirm=\"denyApproval\"\n      @cancel=\"cancelDeny\"\n    >\n      <van-field\n        v-model=\"denyReason\"\n        class=\"deny-reason\"\n        placeholder=\"请输入驳回原因\"\n        type=\"textarea\"\n        rows=\"4\"\n        border\n        maxlength=\"200\"\n      />\n    </van-dialog>\n  </div>\n</template>\n\n<script>\nimport { formateDate } from '../../plugins/common/utils.js';\nimport {\n  getOrderInfo,\n  orderAuditDetailList,\n  approveOrder\n} from './../../api/order';\nimport ingredientCard from './ingredient-card';\nconst initOrderInfo = {\n  originalVegetableWeight: '-',\n  cleanVegetableWeight: '-',\n  costPrice: '-',\n  sellingPrice: '-',\n  status: ''\n};\n//queryType 查询类型：1-食材；2-菜品；9-汇总\nconst queryTypeObj = {\n  total: '9',\n  ingredients: '1',\n  dish: '2'\n};\n//queryType 餐别\nconst mealTypeObj = {\n  '1': '早餐',\n  '2': '午餐',\n  '3': '晚餐'\n};\nexport default {\n  name: 'order-detail',\n  components: {\n    ingredientCard\n  },\n  data() {\n    return {\n      initOrderInfo,\n      queryTypeObj,\n      mealTypeObj,\n      activeName: 'total',\n      totalActiveNames: [],\n      ingredientsActiveNames: [],\n      dishActiveNames: [],\n      orderId: null,\n      contentHei: null,\n      tabsData: {\n        total: [],\n        ingredients: [],\n        dish: []\n      },\n      showPass: false,\n      showDeny: false,\n      denyReason: ''\n    };\n  },\n  created() {\n    this.orderId = this.$route.query.orderId;\n    this.getOrderInfo();\n    this.orderAuditDetailList();\n  },\n  mounted() {},\n  methods: {\n    pass() {\n      this.showPass = true;\n    },\n    deny() {\n      this.showDeny = true;\n    },\n\n    passApproval() {\n      this.approveOrder('30100');\n    },\n    denyApproval() {\n      if (!this.denyReason.trim()) {\n        return;\n      }\n      this.approveOrder('-10200');\n    },\n    dialogStopClose(action, done) {\n      if (action === 'confirm') {\n        if (!this.denyReason.trim()) {\n          return done(false);\n        }\n      }\n      return done();\n    },\n    //审批订单（30100： 通过审批； -10200：驳回）\n    approveOrder(code) {\n      approveOrder({ id: this.orderId, remark: this.denyReason, code })\n        .then(res => {\n          this.$toast('审核完成');\n          this.$router.replace({\n            path: '/approval-list'\n          });\n        })\n        .catch(e => {\n          this.$toast(e);\n        });\n    },\n    cancelDeny() {\n      this.denyReason = '';\n    },\n    changeTabs() {\n      this.orderAuditDetailList();\n    },\n    getOrderInfo() {\n      getOrderInfo({ orderId: this.orderId }).then(res => {\n        const { data } = res;\n        this.initOrderInfo = { ...data };\n      });\n    },\n    orderAuditDetailList() {\n      orderAuditDetailList({\n        orderId: this.orderId,\n        queryType: this.queryTypeObj[this.activeName]\n      }).then(res => {\n        this.tabsData[this.activeName] = res.data || [];\n        this[this.activeName + 'ActiveNames'] = [];\n        for (let i = 0; i < res.data.length; i++) {\n          this[this.activeName + 'ActiveNames'].push(i);\n        }\n      });\n    },\n    formateDate\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.detail-head {\n  padding: 20px 12px;\n  background: #fff;\n  margin-bottom: 10px;\n  color: rgba(0, 0, 0, 0.85);\n  .title {\n    font-size: 14px;\n    color: rgba(0, 0, 0, 0.65);\n    width: 70px;\n    line-height: 18px;\n  }\n}\n.detail-wrapper {\n  // position: fixed;\n  // height: 100%;\n  // background: #fff;\n}\n.detail-top {\n  padding: 20px 12px;\n  background: #fff;\n}\n.detail-top-item {\n  background: #ffffff;\n  box-shadow: 0px 0px 8px 0px rgba(0, 0, 0, 0.1);\n  border-radius: 12px;\n}\n.item-part {\n  display: inline-block;\n  width: 50%;\n  text-align: center;\n}\n.item-part-top {\n  line-height: 44px;\n  font-weight: 500;\n  color: rgba(0, 0, 0, 0.85);\n  font-size: 16px;\n}\n.item-bottom {\n  background: rgba(81, 208, 188, 0.3);\n  line-height: 29px;\n  border-radius: 0px 0px 12px 12px;\n  color: rgba(0, 0, 0, 0.85);\n  font-size: 13px;\n  font-weight: 400;\n}\n.item-bottom-price {\n  background: rgba(255, 175, 114, 0.3);\n}\n.detail-top-item:not(:first-child) {\n  margin-top: 12px;\n}\n/deep/.van-collapse-item__content {\n  padding-right: 0;\n}\n/deep/ .van-cell {\n  font-size: 13px;\n  &:after {\n    border-bottom: none;\n  }\n}\n.detail-item {\n  &.border {\n    &:after {\n      border-bottom: 1px solid #ebedf0;\n    }\n  }\n}\n.detail-bottom {\n  height: 133px;\n  position: fixed;\n  width: 100%;\n  text-align: center;\n  bottom: 0;\n  padding: 30px 12px 0px 12px;\n  box-sizing: border-box;\n  background: linear-gradient(\n    180deg,\n    rgba(255, 255, 255, 0) 0%,\n    rgba(255, 255, 255, 0.9) 0%,\n    #ffffff 100%\n  );\n}\n.button {\n  display: inline-block;\n  width: 158px;\n  height: 49px;\n  line-height: 49px;\n  text-align: center;\n  border: 1px solid #51d0bc;\n  border-radius: 3px;\n  color: #fff;\n  background: #51d0bc;\n}\n.button:first-child {\n  margin-right: 11px;\n  color: #51d0bc;\n  background: #fff;\n}\n.item-head {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 15px;\n  .name {\n    line-height: 35px;\n\n    span {\n      font-size: 14px;\n      // margin-left: 10px;\n      font-family: PingFangSC-Medium, PingFang SC;\n      font-weight: 500;\n      color: rgba(0, 0, 0, 0.85);\n    }\n  }\n}\n.item-content {\n  display: flex;\n  justify-content: space-between;\n  flex-wrap: wrap;\n  li {\n    width: 49%;\n    margin-bottom: 12px;\n    font-size: 14px;\n    color: rgba(0, 0, 0, 0.65);\n    div {\n      display: inline-block;\n      margin-left: 6px;\n      font-weight: 400;\n      color: rgba(0, 0, 0, 0.85);\n    }\n  }\n}\n.collapse-content {\n  overflow-y: auto;\n  box-sizing: border-box;\n  background: #fff;\n}\n.deny-reason {\n  /deep/textarea {\n    border-radius: 4px;\n    border: 1px solid #dedede;\n    line-height: 24px;\n    padding: 10px;\n  }\n}\n.last-item::after {\n  border-bottom: 0px;\n}\n/deep/.van-hairline--top-bottom::after,\n/deep/.van-hairline-unset--top-bottom::after {\n  border-width: 0;\n}\n.order-remark {\n  padding: 16px 12px;\n  font-size: 14px;\n  color: rgba(0, 0, 0, 0.85);\n  background: #fff;\n  margin-top: 10px;\n  .item-head {\n    display: block;\n  }\n  .remark-info {\n    font-weight: 400;\n  }\n}\n.van-collapse-item {\n  /deep/.van-cell--clickable {\n    background: #f5f5f5;\n  }\n}\n.dtatil-status {\n  display: flex;\n  align-items: center;\n}\n.order-status {\n  display: flex;\n  flex: 1;\n  justify-content: flex-end;\n}\n.reject-reason {\n  margin-top: 5px;\n  word-break: break-all;\n  line-height: 18px;\n  .title {\n    float: left;\n  }\n}\n.order-status-image {\n  display: block;\n  width: 21px;\n  height: 21px;\n  margin-right: 6px;\n}\n</style>\n"],"sourceRoot":"src/pages/catering-h5/views/order-detail"}]}
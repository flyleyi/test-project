AOD","sourcesContent":["import axios from '@/plugins/axios';\n/**\n * 查询接送地区信息列表分页\n * @param {*} params 查询参数\n * @param {*} extendDisable 扩展禁用属性\n * @returns {Promise}\n */\nexport function areaQueryPage(params, extendDisable = false) {\n  return axios(\n    {\n      method: 'GET',\n      url: '/shuttleArea/queryPage',\n      params\n    },\n    'simsbff'\n  ).then(res => {\n    let data = res.data;\n    if (extendDisable && Array.isArray(data.records)) {\n      let disabledArr = [];\n      data.records.forEach((item, index) => {\n        if (!item.isShow) {\n          disabledArr.push(item);\n          item.disabled = true;\n          data.records.splice(index, 1);\n        }\n      });\n      data.records.push(...disabledArr);\n    }\n    return res;\n  });\n}\n\n/**\n * 新增\n * @param {*} data 新增接送地区数据\n * @returns {Promise}\n */\nexport function addArea(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttleArea/add',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 编辑\n * @param {*} data 编辑地区数据\n * @returns {Promise}\n */\nexport function updateArea(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttleArea/update',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 查询接送区县信息列表分页\n * @param {*} params 查询参数\n * @param {*} extendDisable 扩展禁用属性\n * @returns {Promise}\n */\nexport function districtQueryPage(params, extendDisable = false) {\n  return axios(\n    {\n      method: 'GET',\n      url: '/shuttleDistrict/queryPage',\n      params\n    },\n    'simsbff'\n  ).then(res => {\n    let data = res.data;\n    if (extendDisable && Array.isArray(data.records)) {\n      let disabledArr = [];\n      data.records.forEach((item, index) => {\n        if (!item.isShow) {\n          disabledArr.push(item);\n          item.disabled = true;\n          data.records.splice(index, 1);\n        }\n      });\n      data.records.push(...disabledArr);\n    }\n    return res;\n  });\n}\n\n/**\n * 新增\n * @param {*} data 新增接送区县数据\n * @returns {Promise}\n */\nexport function addDistrict(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttleDistrict/add',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 编辑\n * @param {*} data 编辑区县数据\n * @returns {Promise}\n */\nexport function updateDistrict(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttleDistrict/update',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 查询下车点信息列表分页\n * @param {*} params 查询参数\n * @param {*} extendDisable 扩展禁用属性\n * @returns {Promise}\n */\nexport function pointQueryPage(params, extendDisable) {\n  return axios(\n    {\n      method: 'GET',\n      url: '/shuttlePoint/queryPage',\n      params\n    },\n    'simsbff'\n  ).then(res => {\n    let data = res.data;\n    extendDisable &&\n      Array.isArray(data.records) &&\n      data.records.forEach(item => {\n        item.disabled = !item.isShow;\n      });\n    return res;\n  });\n}\n\n/**\n * 新增\n * @param {*} data 新增下车点数据\n * @returns {Promise}\n */\nexport function addPoint(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttlePoint/add',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 编辑\n * @param {*} data 编辑下车点数据\n * @returns {Promise}\n */\nexport function updatePoint(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttlePoint/update',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 查询下车点与学校关联列表分页\n * @param {*} params 查询参数\n * @param {*} extendDisable 扩展禁用属性\n * @returns {Promise}\n */\nexport function pointSchoolQueryPage(params, extendDisable = false) {\n  return axios(\n    {\n      method: 'GET',\n      url: '/shuttlePointSchool/queryPage',\n      params\n    },\n    'simsbff'\n  ).then(res => {\n    let data = res.data;\n    if (extendDisable && Array.isArray(data.records)) {\n      let disabledArr = [];\n      data.records.forEach((item, index) => {\n        if (!item.isShow || !item.pointIsShow) {\n          disabledArr.push(item);\n          item.disabled = true;\n          data.records.splice(index, 1);\n        }\n      });\n      data.records.push(...disabledArr);\n    }\n    return res;\n  });\n}\n\n/**\n * 新增\n * @param {*} data 新增下车点与学校关联数据\n * @returns {Promise}\n */\nexport function addPointSchool(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttlePointSchool/add',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 编辑\n * @param {*} data 编辑下车点与学校关联数据\n * @returns {Promise}\n */\nexport function updatePointSchool(data) {\n  return axios(\n    {\n      method: 'POST',\n      url: '/shuttlePointSchool/update',\n      data\n    },\n    'simsbff'\n  );\n}\n\n/**\n * 根据学校编码查询接送\n * @param {String} schoolCode 学校编码\n * @returns {Promise}\n */\nexport function queryShuttleTypeBySchoolCode(schoolCode) {\n  return axios(\n    {\n      method: 'GET',\n      url: `/shuttlePointSchool/shuttleType/${schoolCode}`\n    },\n    'simsbff'\n  );\n}\n"]}]}
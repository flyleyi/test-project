;AACA,MAAA,WAAA,CACA,IADA,CACA,gBAAA;AAAA,YAAA,IAAA,QAAA,IAAA;;AACA,YAAA,IAAA,CAAA,OAAA,IAAA,CAAA,IAAA,IAAA,CAAA,OAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA;;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,KAAA,CAAA,IAAA,CAAA,MAAA,CAAA,IAAA,CAAA,OAAA,CAAA;;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,UAAA,EAAA,KAAA,CAAA,IAAA;;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA,IAAA,CAAA;;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,KAAA,IAAA,IAAA,CAAA,OAAA,GAAA,IAAA,CAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,KAAA,CAAA,UAAA,CAAA,MAAA,GACA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,UAAA,CAAA,MAAA,GAAA,KAAA,CAAA,UAAA,CAAA,KAAA,IAAA,CAAA,IACA,KAAA,CAAA,UAAA,CAAA,KAFA;AAGA;;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,QAAA,UAAA,CAAA,YAAA;AACA,cAAA,KAAA,CAAA,OAAA,EAAA;AACA,YAAA,KAAA,CAAA,MAAA,CAAA,MAAA;AACA;AACA,SAJA,CAAA;AAKA,OAtBA,EAuBA,KAvBA,CAuBA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,OAAA,CAAA;AACA,UAAA,OAAA,EAAA,CAAA,CAAA,OADA;AAEA,UAAA,WAAA,EAAA;AAFA,SAAA;AAIA,OA/BA;AAgCA,KAnDA;AAoDA,IAAA,MApDA,oBAoDA;AACA,UAAA,KAAA,UAAA,EAAA;AACA,aAAA,IAAA,GAAA,EAAA;AACA,aAAA,UAAA,GAAA,KAAA;AACA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,OAAA;AACA,KA3DA;AA4DA,IAAA,SA5DA,uBA4DA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,QAAA,CAAA,KAAA,GAAA,CAAA;AACA,WAAA,UAAA,CAAA,MAAA,GAAA,CAAA,CALA,CAMA;;AACA,WAAA,eAAA,GAAA,CAAA;AACA,WAAA,MAAA;AACA,KArEA;AAsEA,IAAA,MAtEA,kBAsEA,MAtEA,EAsEA;AACA;AACA,MAAA,QAAA,CAAA,eAAA,CAAA,SAAA,GAAA,MAAA;AACA,MAAA,QAAA,CAAA,IAAA,CAAA,SAAA,GAAA,MAAA;AACA,KA1EA;AA2EA,IAAA,YA3EA,wBA2EA,CA3EA,EA2EA;AACA,WAAA,eAAA,GACA,MAAA,CAAA,WAAA,IACA,QAAA,CAAA,eAAA,CAAA,SADA,IAEA,QAAA,CAAA,IAAA,CAAA,SAHA,CADA,CAIA;AACA;AAhFA,GAtBA;AAwGA,EAAA,OAxGA,qBAwGA;AACA,IAAA,MAAA,CAAA,gBAAA,CAAA,QAAA,EAAA,KAAA,YAAA,EAAA,IAAA,EADA,CACA;AACA,GA1GA;AA2GA,EAAA,WA3GA,yBA2GA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,QAAA,EAAA,KAAA,YAAA,EAAA,IAAA;AACA,GA7GA,CA6GA;;AA7GA,CAAA","sourcesContent":["<template>\n  <div>\n    <van-pull-refresh v-model=\"refreshing\" @refresh=\"onRefresh\">\n      <van-list\n        v-model=\"loading\"\n        :finished=\"finished\"\n        finished-text=\"没有更多了\"\n        @load=\"onLoad\"\n        class=\"order-list\"\n        ref=\"listRef\"\n      >\n        <div v-if=\"list.length == 0\">\n          <div v-if=\"loading\" class=\"no-data-loading\">\n            <van-loading></van-loading>\n          </div>\n\n          <no-data\n            v-else\n            :notice=\"hasNotice ? '点击下方按钮立即预约' : ''\"\n          ></no-data>\n        </div>\n\n        <div v-else>\n          <template v-for=\"(item, index) in list\">\n            <slot :item=\"item\" :index=\"index\"></slot>\n          </template>\n        </div>\n      </van-list>\n    </van-pull-refresh>\n  </div>\n</template>\n\n<script>\nimport noData from '../my-visitor/components/NoData.vue';\nexport default {\n  name: 'refresh-list',\n  props: ['getListData', 'cardType', 'otherParams', 'hasNotice', 'isAudit'],\n  components: { noData },\n  data() {\n    return {\n      list: [],\n      loading: false,\n      finished: false,\n      refreshing: false,\n      pageInfo: {\n        //填报信息总页数\n        total: 0\n      },\n      pageParams: {\n        //填报分页参数\n        limit: 10,\n        offset: 0\n      },\n      scrollTopHeight: 0\n    };\n  },\n  methods: {\n    //点击订单跳转详情\n    clickCard(item) {\n      this.$emit('clickCard', item);\n    },\n    /**\n     * 获取列表数据\n     */\n    getData() {\n      if (this.pageParams.offset > this.pageInfo.total) return;\n      // this.load = true;\n      let promiseData;\n\n      promiseData = this.getListData({\n        ...this.pageParams,\n        ...this.otherParams\n      });\n      this.loading = true;\n      let height = this.scrollTopHeight;\n      promiseData\n        .then(({ data }) => {\n          if (data.current == 1 && data.records.length == 0) {\n            this.list = [];\n            this.finished = true;\n          }\n          this.list = this.list.concat(data.records);\n          this.$emit('dataList', this.list);\n          this.pageInfo.total = data.total || 0;\n          if (this.pageInfo.total <= data.current * data.size) {\n            this.finished = true;\n          } else {\n            this.pageParams.offset =\n              (Math.floor(this.pageParams.offset / this.pageParams.limit) + 1) *\n              this.pageParams.limit;\n          }\n          this.loading = false;\n          setTimeout(() => {\n            if (this.isAudit) {\n              this.setTop(height);\n            }\n          });\n        })\n        .catch(e => {\n          this.error = true;\n          this.finished = true;\n          this.loading = false;\n          this.$toast.loading({\n            message: e.message,\n            forbidClick: true\n          });\n        });\n    },\n    onLoad() {\n      if (this.refreshing) {\n        this.list = [];\n        this.refreshing = false;\n      }\n      this.loading = true;\n      this.getData();\n    },\n    onRefresh() {\n      this.list = [];\n      this.finished = false;\n      this.loading = true;\n      this.pageInfo.total = 0;\n      this.pageParams.offset = 0;\n      // 审核列表下拉刷新滚动高度重置\n      this.scrollTopHeight = 0;\n      this.onLoad();\n    },\n    setTop(height) {\n      // window.pageYOffset = height;\n      document.documentElement.scrollTop = height;\n      document.body.scrollTop = height;\n    },\n    handleScroll(e) {\n      this.scrollTopHeight =\n        window.pageYOffset ||\n        document.documentElement.scrollTop ||\n        document.body.scrollTop; //兼容不同的浏览器\n    }\n  },\n  created() {\n    window.addEventListener('scroll', this.handleScroll, true); //监听滚动事件\n  },\n  deactivated() {\n    window.removeEventListener('scroll', this.handleScroll, true);\n  } //关闭页面时需要移除监听事件\n};\n</script>\n\n<style lang=\"less\" scoped>\n.order-list {\n  padding: 12px;\n}\n.no-data-loading {\n  width: 100vw;\n  height: 70vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n</style>\n"],"sourceRoot":"src/pages/visitor-h5/views/refresh-list"}]}
{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/score-analysis-admin/views/components/ExamManage/PaperManage/MarkSubjectInputMode/index.vue?vue&type=style&index=0&id=6ea112b8&lang=less&scoped=true&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/score-analysis-admin/views/components/ExamManage/PaperManage/MarkSubjectInputMode/index.vue","mtime":1645178321164},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/less-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/style-resources-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA43BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/pages/score-analysis-admin/views/components/ExamManage/PaperManage/MarkSubjectInputMode","sourcesContent":["<!-- MarkSubjectInputMode -->\n<template>\n  <div class=\"content\">\n    <div class=\"flex title\">\n      <h2>\n        {{ $route.query.gradeNameList }}{{ $route.query.examName\n        }}<br />试卷：{{ $route.query.paperName }}\n      </h2>\n      <div class=\"btn-box\">\n        <el-button type=\"primary\" size=\"mini\" @click=\"save\" :loading=\"loading\"\n          >保存</el-button\n        >\n        <el-button\n          size=\"mini\"\n          type=\"primary\"\n          icon=\"el-icon-plus\"\n          @click=\"handleAdd(tableData.length)\"\n          >新增题号</el-button\n        >\n      </div>\n    </div>\n\n    <el-table\n      v-loading=\"loading\"\n      :data=\"tableData\"\n      border\n      style=\"width: 100%\"\n      :row-style=\"{ 'font-size': '14px', color: 'rgba(0, 0, 0, 0.65)' }\"\n      :header-cell-style=\"{\n        background: '#F3F4F7',\n        'text-align': 'cneter',\n        color: 'rgba(0, 0, 0, 0.85)',\n        'font-weight': 600\n      }\"\n    >\n      <el-table-column label=\"序号\" type=\"index\" width=\"60\"> </el-table-column>\n      <el-table-column label=\"题号\" width=\"150\">\n        <template slot-scope=\"scope\">\n          <el-input\n            type=\"textarea\"\n            :rows=\"1\"\n            maxlength=\"20\"\n            v-model=\"scope.row.questionNumber\"\n            placeholder=\"题号\"\n            @focus=\"changePage\"\n            v-emoji\n          ></el-input>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"分值\" width=\"120\">\n        <template slot-scope=\"scope\">\n          <el-input\n            @focus=\"changePage\"\n            @input=\"\n              numberChange(scope.$index, arguments[0], routeParams.totalScore)\n            \"\n            @blur=\"\n              numberChangeBlur(\n                scope.$index,\n                scope.row.questionScore,\n                routeParams.totalScore\n              )\n            \"\n            v-model.number=\"scope.row.questionScore\"\n            placeholder=\"请输入分值\"\n          ></el-input>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"考察知识点\">\n        <template slot=\"header\">\n          <div @click=\"openTeachingDialog\" style=\"cursor: pointer;\">\n            <span>考察知识点</span>\n            <span style=\"color: #5481e9;margin-left:5px;font-size:12px;\">\n              <img\n                src=\"./../../../../../assets/images/tip.png\"\n                style=\"width:20px;vertical-align:middle;\"\n              />\n              <span style=\"vertical-align:middle;line-height:20px\"\n                >操作说明</span\n              >\n            </span>\n          </div>\n        </template>\n        <template slot-scope=\"scope\">\n          <div class=\"border-box\" @click=\"inputFocus($event)\">\n            <!-- 知识点列表 -->\n            <div class=\"list-box\">\n              <span\n                class=\"item clearfix\"\n                :class=\"{\n                  'item-gray':\n                    knowledgeIndex === scope.row._selfKnowledgeArr.length - 1 &&\n                    item.name.indexOf('-') === item.name.length - 1,\n                  'item-edit': item.edit,\n                  'item-del':\n                    knowledgeIndex === scope.row._selfKnowledgeArr.length - 1 &&\n                    scope.row.delStatus === 1\n                }\"\n                v-for=\"(item, knowledgeIndex) in scope.row._selfKnowledgeArr\"\n                :key=\"knowledgeIndex\"\n              >\n                <!-- 知识点编辑 -->\n                <div v-if=\"item.edit\" class=\"edit-box\">\n                  <el-input\n                    v-emoji\n                    maxlength=\"100\"\n                    v-model=\"scope.row.knowledgeSecond\"\n                    @keyup.native.enter=\"handelKeyupEnter(scope.row)\"\n                    @input=\"\n                      editKnowledgeInput('second', scope.$index, scope.row)\n                    \"\n                  ></el-input>\n                  <span>-</span>\n                  <el-input\n                    v-emoji\n                    maxlength=\"100\"\n                    v-model=\"scope.row.knowledgeThird\"\n                    @keyup.native.enter=\"handelKeyupEnter(scope.row)\"\n                    @input=\"\n                      editKnowledgeInput('third', scope.$index, scope.row)\n                    \"\n                  ></el-input>\n                </div>\n                <!-- 显示 二级-三级 -->\n                <span v-else>\n                  <el-tooltip\n                    effect=\"dark\"\n                    :content=\"item.name\"\n                    placement=\"top\"\n                  >\n                    <span\n                      @click=\"editKnowledge(scope.row, knowledgeIndex)\"\n                      class=\"text\"\n                      >{{ item.name }}</span\n                    >\n                  </el-tooltip>\n                  <i\n                    class=\"el-icon-close\"\n                    @click=\"delChild('knowledge', scope.row, knowledgeIndex)\"\n                  ></i>\n                </span>\n              </span>\n              <!-- 知识点录入 -->\n              <el-input\n                class=\"self-input\"\n                :class=\"{\n                  'self-input-width': scope.row._selfKnowledgeArr.length === 0\n                }\"\n                v-if=\"!scope.row.editKnowledgeFlag\"\n                maxlength=\"100\"\n                v-emoji\n                :placeholder=\"\n                  scope.row._selfKnowledgeArr.length > 0\n                    ? ''\n                    : '示例：父级知识点-子级知识点1/子级知识点2；'\n                \"\n                @focus=\"changePage\"\n                @blur=\"scope.row.delStatus = 0\"\n                v-model=\"scope.row.knowledge\"\n                @input=\"handelInputKnowledge(scope.$index, scope.row)\"\n                @keyup.native.189=\"handelKeyup('-', scope.$index, scope.row)\"\n                @keyup.native.186=\"handelKeyup(';', scope.$index, scope.row)\"\n                @keyup.native.191=\"handelKeyup('/', scope.$index, scope.row)\"\n                @keyup.native.8=\"handelKeyupDel(scope.row)\"\n                @compositionstart.native=\"compositionstart(scope.row)\"\n                @compositionend.native=\"compositionend(scope.row)\"\n              ></el-input>\n            </div>\n          </div>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"abilityList\" label=\"考察能力\">\n        <template slot-scope=\"scope\">\n          <div class=\"flex scope-box\">\n            <el-select\n              v-model=\"scope.row._selfAbilityArr\"\n              multiple\n              filterable\n              placeholder=\"请选择\"\n              value-key=\"id\"\n              @change=\"handleChangeAbility\"\n            >\n              <el-option\n                v-for=\"item in treedAbilityData\"\n                :key=\"item.id\"\n                :label=\"item.name\"\n                :value=\"item\"\n              >\n              </el-option>\n            </el-select>\n            <div class=\"operat-box\">\n              <div class=\"operat-box-content\">\n                <i\n                  class=\"el-icon-circle-plus-outline\"\n                  @click=\"handleAdd(scope.$index, scope.row)\"\n                ></i>\n                <i\n                  class=\"el-icon-remove-outline\"\n                  @click=\"del(scope.$index, scope.row)\"\n                ></i>\n              </div>\n            </div>\n          </div>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <!-- 教程 start -->\n    <el-dialog\n      title=\"使用教程\"\n      :visible.sync=\"teachingDialogVisible\"\n      width=\"800px\"\n      class=\"teaching-dialog\"\n    >\n      <div class=\"dialog-content\">\n        <div class=\"item\">\n          <div class=\"item-title\">请录入：父级知识点-子级知识点</div>\n          <div>\n            以下3个快捷符号，帮助您更快录入： ① 短横线 “ - ” ② 分号“ ；” ③ 斜杠“\n            / ”\n          </div>\n        </div>\n        <div class=\"item\">\n          <div class=\"item-title\">短横线“ - ”</div>\n          <div>录入父级知识点后，输入短横线 - ，继续输入子级知识点</div>\n          <img src=\"../../../../../assets/images/01.gif\" alt=\"\" />\n        </div>\n        <div class=\"item\">\n          <div class=\"item-title\">分号“ ；”</div>\n          <div>\n            录入父级知识点-子级知识点后，输入分号\n            ；(中英文均可)，表示一组知识点录入完成。将继续输入下一组知识点\n          </div>\n          <img src=\"../../../../../assets/images/02.gif\" alt=\"\" />\n        </div>\n        <div class=\"item\">\n          <div class=\"item-title\">斜杠“ / ”</div>\n          <div>\n            当某个父级知识点下，考核了多个子级知识点时，输入 斜杠/\n            分隔子级知识点，可减少重复录入父级\n          </div>\n          <img src=\"../../../../../assets/images/03.gif\" alt=\"\" />\n        </div>\n        <div class=\"item\">\n          <div class=\"item-title\">编辑已录入的知识点</div>\n          <div>\n            ①点击知识点标签，进入编辑状态；②修改完成按「enter」键提交；\n          </div>\n          <img src=\"../../../../../assets/images/04.gif\" alt=\"\" />\n        </div>\n      </div>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button\n          type=\"primary\"\n          size=\"small\"\n          @click=\"teachingDialogVisible = false\"\n          >关闭</el-button\n        >\n      </span>\n    </el-dialog>\n    <!-- 教程 end -->\n  </div>\n</template>\n\n<script>\nimport api from './../../../../../api/question';\nexport default {\n  name: 'MarkSubjectInputMode',\n  data() {\n    return {\n      changeNosave: false,\n      routeParams: {}, //路由接收参数\n      tableData: [],\n      item: {\n        knowledge: '',\n        questionNumber: null,\n        questionScore: null,\n        _selfKnowledgeArr: [],\n        _selfAbilityArr: []\n      },\n      loading: false,\n      treedAbilityData: [],\n      currentItem: {}, //当前操作项\n      currentIndex: null, //当前操作项index\n      teachingDialogVisible: false\n    };\n  },\n\n  components: {},\n\n  created() {\n    this.routeParams = { ...this.$route.query };\n    // 能力列表\n    this.getAbility();\n  },\n  mounted() {\n    window.onbeforeunload = function(event) {\n      return confirm('确定退出吗');\n    };\n  },\n  beforeDestroy() {\n    window.onbeforeunload = null;\n  },\n  methods: {\n    // 能力点数据\n    getAbility() {\n      api\n        .getAbility(this.routeParams.stageId, this.routeParams.subjectId)\n        .then(({ data }) => {\n          this.loading = true;\n          this.treedAbilityData = data;\n          // 题目列表\n          this.getQuestionById();\n        });\n    },\n    // 题目列表\n    getQuestionById() {\n      api.getQuestionById(this.routeParams.paperId).then(({ data }) => {\n        data.forEach(item => {\n          let abilityList = item.abilityList.split(',');\n          let knowledgeNameList = item.knowledgeNameList.split(';');\n          let abilityNameList = item.abilityNameList.split(';');\n          let abilityDescList =\n            item.abilityDescList && item.abilityDescList.split('$$');\n          let _selfKnowledgeArr = [];\n          let _selfAbilityArr = [];\n\n          knowledgeNameList.forEach((knowledgeItem, index) => {\n            _selfKnowledgeArr.push({\n              edit: false,\n              name: knowledgeItem\n            });\n          });\n          abilityList.forEach((abilityItem, index) => {\n            _selfAbilityArr.push({\n              id: parseInt(abilityItem),\n              name: abilityNameList[index],\n              intro: (abilityDescList && abilityDescList[index]) || ' '\n            });\n          });\n          this.tableData.push({\n            questionNumber: item.questionNumber,\n            questionScore: item.questionScore,\n            _selfKnowledgeArr,\n            _selfAbilityArr,\n            knowledge: '', //正在输入的知识点\n            editKnowledgeFlag: false, //一行中的知识点都没有在编辑状态\n            currentKnowledgeIndex: null, //编辑中的知识点index\n            knowledgeSecond: '', //二级知识点\n            knowledgeThird: '', //三级知识点\n            delStatus: 0 //待删除状态\n          });\n        });\n        if (data.length === 0) {\n          let hasPaperId = false;\n          let localArr =\n            localStorage.getItem('localArr') &&\n            JSON.parse(localStorage.getItem('localArr'));\n          localArr &&\n            localArr.forEach(item => {\n              if (\n                item.paperId == this.routeParams.paperId &&\n                item.tableData.length\n              ) {\n                hasPaperId = true;\n                this.tableData = [];\n                this.tableData.push(...item.tableData);\n              }\n            });\n          if (!hasPaperId) {\n            for (let i = 1; i < Number(this.routeParams.num) + 1; i++) {\n              this.tableData.push({\n                questionNumber: i,\n                questionScore: '',\n                _selfKnowledgeArr: [],\n                _selfAbilityArr: [],\n                knowledge: '', //正在输入的知识点\n                editKnowledgeFlag: false, //一行中的知识点都没有在编辑状态\n                currentKnowledgeIndex: null, //编辑中的知识点index\n                knowledgeSecond: '', //二级知识点\n                knowledgeThird: '', //三级知识点\n                delStatus: 0 //待删除状态\n              });\n            }\n          }\n        }\n        this.loading = false;\n      });\n    },\n    // 保存前验证\n    save() {\n      let arr = [];\n      let _selfTotalScore = 0;\n      let arrD = []; //this.tableData中不重复数据\n      let emptyFlag = false; //列表有空\n      let editFlag = false; //列表有编辑\n      let repeatIndex = []; //重复题号\n      if (this.tableData.length === 0) {\n        return this.$message({\n          type: 'warning',\n          message: '缺少题目信息，无法提交'\n        });\n      }\n      try {\n        this.tableData.forEach((item, index) => {\n          let abilityNameStr = '';\n          let abilityIdStr = '';\n          let abilityDescListStr = '';\n          let knowledgeNameStr = '';\n          _selfTotalScore += item.questionScore;\n\n          item._selfAbilityArr.forEach(ability => {\n            abilityNameStr += ability.name + ';';\n            abilityIdStr += ability.id + ',';\n            if (ability.intro) {\n              abilityDescListStr += ability.intro + '$$';\n            } else {\n              abilityDescListStr += ' ' + '$$';\n            }\n          });\n          item._selfKnowledgeArr.forEach(knowledge => {\n            knowledgeNameStr += knowledge.name + ';';\n          });\n\n          // 列表有空\n          if (\n            item.questionNumber == '' ||\n            item.questionScore == '' ||\n            abilityNameStr == '' ||\n            knowledgeNameStr == ''\n          ) {\n            emptyFlag = true;\n            this.$message({\n              type: 'warning',\n              message: `第${index + 1}行信息不完整，请补充`\n            });\n            throw Error();\n          }\n          // 有编辑状态\n          if (item.editKnowledgeFlag) {\n            editFlag = true;\n            this.$message({\n              type: 'warning',\n              message: `第${index + 1}行知识点正在编辑，请先提交`\n            });\n            throw Error();\n          }\n          // 题号重复判断\n          if (\n            arrD.every(itemD => itemD.questionNumber != item.questionNumber)\n          ) {\n            arrD.push(item);\n          } else {\n            repeatIndex.push(index);\n          }\n          arr.push({\n            questionNumber: item.questionNumber,\n            questionScore: item.questionScore,\n            abilityNameList: abilityNameStr.substr(\n              0,\n              abilityNameStr.length - 1\n            ),\n            abilityList: abilityIdStr.substr(0, abilityIdStr.length - 1),\n            abilityDescList: abilityDescListStr.substr(\n              0,\n              abilityDescListStr.length - 2\n            ),\n            knowledgeNameList: knowledgeNameStr.substr(\n              0,\n              knowledgeNameStr.length - 1\n            ),\n            paperId: this.routeParams.paperId\n          });\n        });\n      } catch (e) {}\n      // 列表有空\n      if (emptyFlag) return;\n      // 列表有编辑\n      if (editFlag) return;\n      // 题号重复\n      if (repeatIndex.length > 0) {\n        let repeatFirstIndex = this.repeatQuestion(repeatIndex);\n        return this.$message({\n          type: 'warning',\n          message: `第${repeatFirstIndex + 1}行和第${repeatIndex[0] +\n            1}行题号重复，请检查`\n        });\n      }\n      // 总分不等于 满分\n      if (_selfTotalScore != this.routeParams.totalScore) {\n        this.tipTotalScore(_selfTotalScore, this.routeParams.totalScore, arr);\n        return;\n      }\n      this.saveQuestion(arr);\n    },\n    // 小题分相加不等于总分提示\n    tipTotalScore(_selfTotalScore, totalScore, arr) {\n      if (_selfTotalScore > 300) {\n        return this.$message({\n          type: 'warning',\n          message: '满分不能超过300分'\n        });\n      } else {\n        this.$confirm(\n          `<div style=\"text-align:center;padding:20px 0\">小题分相加等于${_selfTotalScore}分，与预设试卷满分${totalScore}不符，<br>继续提交将把试卷满分设为${_selfTotalScore}分，请确认！</div>`,\n          '提醒',\n          {\n            dangerouslyUseHTMLString: true,\n            confirmButtonText: '继续提交',\n            cancelButtonText: '返回检查',\n            type: ''\n          }\n        ).then(() => {\n          this.saveQuestion(arr);\n        });\n      }\n    },\n    // 题号重复\n    repeatQuestion(repeatIndex) {\n      let repeatFirstIndex = this.tableData.findIndex(item => {\n        return (\n          item.questionNumber == this.tableData[repeatIndex[0]].questionNumber\n        );\n      });\n      return repeatFirstIndex;\n    },\n    // 保存数据\n    saveQuestion(arr) {\n      console.log(arr);\n      this.loading = true;\n      api\n        .saveQuestion(\n          this.routeParams.paperId,\n          this.routeParams.markType,\n          this.routeParams.stageId,\n          arr\n        )\n        .then(() => {\n          this.$message({\n            type: 'success',\n            message: '保存成功!'\n          });\n          this.changeNosave = false;\n          this.loading = false;\n        })\n        .catch(() => {\n          this.loading = false;\n        });\n    },\n    // 选择能力点\n    handleChangeAbility() {\n      this.setLocalStorageVal();\n    },\n    // 设置 localStorage\n    setLocalStorageVal() {\n      let localArr = JSON.parse(localStorage.getItem('localArr'));\n      console.log(localArr, '设置前');\n      localArr.forEach(item => {\n        if (item.paperId == this.routeParams.paperId) {\n          item.tableData = [];\n          this.tableData.forEach(tableDataItem => {\n            item.tableData.push({\n              ...tableDataItem,\n              currentKnowledgeIndex: null,\n              delStatus: 0,\n              editKnowledgeFlag: false,\n              knowledge: '',\n              knowledgeSecond: '',\n              knowledgeThird: ''\n            });\n          });\n          // item.tableData = this.tableData;\n          item.hasInputVal = !this.judgeArrItemEmpty(this.tableData);\n        }\n      });\n      localStorage.setItem('localArr', JSON.stringify(localArr));\n      console.log(JSON.parse(localStorage.getItem('localArr')), '设置后');\n    },\n    // 删除\n    del(index, row) {\n      if (\n        !row.questionScore &&\n        row._selfKnowledgeArr.length === 0 &&\n        row._selfAbilityArr.length === 0\n      ) {\n        this.tableData.splice(index, 1);\n      } else {\n        this.$confirm('删除后不可恢复，确定删除？', '删除确认', {\n          customClass: 'box-danger',\n          center: true,\n          type: 'warning'\n        })\n          .then(() => {\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            });\n            this.tableData.splice(index, 1);\n            this.changePage();\n            this.setLocalStorageVal();\n          })\n          .catch(() => {\n            this.$message({\n              type: 'info',\n              message: '已取消删除'\n            });\n          });\n      }\n    },\n    // 新增\n    handleAdd(index) {\n      if (this.tableData.length < 100) {\n        this.tableData.splice(index + 1, 0, {\n          questionNumber: '',\n          questionScore: '',\n          _selfKnowledgeArr: [],\n          _selfAbilityArr: [],\n          knowledge: '', //正在输入的知识点\n          editKnowledgeFlag: false, //一行中的知识点都没有在编辑状态\n          currentKnowledgeIndex: null, //编辑中的知识点index\n          knowledgeSecond: '', //二级知识点\n          knowledgeThird: '', //三级知识点\n          delStatus: 0 //待删除状态\n        });\n      } else {\n        this.$message({\n          type: 'warning',\n          message: '题目数量最多为100，无法新增更多'\n        });\n      }\n    },\n    // 点击border-box 录入输入框获取焦点\n    inputFocus(e) {\n      if (\n        e.target.getAttribute('class') === 'list-box' ||\n        e.target.getAttribute('class') === 'border-box'\n      ) {\n        e.currentTarget.getElementsByTagName('input')[0].focus();\n      }\n    },\n    // 知识点录入\n    handelInputKnowledge(index, row) {\n      row.delStatus = -1;\n      this.currentItem = row;\n      this.currentIndex = index;\n    },\n    // - ;键盘事件\n    handelKeyup(type, index, row) {\n      this.currentItem = row;\n      this.currentIndex = index;\n      let regAll = /^[-;；/]/;\n      let reg = /[-]$/; //检测二级知识点\n      let flag =\n        row._selfKnowledgeArr.length &&\n        reg.test(row._selfKnowledgeArr[row._selfKnowledgeArr.length - 1].name); // 上次输入的二级知识点-未完成\n      let value = row.knowledge.replace(/\\s/g, '');\n      // 当前输入的为二级知识点 && 二级知识点长度为100\n      if (!flag && value.length === 100) {\n        value += '-';\n      }\n      // 内容不能为空\n      if (regAll.test(value)) {\n        row.knowledge = '';\n        this.$message({\n          type: 'warning',\n          message: '内容不能为空'\n        });\n        return;\n      }\n      row.delStatus = 0;\n      switch (type) {\n        case '-':\n          if (flag) {\n            this.$message({\n              type: 'warning',\n              message: '已有二级知识点，请规范输入'\n            });\n          } else {\n            row._selfKnowledgeArr.push({\n              edit: false,\n              name: value\n            });\n          }\n          row.knowledge = '';\n          break;\n        case ';':\n          value = value.replace(/[;；]$/, '');\n          if (flag) {\n            if (value.indexOf('/') > -1) {\n              var secondKnowledge = row._selfKnowledgeArr[\n                row._selfKnowledgeArr.length - 1\n              ].name.split('-')[0];\n              // 去掉三级知识点空项\n              var thirdArr = value.split('/').filter(function(e) {\n                return e;\n              });\n              // 删除未完成的二级\n              row._selfKnowledgeArr.splice(row._selfKnowledgeArr.length - 1, 1);\n              // 知识点添加\n              thirdArr.forEach(item => {\n                row._selfKnowledgeArr.push({\n                  edit: false,\n                  name: secondKnowledge + '-' + item\n                });\n              });\n            } else {\n              row._selfKnowledgeArr[\n                row._selfKnowledgeArr.length - 1\n              ].name += value;\n            }\n            this.setLocalStorageVal();\n          } else {\n            this.$message({\n              type: 'warning',\n              message: '未检测到二级知识点，请规范输入'\n            });\n            // row._selfKnowledgeArr.push(value);\n          }\n          row.knowledge = '';\n          break;\n        case '/':\n          if (!flag) {\n            row.knowledge = '';\n            this.$message({\n              type: 'warning',\n              message: '未检测到二级知识点，请规范输入'\n            });\n          }\n          break;\n        // default:\n      }\n    },\n    // 知识点编辑\n    editKnowledge(row, knowledgeIndex) {\n      this.resetEditKnowledge(row);\n      row.editKnowledgeFlag = true;\n      row._selfKnowledgeArr[knowledgeIndex].edit = true;\n      row.knowledgeSecond = row._selfKnowledgeArr[knowledgeIndex].name.split(\n        '-'\n      )[0];\n      row.knowledgeThird = row._selfKnowledgeArr[knowledgeIndex].name.split(\n        '-'\n      )[1];\n      row.currentKnowledgeIndex = knowledgeIndex;\n    },\n    // 知识点编辑edit重置为false\n    resetEditKnowledge(row) {\n      row._selfKnowledgeArr.forEach(item => {\n        item.edit = false;\n      });\n    },\n    // 知识点编辑输入框事件\n    editKnowledgeInput(type, index, row) {\n      let regAll = /[-;；/]/;\n      let value1 = row.knowledgeSecond.replace(/\\s/g, '');\n      let value2 = row.knowledgeThird.replace(/\\s/g, '');\n      if (type === 'second' && regAll.test(value1)) {\n        row.knowledgeSecond = '';\n        this.$message({\n          type: 'warning',\n          message: '请规范输入'\n        });\n        return;\n      }\n      if (type === 'third' && regAll.test(value2)) {\n        row.knowledgeThird = '';\n        this.$message({\n          type: 'warning',\n          message: '请规范输入'\n        });\n        return;\n      }\n    },\n    // 回车事件\n    handelKeyupEnter(row) {\n      if (\n        row.knowledgeSecond.replace(/\\s/g, '') === '' ||\n        row.knowledgeThird.replace(/\\s/g, '') === ''\n      ) {\n        return this.$message({\n          type: 'warning',\n          message: '知识点不能为空'\n        });\n      }\n      row._selfKnowledgeArr.splice(row.currentKnowledgeIndex, 1, {\n        eidt: false,\n        name: row.knowledgeSecond + '-' + row.knowledgeThird\n      });\n      row.knowledgeSecond = '';\n      row.knowledgeThird = '';\n      row.editKnowledgeFlag = false;\n      this.setLocalStorageVal();\n    },\n    // delate删除事件\n    handelKeyupDel(row) {\n      if (this.compositionstartFlag || row.knowledge !== '') return;\n      ++row.delStatus;\n      if (row.delStatus === 2) {\n        row.delStatus = 0;\n        row._selfKnowledgeArr.pop();\n      }\n      this.setLocalStorageVal();\n    },\n    // 中文输入法事件\n    compositionstart(row) {\n      row.delStatus = -1;\n      this.compositionstartFlag = true;\n    },\n    compositionend(row) {\n      row.delStatus = -1;\n      this.compositionstartFlag = false;\n    },\n    // 删除选中的知识点/能力点\n    delChild(type, row, childIndex) {\n      this.changePage();\n      switch (type) {\n        case 'knowledge':\n          row._selfKnowledgeArr.splice(childIndex, 1);\n          break;\n        case 'ability':\n          row._selfAbilityArr.splice(childIndex, 1);\n          break;\n      }\n      this.setLocalStorageVal();\n    },\n    // 设定数字输入范围\n    numberChange(index, val, maxNum) {\n      val = val.replace(/[^0-9.]/g, '');\n      if (val === '') {\n        this.tableData[index].questionScore = '';\n        return;\n      }\n      if (val == '0') return;\n      let reg = /^[0-9]+(\\.?[0-9]{0,1})$/;\n      if (reg.test(val)) {\n        this.tableData[index].questionScore = val;\n        if (Number(val) > maxNum) {\n          this.tableData[index].questionScore = maxNum;\n        }\n      } else {\n        this.tableData[index].questionScore = maxNum;\n      }\n    },\n    // 数字输入框失去焦点\n    numberChangeBlur(index, val, maxNum) {\n      if (val === '') {\n        this.tableData[index].questionScore = '';\n        return;\n      }\n      val = Number(val);\n      val == 0 ? (val = Number(maxNum)) : '';\n      this.tableData[index].questionScore = val;\n      this.setLocalStorageVal();\n    },\n    // 页面有变动\n    changePage() {\n      this.changeNosave = true;\n    },\n    // 判断知识点/能力点是否为空\n    judgeArrItemEmpty(arr) {\n      let flag = arr.every(item => {\n        return (\n          item._selfKnowledgeArr.length === 0 &&\n          item._selfAbilityArr.length === 0\n        );\n      });\n      // arr每一项知识点/能力点为空返回true\n      return flag;\n    },\n    // 操作说明\n    openTeachingDialog() {\n      this.teachingDialogVisible = true;\n    }\n  },\n  beforeRouteLeave(to, from, next) {\n    if (this.changeNosave) {\n      this.$confirm('您填写的内容未保存，确定离开吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning',\n        distinguishCancelAndClose: false\n      }).then(() => {\n        next();\n      });\n      next(false);\n    } else {\n      next();\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.content {\n  .self-input {\n    border: none;\n    background: none;\n    display: inline-block;\n    width: 50%;\n    max-width: 100%;\n    &-width {\n      width: 100%;\n    }\n    /deep/ .el-input__inner {\n      border: none;\n      background: none;\n      height: 32px;\n      line-height: 32px;\n      padding: 0 5px;\n    }\n  }\n  .title {\n    justify-content: space-between;\n  }\n  h2 {\n    font-size: 16px;\n    margin-bottom: 17px;\n    font-weight: 600;\n  }\n  .icon-choose {\n    width: 16px;\n    height: 16px;\n    margin-right: 5px;\n    vertical-align: middle;\n    & + span {\n      vertical-align: middle;\n    }\n  }\n  .scope-box {\n    align-items: center;\n    justify-content: space-between;\n    .choose-box {\n      width: 100%;\n      justify-content: space-between;\n      padding-right: 8px;\n    }\n    .el-icon-edit-outline {\n      width: 16px;\n    }\n  }\n  .choose-box {\n    align-items: center;\n  }\n  .operat-box {\n    width: 40px;\n    flex-shrink: 0;\n    text-align: center;\n    &-content {\n      display: none;\n    }\n\n    .el-icon-circle-plus-outline {\n      font-weight: normal;\n      margin-right: 5px;\n      cursor: pointer;\n    }\n    .el-icon-remove-outline {\n      font-weight: normal;\n      color: indianred;\n      cursor: pointer;\n    }\n  }\n  .edit-box {\n    display: inline-block;\n    width: 100%;\n    .el-input {\n      width: 45%;\n      height: 22px;\n      min-width: 150px;\n    }\n    /deep/ .el-input__inner {\n      height: 22px;\n      line-height: 22px;\n      padding: 0 5px;\n    }\n    span {\n      padding: 0 5px;\n    }\n  }\n  .border-box {\n    border: 1px solid #dcdfe6;\n    padding: 6px;\n    border-radius: 4px;\n  }\n  .list-box {\n    .item {\n      display: inline-block;\n      margin-right: 20px;\n      margin-bottom: 5px;\n      margin-top: 5px;\n      background: rgba(99, 149, 250, 0.2);\n      border-radius: 11px;\n      padding: 0 10px;\n      box-sizing: border-box;\n      vertical-align: middle;\n      &.item-gray {\n        border: 1px solid rgba(0, 0, 0, 0.25);\n        background-color: #fff;\n      }\n      &.item-del {\n        border: 1px solid rgba(0, 0, 0, 0.25);\n        background: rgba(99, 149, 250, 0.4);\n      }\n      &.item-edit {\n        background: none;\n        border: none;\n        width: 100%;\n      }\n      &:hover {\n        margin-right: 8px;\n        padding-right: 5px;\n        .el-icon-close {\n          display: inline-block;\n        }\n      }\n      .el-icon-close {\n        color: #999;\n        padding-left: 1px;\n        line-height: 22px;\n        display: none;\n        float: left;\n      }\n\n      .text {\n        line-height: 22px;\n        box-sizing: border-box;\n        text-overflow: ellipsis;\n        overflow: hidden;\n        cursor: pointer;\n        display: -webkit-box;\n        max-width: 180px;\n        height: 22px;\n        -webkit-line-clamp: 1;\n        -webkit-box-orient: vertical;\n        float: left;\n      }\n    }\n  }\n  [class*=' el-icon-'],\n  [class^='el-icon-'] {\n    color: #5481e9;\n    font-weight: 600;\n    font-size: 16px;\n    cursor: pointer;\n  }\n  /deep/ .el-table {\n    font-size: 14px;\n    font-family: PingFangSC-Regular, PingFang SC;\n    font-weight: 400;\n    color: rgba(0, 0, 0, 0.65);\n    .el-table__body {\n      tr:hover {\n        .operat-box-content {\n          display: block;\n        }\n      }\n    }\n  }\n  /deep/ .el-table thead,\n  /deep/ .el-table th {\n    background-color: #fafafa;\n    font-size: 14px;\n    font-family: PingFangSC-Medium, PingFang SC;\n    font-weight: 600;\n    color: rgba(0, 0, 0, 0.85);\n  }\n  /deep/ .el-table th {\n    padding: 12px 0;\n  }\n  /deep/ .el-dialog__footer {\n    text-align: center;\n  }\n  /deep/ .el-button {\n    border-radius: 2px;\n    border: none;\n    color: #fff;\n    font-size: 14px;\n    font-weight: normal;\n  }\n  /deep/ .el-button--default {\n    border: 1px solid #5481e9;\n    color: #5481e9;\n  }\n  /deep/ .el-button--primary {\n    background: #5481e9;\n    border-color: #5481e9;\n    border-radius: 2px;\n  }\n  /deep/ .el-button--text {\n    color: #5481e9;\n    font-size: 14px;\n    font-family: PingFangSC-Regular, PingFang SC;\n    font-weight: 400;\n    background: none;\n  }\n  .el-icon-close,\n  .el-icon-edit {\n    cursor: pointer;\n  }\n  /deep/ .el-dialog {\n    .el-dialog__header {\n      text-align: left;\n      border-bottom: 1px solid #f5f5f5;\n    }\n    .el-button--default {\n      border: 1px solid #5481e9;\n      background: #fff;\n      color: #5481e9;\n    }\n    .el-button + .el-button {\n      margin-left: 12px;\n    }\n    .el-dialog__body {\n      padding: 16px 20px;\n    }\n    .el-form-item {\n      float: left;\n      margin-bottom: 16px;\n    }\n    .el-form-item__content {\n      float: left;\n      margin-left: 0px !important;\n    }\n    .tree-box {\n      height: 392px;\n      overflow-y: auto;\n      background: #f6f6f6;\n      border-radius: 2px;\n      padding: 16px;\n      box-sizing: border-box;\n    }\n    .el-tree {\n      background: #f6f6f6;\n    }\n  }\n  /deep/ .teaching-dialog {\n    .dialog-content {\n      height: 54vh;\n      overflow-y: auto;\n      .item {\n        font-size: 14px;\n        font-family: PingFangSC-Medium, PingFang SC;\n        font-weight: 500;\n        color: #000000;\n        line-height: 20px;\n        padding-bottom: 20px;\n        margin-bottom: 20px;\n        border-bottom: 1px solid #f5f5f5;\n        &:last-child {\n          border-bottom: 0;\n          margin-bottom: 0;\n        }\n        &-title {\n          font-size: 16px;\n          font-family: PingFangSC-Regular, PingFang SC;\n          font-weight: 400;\n          color: #364a76;\n          line-height: 22px;\n          position: relative;\n          display: inline-block;\n          margin-bottom: 8px;\n          &::after {\n            display: block;\n            content: '';\n            height: 3px;\n            background: #5481e9;\n            border-radius: 2px;\n            opacity: 0.2;\n            width: 100%;\n            position: absolute;\n            bottom: 4px;\n            left: 0;\n          }\n        }\n      }\n      img {\n        width: 370px;\n        margin-top: 10px;\n        display: block;\n      }\n    }\n    .el-dialog__body {\n      padding-bottom: 0;\n    }\n    .el-dialog__footer {\n      border-top: 1px solid #f5f5f5;\n      padding-top: 20px;\n    }\n  }\n  .choose-num {\n    font-size: 14px;\n    color: #000;\n    float: right;\n    line-height: 40px;\n    span {\n      color: #5481e9;\n      padding-left: 10px;\n    }\n  }\n}\n/deep/ textarea {\n  resize: none;\n}\n</style>\n"]}]}
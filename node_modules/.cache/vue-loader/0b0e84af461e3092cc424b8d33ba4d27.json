{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/visitor-h5/views/verify-visitor/components/CheckPanel.vue?vue&type=style&index=0&id=11d1e420&lang=less&scoped=true&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/visitor-h5/views/verify-visitor/components/CheckPanel.vue","mtime":1645178321204},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/less-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/style-resources-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["CheckPanel.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8LA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"CheckPanel.vue","sourceRoot":"src/pages/visitor-h5/views/verify-visitor/components","sourcesContent":["<template>\n  <div class=\"item-checkwrapper\" v-if=\"item.visitorList\">\n    <div v-if=\"item.visitorList.length > 1\">\n      <van-sticky>\n        <div class=\"item total-check\" :class=\"radius ? 'active' : ''\">\n          <div class=\"left\">\n            <van-checkbox\n              @click.stop.native=\"checkAll\"\n              label-disabled\n              shape=\"square\"\n              v-model=\"checked\"\n            ></van-checkbox>\n            <span>全选到达 共{{ result.length }}人</span>\n          </div>\n          <div class=\"right\">\n            <span class=\"confirm-arrive\" @click.stop=\"confirmArrive\"\n              >确认到达</span\n            >\n          </div>\n        </div>\n      </van-sticky>\n      <ul class=\"item-checkList\" :class=\"type == 'outward' ? 'no-radius' : ''\">\n        <van-checkbox-group v-model=\"result\" ref=\"checkboxGroup\">\n          <li v-for=\"(el, index) in item.visitorList\" :key=\"index + !!el.code\">\n            <div class=\"check-item\">\n              <div class=\"left\">\n                <van-checkbox\n                  ref=\"checkboxes\"\n                  label-disabled\n                  shape=\"square\"\n                  :name=\"el.id\"\n                  @click.stop.native=\"toggle(el, index)\"\n                  >{{ el.visitorName }}</van-checkbox\n                >\n                <span\n                  v-if=\"\n                    (el.visitorType == 1 && item.visitorList.length > 1) ||\n                      (el.visitorType == 4 && item.visitorList.length > 1)\n                  \"\n                  >领队</span\n                >\n              </div>\n              <div class=\"right\">\n                <span class=\"verify-code\">{{ el.visitorCode }}</span>\n              </div>\n            </div>\n            <!--  <div\n              class=\"opeartion-item\"\n              v-if=\"el.inOutRecordList && el.inOutRecordList.length > 0\"\n            >\n              <ul>\n                <li v-for=\"operation in el.inOutRecordList\" :key=\"operation.id\">\n                  <span class=\"operateTime\">{{\n                    formateDate(operation.createTime, 3)\n                  }}</span>\n                  <span>{{\n                    operation.transportNames ? '出门核销' : '进门核验'\n                  }}</span>\n                </li>\n              </ul>\n            </div> -->\n          </li>\n        </van-checkbox-group>\n      </ul>\n    </div>\n\n    <div class=\"single-visitor\" v-if=\"item.visitorList.length === 1\">\n      <div class=\"left\">\n        <span> {{ item.visitorList[0].visitorName }}</span>\n      </div>\n      <div class=\"right\">\n        <span class=\"verify-code\">{{ item.visitorList[0].visitorCode }}</span>\n        <span\n          class=\"confirm-arrive\"\n          @click.stop=\"confirmArrive(item.visitorList[0])\"\n          >确认到达</span\n        >\n      </div>\n    </div>\n    <!--  <ul class=\"item-operationList\">\n      <li v-for=\"operation in item.inOutRecordRespDtoList\" :key=\"operation.id\">\n        <span class=\"operateTime\">{{\n          formateDate(operation.createTime, 3)\n        }}</span>\n        <span\n          >{{\n            operation.visitorNames +\n              '' +\n              (operation.transportNames ? '出门核销' : '进门核验')\n          }}\n        </span>\n      </li>\n    </ul> -->\n    <confirm-modal\n      :showPanel=\"showPanel\"\n      @closePanel=\"closePanel\"\n    ></confirm-modal>\n  </div>\n</template>\n\n<script>\nimport confirmModal from './ConfirmModal.vue';\nimport { writeOffVisitor } from '@/pages/visitor-h5/api/verify';\nimport { formateDate } from '@/pages/visitor-h5/plugins/common/utils';\nexport default {\n  components: { confirmModal },\n  props: ['item', 'radius', 'type'],\n  data() {\n    return {\n      checked: true,\n      result: this.getResult(),\n      showPanel: false,\n      formateDate\n    };\n  },\n  methods: {\n    getResult() {\n      if (this.item && this.item.visitorList) {\n        return this.item.visitorList.map(el => {\n          return el.id;\n        });\n      } else {\n        return [];\n      }\n    },\n    isClick() {\n      if (\n        this.$refs.checkAll ||\n        this.$refs.checkItem ||\n        this.$refs.confirmSpan\n      ) {\n        return true;\n      }\n    },\n    confirmArrive(row) {\n      if (this.result.length == 0) {\n        this.$notify('请选择核销人员');\n        return;\n      }\n\n      const loading = this.$toast.loading({\n        mask: true\n      });\n      writeOffVisitor({\n        guestBaseId: this.item.id,\n        visitorIds: row.id ? [row.id] : this.result,\n        cameType: 1\n      })\n        .then(res => {\n          if (res.success) {\n            this.showPanel = true;\n          } else {\n            this.$notify(res.message);\n          }\n          loading.clear();\n        })\n        .catch(res => {\n          loading.clear();\n          this.$notify(res.message);\n        });\n    },\n    checkAll() {\n      this.$nextTick(() => {\n        this.$refs.checkboxGroup.toggleAll({\n          checked: !this.checked,\n          skipDisabled: true\n        });\n        this.checked = !this.checked;\n      });\n    },\n    toggle(item, index) {\n      setTimeout(() => {\n        if (this.result.length == this.item.visitorList.length) {\n          this.checked = true;\n        } else {\n          this.checked = false;\n        }\n      });\n    },\n    closePanel() {\n      this.showPanel = false;\n      this.$emit('onRefresh');\n      this.$emit('onClose');\n      this.$emit('onSearchClose');\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n/deep/ .van-checkbox__label {\n  font-size: 17px;\n}\n.item-checkwrapper {\n  .single-visitor,\n  .total-check {\n    line-height: 50px;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    background: #ebf4ff;\n    padding: 0 15px;\n    &.active {\n      border-top-right-radius: 10px;\n      border-top-left-radius: 10px;\n    }\n    /deep/.van-checkbox__label,\n    .left {\n      display: flex;\n      font-size: 17px;\n      font-family: PingFangSC-Medium, PingFang SC;\n      font-weight: 500;\n      color: rgba(0, 0, 0, 0.85);\n      span {\n        margin-left: 6px;\n      }\n    }\n  }\n\n  .single-visitor {\n    background: #ffffff;\n    .right {\n      display: flex;\n      align-items: center;\n    }\n  }\n  .item-checkList {\n    background: #fff;\n    padding: 0 15px;\n    // border-bottom-right-radius: 10px;\n    // border-bottom-left-radius: 10px;\n    &.no-radius {\n      border-bottom-right-radius: 0;\n      border-bottom-left-radius: 0;\n    }\n    li {\n      .check-item {\n        line-height: 50px;\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        .left {\n          display: flex;\n          span {\n            display: inline-block;\n            text-align: center;\n            width: 35px;\n            height: 17px;\n            line-height: 17px;\n            margin-left: 6px;\n            background: rgba(255, 87, 86, 0.06);\n            border-radius: 2px;\n            border: 1px solid #ff5756;\n            font-size: 10px;\n            font-family: PingFangSC-Regular, PingFang SC;\n            font-weight: 400;\n            color: #ff5756;\n          }\n        }\n      }\n    }\n  }\n}\n\n.right {\n  .verify-code {\n    font-size: 22px;\n    font-family: PingFangSC-Medium, PingFang SC;\n    font-weight: 500;\n    color: rgba(0, 0, 0, 0.85);\n    margin-right: 22px;\n  }\n  .confirm-arrive {\n    display: inline-block;\n    width: 99px;\n    height: 40px;\n    line-height: 40px;\n    background: #3388ff;\n    border-radius: 3px;\n    text-align: center;\n    font-size: 17px;\n    font-family: PingFangSC-Medium, PingFang SC;\n    font-weight: 500;\n    color: #ffffff;\n  }\n}\n\n.item-operationList,\n.opeartion-item {\n  background: #fbfbfc;\n  padding: 12px;\n  li {\n    font-size: 12px;\n    font-family: PingFangSC-Regular, PingFang SC;\n    font-weight: 400;\n    color: rgba(0, 0, 0, 0.45);\n    line-height: 20px;\n    .operateTime {\n      margin-right: 12px;\n    }\n  }\n}\n</style>\n"]}]}
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAy4BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/pages/teaching-points-h5/views/course-research","sourcesContent":["<template>\r\n  <div class=\"course-search\">\r\n    <van-tabs\r\n      v-model=\"active\"\r\n      color=\"#1DAACC\"\r\n      title-active-color=\"#489EF3\"\r\n      title-inactive-color=\"rgba(0,0,0,0.65)\"\r\n      animated\r\n      @change=\"changeTab\"\r\n    >\r\n      <van-tab v-for=\"type in titleData\" :key=\"type\" :title=\"type\">\r\n        <!-- <button @click=\"searchPage\">搜索</button> -->\r\n        <filter-component\r\n          :schoolPopup=\"show\"\r\n          :subjectPopup=\"show2\"\r\n          :topicThesisLevePopup=\"show3\"\r\n          :topicThesisTypePopup=\"show4\"\r\n          :statusPopup=\"show5\"\r\n          :checkDate=\"checkDate\"\r\n          :checkDate2=\"checkDate2\"\r\n          :type=\"type\"\r\n          :schoolNameList=\"schoolNameList\"\r\n          :subjectNameList=\"subjectNameList\"\r\n          :topicThesisLeve=\"topicThesisLeve\"\r\n          :topicThesisType=\"topicThesisType\"\r\n          :status=\"status\"\r\n          @startDateShow=\"startDateShow\"\r\n          @startDateShow2=\"startDateShow2\"\r\n          @schoolShow=\"schoolShow\"\r\n          @subjectShow=\"subjectShow\"\r\n          @topicThesisLeveShow=\"topicThesisLeveShow\"\r\n          @topicThesisTypeShow=\"topicThesisTypeShow\"\r\n          @statusShow=\"statusShow\"\r\n        />\r\n      </van-tab>\r\n    </van-tabs>\r\n    <div class=\"list-data\">\r\n      <tr v-show=\"titleData[active] === '课题'\" class=\"list-th\">\r\n        <th>课题名称</th>\r\n        <th>负责人</th>\r\n        <th>状态</th>\r\n      </tr>\r\n      <tr v-show=\"titleData[active] === '论文'\" class=\"list-th\">\r\n        <th>论文名称</th>\r\n        <th>作者</th>\r\n        <th>级别</th>\r\n      </tr>\r\n      <van-pull-refresh v-model=\"refreshing\" @refresh=\"onRefresh\">\r\n        <van-list\r\n          v-model=\"loading\"\r\n          :finished=\"finished\"\r\n          :offset=\"20\"\r\n          :error.sync=\"error\"\r\n          error-text=\"请求失败，点击重新加载\"\r\n          :finished-text=\"finishedText\"\r\n          @load=\"onLoad\"\r\n        >\r\n          <dl\r\n            :class=\"{ 'no-bc-list': finished && teacherTableData.length === 0 }\"\r\n          >\r\n            <cList\r\n              :finished=\"finished\"\r\n              :active=\"titleData[active]\"\r\n              :teacherTableData=\"teacherTableData\"\r\n            ></cList>\r\n          </dl>\r\n        </van-list>\r\n      </van-pull-refresh>\r\n    </div>\r\n\r\n    <van-calendar\r\n      ref=\"calendar\"\r\n      class=\"timePicker\"\r\n      v-model=\"visibleStartTime\"\r\n      :allow-same-day=\"true\"\r\n      type=\"range\"\r\n      color=\"#489EF3\"\r\n      :min-date=\"minDate\"\r\n      :max-date=\"maxDate\"\r\n      @select=\"seleceDate\"\r\n      :title=\"getTitle\"\r\n      get-container=\"#app\"\r\n    >\r\n      <template v-slot:footer>\r\n        <van-button round @click=\"resetDate\" plain type=\"info\">重置</van-button>\r\n        <van-button round @click=\"handleDate\" :disabled=\"disabled1\" type=\"info\"\r\n          >确定</van-button\r\n        >\r\n      </template>\r\n    </van-calendar>\r\n\r\n    <van-calendar\r\n      ref=\"calendar2\"\r\n      class=\"timePicker\"\r\n      v-model=\"visibleStartTime2\"\r\n      :allow-same-day=\"true\"\r\n      type=\"range\"\r\n      color=\"#489EF3\"\r\n      :min-date=\"minDate\"\r\n      :max-date=\"maxDate\"\r\n      @select=\"seleceDate2\"\r\n      title=\"结题日期\"\r\n      get-container=\"#app\"\r\n    >\r\n      <template v-slot:footer>\r\n        <van-button round @click=\"resetDate2\" plain type=\"info\"\r\n          >重置</van-button\r\n        >\r\n        <van-button round @click=\"handleDate2\" :disabled=\"disabled2\" type=\"info\"\r\n          >确定</van-button\r\n        >\r\n      </template>\r\n    </van-calendar>\r\n    <van-overlay :lock-scroll=\"false\" :show=\"show\" @click=\"show = false\">\r\n      <div class=\"wrapper\">\r\n        <div class=\"block\">\r\n          <van-checkbox-group v-model=\"schoolCodeList\">\r\n            <van-cell-group>\r\n              <van-cell\r\n                v-for=\"(item, index) in getSchoolOptions\"\r\n                clickable\r\n                :key=\"item.dictionaryKey\"\r\n                :title=\"item.dictionaryText\"\r\n                @click.stop=\"toggle(index)\"\r\n              >\r\n                <template #right-icon>\r\n                  <van-checkbox :name=\"item.dictionaryKey\" ref=\"checkboxes\">\r\n                    <template #icon=\"props\">\r\n                      <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                    </template>\r\n                  </van-checkbox>\r\n                </template>\r\n              </van-cell>\r\n            </van-cell-group>\r\n          </van-checkbox-group>\r\n          <div class=\"fixed-btn-group\">\r\n            <van-button\r\n              @click.stop=\"resetSchool\"\r\n              icon=\"replay\"\r\n              plain\r\n              type=\"info\"\r\n              >重置</van-button\r\n            >\r\n            <van-button @click=\"confirmSchool\" type=\"info\">确定</van-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </van-overlay>\r\n    <van-overlay :lock-scroll=\"false\" :show=\"show2\" @click=\"show2 = false\">\r\n      <div class=\"wrapper\">\r\n        <div class=\"block\">\r\n          <van-checkbox-group v-model=\"subjectCodeList\">\r\n            <van-cell-group>\r\n              <van-cell\r\n                v-for=\"(item, index) in getSubjectOptions\"\r\n                clickable\r\n                :key=\"item.dictionaryKey\"\r\n                :title=\"item.dictionaryText\"\r\n                @click.stop=\"toggle2(index)\"\r\n              >\r\n                <template #right-icon>\r\n                  <van-checkbox :name=\"item.dictionaryKey\" ref=\"checkboxes2\">\r\n                    <template #icon=\"props\">\r\n                      <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                    </template>\r\n                  </van-checkbox>\r\n                </template>\r\n              </van-cell>\r\n            </van-cell-group>\r\n          </van-checkbox-group>\r\n          <div class=\"fixed-btn-group\">\r\n            <van-button\r\n              @click.stop=\"resetSubject\"\r\n              icon=\"replay\"\r\n              plain\r\n              type=\"info\"\r\n              >重置</van-button\r\n            >\r\n            <van-button @click=\"confirmSubject\" type=\"info\">确定</van-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </van-overlay>\r\n    <van-overlay :lock-scroll=\"false\" :show=\"show3\" @click=\"show3 = false\">\r\n      <div class=\"wrapper\">\r\n        <div class=\"block\">\r\n          <van-radio-group v-model=\"topicThesisLeveKey\">\r\n            <van-cell-group>\r\n              <van-cell\r\n                v-for=\"(item, index) in getTopicLevelOptions\"\r\n                clickable\r\n                :key=\"item.dictionaryKey\"\r\n                :title=\"item.dictionaryText\"\r\n                @click.stop=\"toggle3(index)\"\r\n              >\r\n                <template #right-icon>\r\n                  <van-radio :name=\"item.dictionaryKey\" ref=\"checkboxes3\">\r\n                    <template #icon=\"props\">\r\n                      <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                    </template>\r\n                  </van-radio>\r\n                </template>\r\n              </van-cell>\r\n            </van-cell-group>\r\n          </van-radio-group>\r\n          <div class=\"fixed-btn-group\">\r\n            <van-button\r\n              @click.stop=\"resetTopicThesisLeve\"\r\n              icon=\"replay\"\r\n              plain\r\n              type=\"info\"\r\n              >重置</van-button\r\n            >\r\n            <van-button @click=\"confirmTopicThesisLeve\" type=\"info\"\r\n              >确定</van-button\r\n            >\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </van-overlay>\r\n    <van-overlay :lock-scroll=\"false\" :show=\"show4\" @click=\"show4 = false\">\r\n      <div class=\"wrapper\">\r\n        <div class=\"block\">\r\n          <van-radio-group v-model=\"topicThesisTypeKey\">\r\n            <van-cell-group>\r\n              <van-cell\r\n                v-for=\"(item, index) in getTopicTypeOptions\"\r\n                clickable\r\n                :key=\"item.dictionaryKey\"\r\n                :title=\"item.dictionaryText\"\r\n                @click.stop=\"toggle4(index)\"\r\n              >\r\n                <template #right-icon>\r\n                  <van-radio :name=\"item.dictionaryKey\" ref=\"checkboxes4\">\r\n                    <template #icon=\"props\">\r\n                      <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                    </template>\r\n                  </van-radio>\r\n                </template>\r\n              </van-cell>\r\n            </van-cell-group>\r\n          </van-radio-group>\r\n          <div class=\"fixed-btn-group\">\r\n            <van-button\r\n              @click.stop=\"resetTopicThesisType\"\r\n              icon=\"replay\"\r\n              plain\r\n              type=\"info\"\r\n              >重置</van-button\r\n            >\r\n            <van-button @click=\"confirmTopicThesisType\" type=\"info\"\r\n              >确定</van-button\r\n            >\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </van-overlay>\r\n    <van-overlay :lock-scroll=\"false\" :show=\"show5\" @click=\"show5 = false\">\r\n      <div class=\"wrapper\">\r\n        <div class=\"block\">\r\n          <van-radio-group v-model=\"statusKey\">\r\n            <van-cell-group>\r\n              <van-cell\r\n                v-for=\"(item, index) in getStatusOptions\"\r\n                clickable\r\n                :key=\"item.dictionaryKey\"\r\n                :title=\"item.dictionaryText\"\r\n                @click.stop=\"toggle5(index)\"\r\n              >\r\n                <template #right-icon>\r\n                  <van-radio :name=\"item.dictionaryKey\" ref=\"checkboxes5\">\r\n                    <template #icon=\"props\">\r\n                      <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                    </template>\r\n                  </van-radio>\r\n                </template>\r\n              </van-cell>\r\n            </van-cell-group>\r\n          </van-radio-group>\r\n          <div class=\"fixed-btn-group\">\r\n            <van-button\r\n              @click.stop=\"resetStatus\"\r\n              icon=\"replay\"\r\n              plain\r\n              type=\"info\"\r\n              >重置</van-button\r\n            >\r\n            <van-button @click=\"confirmStatus\" type=\"info\">确定</van-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </van-overlay>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { parseTime } from '../../plugins/core/utils';\r\nimport {\r\n  Tab,\r\n  Tabs,\r\n  DatetimePicker,\r\n  Overlay,\r\n  Checkbox,\r\n  CheckboxGroup,\r\n  RadioGroup,\r\n  Radio,\r\n  Cell,\r\n  CellGroup,\r\n  Icon,\r\n  Button,\r\n  Toast,\r\n  List,\r\n  PullRefresh,\r\n  Grid,\r\n  GridItem,\r\n  Calendar\r\n} from 'vant';\r\nimport { getTableData } from '../../api/base/course-all';\r\nimport filterComponent from '../normal-examine/components/filter';\r\nimport fMixin from '../normal-examine/components/mixin';\r\nimport cList from './components/list';\r\nimport utils from '@/plugins/common/utils';\r\nexport default {\r\n  name: 'courseResearch',\r\n  data() {\r\n    return {\r\n      active: this.$route.query.active || 0,\r\n      currentStartTime: '',\r\n      currentStartTime2: '',\r\n      currentEndTime: '',\r\n      currentCheckDate: '',\r\n      currentEndTime2: '',\r\n      currentCheckDate2: '',\r\n      visibleStartTime: false,\r\n      visibleStartTime2: false,\r\n      visibleEndTime: false,\r\n      visibleEndTime2: false,\r\n      checkDate: '',\r\n      checkDate2: '',\r\n      show: false,\r\n      show2: false,\r\n      show3: false,\r\n      show4: false,\r\n      show5: false,\r\n      schoolNameList: '',\r\n      subjectNameList: '',\r\n      topicThesisLeve: '',\r\n      topicThesisType: '',\r\n      topicThesisLeveKey: '',\r\n      topicThesisTypeKey: '',\r\n      schoolCodeList: [],\r\n      subjectCodeList: [],\r\n      status: '',\r\n      statusKey: '',\r\n\r\n      titleData: [],\r\n\r\n      formData: {\r\n        // 查询参数\r\n        teacherName: '',\r\n        startTime: '',\r\n        endTime: '',\r\n        conStartTime: '',\r\n        conEndTime: '',\r\n        schoolCodeList: [],\r\n        subjectCodeList: [],\r\n        topicThesisLeve: '',\r\n        topicThesisType: '',\r\n        status: ''\r\n      },\r\n      params: {},\r\n      pageInfo: {\r\n        //填报信息总页数\r\n        total: 0\r\n      },\r\n      pagingParams: {\r\n        //填报分页参数\r\n        limit: 15,\r\n        offset: 0\r\n      },\r\n      load: true,\r\n      teacherTableData: [],\r\n\r\n      loading: false,\r\n      finished: false,\r\n      refreshing: false,\r\n      error: false,\r\n      showDatePicker: false,\r\n      iconSearch: require('../../assets/images/icon/icon-search.png'),\r\n      trueCurrenStart: '',\r\n      trueCurrenStart2: '',\r\n      minDate: new Date(new Date().getFullYear() - 5, 0, 1),\r\n      maxDate: new Date(new Date().getFullYear() + 5, 12, 31),\r\n      defaultDate: new Date(),\r\n      disabled1: false,\r\n      disabled2: false\r\n    };\r\n  },\r\n  mixins: [fMixin],\r\n  components: {\r\n    [Tab.name]: Tab,\r\n    [Tabs.name]: Tabs,\r\n    [DatetimePicker.name]: DatetimePicker,\r\n    [Overlay.name]: Overlay,\r\n    [Checkbox.name]: Checkbox,\r\n    [CheckboxGroup.name]: CheckboxGroup,\r\n    [Cell.name]: Cell,\r\n    [CellGroup.name]: CellGroup,\r\n    [Icon.name]: Icon,\r\n    [Button.name]: Button,\r\n    [Toast.name]: Toast,\r\n    [PullRefresh.name]: PullRefresh,\r\n    [List.name]: List,\r\n    [Grid.name]: Grid,\r\n    [GridItem.name]: GridItem,\r\n    [RadioGroup.name]: RadioGroup,\r\n    [Radio.name]: Radio,\r\n    [Calendar.name]: Calendar,\r\n    filterComponent,\r\n    cList\r\n  },\r\n  computed: {\r\n    showSum() {\r\n      return (\r\n        this.show ||\r\n        this.show2 ||\r\n        this.show3 ||\r\n        this.show4 ||\r\n        this.show5 ||\r\n        this.visibleStartTime ||\r\n        this.visibleEndTime ||\r\n        this.visibleStartTime2 ||\r\n        this.visibleEndTime2\r\n      );\r\n    },\r\n    getTitle() {\r\n      let title;\r\n      if (this.titleData[this.active] === '课题') {\r\n        title = '立项日期';\r\n      } else if (this.titleData[this.active] === '论文') {\r\n        title = '发表时间';\r\n      }\r\n      return title;\r\n    }\r\n  },\r\n  watch: {\r\n    showSum(val) {\r\n      if (val) {\r\n        document.querySelector('body').style.overflow = 'hidden';\r\n      } else {\r\n        document.querySelector('body').style.overflow = 'auto';\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    if (sessionStorage.getItem('reserachActive')) {\r\n      this.active = Number(sessionStorage.getItem('reserachActive'));\r\n    }\r\n    this.init();\r\n  },\r\n  methods: {\r\n    init() {\r\n      let _self = this;\r\n      let datas = [];\r\n      _self.$store.state.user.menus.forEach(item => {\r\n        if (item.url === '/course-research/task') {\r\n          datas.push({ name: '课题', num: 1 });\r\n        }\r\n        if (item.url === '/course-research/thesis') {\r\n          datas.push({ name: '论文', num: 2 });\r\n        }\r\n      });\r\n      datas = datas.sort((a, b) => {\r\n        return a.num - b.num;\r\n      });\r\n      datas.forEach(item => {\r\n        _self.titleData.push(item.name);\r\n      });\r\n      _self.$dd.biz.navigation.setMenu({\r\n        backgroundColor: '#20D1CD',\r\n        textColor: '#ADD8E611',\r\n        items: [\r\n          {\r\n            id: '1',\r\n            iconId: 'search',\r\n            text: '搜索',\r\n            url: _self.iconSearch\r\n          }\r\n        ],\r\n        onSuccess: function(res) {\r\n          // 调用成功时回调\r\n          if (res.id === '1') {\r\n            _self.searchPage();\r\n          }\r\n        },\r\n        onFail: function(err) {\r\n          // 调用失败时回调\r\n          console.log(err);\r\n        }\r\n      });\r\n    },\r\n    /**\r\n     * 点击搜索icon 进入搜索页面\r\n     */\r\n    searchPage() {\r\n      this.$router.push({\r\n        name: 'courseResearchSearch',\r\n        query: {\r\n          active: this.titleData[this.active]\r\n        }\r\n      });\r\n    },\r\n    /**\r\n     * 重置学校下拉框\r\n     */\r\n    resetSchool() {\r\n      this.schoolNameList = '';\r\n      this.formData.schoolCodeList = this.schoolCodeList = [];\r\n      this.show = false;\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 确定学校下拉框\r\n     */\r\n    confirmSchool() {\r\n      let names = [];\r\n      this.getSchoolOptions.map((item, index) => {\r\n        this.schoolCodeList.map(i => {\r\n          if (item.dictionaryKey == i) {\r\n            names.push(item.dictionaryText);\r\n          }\r\n        });\r\n      });\r\n      this.schoolNameList = names.join(',');\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 重置学科下拉框\r\n     */\r\n    resetSubject() {\r\n      this.subjectNameList = '';\r\n      this.formData.subjectCodeList = this.subjectCodeList = [];\r\n      this.show2 = false;\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 确定学科下拉框\r\n     */\r\n    confirmSubject() {\r\n      let names = [];\r\n      this.getSubjectOptions.map(item => {\r\n        this.subjectCodeList.map(i => {\r\n          if (item.dictionaryKey == i) {\r\n            names.push(item.dictionaryText);\r\n          }\r\n        });\r\n      });\r\n      this.subjectNameList = names.join(',');\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 重置级别下拉框\r\n     */\r\n    resetTopicThesisLeve() {\r\n      this.formData.topicThesisLeve = this.topicThesisLeve = this.topicThesisLeveKey =\r\n        '';\r\n      this.show3 = false;\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 确定级别下拉框\r\n     */\r\n    confirmTopicThesisLeve() {\r\n      this.getTopicLevelOptions.map(item => {\r\n        if (item.dictionaryKey == this.topicThesisLeveKey) {\r\n          this.topicThesisLeve = item.dictionaryText;\r\n        }\r\n      });\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 重置类别下拉框\r\n     */\r\n    resetTopicThesisType() {\r\n      this.formData.topicThesisType = this.topicThesisType
CA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SignOut.vue","sourceRoot":"src/pages/visitor-h5/views/verify-visitor/components","sourcesContent":["<template>\n  <div class=\"item-checkwrapper\" v-if=\"item.offVisitorList\">\n    <div v-if=\"item.offVisitorList.length > 1\">\n      <div class=\"item total-check\" :class=\"radius ? 'active' : ''\">\n        <div class=\"left\">\n          <span> 共{{ item.offVisitorList.length }}人</span>\n        </div>\n        <div class=\"right\">\n          <span class=\"confirm-arrive\" @click.stop=\"confirmArrive\">已签离</span>\n        </div>\n      </div>\n      <ul class=\"item-checkList\" :class=\"type == 'outward' ? 'no-radius' : ''\">\n        <van-checkbox-group v-model=\"result\" ref=\"checkboxGroup\">\n          <li\n            v-for=\"(el, index) in item.offVisitorList\"\n            :key=\"index + !!el.code\"\n          >\n            <div class=\"check-item\">\n              <div class=\"item-operate\">\n                <div class=\"left\">\n                  <div>\n                    {{ el.visitorName }}\n                    <span\n                      v-if=\"\n                        (el.visitorType == 1 &&\n                          item.offVisitorList.length > 1) ||\n                          (el.visitorType == 4 &&\n                            item.offVisitorList.length > 1)\n                      \"\n                      >领队</span\n                    >\n                  </div>\n                </div>\n                <div class=\"right\">\n                  <span class=\"verify-code\">{{ el.visitorCode }}</span>\n                </div>\n              </div>\n              <div\n                v-for=\"opeartion in el.inOutRecordList\"\n                class=\"item-signInfo\"\n                :key=\"opeartion.id\"\n              >\n                <span>{{ formateDate(opeartion.createTime, 3) }} </span>已由{{\n                  opeartion.operationName\n                }}{{ opeartion.state == 1 ? '进门' : '出门' }}核验\n              </div>\n            </div>\n          </li>\n        </van-checkbox-group>\n      </ul>\n    </div>\n    <div class=\"single-visitor\" v-if=\"item.offVisitorList.length === 1\">\n      <div class=\"single-visitor-top\">\n        <div class=\"left\">\n          <span> {{ item.offVisitorList[0].visitorName }}</span>\n        </div>\n        <div class=\"right\">\n          <span class=\"verify-code\">{{\n            item.offVisitorList[0].visitorCode\n          }}</span>\n          <span class=\"confirm-arrive\">已签离</span>\n        </div>\n      </div>\n      <div class=\"single-visitor-bottom\">\n        <div\n          v-for=\"opeartion in item.offVisitorList[0].inOutRecordList\"\n          class=\"item-signInfo \"\n          :key=\"opeartion.id\"\n        >\n          <span>{{ formateDate(opeartion.createTime, 3) }} </span>已由{{\n            opeartion.operationName\n          }}{{ opeartion.state == 1 ? '进门' : '出门' }}核验\n        </div>\n      </div>\n    </div>\n    <confirm-modal\n      :showPanel=\"showPanel\"\n      @closePanel=\"closePanel\"\n    ></confirm-modal>\n  </div>\n</template>\n\n<script>\nimport confirmModal from './ConfirmModal.vue';\nimport { writeOffVisitor } from '@/pages/visitor-h5/api/verify';\nimport { formateDate } from '@/pages/visitor-h5/plugins/common/utils';\nexport default {\n  components: { confirmModal },\n  props: ['item', 'radius', 'type'],\n  data() {\n    return {\n      checked: true,\n      result: this.getResult(),\n      showPanel: false,\n      formateDate\n    };\n  },\n  methods: {\n    getResult() {\n      if (this.item && this.item.offVisitorList) {\n        return this.item.offVisitorList.map(el => {\n          return el.id;\n        });\n      } else {\n        return [];\n      }\n    },\n    isClick() {\n      if (\n        this.$refs.checkAll ||\n        this.$refs.checkItem ||\n        this.$refs.confirmSpan\n      ) {\n        return true;\n      }\n    },\n    confirmArrive() {\n      if (this.result.length == 0) {\n        this.$notify('请选择核销人员');\n        return;\n      }\n      writeOffVisitor({\n        guestBaseId: this.item.id,\n        visitorIds: this.result,\n        cameType: 1\n      })\n        .then(res => {\n          if (res.success) {\n            this.showPanel = true;\n          } else {\n            this.$notify(res.message);\n          }\n        })\n        .catch(res => {\n          this.$notify(res.message);\n        });\n    },\n    checkAll() {\n      this.$nextTick(() => {\n        this.$refs.checkboxGroup.toggleAll({\n          checked: !this.checked,\n          skipDisabled: true\n        });\n        this.checked = !this.checked;\n      });\n    },\n    toggle(item, index) {\n      setTimeout(() => {\n        if (this.result.length == this.item.offVisitorList.length) {\n          this.checked = true;\n        } else {\n          this.checked = false;\n        }\n      });\n    },\n    closePanel() {\n      this.showPanel = false;\n      this.$emit('onRefresh');\n      this.$emit('onClose');\n      this.$emit('onSearchClose');\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.van-checkbox-group {\n  /deep/ li {\n    margin-left: 6px;\n  }\n}\n.item-checkList {\n  font-size: 17px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: rgba(0, 0, 0, 0.45);\n  padding-bottom: 12px !important;\n}\n.item-checkwrapper {\n  .single-visitor-top,\n  .total-check {\n    line-height: 50px;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    background: rgba(51, 136, 255, 0.06);\n    padding: 0 15px;\n    &.active {\n      border-top-right-radius: 10px;\n      border-top-left-radius: 10px;\n    }\n    /deep/.van-checkbox__label,\n    .left {\n      display: flex;\n      font-size: 17px;\n      font-family: PingFangSC-Medium, PingFang SC;\n      font-weight: 500;\n      color: rgba(0, 0, 0, 0.85);\n      span {\n        margin-left: 6px;\n      }\n    }\n  }\n  .item-checkList {\n    background: #fff;\n    padding: 0 15px;\n    border-bottom-right-radius: 10px;\n    border-bottom-left-radius: 10px;\n    &.no-radius {\n      border-bottom-right-radius: 0;\n      border-bottom-left-radius: 0;\n    }\n    li {\n      .check-item {\n        .item-operate {\n          line-height: 50px;\n          display: flex;\n          justify-content: space-between;\n          align-items: center;\n          .left {\n            display: flex;\n            span {\n              display: inline-block;\n              text-align: center;\n              width: 35px;\n              height: 17px;\n              line-height: 17px;\n              margin-left: 6px;\n              background: rgba(255, 87, 86, 0.06);\n              border-radius: 2px;\n              border: 1px solid #ff5756;\n              font-size: 10px;\n              font-family: PingFangSC-Regular, PingFang SC;\n              font-weight: 400;\n              color: #ff5756;\n            }\n          }\n        }\n      }\n    }\n  }\n}\n.single-visitor {\n  background: #ffffff;\n  .left {\n    span {\n      color: rgba(0, 0, 0, 0.45) !important;\n    }\n  }\n  .single-visitor-top {\n    background: #ffffff;\n    .right {\n      display: flex;\n      align-items: center;\n    }\n  }\n  .single-visitor-bottom {\n    padding-bottom: 10px;\n    .item-signInfo {\n      margin-left: 22px;\n    }\n  }\n}\n\n.right {\n  .verify-code {\n    font-size: 22px;\n    font-family: PingFangSC-Medium, PingFang SC;\n    font-weight: 500;\n    color: rgba(0, 0, 0, 0.45);\n    margin-right: 22px;\n  }\n  .confirm-arrive {\n    display: inline-block;\n    width: 99px;\n    height: 40px;\n    line-height: 40px;\n    background: #aeb1b5;\n    border-radius: 3px;\n    text-align: center;\n    font-size: 17px;\n    font-family: PingFangSC-Medium, PingFang SC;\n    font-weight: 500;\n    color: #ffffff;\n  }\n}\n\n.item-operationList,\n.opeartion-item {\n  background: #fbfbfc;\n  padding: 12px;\n  li {\n    font-size: 12px;\n    font-family: PingFangSC-Regular, PingFang SC;\n    font-weight: 400;\n    color: rgba(0, 0, 0, 0.45);\n    line-height: 20px;\n    .operateTime {\n      margin-right: 12px;\n    }\n  }\n}\n\n.item-signInfo {\n  // margin-left: 0.56rem;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: rgba(0, 0, 0, 0.45);\n  margin-bottom: 4px;\n}\n</style>\n"]}]}
{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/teaching-points-h5/views/course-research/thesis.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/teaching-points-h5/views/course-research/thesis.vue","mtime":1627979214412},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["thesis.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"thesis.vue","sourceRoot":"src/pages/teaching-points-h5/views/course-research","sourcesContent":["<template>\r\n  <div class=\"task-detail\">\r\n    <van-cell-group>\r\n      <van-cell title=\"论文名称:\" :value=\"formData.topicThesisName\" />\r\n      <van-cell title=\"论文级别:\" :value=\"formData.topicThesisLeveName\" />\r\n      <van-cell title=\"论文类别:\" :value=\"formData.topicThesisTypeName\" />\r\n      <van-cell title=\"学科类别:\" :value=\"formData.subjectTypeName\" />\r\n      <van-cell title=\"学校:\" :value=\"formData.schoolName\" />\r\n      <van-cell title=\"作者:\" :value=\"formData.leaderAuthorName\" />\r\n      <van-cell title=\"发表时间:\" :value=\"formData.formateProjectStartTime\" />\r\n      <van-cell title=\"摘要:\" :value=\"formData.abstractDescription\" />\r\n      <van-cell title=\"关键词:\" :value=\"formData.keyWord\" />\r\n      <van-cell class=\"separate\" title=\"论文正文:\">\r\n        <template #extra>\r\n          <h3\r\n            class=\"extra-index\"\r\n            :class=\"{ opinion: formData.fileList && formData.researchContent }\"\r\n            v-if=\"formData.fileList && formData.researchContent\"\r\n          >\r\n            1、\r\n          </h3>\r\n          <p v-html=\"formData.researchContent\"></p>\r\n          <ul class=\"extra-fileList\">\r\n            <h3\r\n              class=\"extra-index\"\r\n              :class=\"{\r\n                opinion: formData.fileList && formData.researchContent\r\n              }\"\r\n              v-if=\"formData.fileList && formData.researchContent\"\r\n            >\r\n              2、\r\n              <p>(word、ppt、excel附件下载请到电脑端操作)</p>\r\n            </h3>\r\n            <li\r\n              v-for=\"(item, index) in formData.fileList\"\r\n              @click=\"toPdfPreview(item)\"\r\n              :key=\"index\"\r\n            >\r\n              <img :src=\"item | ComputedImg\" alt=\"\" />\r\n              <p>{{ item.name }}</p>\r\n            </li>\r\n          </ul>\r\n        </template>\r\n      </van-cell>\r\n    </van-cell-group>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { parseTime } from '../../plugins/core/utils';\r\nimport { Cell, CellGroup, Dialog, Toast } from 'vant';\r\nimport { getDataDetails, deleteDataDetails } from '../../api/base/course-all';\r\nimport { compareAuth } from '../../plugins/common/auth';\r\nimport { toPreview } from '../../plugins/common/utils';\r\nexport default {\r\n  name: 'courseResearchThesis',\r\n  components: {\r\n    [Cell.name]: Cell,\r\n    [CellGroup.name]: CellGroup,\r\n    [Dialog.name]: Dialog,\r\n    [Toast.name]: Toast\r\n  },\r\n  data() {\r\n    return {\r\n      id: this.$route.query.id,\r\n      formData: {\r\n        topicThesisName: '',\r\n        schoolName: '',\r\n        subjectTypeName: '',\r\n        topicThesisTypeName: '',\r\n        topicThesisLeveName: '',\r\n        leaderAuthorName: '',\r\n        projectStartTime: '',\r\n        formateProjectStartTime: '',\r\n        researchContent: '',\r\n        abstractDescription: '',\r\n        keyWord: '',\r\n        fileList: []\r\n      },\r\n      load: true,\r\n      iconDelete: require('../../assets/images/icon/icon-delete.png')\r\n    };\r\n  },\r\n  created() {\r\n    this.init();\r\n    this.getDetails();\r\n  },\r\n  filters: {\r\n    ComputedImg(val) {\r\n      let url;\r\n      switch (val.type) {\r\n        case 'zip':\r\n          url = require('../../assets/images/zip.png');\r\n          break;\r\n        case 'avi':\r\n          url = require('../../assets/images/avi.png');\r\n          break;\r\n        case 'mp4':\r\n          url = require('../../assets/images/mp4.png');\r\n          break;\r\n        case 'doc':\r\n        case 'docx':\r\n        case 'DOC':\r\n        case 'DOCX':\r\n          url = require('../../assets/images/word.png');\r\n          break;\r\n        case 'xlsx':\r\n        case 'xls':\r\n        case 'XLSX':\r\n        case 'XLS':\r\n          url = require('../../assets/images/excel.png');\r\n          break;\r\n        case 'ppt':\r\n        case 'pptx':\r\n        case 'PPT':\r\n        case 'PPTX':\r\n          url = require('../../assets/images/ppt.png');\r\n          break;\r\n        case 'pdf':\r\n        case 'PDF':\r\n          url = require('../../assets/images/pdf.png');\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      return url;\r\n    }\r\n  },\r\n  methods: {\r\n    toPdfPreview(item) {\r\n      toPreview(item);\r\n    },\r\n    init() {\r\n      let _self = this;\r\n      let isDelete = compareAuth('thesis-delete');\r\n      if (isDelete) {\r\n        _self.$dd.biz.navigation.setMenu({\r\n          backgroundColor: '#20D1CD',\r\n          textColor: '#ADD8E611',\r\n          items: [\r\n            {\r\n              id: '1',\r\n              iconId: 'delete',\r\n              text: '删除',\r\n              url: _self.iconDelete\r\n            }\r\n          ],\r\n          onSuccess: function(res) {\r\n            // 调用成功时回调\r\n            if (res.id === '1') {\r\n              _self.deleteDetails();\r\n            }\r\n          },\r\n          onFail: function(err) {\r\n            // 调用失败时回调\r\n            console.log(err);\r\n          }\r\n        });\r\n      } else {\r\n        _self.$dd.biz.navigation.setRight({\r\n          show: false //控制按钮显示， true 显示， false 隐藏， 默认true\r\n        });\r\n      }\r\n    },\r\n    /**\r\n     * 编辑页面的获取详细信息\r\n     */\r\n    getDetails() {\r\n      this.load = true;\r\n      getDataDetails({ id: this.id }).then(({ data }) => {\r\n        for (let i in this.formData) {\r\n          this.formData[i] = data[i];\r\n        }\r\n        this.formData.formateProjectStartTime = parseTime(\r\n          this.formData.projectStartTime,\r\n          '{y}年{m}月{d}日'\r\n        );\r\n        if (data.resultUrl) {\r\n          let resultName = data.resultUrl.split(',');\r\n          let resultTitle = data.resultUrlName.split(',');\r\n          this.formData.fileList = data.resultUrl\r\n            .split(',')\r\n            .map((item, index) => {\r\n              let arr = resultName[index].split('.');\r\n              return Object.assign(\r\n                {},\r\n                {\r\n                  type: arr[arr.length - 1],\r\n                  url: item,\r\n                  name: resultTitle[index]\r\n                }\r\n              );\r\n            });\r\n        }\r\n        this.load = false;\r\n      });\r\n    },\r\n    /**\r\n     * 删除\r\n     */\r\n    deleteDetails() {\r\n      Dialog.confirm({\r\n        message: '确认要删除该条数据?',\r\n        confirmButtonColor: '#489EF3'\r\n      })\r\n        .then(() => {\r\n          deleteDataDetails({ id: this.id })\r\n            .then(res => {\r\n              if (res.status === 200) {\r\n                Toast.success('删除成功!');\r\n                this.$router.go(-1);\r\n              } else {\r\n                Toast.fail(res.message);\r\n              }\r\n            })\r\n            .catch(err => {\r\n              Toast.fail(err.message);\r\n            });\r\n        })\r\n        .catch(() => {\r\n          Toast.fail('已取消删除');\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.task-detail {\r\n  width: 100%;\r\n  height: 100%;\r\n  background: #f6f6f6;\r\n  padding-top: 0.2rem;\r\n  box-sizing: border-box;\r\n  /deep/ .van-cell {\r\n    padding: 0.4rem 0.32rem;\r\n    line-height: 0.4rem;\r\n    .opinion {\r\n      word-break: break-all;\r\n      img {\r\n        max-width: 100%;\r\n      }\r\n    }\r\n    .extra-fileList {\r\n      display: flex;\r\n      width: 100%;\r\n      flex-wrap: wrap;\r\n      > li {\r\n        width: 33.3%;\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n        img {\r\n          width: 1.6rem;\r\n          height: 1.6rem;\r\n        }\r\n        p {\r\n          text-align: center;\r\n          height: 0.4rem;\r\n          width: 100%;\r\n          overflow: hidden;\r\n          text-overflow: ellipsis;\r\n          white-space: nowrap;\r\n        }\r\n      }\r\n    }\r\n    &::after {\r\n      border-bottom: 0.02rem solid rgba(25, 31, 37, 0.08);\r\n    }\r\n  }\r\n  /deep/ .van-cell__title,\r\n  .van-cell__value {\r\n    width: auto;\r\n    flex: auto;\r\n    -webkit-box-flex: inherit;\r\n    -webkit-flex: inherit;\r\n    padding-right: 0.2rem;\r\n  }\r\n  /deep/ .van-cell__title {\r\n    color: rgba(0, 0, 0, 0.45);\r\n    white-space: nowrap;\r\n  }\r\n  /deep/ .van-cell__value {\r\n    color: rgba(0, 0, 0, 0.85);\r\n    text-align: start;\r\n  }\r\n  .separate {\r\n    display: flex;\r\n    flex-direction: column;\r\n    .opinion {\r\n      padding-top: 0.3rem;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}
odel=\"formInline\" class=\"demo-form-inline\">\n      <el-form-item label=\"薪酬看板成本中心\">\n        <el-select\n          v-model=\"formInline.salary\"\n          @change=\"changeSalary\"\n          clearable\n          filterable\n          placeholder=\"请选择薪酬看板成本中心\"\n        >\n          <el-option\n            v-for=\"item in salaryList\"\n            :key=\"item\"\n            :label=\"item\"\n            :value=\"item\"\n          ></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"财务成本中心\">\n        <el-select\n          v-model=\"formInline.finance\"\n          @change=\"changeFinance\"\n          clearable\n          placeholder=\"请选择财务成本中心\"\n        >\n          <el-option\n            v-for=\"item in financeList\"\n            :key=\"item\"\n            :label=\"item\"\n            :value=\"item\"\n          ></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"人资成本中心\">\n        <el-select\n          v-model=\"formInline.hummanResource\"\n          clearable\n          placeholder=\"请选择人资成本中心\"\n        >\n          <el-option\n            v-for=\"item in hummanResourceList\"\n            :key=\"item\"\n            :label=\"item\"\n            :value=\"item\"\n          ></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"操作时间\">\n        <el-date-picker\n          v-model=\"formInline.operaDate\"\n          type=\"date\"\n          :picker-options=\"pickerOptions\"\n          format=\"yyyy-MM-dd\"\n          value-format=\"yyyy-MM-dd\"\n          placeholder=\"选择日期\"\n        >\n        </el-date-picker>\n      </el-form-item>\n      <el-form-item>\n        <el-button @click=\"addItem\" type=\"primary\" size=\"primary\"\n          >新增</el-button\n        >\n      </el-form-item>\n\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onQuery\">查询</el-button>\n      </el-form-item>\n\n      <el-form-item>\n        <el-upload\n          class=\"upload-demo\"\n          :action=\"importUrl\"\n          :headers=\"{\n            Authorization: token\n          }\"\n          accept=\".xls,.xlsx\"\n          :on-error=\"handleError\"\n          :on-progress=\"handleProgress\"\n          :on-success=\"handleSuccess\"\n          :show-file-list=\"false\"\n        >\n          <el-button type=\"primary\" @click=\"onImport\">导入</el-button>\n        </el-upload>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onExport\">导出</el-button>\n      </el-form-item>\n    </el-form>\n    <div>\n      <el-table border :data=\"tableData\">\n        <el-table-column label=\"薪酬看板成本中心\" prop=\"salaryCostCenter\">\n        </el-table-column>\n        <el-table-column label=\"财务成本中心\" prop=\"financeCostCenter\">\n        </el-table-column>\n        <el-table-column label=\"人资成本中心\" prop=\"hrCostCenter\">\n        </el-table-column>\n        <el-table-column label=\"人资成本中心代码\" prop=\"hrCostCenterCode\">\n        </el-table-column>\n        <el-table-column label=\"课时工资\" prop=\"classHourSalary\">\n        </el-table-column>\n        <el-table-column label=\"班主任补贴\" prop=\"classTeacherSubsidy\">\n        </el-table-column>\n        <el-table-column label=\"兼职补贴\" prop=\"partTimeSubsidy\">\n        </el-table-column>\n        <el-table-column label=\"学期期末考核奖\" prop=\"termEndBonus\">\n        </el-table-column>\n        <el-table-column label=\"操作\">\n          <template slot-scope=\"scope\">\n            <!-- <el-button @click=\"addItem(scope.row)\" type=\"text\" size=\"small\"\n              >新增</el-button\n            > -->\n            <el-button @click=\"editItem(scope.row)\" type=\"text\" size=\"small\"\n              >编辑</el-button\n            >\n            <el-button\n              @click=\"confirmDialog(scope.row)\"\n              type=\"text\"\n              size=\"small\"\n              >删除</el-button\n            >\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作时间\" prop=\"updateTime\" width=\"180\">\n          <template slot-scope=\"scope\">\n            {{ formatTime(scope.row.updateTime) }}\n          </template>\n        </el-table-column>\n      </el-table>\n    </div>\n    <div>\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"currentPage\"\n        :page-sizes=\"[10, 50, 100]\"\n        :page-size=\"currentSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n      >\n      </el-pagination>\n    </div>\n    <el-dialog\n      :title=\"dialogTitle\"\n      @close=\"dialogCancel\"\n      center\n      :visible.sync=\"dialogFormVisible\"\n    >\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"roleForm\">\n        <el-form-item\n          label=\"薪酬看板成本中心\"\n          prop=\"salaryCostCenter\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.salaryCostCenter\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"财务成本中心\"\n          prop=\"financeCostCenter\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.financeCostCenter\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"人资成本中心\"\n          prop=\"hrCostCenter\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.hrCostCenter\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"人资成本中心代码\"\n          prop=\"hrCostCenterCode\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.hrCostCenterCode\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"课时工资\"\n          prop=\"classHourSalary\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.classHourSalary\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"班主任补贴\"\n          prop=\"classTeacherSubsidy\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.classTeacherSubsidy\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"兼职补贴\"\n          prop=\"partTimeSubsidy\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.partTimeSubsidy\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"学期期末考核\"\n          prop=\"termEndBonus\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.termEndBonus\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogCancel\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"dialogConfirm('roleForm')\"\n          >确 定</el-button\n        >\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport {\n  getSalaryCostCenterList,\n  getFinanceCostCenterList,\n  getHrCostCenterList,\n  getCostCenterList,\n  getCostCenterItem,\n  saveCostCenterItem,\n  deleteCostCenterItem,\n  importCostCenterData,\n  exportCostCenterData\n} from './../../api/data.js';\nimport { getToken } from '@/plugins/core/auth';\nimport API_PREFIX from '@/plugins/common/api.js';\nimport laborcostMixin from '../../assets/mixins/laborcostMixin';\nexport default {\n  name: 'CoseCenter',\n  mixins: [laborcostMixin],\n  data() {\n    return {\n      formInline: {\n        salary: '',\n        finance: '',\n        hummanResource: '',\n        operaDate: ''\n      },\n      importUrl: API_PREFIX['laborcost'] + '/costCenterData/import',\n      tableData: [],\n      salaryList: [],\n      financeList: [],\n      hummanResourceList: [],\n      dialogFormVisible: false,\n      dialogTitle: '新增',\n      currentPage: 1,\n      currentSize: 10,\n      token: getToken(),\n      total: 0,\n      form: {\n        id: null,\n        salaryCostCenter: '',\n        financeCostCenter: '',\n        hrCostCenter: '',\n        hrCostCenterCode: '',\n        classHourSalary: '',\n        classTeacherSubsidy: '',\n        termEndBonus: '',\n        partTimeSubsidy: ''\n      },\n      rules: {\n        salaryCostCenter: [\n          {\n            required: true,\n            message: '请输入薪酬看板成本中心',\n            trigger: 'blur'\n          }\n        ],\n        financeCostCenter: [\n          { required: true, message: '请输入财务成本中心', trigger: 'blur' }\n        ]\n      },\n      pickerOptions: {\n        disabledDate(time) {\n          return time.getTime() > Date.now();\n        }\n      }\n    };\n  },\n  created() {\n    this.getSalaryCostCenterList();\n    this.getCostCenterList();\n  },\n  methods: {\n    changeSalary() {\n      this.formInline.finance = '';\n      this.formInline.hummanResource = '';\n      this.getFinanceCostCenterList();\n    },\n    changeFinance() {\n      this.formInline.hummanResource = '';\n      this.getHrCostCenterList();\n    },\n    handleSizeChange(val) {\n      this.currentPage = 1;\n      this.currentSize = val;\n      this.getCostCenterList();\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val;\n      this.getCostCenterList();\n    },\n    getSalaryCostCenterList() {\n      getSalaryCostCenterList({}).then(res => {\n        this.salaryList = res.data;\n      });\n    },\n    getFinanceCostCenterList() {\n      if (!this.formInline.salary) {\n        this.financeList = [];\n        return;\n      }\n      getFinanceCostCenterList({\n        salaryCostCenter: this.formInline.salary\n      }).then(res => {\n        this.financeList = res.data;\n      });\n    },\n    getHrCostCenterList() {\n      if (!this.formInline.finance) {\n        this.hummanResourceList = [];\n        return;\n      }\n      getHrCostCenterList({\n        salaryCostCenter: this.formInline.salary,\n        financeCostCenter: this.formInline.finance\n      }).then(res => {\n        this.hummanResourceList = res.data;\n      });\n    },\n    getCostCenterList() {\n      const { salary, finance, hummanResource, operaDate } = this.formInline;\n      let params = {\n        current: this.currentPage,\n        size: this.currentSize,\n        salaryCostCenter: salary,\n        financeCostCenter: finance,\n        hrCostCenter: hummanResource,\n        // hrCostCenterCode: null,\n        updateTimeStr: operaDate\n      };\n      getCostCenterList(params).then(res => {\n        this.tableData = res.data.records || [];\n        this.total = res.data.total;\n        if (this.currentPage != res.data.current) {\n          this.currentPage = res.data.current;\n        }\n      });\n    },\n    dialogConfirm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.saveCostCenterItem();\n        } else {\n          return false;\n        }\n      });\n    },\n    dialogCancel() {\n      this.dialogFormVisible = false;\n      this.$refs['roleForm'].resetFields();\n    },\n    saveCostCenterItem() {\n      let params = Object.assign(this.form);\n      saveCostCenterItem(params)\n        .then(res => {\n          this.dialogFormVisible = false;\n          this.getCostCenterList();\n          if (this.form.id) {\n            this.warning('修改成功', 'success');\n          } else {\n            this.warning('新增成功', 'success');\n          }\n        })\n        .catch(e => {\n          if (e.status && e.status == 511) {\n            this.$confirm('该条记录已存在！', '', {\n              confirmButtonText: '确定',\n              cancelButtonText: '取消',\n              center: true\n            })\n              .then(() => {})\n              .catch(() => {\n                // cancelFun && cancelFun();\n              });\n          }\n        });\n    },\n    confirmDialog(row) {\n      this.$confirm('是否确认删除本行？', '', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        center: true\n      })\n        .then(() => {\n          this.deleteItem(row);\n        })\n        .catch(() => {\n          // cancelFun && cancelFun();\n        });\n    },\n    deleteItem(row) {\n      deleteCostCenterItem({ id: row.id }).then(res => {\n        this.warning('删除成功', 'success');\n        this.getCostCenterList();\n      });\n    },\n    onQuery() {\n      this.getCostCenterList();\n    },\n    onImport(file) {\n      // importCostCenterData({ file: file }).then(res => {});\n    },\n    onExport() {\n      exportCostCenterData({}).then(res => {\n        const content = res;\n        this.$util.downLoadBlob(content, '成本中心对照表');\n      });\n      // const url = window.location.origin + '/laborcost/costCenterData/import';\n      // window.open(url);\n    },\n    addItem() {\n      this.dialogFormVisible = true;\n    },\n    editItem(row) {\n      this.dialogFormVisible = true;\n\n      this.$nextTick(function() {\n        this.form = Object.assign({}, row);\n      });\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.cost-center-wrapper {\n  padding: 20px;\n}\n</style>\n"]}]}
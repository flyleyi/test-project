lick-overlay=\"false\"\n    >\n      <van-datetime-picker\n        v-model=\"selectedTime\"\n        type=\"time\"\n        title=\"选择时间\"\n        :min-hour=\"minHour\"\n        :min-minute=\"minMinute\"\n        @confirm=\"onTimeConfirm\"\n      />\n    </van-popup>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      showTime: false,\n      selectedTime: '',\n      minHour: '09',\n      minMinute: '00'\n    };\n  },\n  methods: {\n    handleShow() {\n      this.showTime = true;\n      this.minHour = new Date().getHours();\n      this.minMinute = new Date().getMinutes();\n    },\n    onTimeConfirm(time) {\n      this.selectedTime = time;\n      this.showTime = false;\n      this.$emit('setTime', this.selectedTime);\n    },\n    onCancel() {\n      this.selectedTime = '';\n      this.showTime = false;\n      setTimeout(() => {\n        this.selectedTime = '';\n      });\n      this.$emit('setTime', '');\n    }\n  },\n  watch: {\n    selectedTime(v) {\n      if (v) {\n        let arr = v.split(':');\n        if (arr[0] > new Date().getHours()) {\n          this.minMinute = '00';\n        } else {\n          this.minMinute = new Date().getMinutes();\n        }\n      }\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.show-text {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  justify-content: space-between;\n  box-sizing: border-box;\n  align-items: center;\n  padding: 0 10px;\n  line-height: 35px;\n  img {\n    width: 22px;\n  }\n}\n\n/deep/ .van-picker__cancel {\n  opacity: 0;\n}\n</style>\n"]}]}
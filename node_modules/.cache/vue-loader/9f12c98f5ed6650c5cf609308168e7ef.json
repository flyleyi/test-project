;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SignIn.vue","sourceRoot":"src/pages/visitor-h5/views/verify-visitor/components","sourcesContent":["<template>\n  <div class=\"item-checkwrapper\" v-if=\"item.leaveVisitorList\">\n    <div v-if=\"item.leaveVisitorList.length > 1\">\n      <van-sticky>\n        <div class=\"item total-check\" :class=\"radius ? 'active' : ''\">\n          <div class=\"left\">\n            <van-checkbox\n              @click.stop.native=\"checkAll\"\n              label-disabled\n              shape=\"square\"\n              v-model=\"checked\"\n            ></van-checkbox>\n            <span>全选签离 共{{ result.length }}人</span>\n          </div>\n          <div class=\"right\">\n            <span class=\"confirm-signOut\" @click.stop=\"confirmSignOut\"\n              >访客签离</span\n            >\n          </div>\n        </div>\n      </van-sticky>\n      <ul class=\"item-checkList\" :class=\"type == 'outward' ? 'no-radius' : ''\">\n        <van-checkbox-group v-model=\"result\" ref=\"checkboxGroup\">\n          <li\n            v-for=\"(el, index) in item.leaveVisitorList\"\n            :key=\"index + !!el.code\"\n          >\n            <div class=\"check-item\">\n              <div class=\"item-operate\">\n                <div class=\"left\">\n                  <van-checkbox\n                    ref=\"checkboxes\"\n                    label-disabled\n                    shape=\"square\"\n                    :name=\"el.id\"\n                    @click.stop.native=\"toggle(el, index)\"\n                    >{{ el.visitorName }}</van-checkbox\n                  >\n                  <span\n                    v-if=\"\n                      (el.visitorType == 1 &&\n                        item.leaveVisitorList.length > 1) ||\n                        (el.visitorType == 4 &&\n                          item.leaveVisitorList.length > 1)\n                    \"\n                    >领队</span\n                  >\n                </div>\n                <div class=\"right\">\n                  <span class=\"verify-code\">{{ el.visitorCode }}</span>\n                </div>\n              </div>\n              <div\n                v-for=\"opeartion in el.inOutRecordList\"\n                class=\"item-signInfo\"\n                :key=\"opeartion.id\"\n              >\n                <span>{{ formateDate(opeartion.createTime, 3) }} </span>已由{{\n                  opeartion.operationName\n                }}进门核验\n              </div>\n            </div>\n          </li>\n        </van-checkbox-group>\n      </ul>\n    </div>\n    <div class=\"single-visitor\" v-if=\"item.leaveVisitorList.length === 1\">\n      <div class=\"single-visitor-top\">\n        <div class=\"left\">\n          <span> {{ item.leaveVisitorList[0].visitorName }}</span>\n        </div>\n        <div class=\"right\">\n          <span class=\"verify-code\">{{\n            item.leaveVisitorList[0].visitorCode\n          }}</span>\n          <span\n            class=\"confirm-signOut\"\n            @click.stop=\"confirmSignOut(item.leaveVisitorList[0])\"\n            >访客签离</span\n          >\n        </div>\n      </div>\n      <div class=\"single-visitor-bottom\">\n        <div\n          v-for=\"opeartion in item.leaveVisitorList[0].inOutRecordList\"\n          class=\"item-signInfo \"\n          :key=\"opeartion.id\"\n        >\n          <span>{{ formateDate(opeartion.createTime, 3) }} </span>已由{{\n            opeartion.operationName\n          }}进门核验\n        </div>\n      </div>\n    </div>\n    <confirm-modal\n      :showPanel=\"showPanel\"\n      @closePanel=\"closePanel\"\n    ></confirm-modal>\n  </div>\n</template>\n\n<script>\nimport confirmModal from './ConfirmModal.vue';\nimport { writeOffVisitor, visitorLeave } from '@/pages/visitor-h5/api/verify';\nimport { formateDate } from '@/pages/visitor-h5/plugins/common/utils';\nexport default {\n  components: { confirmModal },\n  props: ['item', 'radius', 'type'],\n  data() {\n    return {\n      checked: true,\n      result: this.getResult(),\n      showPanel: false,\n      formateDate\n    };\n  },\n  watch: {\n    item: {\n      deep: true,\n      handler(val) {\n        this.checked = true;\n        this.result = this.getResult();\n      }\n    }\n  },\n  methods: {\n    getResult() {\n      if (this.item && this.item.leaveVisitorList) {\n        return this.item.leaveVisitorList.map(el => {\n          return el.id;\n        });\n      } else {\n        return [];\n      }\n    },\n    isClick() {\n      if (\n        this.$refs.checkAll ||\n        this.$refs.checkItem ||\n        this.$refs.confirmSpan\n      ) {\n        return true;\n      }\n    },\n    confirmSignOut(row) {\n      if (this.result.length == 0) {\n        this.$notify('请选择需签离人员');\n        return;\n      }\n\n      const loading = this.$toast.loading({\n        mask: true\n      });\n      visitorLeave({\n        // guestBaseId: this.item.id,\n        visitorIds: row.id ? [row.id] : this.result,\n        leaveSource: 1\n      })\n        .then(res => {\n          if (res.success) {\n            this.showPanel = true;\n          } else {\n            this.$notify(res.message);\n          }\n          loading.clear();\n        })\n        .catch(res => {\n          this.$notify(res.message);\n          loading.clear();\n        });\n    },\n    checkAll() {\n      this.$nextTick(() => {\n        this.$refs.checkboxGroup.toggleAll({\n          checked: !this.checked,\n          skipDisabled: true\n        });\n        this.checked = !this.checked;\n      });\n    },\n    toggle(item, index) {\n      setTimeout(() => {\n        if (this.result.length == this.item.leaveVisitorList.length) {\n          this.checked = true;\n        } else {\n          this.checked = false;\n        }\n      });\n    },\n    closePanel() {\n      this.showPanel = false;\n      this.$emit('onRefresh');\n      this.$emit('onClose');\n      this.$emit('onSearchClose');\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n/deep/ .van-checkbox__label {\n  font-size: 17px;\n}\n.item-checkList {\n  padding-bottom: 12px !important;\n}\n.item-checkwrapper {\n  .single-visitor-top,\n  .total-check {\n    line-height: 50px;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    background: #f1f6fe;\n    padding: 0 15px;\n    &.active {\n      border-top-right-radius: 10px;\n      border-top-left-radius: 10px;\n    }\n    /deep/.van-checkbox__label,\n    .left {\n      display: flex;\n      font-size: 17px;\n      font-family: PingFangSC-Medium, PingFang SC;\n      font-weight: 500;\n      color: rgba(0, 0, 0, 0.85);\n      span {\n        margin-left: 6px;\n      }\n    }\n  }\n\n  .single-visitor {\n    background: #ffffff;\n    .single-visitor-top {\n      background: #ffffff;\n      .right {\n        display: flex;\n        align-items: center;\n      }\n    }\n    .single-visitor-bottom {\n      padding-bottom: 10px;\n      .item-signInfo {\n        margin-left: 22px;\n      }\n    }\n  }\n\n  .item-checkList {\n    background: #fff;\n    padding: 0 15px;\n    // border-bottom-right-radius: 10px;\n    // border-bottom-left-radius: 10px;\n    &.no-radius {\n      border-bottom-right-radius: 0;\n      border-bottom-left-radius: 0;\n    }\n    li {\n      .check-item {\n        .item-operate {\n          line-height: 50px;\n          display: flex;\n          justify-content: space-between;\n          align-items: center;\n          .left {\n            display: flex;\n            span {\n              display: inline-block;\n              text-align: center;\n              width: 35px;\n              height: 17px;\n              line-height: 17px;\n              margin-left: 6px;\n              background: rgba(255, 87, 86, 0.06);\n              border-radius: 2px;\n              border: 1px solid #ff5756;\n              font-size: 10px;\n              font-family: PingFangSC-Regular, PingFang SC;\n              font-weight: 400;\n              color: #ff5756;\n            }\n          }\n        }\n      }\n    }\n  }\n}\n\n.item-signInfo {\n  margin-left: 28px;\n  line-height: 16.5px;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: rgba(0, 0, 0, 0.45);\n}\n.right {\n  .verify-code {\n    font-size: 22px;\n    font-family: PingFangSC-Medium, PingFang SC;\n    font-weight: 500;\n    color: rgba(0, 0, 0, 0.85);\n    margin-right: 22px;\n  }\n  .confirm-signOut {\n    display: inline-block;\n    width: 99px;\n    height: 40px;\n    line-height: 40px;\n    background: #595959;\n    border-radius: 3px;\n    text-align: center;\n    font-size: 17px;\n    font-family: PingFangSC-Medium, PingFang SC;\n    font-weight: 500;\n    color: #ffffff;\n  }\n}\n\n.item-operationList,\n.opeartion-item {\n  background: #fbfbfc;\n  padding: 12px;\n  li {\n    font-size: 12px;\n    font-family: PingFangSC-Regular, PingFang SC;\n    font-weight: 400;\n    color: rgba(0, 0, 0, 0.45);\n    line-height: 20px;\n    .operateTime {\n      margin-right: 12px;\n    }\n  }\n}\n</style>\n"]}]}
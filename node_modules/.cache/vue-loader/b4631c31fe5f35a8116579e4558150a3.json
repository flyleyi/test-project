  @click=\"setRadio(item)\"\n          :class=\"radio == item.id ? 'active' : ''\"\n        >\n          <span>{{ item.purposeName }}</span>\n          <van-radio :name=\"item.id + ''\" shape=\"square\" />\n        </li>\n      </ul>\n    </van-radio-group>\n  </div>\n</template>\n\n<script>\nimport eventBus from '@/pages/visitor-h5/plugins/common/eventBus.js';\nimport { getPurposeList } from '@/pages/visitor-h5/api/myVisitor';\nexport default {\n  data() {\n    return {\n      radio: '',\n      purposeList: [],\n      initPurposeId: ''\n    };\n  },\n  methods: {\n    setRadio(item) {\n      this.radio = item.id;\n      eventBus.$emit('selectPurpose', item);\n      eventBus.$emit('setcurType', this.$route.query.type);\n      this.$router.go(-1);\n    },\n    getPurposeList() {\n      getPurposeList().then(res => {\n        this.purposeList = res.data;\n      });\n    }\n  },\n  created() {\n    this.initPurposeId = this.$route.query.data;\n    if (this.initPurposeId) {\n      this.radio = this.initPurposeId;\n    }\n    this.getPurposeList();\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.purpose-wrapper {\n  background: #f5f6f8;\n  padding: 0 16px;\n  p {\n    line-height: 50px;\n    font-size: 15px;\n    font-family: PingFangSC-Regular, PingFang SC;\n    font-weight: 400;\n    color: rgba(0, 0, 0, 0.85);\n  }\n  .purpose-list {\n    li {\n      display: flex;\n      justify-content: space-between;\n      height: 40px;\n      line-height: 40px;\n      padding: 0 12px;\n      background: #ffffff;\n      border-radius: 5px;\n      border: 1px solid #abbfd6;\n      margin-bottom: 7px;\n      font-size: 14px;\n      font-family: PingFangSC-Regular, PingFang SC;\n      font-weight: 400;\n      color: rgba(0, 0, 0, 0.65);\n      &.active {\n        border: 1px solid #3388ff;\n        color: #3388ff;\n      }\n    }\n  }\n}\n</style>\n"]}]}
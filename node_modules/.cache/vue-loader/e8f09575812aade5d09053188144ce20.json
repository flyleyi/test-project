/plugins/axios';\r\nexport default {\r\n  name: 'BUserManage',\r\n  // 注册组件\r\n  components: {\r\n    BTable\r\n  },\r\n  props: {\r\n    /**\r\n     * 服务器类型\r\n     */\r\n    serverType: {\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  // 定义数据\r\n  data() {\r\n    // 权限校验方法\r\n    let $has = this.$has;\r\n    typeof $has !== 'function' && ($has = () => true);\r\n    return {\r\n      $has,\r\n      // 定义查询表单\r\n      formItems: [\r\n        {\r\n          type: 'input',\r\n          prop: 'staffName',\r\n          label: '姓名：'\r\n        },\r\n        {\r\n          type: 'input',\r\n          prop: 'staffCode',\r\n          label: '工号：'\r\n        },\r\n        {\r\n          type: 'select',\r\n          options: [\r\n            {\r\n              value: '0',\r\n              label: '启用'\r\n            },\r\n            {\r\n              value: '1',\r\n              label: '禁用'\r\n            }\r\n          ],\r\n          prop: 'isShow',\r\n          label: '状态：'\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  // 计算属性\r\n  computed: {\r\n    // 定义列\r\n    columns() {\r\n      let btns = [\r\n        {\r\n          label: '配置数据权限',\r\n          click: row => {\r\n            //菜单授权\r\n            this.$router.push({\r\n              path: '/system/userPermission',\r\n              query: {\r\n                staffCode: row.staffCode\r\n              }\r\n            });\r\n          },\r\n          auth: ['user-permission'],\r\n          size: 'mini',\r\n          type: 'primary'\r\n        }\r\n      ];\r\n      return [\r\n        {\r\n          label: '姓名',\r\n          prop: 'staffName'\r\n        },\r\n        {\r\n          label: '工号',\r\n          prop: 'staffCode'\r\n        },\r\n        {\r\n          label: '手机号码',\r\n          prop: 'mobile'\r\n        },\r\n        {\r\n          label: '性别',\r\n          prop: 'gender'\r\n        },\r\n        {\r\n          label: '状态',\r\n          prop: 'isShow',\r\n          formatter(row, column, cellValue) {\r\n            return cellValue == '0' ? '启用' : '禁用';\r\n          }\r\n        },\r\n        // {\r\n        //   label: '具备角色',\r\n        //   prop: 'gender1'\r\n        // },\r\n        {\r\n          label: '数据权限',\r\n          prop: 'authNameListStr'\r\n        },\r\n        {\r\n          label: '单位/部门',\r\n          prop: 'organization',\r\n          'show-overflow-tooltip': true\r\n        },\r\n        {\r\n          label: '操作',\r\n          colType: 'btns',\r\n          btns: btns.filter(item => this.$has(item.auth)),\r\n          width: '130px'\r\n        }\r\n      ];\r\n    }\r\n  },\r\n  // 侦听器\r\n  watch: {},\r\n  // 创建完成钩子\r\n  created() {},\r\n  // 挂在完成钩子\r\n  mounted() {},\r\n  // 方法定义\r\n  methods: {\r\n    /**\r\n     * 获取用户数据\r\n     */\r\n    ajaxGetUserData(params) {\r\n      return axios(\r\n        {\r\n          method: 'GET',\r\n          url: '/userManage/queryUserInfo',\r\n          params\r\n        },\r\n        this.serverType\r\n      );\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.user {\r\n  background: #fff;\r\n  border-radius: 2px;\r\n  padding: 0 25px 0;\r\n  &-table {\r\n    padding-top: 23px;\r\n  }\r\n}\r\n</style>\r\n"]}]}
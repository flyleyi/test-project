r-view>\r\n      </keep-alive>\r\n    </transition>\r\n    <transition :name=\"transitionName\">\r\n      <router-view class=\"child-view\" v-if=\"!$route.meta.cache\"></router-view>\r\n    </transition>\r\n    <van-tabbar\r\n      class=\"footer\"\r\n      v-show=\"isShowTabbar\"\r\n      v-model=\"active\"\r\n      active-color=\"#489EF3\"\r\n      inactive-color=\"rgba(0,0,0,0.45)\"\r\n      style=\"z-index:9999;\"\r\n    >\r\n      <van-tabbar-item\r\n        v-for=\"item in plates\"\r\n        :key=\"item.routes\"\r\n        replace\r\n        :to=\"item.routes\"\r\n      >\r\n        <span>{{ item.name }}</span>\r\n        <template #icon=\"props\">\r\n          <img\r\n            :src=\"\r\n              props.active\r\n                ? require(`./assets/images/${item.activeIcon}.png`)\r\n                : require(`./assets/images/${item.inActiveIcon}.png`)\r\n            \"\r\n            alt=\"\"\r\n          />\r\n        </template>\r\n      </van-tabbar-item>\r\n    </van-tabbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Tabbar, TabbarItem } from 'vant';\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    [Tabbar.name]: Tabbar,\r\n    [TabbarItem.name]: TabbarItem\r\n  },\r\n  data() {\r\n    return {\r\n      active: 0,\r\n      transitionName: 'slide-left',\r\n      height: 0\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['plates']),\r\n    isShowTabbar() {\r\n      let boolenType;\r\n      if (\r\n        this.$route.name == 'teachingActivity' ||\r\n        this.$route.name == 'normalExamine' ||\r\n        this.$route.name == 'courseResearch' ||\r\n        this.$route.name == 'teacherPoints' ||\r\n        this.$route.name == 'mySetting'\r\n      ) {\r\n        boolenType = true;\r\n      } else {\r\n        boolenType = false;\r\n      }\r\n      return boolenType;\r\n    }\r\n  },\r\n  watch: {\r\n    $route(to, from) {\r\n      const toDepth = to.path.split('/').length;\r\n      const fromDepth = from.path.split('/').length;\r\n      this.transitionName = toDepth < fromDepth ? 'slide-right' : 'slide-left';\r\n      if (this.plates.length > 0) {\r\n        this.plates.forEach(item => {\r\n          if (item.routes.indexOf(to.path) > -1) {\r\n            this.active = Number(item.activeNum);\r\n          }\r\n        });\r\n        if (\r\n          from.path.indexOf('login') > -1 &&\r\n          to.path !== this.plates[0].routes\r\n        ) {\r\n          this.$router.replace({ path: this.plates[0].routes }); //重定向\r\n        }\r\n        if (to.name === 'NotFound' && from.path === this.plates[0].routes) {\r\n          this.$router.replace({ path: '/ding-login' });\r\n        }\r\n      }\r\n    },\r\n    plates(val) {\r\n      if (val.length > 0) {\r\n        this.plates.forEach(item => {\r\n          if (item.routes.indexOf(this.$route.path) > -1) {\r\n            this.active = Number(item.activeNum);\r\n          }\r\n        });\r\n        if (this.$route.path === '/') {\r\n          this.$router.replace({ path: this.plates[0].routes }); //重定向\r\n        }\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.$nextTick(() => {\r\n      let u = navigator.userAgent;\r\n      let isiOS = !!u.match(/\\(i[^;]+;( U;)? CPU.+Mac OS X/); //ios终端\r\n      if (isiOS) {\r\n        document.querySelector('.footer').classList.add('ios-tabbar');\r\n        this.$dd.ui.webViewBounce.disable();\r\n      }\r\n    });\r\n  }\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n#app {\r\n  font-family: 'PingFangSC-Regular', 'Avenir', Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  width: 100%;\r\n  height: 100%;\r\n  min-height: 100vh;\r\n  /deep/ .svg-icon {\r\n    width: 0.48rem;\r\n    height: 0.48rem;\r\n  }\r\n  /deep/ .van-tabbar--fixed {\r\n    // border-top: 1px solid rgba(25, 31, 37, 0.12);\r\n    border-bottom: 0;\r\n    padding-bottom: constant(safe-area-inset-bottom); /* 兼容 iOS < 11.2 */\r\n    padding-bottom: env(safe-area-inset-bottom);\r\n  }\r\n  /deep/ .van-tabbar {\r\n    height: 1rem;\r\n    position: fixed;\r\n    bottom: 0;\r\n    left: 0;\r\n    border-top: 0 !important;\r\n    -webkit-overflow-scrolling: auto;\r\n    .van-tabbar-item__icon {\r\n      margin-bottom: 0.04rem;\r\n      img {\r\n        width: 0.48rem;\r\n        height: 0.48rem;\r\n      }\r\n    }\r\n    .van-tabbar-item__text {\r\n      font-size: 0.2rem;\r\n      -webkit-text-size-adjust: 100%;\r\n    }\r\n  }\r\n  /deep/ .van-picker__confirm {\r\n    color: #489ef3;\r\n  }\r\n\r\n  /deep/ .van-hairline--top-bottom::after,\r\n  .van-hairline-unset--top-bottom::after {\r\n    border-width: 2px 0;\r\n  }\r\n  /deep/ .ios-tabbar::after {\r\n    border-width: 1px 0 !important;\r\n  }\r\n}\r\n.child-view {\r\n  position: fixed;\r\n  width: 100%;\r\n  height: calc(100% - 1rem);\r\n  overflow-y: auto;\r\n  background: #f6f6f6;\r\n  box-sizing: border-box;\r\n  padding-bottom: constant(safe-area-inset-bottom); /* 兼容 iOS < 11.2 */\r\n  padding-bottom: env(safe-area-inset-bottom); /* 兼容 iOS >= 11.2 */\r\n  /* 增加该属性，可以增加弹性 */\r\n  -webkit-overflow-scrolling: auto;\r\n}\r\n</style>\r\n"]}]}
ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/pages/laborcost/views/HumanResource","sourcesContent":["<template>\n  <div class=\"human-resource-wrapper\">\n    <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\n      <el-form-item label=\"导入类型\">\n        <el-select\n          v-model=\"formInline.importType\"\n          @change=\"changeImportType\"\n          clearable\n          placeholder=\"请选择导入类型\"\n        >\n          <el-option\n            v-for=\"item in importTypeList\"\n            :key=\"item\"\n            :label=\"item\"\n            :value=\"item\"\n          ></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"日期\">\n        <el-date-picker\n          v-model=\"formInline.dateRange\"\n          type=\"month\"\n          format=\"yyyyMM\"\n          value-format=\"yyyyMM\"\n          :picker-options=\"pickerOptions\"\n          placeholder=\"选择日期\"\n        >\n        </el-date-picker>\n      </el-form-item>\n      <el-form-item label=\"人资成本中心\">\n        <el-select\n          v-model=\"formInline.hummanResource\"\n          clearable\n          placeholder=\"请选择人资成本中心\"\n        >\n          <el-option\n            v-for=\"item in hummanResourceList\"\n            :key=\"item\"\n            :label=\"item\"\n            :value=\"item\"\n          ></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"操作时间\">\n        <el-date-picker\n          v-model=\"formInline.operaDate\"\n          type=\"date\"\n          format=\"yyyy-MM-dd\"\n          value-format=\"yyyy-MM-dd\"\n          :picker-options=\"pickerOptions\"\n          placeholder=\"选择日期\"\n        >\n        </el-date-picker>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onQuery\">查询</el-button>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"addItem\">新增</el-button>\n      </el-form-item>\n      <el-form-item>\n        <el-upload\n          class=\"upload-demo\"\n          :action=\"importUrl\"\n          :headers=\"{\n            Authorization: token\n          }\"\n          :on-progress=\"handleProgress\"\n          :on-success=\"handleSuccess\"\n          :on-error=\"handleError\"\n          accept=\".xls,.xlsx\"\n          :show-file-list=\"false\"\n        >\n          <el-button type=\"primary\" @click=\"onImport\">导入</el-button>\n        </el-upload>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onExport\">导出</el-button>\n      </el-form-item>\n    </el-form>\n    <div>\n      <el-table border :data=\"tableData\">\n        <el-table-column label=\"导入类型\" prop=\"importType\"> </el-table-column>\n        <el-table-column label=\"人资成本中心代码\" prop=\"hrCostCenterCode\">\n        </el-table-column>\n        <el-table-column label=\"人资成本中心\" prop=\"hrCostCenter\">\n        </el-table-column>\n        <el-table-column label=\"人员类型\" prop=\"staffType\"> </el-table-column>\n        <el-table-column label=\"人数\" prop=\"staffCount\"> </el-table-column>\n        <el-table-column label=\"金额/元\" prop=\"costAmount\"> </el-table-column>\n        <el-table-column label=\"日期\" prop=\"dateRange\"> </el-table-column>\n        <el-table-column label=\"操作\">\n          <template slot-scope=\"scope\">\n            <!-- <el-button @click=\"addItem()\" type=\"text\" size=\"small\"\n              >新增</el-button\n            > -->\n            <el-button @click=\"editItem(scope.row)\" type=\"text\" size=\"small\"\n              >编辑</el-button\n            >\n            <el-button\n              @click=\"confirmDialog(scope.row)\"\n              type=\"text\"\n              size=\"small\"\n              >删除</el-button\n            >\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作时间\" prop=\"updateTime\" width=\"180\">\n          <template slot-scope=\"scope\">\n            {{ formatTime(scope.row.updateTime) }}\n          </template>\n        </el-table-column>\n      </el-table>\n    </div>\n    <div>\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"currentPage\"\n        :page-sizes=\"[10, 50, 100]\"\n        :page-size=\"currentSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n      >\n      </el-pagination>\n    </div>\n    <el-dialog\n      :title=\"dialogTitle\"\n      center\n      @close=\"dialogCancel\"\n      :visible.sync=\"dialogFormVisible\"\n    >\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"roleForm\">\n        <el-form-item label=\"导入类型\" prop=\"importType\" label-width=\"160px\">\n          <el-input\n            v-model=\"form.importType\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"人资成本中心代码\"\n          prop=\"hrCostCenterCode\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.hrCostCenterCode\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"人资成本中心\"\n          prop=\"hrCostCenter\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.hrCostCenter\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"人员类别\" prop=\"staffType\" label-width=\"160px\">\n          <el-input\n            v-model=\"form.staffType\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"人数\" prop=\"staffCount\" label-width=\"160px\">\n          <el-input\n            v-model.number=\"form.staffCount\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"金额/元\" prop=\"costAmount\" label-width=\"160px\">\n          <el-input\n            v-model=\"form.costAmount\"\n            type=\"number\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"日期\" prop=\"dateRange\" label-width=\"160px\">\n          <el-date-picker\n            v-model=\"form.dateRange\"\n            type=\"month\"\n            format=\"yyyyMM\"\n            value-format=\"yyyyMM\"\n            :picker-options=\"pickerOptions\"\n            placeholder=\"选择日期\"\n          >\n          </el-date-picker>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogCancel\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"dialogConfirm('roleForm')\"\n          >确 定</el-button\n        >\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport {\n  getImportTypeList,\n  getHrDataCenterList,\n  getHrDataList,\n  getHrDataItem,\n  saveHrDataItem,\n  deleteHrDataItem,\n  exportHrData\n} from './../../api/data.js';\nimport { getToken } from '@/plugins/core/auth';\nimport API_PREFIX from '@/plugins/common/api.js';\nimport laborcostMixin from '../../assets/mixins/laborcostMixin';\nconst checkNumber = (rule, value, callback) => {\n  const reg = /^(-?\\d+)(\\.\\d{1,2})?$/;\n  if (value && !reg.test(value)) {\n    callback(new Error('金额最多两位小数'));\n  }\n  callback();\n};\nexport default {\n  name: 'humanResource',\n  mixins: [laborcostMixin],\n  data() {\n    return {\n      importUrl: API_PREFIX['laborcost'] + '/hrData/import',\n      formInline: {\n        importType: '',\n        dateRange: '',\n        hummanResource: '',\n        operaDate: ''\n      },\n      form: {\n        id: null,\n        importType: null,\n        hrCostCenterCode: null,\n        hrCostCenter: null,\n        staffType: null,\n        staffCount: null,\n        costAmount: null,\n        dateRange: null\n      },\n      dialogTitle: '新增',\n      currentPage: 1,\n      currentSize: 10,\n      token: getToken(),\n      total: 0,\n      tableData: [],\n      importTypeList: [],\n      hummanResourceList: [],\n      dialogFormVisible: false,\n      rules: {\n        importType: [\n          {\n            required: true,\n            message: '请输入导入类型',\n            trigger: 'blur'\n          }\n        ],\n        hrCostCenterCode: [\n          { required: true, message: '请输入财务成本中心代码', trigger: 'blur' }\n        ],\n        hrCostCenter: [\n          { required: true, message: '请输入财务成本中心', trigger: 'blur' }\n        ],\n        costAmount: [{ validator: checkNumber, trigger: 'blur' }],\n        dateRange: [\n          { required: true, message: '请输入财务成本中心', trigger: 'blur' }\n        ]\n      },\n      pickerOptions: {\n        disabledDate(time) {\n          return time.getTime() > Date.now();\n        }\n      }\n    };\n  },\n  created() {\n    this.getImportTypeList();\n    this.getHrDataList();\n  },\n  methods: {\n    changeImportType() {},\n    getImportTypeList() {\n      getImportTypeList().then(res => {\n        this.importTypeList = res.data;\n      });\n    },\n    handleSizeChange(val) {\n      this.currentPage = 1;\n      this.currentSize = val;\n      this.getHrDataList();\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val;\n      this.getHrDataList();\n    },\n    getHrDataCenterList() {\n      getHrDataCenterList().then(res => {\n        this.hummanResourceList = res.data;\n      });\n    },\n    addItem() {\n      this.dialogFormVisible = true;\n    },\n    getHrDataList() {\n      const {\n        importType,\n        dateRange,\n        hummanResource,\n        operaDate\n      } = this.formInline;\n      let params = {\n        current: this.currentPage,\n        size: this.currentSize,\n        importType: importType,\n        dateRange: dateRange,\n        hrCostCenterCode: hummanResource,\n        // hrCostCenterCode: null,\n        updateTimeStr: operaDate\n      };\n      getHrDataList(params).then(res => {\n        this.tableData = res.data.records || [];\n        this.total = res.data.total;\n        if (this.currentPage != res.data.current) {\n          this.currentPage = res.data.current;\n        }\n      });\n    },\n    editItem(row) {\n      this.dialogFormVisible = true;\n      this.$nextTick(function() {\n        this.form = Object.assign({}, row);\n      });\n    },\n    confirmDialog(row) {\n      this.$confirm('是否确认删除本行？', '', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        center: true\n      })\n        .then(() => {\n          this.deleteItem(row);\n        })\n        .catch(() => {});\n    },\n    dialogConfirm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.saveHrDataItem();\n        } else {\n          return false;\n        }\n      });\n    },\n    saveHrDataItem() {\n      let params = Object.assign({ id: null }, this.form);\n      saveHrDataItem(params)\n        .then(res => {\n          this.getHrDataList();\n          if (this.form.id) {\n            this.warning('修改成功', 'success');\n          } else {\n            this.warning('新增成功', 'success');\n          }\n          this.dialogFormVisible = false;\n        })\n        .catch(e => {\n          if (e.status && e.status == 511) {\n            this.$confirm('该条记录已存在！', '', {\n              confirmButtonText: '确定',\n              cancelButtonText: '取消',\n              center: true\n            })\n              .then(() => {})\n              .catch(() => {\n                // cancelFun && cancelFun();\n              });\n          }\n        });\n    },\n    dialogCancel() {\n      this.dialogFormVisible = false;\n      this.$refs['roleForm'].resetFields();\n    },\n    onQuery() {\n      this.getHrDataList();\n    },\n    onImport() {},\n    onExport() {\n      exportHrData({}).then(res => {\n        const content = res;\n        this.$util.downLoadBlob(content, '人资导入数据表');\n      });\n    },\n    deleteItem(row) {\n      deleteHrDataItem({ id: row.id }).then(res => {\n        this.warning('删除成功', 'success');\n        this.getHrDataList();\n      });\n    }\n  }\n};\n</script>\n\n<style>\n.human-resource-wrapper {\n  padding: 20px;\n}\n</style>\n"]}]}
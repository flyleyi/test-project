addingStyle\">\n      <div class=\"failsTimesStyle\">共{{ total }}张</div>\n      <div class=\"failedHomeworkPictureStyle\">\n        <div\n          class=\"failedPictureStyle\"\n          v-for=\"item in failedPictruesArray\"\n          :key=\"item.id\"\n        >\n          <img\n            :src=\"item.data[0].resUrl\"\n            class=\"a3pictureStyle\"\n            v-if=\"item.data[0].paperType == 'A3'\"\n          />\n          <img\n            :src=\"item.data[0].resUrl\"\n            class=\"a4pictureStyle\"\n            v-else-if=\"item.data[0].paperType == 'A4'\"\n          />\n          <img\n            :src=\"item.data[1].resUrl\"\n            class=\"a4pictureStyle\"\n            v-if=\"item.data[1].paperType == 'A4'\"\n          />\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Upload } from 'element-ui';\nimport a3pic from './a3.png';\nimport a4pic from './a4.png';\n\nexport default {\n  name: 'DynamicFormPage',\n  components: {},\n  props: {\n    id: {\n      type: Number,\n      default: 0\n    },\n    studentListArray: {\n      type: Array,\n      default: () => []\n    },\n    total: {\n      type: Number,\n      default: 0\n    }\n  },\n  data() {\n    return {\n      a3pic,\n      a4pic,\n      failedPictruesArray: []\n      // studentListArray: [\n      //   { id: 111, paperType: 'A3', resUrl: 'a3' },\n      //   { id: 112, paperType: 'A4', resUrl: 'a4' },\n      //   { id: 113, paperType: 'A3', resUrl: 'a3' },\n      //   { id: 114, paperType: 'A3', resUrl: 'a3' },\n      //   { id: 115, paperType: 'A4', resUrl: 'a4' },\n      //   { id: 116, paperType: 'A3', resUrl: 'a3' },\n      //   { id: 117, paperType: 'A4', resUrl: 'a4' },\n      //   { id: 118, paperType: 'A3', resUrl: 'a3' },\n      //   { id: 119, paperType: 'A4', resUrl: 'a4' },\n      //   { id: 120, paperType: 'A4', resUrl: 'a4' }\n      // ]\n      // studentListArray: [\n      //   { resUrl: 'a3' },\n      //   { resUrl: 'a4' },\n      //   { resUrl: 'a3' },\n      //   { resUrl: 'a3' },\n      //   { resUrl: 'a4' },\n      //   { resUrl: 'a3' },\n      //   { resUrl: 'a4' },\n      //   { resUrl: 'a3' },\n      //   { resUrl: 'a4' },\n      //   { resUrl: 'a4' }\n      // ]\n    };\n  },\n  computed: {},\n  watch: {\n    studentListArray(newArray, oldArray) {\n      this.changeArrayList(newArray);\n      console.log('watch调用studentListArray = ', newArray);\n      console.log('watch调用studentListArray = ', oldArray);\n    }\n  },\n  mounted() {\n    // this.changeArrayList(this.studentListArray);\n    console.log('mounted调用 = ', this.studentListArray);\n  },\n  methods: {\n    changeArrayList(array) {\n      let finishArray = [];\n      let itemIsOK = false; //判断单个组合是否完成\n      let itemArray = [];\n      for (const index in array) {\n        console.log('下一次循环开始==', index);\n\n        if (itemIsOK) {\n          itemArray = [];\n        }\n        let item = array[index];\n        if (item.paperType == 'A3') {\n          if (!itemIsOK && finishArray.length > 0) {\n            itemArray.push({});\n            finishArray.push({\n              data: itemArray,\n              // id: parseFloat(index) - 1 + 100\n              id: array[index - 1].id\n            });\n          }\n          itemArray = [item, {}];\n          itemIsOK = true;\n          finishArray.push({\n            data: itemArray,\n            // id: parseFloat(index) + 100\n            id: item.id\n          });\n        } else if (item.paperType == 'A4') {\n          itemArray.push(item);\n          if (itemArray.length == 2) {\n            itemIsOK = true;\n            finishArray.push({\n              data: itemArray,\n              // id: parseFloat(index) + 100\n              id: item.id\n            });\n          } else {\n            itemIsOK = false;\n            if (array.length - 1 == index) {\n              //处理单个数据的情况\n              itemArray.push({});\n              finishArray.push({\n                data: itemArray,\n                // id: parseFloat(index) + 100\n                id: item.id\n              });\n            }\n            console.log('a4中的finishArray==', finishArray);\n          }\n        }\n      }\n      console.log('finishArray==', finishArray);\n      this.failedPictruesArray = finishArray;\n    }\n  }\n};\n</script>\n\n<style></style>\n<style lang=\"less\" scoped>\n.centerDicStyle {\n  max-width: 1200px;\n  height: 944px;\n  border-radius: 2px;\n  margin-top: 9px;\n}\n.failsTimesStyle {\n  //   margin-top: 24px;\n  color: #484848;\n  font-size: 14px;\n  //   height: 14px;\n  //   line-height: 14px;\n  //   margin-left: 24px;\n}\n.paddingStyle {\n  padding-top: 24px;\n  padding-left: 24px;\n  padding-right: 24px;\n  background-color: white;\n  height: 944px;\n  max-width: 1200px;\n}\n.failedHomeworkPictureStyle {\n  margin-top: 20px;\n  width: 1152px;\n  height: 862px;\n  display: flex;\n  flex-wrap: wrap;\n  background-color: white;\n  justify-content: space-between;\n  // 滑动\n  overflow-x: hidden;\n  overflow-y: auto;\n}\n.failedPictureStyle {\n  //   margin-right: 24px;\n  margin-bottom: 24px;\n  width: 564px; //282*2\n  height: 398px;\n  background-color: white;\n  display: flex;\n  flex-direction: row;\n}\n.a3pictureStyle {\n  width: 564px;\n  height: 398px;\n}\n.a4pictureStyle {\n  width: 282px;\n  height: 398px;\n}\n</style>\n"]}]}
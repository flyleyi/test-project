ons=\"statuList\"\n        @change=\"changeSelect\"\n      />\n      <van-dropdown-item\n        v-model=\"handler\"\n        :options=\"handlerList\"\n        @change=\"changeSelect\"\n      />\n    </van-dropdown-menu>\n    <div class=\"tab-content\">\n      <fill-table\n        :tableData=\"handleDataList\"\n        :isEmpty=\"isEmpty\"\n        :identity=\"identity\"\n      ></fill-table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getServeList, getDict } from './../../api/serve.js';\nimport FillTable from '../filler/FillTable.vue';\nexport default {\n  name: 'handler',\n  data() {\n    return {\n      identity: 'handler',\n      activeName: 'check',\n      section: '',\n      sectionList: [{ text: '全部板块', value: '' }],\n      statu: '',\n      statuList: [{ text: '全部状态', value: '' }],\n      handler: '',\n      handlerList: [{ text: '全部下单人', value: '' }],\n      handleDataList: [],\n      defaultStatus: [2, 3, 4],\n      isEmpty: false\n    };\n  },\n  created() {\n    this.getDict();\n    this.getTableData();\n  },\n  methods: {\n    changeSelect() {\n      this.getTableData(1);\n    },\n    getTableData(current = 1, status = null) {\n      console.log('tableData', current);\n      let params = {\n        offset: current,\n        pageType: 4,\n        limit: 10000,\n        cybkNo: this.section ? this.section : null,\n        serveStatus: !this.statu ? this.defaultStatus : [this.statu],\n        submitter: this.handler ? this.handler : null\n      };\n      getServeList(params)\n        .then(res => {\n          const data = res.data || {};\n          this.handleDataList = data.records || [];\n          this.handleDataTotal = data.total;\n          if (!this.handleDataList.length) {\n            this.isEmpty = true;\n          } else {\n            this.isEmpty = false;\n          }\n          return;\n        })\n        .catch(e => {\n          this.handleDataList = [];\n          this.handleDataTotal = 0;\n          this.isEmpty = true;\n          return;\n        });\n    },\n    getDict() {\n      getDict().then(res => {\n        console.log('dicaRes:', res);\n        const data = res.data || {};\n        this.sectionList = this.objToList(data.plate, this.sectionList) || [];\n        this.statuList = this.objToList(data.serveStatus, this.statuList) || [];\n        this.handlerList =\n          this.objToList(data.submitter, this.handlerList) || [];\n        this.statuList = this.statuList.filter(item => {\n          return (\n            this.defaultStatus.includes(parseInt(item.value)) || !item.value\n          );\n        });\n      });\n    },\n    objToList(obj, list) {\n      let keys = Object.keys(obj);\n      for (let i = 0; i < keys.length; i++) {\n        list.push({\n          text: obj[keys[i]],\n          value: keys[i]\n        });\n      }\n      return list;\n    }\n  },\n  components: {\n    FillTable\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.fill-warpper {\n  position: fixed;\n  width: 100%;\n  height: calc(100% - 50px);\n  overflow-y: auto;\n}\n.tab-content {\n  position: fixed;\n  padding: 0.2rem 0 0 0;\n  top: 44px;\n  background: #f6f6f6;\n  width: 100%;\n  height: calc(100% - 98px);\n  overflow-y: auto;\n  box-sizing: border-box;\n}\n/deep/.van-dropdown-menu__title::after {\n  border-color: transparent transparent #000 #000;\n}\n/deep/.van-tab--active {\n  color: #ff9800;\n}\n/deep/.van-tabs__line {\n  background-color: #ff9800;\n}\n</style>\n"]}]}
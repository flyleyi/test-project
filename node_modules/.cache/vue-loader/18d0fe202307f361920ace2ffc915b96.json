bonus-tool-admin/views/taskDetail","sourcesContent":["<template>\n  <!-- 任务详情单条内容查看 -->\n  <div class=\"app-container\" v-loading=\"loading\">\n    <div class=\"school-info\">\n      <h1>{{ taskName }}</h1>\n      <p>\n        <span>校方：{{ schoolName }}</span>\n        <span\n          >奖金池额度：<b>{{\n            bonus | formatNullValue | formatAmountValue\n          }}</b></span\n        >\n      </p>\n    </div>\n    <div class=\"common-container container-wrap\">\n      <el-button\n        type=\"primary\"\n        size=\"medium\"\n        class=\"btn-download\"\n        @click=\"onDownload()\"\n        >下载</el-button\n      >\n      <div class=\"school-main\">\n        <div class=\"main-left\">\n          <ranking-panel :data=\"tableData\"></ranking-panel>\n        </div>\n        <div class=\"main-right\">\n          <div class=\"main-right-top\">\n            <match-panel :data=\"data\"></match-panel>\n          </div>\n          <div class=\"main-right-bottom\">\n            <salary-panel :data=\"data\"></salary-panel>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Button } from 'element-ui';\nimport RankingPanel from './components/RankingPanel.vue';\nimport SalaryPanel from './components/SalaryPanel.vue';\nimport MatchPanel from './components/MatchPanel.vue';\nimport { taskDetailsOne, downloadCompleteList } from '../../api/taskDetail';\nexport default {\n  name: 'SingleDetail',\n  components: {\n    [Button.name]: Button,\n    RankingPanel,\n    SalaryPanel,\n    MatchPanel\n  },\n  props: {},\n  data() {\n    return {\n      loading: true,\n      // 表格数据\n      tableData: [],\n      // 取数组第一条数据，用来取匹配度和年薪系数\n      data: {}\n    };\n  },\n  computed: {\n    taskName() {\n      // 任务名\n      return this.$route.query.taskName;\n    },\n    schoolName() {\n      // 学校名\n      return this.$route.query.schoolName;\n    },\n    bonus() {\n      // 额度\n      return Number(this.$route.query.bonus);\n    }\n  },\n  watch: {},\n  created() {\n    this.taskDetailsOne();\n  },\n  mounted() {},\n  methods: {\n    /**\n     * 获取单条详情数据\n     */\n    taskDetailsOne() {\n      taskDetailsOne({\n        taskName: this.taskName,\n        schoolName: this.schoolName\n      })\n        .then(({ data }) => {\n          this.tableData = data;\n          console.log(data);\n          this.data = data.length > 0 ? data[0] : {};\n        })\n        .finally(() => {\n          this.loading = false;\n        });\n    },\n    /**\n     * 下载\n     */\n    onDownload() {\n      let url = downloadCompleteList({\n        taskName: this.taskName,\n        schoolName: this.schoolName\n      });\n      window.open(url);\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n@import url('../../assets/less/global/variable.less');\n.school-info {\n  padding: 20px 32px;\n  margin-bottom: 10px;\n  background: #fff;\n  border-radius: 4px;\n  color: @mainColor;\n  font-size: 16px;\n  h1 {\n    width: 269px;\n    height: 33px;\n    font-size: 24px;\n    font-family: 'PingFangSC-Medium, PingFang SC';\n    font-weight: 500;\n    color: @darkColor;\n    line-height: 33px;\n  }\n  p {\n    span {\n      margin-right: 104px;\n    }\n    b {\n      font-size: 28px;\n      font-family: 'PingFangSC-Medium, PingFang SC';\n      font-weight: 500;\n      color: #0076f6;\n      line-height: 40px;\n    }\n  }\n}\n.btn-download {\n  margin-bottom: 14px;\n}\n.container-wrap {\n  height: calc(100vh - 268px);\n  min-height: 552px;\n}\n.school-main {\n  height: calc(100% - 50px);\n  min-height: calc(100% - 50px);\n  display: flex;\n  justify-content: space-between;\n  .main-left {\n    width: 68%;\n  }\n  .main-right {\n    width: 30%;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    .main-right-top {\n    }\n    .main-right-bottom {\n    }\n  }\n}\n</style>\n"]}]}
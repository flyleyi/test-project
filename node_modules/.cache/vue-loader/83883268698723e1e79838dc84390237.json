    :show-file-list=\"false\"\r\n        >\r\n          <el-button type=\"primary\" @click=\"onImport\">导入</el-button>\r\n        </el-upload>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onExport\">导出</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div>\r\n      <el-table border :data=\"tableData\">\r\n        <el-table-column label=\"导入类型\" prop=\"importType\"> </el-table-column>\r\n        <el-table-column label=\"人资成本中心代码\" prop=\"hrCostCenterCode\">\r\n        </el-table-column>\r\n        <el-table-column label=\"人资成本中心\" prop=\"hrCostCenter\">\r\n        </el-table-column>\r\n        <el-table-column label=\"人资成本中心代码\" prop=\"hrCostCenterCode\">\r\n        </el-table-column>\r\n        <el-table-column label=\"人员类型\" prop=\"staffType\"> </el-table-column>\r\n        <el-table-column label=\"人数\" prop=\"staffCount\"> </el-table-column>\r\n        <el-table-column label=\"金额/元\" prop=\"costAmount\"> </el-table-column>\r\n        <el-table-column label=\"日期\" prop=\"dateRange\"> </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <!-- <el-button @click=\"addItem()\" type=\"text\" size=\"small\"\r\n              >新增</el-button\r\n            > -->\r\n            <el-button @click=\"editItem(scope.row)\" type=\"text\" size=\"small\"\r\n              >编辑</el-button\r\n            >\r\n            <el-button\r\n              @click=\"confirmDialog(scope.row)\"\r\n              type=\"text\"\r\n              size=\"small\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作时间\" prop=\"updateTime\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            {{ formatTime(scope.row.updateTime) }}\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <div>\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-sizes=\"[10, 50, 100]\"\r\n        :page-size=\"currentSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n    <el-dialog\r\n      :title=\"dialogTitle\"\r\n      center\r\n      @close=\"dialogCancel\"\r\n      :visible.sync=\"dialogFormVisible\"\r\n    >\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"roleForm\">\r\n        <el-form-item label=\"导入类型\" prop=\"importType\" label-width=\"160px\">\r\n          <el-input\r\n            v-model=\"form.importType\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"人资成本中心代码\"\r\n          prop=\"hrCostCenterCode\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.hrCostCenterCode\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"人资成本中心\"\r\n          prop=\"hrCostCenter\"\r\n          label-width=\"160px\"\r\n        >\r\n          <el-input\r\n            v-model=\"form.hrCostCenter\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"人员类别\" prop=\"staffType\" label-width=\"160px\">\r\n          <el-input\r\n            v-model=\"form.staffType\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"人数\" prop=\"staffCount\" label-width=\"160px\">\r\n          <el-input\r\n            v-model.number=\"form.staffCount\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"金额/元\" prop=\"costAmount\" label-width=\"160px\">\r\n          <el-input\r\n            v-model=\"form.costAmount\"\r\n            type=\"number\"\r\n            maxlength=\"24\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"日期\" prop=\"dateRange\" label-width=\"160px\">\r\n          <el-date-picker\r\n            v-model=\"form.dateRange\"\r\n            type=\"month\"\r\n            format=\"yyyyMM\"\r\n            value-format=\"yyyyMM\"\r\n            :picker-options=\"pickerOptions\"\r\n            placeholder=\"选择日期\"\r\n          >\r\n          </el-date-picker>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogCancel\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogConfirm('roleForm')\"\r\n          >确 定</el-button\r\n        >\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getImportTypeList,\r\n  getHrDataCenterList,\r\n  getHrDataList,\r\n  getHrDataItem,\r\n  saveHrDataItem,\r\n  deleteHrDataItem,\r\n  exportHrData\r\n} from './../../api/data.js';\r\nimport { getToken } from '@/plugins/core/auth';\r\nimport laborcostMixin from '../../assets/mixins/laborcostMixin';\r\nconst checkNumber = (rule, value, callback) => {\r\n  const reg = /^(-?\\d+)(\\.\\d{1,2})?$/;\r\n  if (value && !reg.test(value)) {\r\n    callback(new Error('金额最多两位小数'));\r\n  }\r\n};\r\nexport default {\r\n  name: 'humanResource',\r\n  mixins: [laborcostMixin],\r\n  data() {\r\n    return {\r\n      formInline: {\r\n        importType: '',\r\n        dateRange: '',\r\n        hummanResource: '',\r\n        operaDate: ''\r\n      },\r\n      form: {\r\n        id: null,\r\n        importType: null,\r\n        hrCostCenterCode: null,\r\n        hrCostCenter: null,\r\n        staffType: null,\r\n        staffCount: null,\r\n        costAmount: null,\r\n        dateRange: null\r\n      },\r\n      dialogTitle: '新增',\r\n      currentPage: 1,\r\n      currentSize: 10,\r\n      token: getToken(),\r\n      total: 0,\r\n      tableData: [],\r\n      importTypeList: [],\r\n      hummanResourceList: [],\r\n      dialogFormVisible: false,\r\n      rules: {\r\n        importType: [\r\n          {\r\n            required: true,\r\n            message: '请输入导入类型',\r\n            trigger: 'blur'\r\n          }\r\n        ],\r\n        hrCostCenterCode: [\r\n          { required: true, message: '请输入财务成本中心代码', trigger: 'blur' }\r\n        ],\r\n        hrCostCenter: [\r\n          { required: true, message: '请输入财务成本中心', trigger: 'blur' }\r\n        ],\r\n        costAmount: [{ validator: checkNumber, trigger: 'blur' }],\r\n        dateRange: [\r\n          { required: true, message: '请输入财务成本中心', trigger: 'blur' }\r\n        ]\r\n      },\r\n      pickerOptions: {\r\n        disabledDate(time) {\r\n          return time.getTime() > Date.now();\r\n        }\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getImportTypeList();\r\n    this.getHrDataList();\r\n  },\r\n  methods: {\r\n    changeImportType() {},\r\n    getImportTypeList() {\r\n      getImportTypeList().then(res => {\r\n        this.importTypeList = res.data;\r\n      });\r\n    },\r\n    handleSizeChange(val) {\r\n      this.currentPage = 1;\r\n      this.currentSize = val;\r\n      this.getHrDataList();\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n      this.getHrDataList();\r\n    },\r\n    getHrDataCenterList() {\r\n      getHrDataCenterList().then(res => {\r\n        this.hummanResourceList = res.data;\r\n      });\r\n    },\r\n    addItem() {\r\n      this.dialogFormVisible = true;\r\n    },\r\n    getHrDataList() {\r\n      const {\r\n        importType,\r\n        dateRange,\r\n        hummanResource,\r\n        operaDate\r\n      } = this.formInline;\r\n      let params = {\r\n        current: this.currentPage,\r\n        size: this.currentSize,\r\n        importType: importType,\r\n        dateRange: dateRange,\r\n        hrCostCenterCode: hummanResource,\r\n        // hrCostCenterCode: null,\r\n        updateTimeStr: operaDate\r\n      };\r\n      getHrDataList(params).then(res => {\r\n        this.tableData = res.data.records || [];\r\n        this.total = res.data.total;\r\n        if (this.currentPage != res.data.current) {\r\n          this.currentPage = res.data.current;\r\n        }\r\n      });\r\n    },\r\n    editItem(row) {\r\n      this.dialogFormVisible = true;\r\n      this.$nextTick(function() {\r\n        this.form = Object.assign({}, row);\r\n      });\r\n    },\r\n    confirmDialog(row) {\r\n      this.$confirm('是否确认删除本行？', '', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        center: true\r\n      })\r\n        .then(() => {\r\n          this.deleteItem(row);\r\n        })\r\n        .catch(() => {});\r\n    },\r\n    dialogConfirm(formName) {\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          this.saveHrDataItem();\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    saveHrDataItem() {\r\n      let params = Object.assign({ id: null }, this.form);\r\n      saveHrDataItem(params)\r\n        .then(res => {\r\n          this.getHrDataList();\r\n          if (this.form.id) {\r\n            this.warning('修改成功', 'success');\r\n          } else {\r\n            this.warning('新增成功', 'success');\r\n          }\r\n          this.dialogFormVisible = false;\r\n        })\r\n        .catch(e => {\r\n          if (e.status && e.status == 511) {\r\n            this.$confirm('该条记录已存在！', '', {\r\n              confirmButtonText: '确定',\r\n              cancelButtonText: '取消',\r\n              center: true\r\n            })\r\n              .then(() => {})\r\n              .catch(() => {\r\n                // cancelFun && cancelFun();\r\n              });\r\n          }\r\n        });\r\n    },\r\n    dialogCancel() {\r\n      this.dialogFormVisible = false;\r\n      this.$refs['roleForm'].resetFields();\r\n    },\r\n    onQuery() {\r\n      this.getHrDataList();\r\n    },\r\n    onImport() {},\r\n    onExport() {\r\n      exportHrData({}).then(res => {\r\n        const content = res;\r\n        this.$util.downLoadBlob(content);\r\n      });\r\n    },\r\n    deleteItem(row) {\r\n      deleteHrDataItem({ id: row.id }).then(res => {\r\n        this.warning('删除成功', 'success');\r\n        this.getHrDataList();\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.human-resource-wrapper {\r\n  padding: 20px;\r\n}\r\n</style>\r\n"]}]}
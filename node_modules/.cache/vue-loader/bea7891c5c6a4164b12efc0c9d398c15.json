{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/executive-serveh5/views/filler/FillDetail.vue?vue&type=style&index=0&id=45769ac6&lang=less&scoped=true&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/executive-serveh5/views/filler/FillDetail.vue","mtime":1627979214302},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/less-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/style-resources-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["FillDetail.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAo4BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"FillDetail.vue","sourceRoot":"src/pages/executive-serveh5/views/filler","sourcesContent":["<template>\n  <div class=\"fill-detail-wrapper\">\n    <div class=\"fill-detail-top\">\n      <div class=\"fill-detail-top-title\">\n        {{ serveDetail.serveName || '' }}\n      </div>\n      <div class=\"fill-detail-top-apply\">\n        <span class=\"fill-detail-top-apply-item\"\n          ><img\n            class=\"fill-detail-top-apply-item-icon\"\n            src=\"../../assets/images/icon_people_white.png\"\n          />{{ serveDetail.userName || '' }}</span\n        >\n        <span class=\"fill-detail-top-apply-item\"\n          ><img\n            class=\"fill-detail-top-apply-item-icon\"\n            src=\"../../assets/images/icon_bankuai_white.png\"\n          />{{ serveDetail.cybkName || '' }}</span\n        >\n        <span class=\"fill-detail-top-apply-item\">\n          <img\n            class=\"fill-detail-top-apply-item-icon\"\n            src=\"../../assets/images/icon_time_white.png\"\n          />{{ serveDetail.createTime || '' }}\n        </span>\n      </div>\n    </div>\n    <div class=\"fill-detail-bottom\">\n      <van-tabs v-model=\"activeName\">\n        <van-tab title=\"服务内容\" name=\"content\">\n          <div class=\"tab-content\">\n            <div class=\"serve-item\">\n              <div class=\"serve-title\">\n                服务需求\n              </div>\n              <div class=\"serve-content  pre-line\">\n                {{ serveDetail.serveDesc || '暂无' }}\n              </div>\n              <div\n                v-if=\"serveDetail.attachments && serveDetail.attachments.length\"\n              >\n                <div class=\"serve-content\">附件：</div>\n                <div\n                  v-for=\"item in serveDetail.attachments\"\n                  :class=\"{ 'file-image': getItemTypeImage(item) }\"\n                  :key=\"item.id\"\n                >\n                  <div class=\"image\" v-if=\"getItemTypeImage(item)\">\n                    <van-image\n                      width=\"1rem\"\n                      height=\"1rem\"\n                      fit=\"cover\"\n                      @click=\"getImg([item.url])\"\n                      :src=\"item.url\"\n                    />\n                  </div>\n                  <div class=\"serve-content\" v-else>\n                    <span class=\"file-name\">{{ item.fileName }}</span>\n                    <span\n                      v-if=\"deviceInfo == 'pc'\"\n                      class=\"file-preview\"\n                      @click=\"previewPc(item)\"\n                      >下载</span\n                    >\n                    <span\n                      v-if=\"deviceInfo == 'iphone'\"\n                      class=\"file-preview\"\n                      @click=\"previewIphone(item)\"\n                      >预览</span\n                    >\n                    <span\n                      v-if=\"deviceInfo == 'android'\"\n                      class=\"file-preview\"\n                      @click=\"previewAndroid(item)\"\n                      >下载</span\n                    >\n                    <!-- <a\n                      v-else\n                      class=\"file-preview\"\n                      :download=\"item.fileName\"\n                      :href=\"item.url\"\n                      >{{ deviceInfo == 'iphone' ? '预览' : '下载' }}</a\n                    > -->\n                  </div>\n                </div>\n              </div>\n              <div\n                v-if=\"identity === 'supervisor' && roles.includes('监督人')\"\n                class=\"serve-del\"\n              >\n                订单如有涉及法律红线问题，可选择\n                <span class=\"serve-del-btn\" @click=\"delServe\">删除订单 ></span>\n              </div>\n              <div\n                class=\"serve-del\"\n                v-if=\"\n                  identity === 'filler' &&\n                    serveDetail.serveStatus != 6 &&\n                    serveDetail.serveStatus != 3 &&\n                    serveDetail.serveStatus != 4\n                \"\n              >\n                如有变动，可选择\n                <span class=\"serve-del-btn\" @click=\"revokeServe\"\n                  >撤销订单 ></span\n                >\n              </div>\n            </div>\n            <div\n              class=\"serve-item\"\n              v-if=\"\n                (status === 'handle' ||\n                  status === 'unevaluate' ||\n                  status === 'evaluate') &&\n                  handlerList.length > 0\n              \"\n            >\n              <div class=\"serve-title\">\n                抢单人\n              </div>\n              <div class=\"serve-content\">\n                <span\n                  v-for=\"item in handlerList\"\n                  :key=\"item.id\"\n                  class=\"serve-handler\"\n                >\n                  <img\n                    class=\"fill-detail-top-apply-item-icon\"\n                    src=\"../../assets/images/icon_people_gray.png\"\n                  />{{ item.userName }}\n                </span>\n              </div>\n            </div>\n            <div\n              class=\"serve-item\"\n              v-if=\"\n                (status === 'handle' &&\n                  (identity === 'filler' ||\n                    identity === 'checker' ||\n                    (identity === 'supervisor' &&\n                      (serveDetail.isMain ||\n                        serveDetail.isGrabSingle ||\n                        serveDetail.userName === userInfo.userName)) ||\n                    roles.includes('监督人') ||\n                    (identity === 'handler' &&\n                      (serveDetail.isMain ||\n                        serveDetail.isGrabSingle ||\n                        serveDetail.userName === userInfo.userName)))) ||\n                  status === 'unevaluate' ||\n                  status === 'evaluate' ||\n                  status === 'revoke'\n              \"\n            >\n              <div class=\"serve-title\">\n                服务记录\n                <span\n                  v-if=\"\n                    (serveDetail.serveStatus == 2 ||\n                      serveDetail.serveStatus == 3) &&\n                      (serveDetail.isMain ||\n                        serveDetail.isGrabSingle ||\n                        identity === 'filler' ||\n                        identity === 'checker')\n                  \"\n                  @click=\"addRecord()\"\n                  class=\"add-record\"\n                  >+添加</span\n                >\n              </div>\n              <div v-if=\"!handlerRecordList.length\" class=\"serve-content\">\n                暂无\n              </div>\n              <div\n                v-for=\"item in handlerRecordList\"\n                class=\"serve-content-record\"\n                :key=\"item.id\"\n              >\n                <div class=\"serve-content\">\n                  <div class=\"serve-record-title\">\n                    <span class=\"serve-record-name\">{{ item.userName }}</span>\n                    <span class=\"serve-record-time\">{{ item.createTime }}</span>\n                  </div>\n                  <div class=\"serve-record-content\">\n                    {{ item.content }}\n                  </div>\n                  <div class=\"serve-record-content-btn\">\n                    <span\n                      v-if=\"\n                        userInfo.userName != item.userName &&\n                          (serveDetail.serveStatus == 2 ||\n                            serveDetail.serveStatus == 3) &&\n                          (serveDetail.isMain ||\n                            serveDetail.isGrabSingle ||\n                            identity === 'filler' ||\n                            identity === 'checker')\n                      \"\n                      @click=\"addRecord(item.id)\"\n                      ><img\n                        class=\"fill-detail-top-apply-item-icon\"\n                        src=\"../../assets/images/icon_m_huifu_32@2x.png\"/></span\n                    ><span\n                      v-if=\"\n                        roles.includes('监督人') ||\n                          item.userName == userInfo.userName\n                      \"\n                      @click=\"deleteRecord(item.id, 1)\"\n                      ><img\n                        class=\"fill-detail-top-apply-item-icon\"\n                        src=\"../../assets/images/icon_m_del_32@2x.png\"\n                    /></span>\n                  </div>\n                </div>\n                <div\n                  class=\"serve-content-reply-wrapper\"\n                  v-if=\"item.children.length\"\n                >\n                  <div\n                    class=\"serve-content-reply\"\n                    v-for=\"it in item.children\"\n                    :key=\"it.id\"\n                  >\n                    <div class=\"serve-record-title\">\n                      <span class=\"serve-record-name\">{{ it.userName }}</span\n                      >回复<span class=\"serve-record-name\"\n                        ><span v-if=\"it.parentId != item.id\">{{\n                          it.parentUserName\n                        }}</span></span\n                      >\n                      <span class=\"serve-record-time\">{{ it.createTime }}</span>\n                    </div>\n                    <div class=\"serve-record-content\">\n                      {{ it.content }}\n                    </div>\n                    <div class=\"serve-record-content-btn\">\n                      <span\n                        v-if=\"\n                          userInfo.userName != it.userName &&\n                            (serveDetail.serveStatus == 2 ||\n                              serveDetail.serveStatus == 3) &&\n                            (serveDetail.isMain ||\n                              serveDetail.isGrabSingle ||\n                              identity === 'filler' ||\n                              identity === 'checker')\n                        \"\n                        @click=\"addRecord(it.id)\"\n                        ><img\n                          class=\"fill-detail-top-apply-item-icon\"\n                          src=\"../../assets/images/icon_m_huifu_32@2x.png\"/></span\n                      ><span\n                        v-if=\"\n                          roles.includes('监督人') ||\n                            it.userName == userInfo.userName\n                        \"\n                        @click=\"deleteRecord(it.id, 2)\"\n                        ><img\n                          class=\"fill-detail-top-apply-item-icon\"\n                          src=\"../../assets/images/icon_m_del_32@2x.png\"\n                      /></span>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"serve-item\" v-if=\"status === 'revoke'\">\n              <div class=\"serve-title\">\n                撤销记录\n              </div>\n              <div class=\"serve-content\">\n                <div class=\"serve-record-title\">\n                  <span class=\"serve-record-name  serve-content\">{{\n                    serveDetail.userName\n                  }}</span>\n                  <span class=\"serve-record-time\">{{\n                    serveDetail.updateTime\n                  }}</span>\n                </div>\n\n                <div class=\"serve-record-content serve-content\">\n                  {{ serveDetail.reason }}\n                </div>\n              </div>\n            </div>\n            <div class=\"serve-item\" v-if=\"status === 'unpass'\">\n              <div class=\"serve-title\">\n                审核记录\n              </div>\n              <div class=\"serve-content\">\n                <div class=\"serve-record-title\">\n                  <span class=\"serve-record-name  serve-content\">{{\n                    serveDetail.reviewer\n                  }}</span>\n                  <span class=\"serve-record-time\">{{\n                    serveDetail.updateTime\n                  }}</span>\n                </div>\n                <div class=\"serve-eaxm-statu\">\n                  审核不通过\n                </div>\n                <div class=\"serve-record-content serve-content\">\n                  {{ serveDetail.reason }}\n                </div>\n              </div>\n            </div>\n            <div class=\"serve-item\" v-if=\"status === 'evaluate'\">\n              <div class=\"serve-title\">\n                评价\n              </div>\n              <div class=\"serve-content\">\n                <div class=\"serve-rate-item\">\n                  <div class=\"serve-rate-item-title\">\n                    服务效率：\n                  </div>\n                  <div class=\"serve-rate-item-content\">\n                    <van-rate\n                      readonly\n                      allow-half\n                      color=\"#ffd21e\"\n                      v-model=\"serveEvaluateObj.finishScore\"\n                    />\n                  </div>\n                </div>\n                <div class=\"serve-rate-item\">\n                  <div class=\"serve-rate-item-title\">\n                    服务满意度：\n                  </div>\n                  <div class=\"serve-rate-item-content\">\n                    <van-rate\n                      readonly\n                      allow-half\n                      color=\"#ffd21e\"\n                      v-model=\"serveEvaluateObj.satisfiedScore\"\n                    />\n                  </div>\n                </div>\n                <div class=\"serve-rate-item\">\n                  <div class=\"serve-rate-item-title\">\n                    评语：\n                  </div>\n                  <div class=\"serve-rate-item-content\">\n                    {{ serveEvaluateObj.content || '暂无' }}\n                  </div>\n                </div>\n                <div class=\"serve-rate-tag\">\n                  <span\n                    v-for=\"item in serveEvaluateTag\"\n                    :key=\"item\"\n                    class=\"serve-rate-tag-item\"\n                    >{{ item }}</span\n                  >\n                </div>\n              </div>\n            </div>\n            <div class=\"btn-wrapper\">\n              <div v-if=\"identity === 'filler' && status === 'unevaluate'\">\n                <span class=\"btn-one serve-btn\" @click=\"evaluateServe\"\n                  >评价</span\n                >\n              </div>\n              <div v-if=\"identity === 'filler' && status === 'unpass'\">\n                <span class=\"btn-one serve-btn\" @click=\"modifyServe\"\n                  >重新修改提交</span\n                >\n              </div>\n              <div v-if=\"identity === 'checker' && status === 'uncheck'\">\n                <span class=\"btn-one serve-btn\" @click=\"checkServe\">审核</span>\n              </div>\n              <div\n                v-if=\"\n                  identity === 'supervisor' &&\n                    roles.includes('处理人') &&\n                    userInfo.userName != serveDetail.userName &&\n                    !serveDetail.isMain &&\n                    !serveDetail.isGrabSingle &&\n                    serveDetail.userCount < 3 &&\n                    status == 'handle'\n                \"\n              >\n                <span class=\"btn-one serve-btn\" @click=\"snagServe\">抢单</span>\n              </div>\n              <div v-if=\"identity === 'filler' && status === 'uncheck'\">\n                <span class=\"btn-one serve-btn btn-modify\" @click=\"modifyServe\"\n                  >修改</span\n                >\n              </div>\n              <div\n                v-if=\"\n                  identity === 'handler' &&\n                    status === 'handle' &&\n                    serveDetail.isMain\n                \"\n              >\n                <span class=\"btn-finish serve-btn\" @click=\"confirmFinish\"\n                  >完成订单</span\n                >\n                <span class=\"btn-quit serve-btn\" @click=\"quitServe\">{{\n                  serveDetail.userCount == '1' ? '退回' : '退出'\n                }}</span>\n              </div>\n              <div>\n                <span\n                  v-if=\"\n                    identity === 'handler' &&\n                      status === 'handle' &&\n                      !serveDetail.isMain &&\n                      serveDetail.isGrabSingle\n                  \"\n                  class=\"btn-one serve-btn btn-modify\"\n                  @click=\"quitServe\"\n                  >退出</span\n                >\n              </div>\n            </div>\n          </div>\n        </van-tab>\n        <van-tab title=\"服务状态\" name=\"status\">\n          <div class=\"tab-content\">\n            <div class=\"status-wrapper\">\n              <div\n                class=\"status-box\"\n                v-for=\"(item, index) in serveStatusList\"\n                :key=\"item.id\"\n              >\n                <div class=\"status-title\">\n                  <div class=\"status-circle-box\">\n                    <span\n                      class=\"status-circle-default\"\n                      :class=\"{ 'status-circle-active': item.isFinish }\"\n                    >\n                      <span class=\"status-circle-outer\">\n                        <span class=\"status-circle-inner\"></span>\n                      </span>\n                    </span>\n                  </div>\n                  <span\n                    class=\"status-item-title\"\n                    :style=\"{ color: statusCurrent === index ? '#FF9800' : '' }\"\n                  >\n                    {{ item.serveNode }}</span\n                  >\n                </div>\n                <div class=\"status-content\">\n                  <div\n                    class=\"status-line-box\"\n                    v-if=\"index != serveStatusList.length - 1\"\n                  >\n                    <div class=\"status-line\"></div>\n                  </div>\n                  <div\n                    class=\"status-content-item\"\n                    v-for=\"it in item.childNode\"\n                    :key=\"it.id\"\n                  >\n                    <div\n                      v-if=\"index != serveStatusList.length - 1\"\n                      class=\"status-content-dot-box\"\n                    >\n                      <div class=\"status-content-dot\"></div>\n                    </div>\n                    {{ it.serveNode }}\n                    <span class=\"status-content-item-time\">{{\n                      formatDate(it.createTime)\n                    }}</span>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </van-tab>\n      </van-tabs>\n    </div>\n  </div>\n</template>\n<script>\n/*\n* @status{'uncheck': 待审核 ，'unpass': 审核未通过 ，'unhandle': 待处理，\n'handle': 处理，'revoke'：已撤销, 'unevaluate': 待评价，\n'evaluate': 已评价,}\n*/\nimport {\n  handleServe,\n  getServeDetail,\n  addRecord,\n  getHandlerList,\n  getServeStatus,\n  getRecordList,\n  getEvaluateList,\n  downloadFile,\n  deleteRecord\n} from './../../api/serve.js';\nimport config from './../../config';\nimport { ImagePreview } from 'vant';\nimport * as dd from 'dingtalk-jsapi';\nexport default {\n  name: 'fillDetail',\n  data() {\n    return {\n      identity: '',\n      status: '',\n      activeName: 'content',\n      handlerList: [],\n      handlerRecordList: [],\n      serveEvaluateObj: {},\n      userInfo: {},\n      roles: [],\n      serveStatusList: [],\n      serveDetail: {},\n      serveEvaluateTag: [],\n      disableBtn: false,\n      redirect: false,\n      deviceInfo: this.getType() //设备信息\n    };\n  },\n  created() {\n    const { params, query } = this.$route;\n    const userInfo = JSON.parse(localStorage.getItem('userInfo')) || {};\n    this.identity = query.identity;\n    const redirect = query.redirect;\n    if (redirect == 1) {\n      this.redirect = true;\n    }\n    if (params.id) {\n      this.serveId = params.id;\n      this.getServeDetail(this.serveId);\n    }\n    this.userInfo = userInfo;\n    this.roles = userInfo.roles || [];\n    this.getServeStatusList();\n  },\n  // beforeRouteLeave(to, from, next) {\n  //   if (this.redirect) {\n  //     this.redirect = 0;\n  //     next({ path: this.toPath() });\n  //   }\n  //   next();\n  //   // next();\n  // },\n  methods: {\n    getType() {\n      let type;\n      if (/(iPhone|iPad|iPod|iOS)/i.test(navigator.userAgent)) {\n        type = 'iphone';\n      } else if (/(Android)/i.test(navigator.userAgent)) {\n        type = 'android';\n      } else {\n        type = 'pc';\n      }\n      return type;\n    },\n    toPath() {\n      let path = '';\n      switch (this.identity) {\n        case 'filler':\n          path = '/fill';\n          break;\n        case 'checker':\n          path = '/check';\n          break;\n        case 'supervisor':\n          path = '/supervise';\n          break;\n        case 'handler':\n          path = '/handle';\n          break;\n        default:\n          break;\n      }\n      return path;\n    },\n    getServeDetail(id) {\n      getServeDetail(id).then(res => {\n        console.log('detail:', res);\n        this.serveDetail = (res && res.data) || {};\n        this.getServeStatus(this.serveDetail.serveStatus || '');\n        console.log('status', this.status);\n        if (this.serveDetail.userCount > 0) {\n          this.getHandlerList();\n        }\n        if (\n          this.serveDetail.serveStatus != 1 ||\n          this.serveDetail.serveStatus != 5\n        ) {\n          this.getRecordList();\n        }\n        if (this.serveDetail.serveStatus == 4) {\n          this.getEvaluateList();\n        }\n      });\n    },\n    getServeStatus(code) {\n      switch (code) {\n        case 1:\n          this.status = 'uncheck';\n          break;\n        case 2:\n          this.status = 'handle';\n          break;\n        case 3:\n          this.status = 'unevaluate';\n          break;\n        case 4:\n          this.status = 'evaluate';\n          break;\n        case 5:\n          this.status = 'unpass';\n          break;\n        case 6:\n          this.status = 'revoke';\n          break;\n        default:\n          break;\n      }\n    },\n    getServeStatusList() {\n      getServeStatus(this.serveId).then(res => {\n        this.serveStatusList = res.data || [];\n        let statusCurrent = this.serveStatusList.findIndex(\n          item => item.isFinish != 1\n        );\n\n        this.statusCurrent =\n          statusCurrent == -1\n            ? this.serveStatusList.length - 1\n            : statusCurrent - 1;\n      });\n    },\n    getHandlerList() {\n      getHandlerList(this.serveId).then(res => {\n        this.handlerList = res.data || [];\n      });\n    },\n    addRecord(recordId) {\n      var query = {\n        serveId: this.serveId\n      };\n      if (recordId) {\n        query.recordId = recordId;\n        this.$router.push({\n          path: '/fill/addReply',\n          query: query\n        });\n      } else {\n        this.$router.push({\n          path: '/fill/addRecord',\n          query: query\n        });\n      }\n    },\n    quitServe() {\n      //处理类型(1提交完成服务2退出服务(其余处理人)3退回服务(唯一主处理人))\n      if (this.serveDetail.isMain && this.serveDetail.userCount == '1') {\n        this.$router.push({\n          path: '/fill/quit',\n          query: {\n            serveId: this.serveId\n          }\n        });\n      } else {\n        this.$router.push({\n          path: '/fill/back',\n          query: {\n            serveId: this.serveId\n          }\n        });\n      }\n    },\n    deleteRecord(recordId, type) {\n      var query = {\n        serveId: this.serveId\n      };\n      if (recordId) {\n        query.recordId = recordId;\n      }\n      if (type == 1) {\n        this.$router.push({\n          path: '/fill/deleteRecord',\n          query: query\n        });\n      } else {\n        this.$router.push({\n          path: '/fill/deleteReply',\n          query: query\n        });\n      }\n    },\n    getRecordList() {\n      getRecordList(this.serveId).then(res => {\n        const list = res.data || [];\n        this.handlerRecordList = this.handlerRecord(list);\n      });\n    },\n    handlerRecord(list) {\n      let result = [];\n      for (let i = 0; i < list.length; i++) {\n        const item = list[i];\n        let arr = [];\n        let children = this.getChildRecord(list[i].children);\n        children.sort((a, b) => {\n          return (\n            new Date(a.createTime.replace(/-/g, '/')).getTime() -\n            new Date(b.createTime.replace(/-/g, '/')).getTime()\n          );\n        });\n        item.children = children;\n        result.push(item);\n      }\n      return result;\n    },\n    getChildRecord(children, result = []) {\n      for (let i = 0; i < children.length; i++) {\n        result.push(children[i]);\n        if (children[i].children) {\n          this.getChildRecord(children[i].children, result);\n        }\n      }\n      return result;\n    },\n    checkServe() {\n      this.$router.push({\n        path: '/fill/exam',\n        query: {\n          serveId: this.serveId\n        }\n      });\n    },\n    snagServe() {\n      //抢单\n      if (this.disableBtn) {\n        return;\n      }\n      let params = {\n        serveId: this.serveId,\n        processType: 2\n      };\n      this.disableBtn = true;\n      handleServe(params)\n        .then(res => {\n          this.disableBtn = false;\n          this.$toast('恭喜抢单成功，您可以在【我的抢单】中找到已抢到的订单!');\n          this.$router.push({\n            path: '/supervise'\n          });\n        })\n        .catch(e => {\n          this.disableBtn = false;\n        });\n    },\n    modifyServe() {\n      //修改服务\n      this.$router.push({\n        path: '/fill/apply',\n        query: {\n          serveId: this.serveId\n        }\n      });\n    },\n    confirmFinish() {\n      this.$dialog\n        .confirm({\n          title: '完成订单',\n          message: '确认已经得到有效解决落实？？'\n        })\n        .then(() => {\n          this.finishServe(1);\n        })\n        .catch(() => {\n          // on cancel\n        });\n    },\n    finishServe(handleType) {\n      //处理类型(1提交完成服务2退出服务(非主处理人)3退回服务(主处理人))\n      let params = {\n        serveId: this.serveId,\n        processType: 3,\n        handleType: handleType,\n        content: ''\n      };\n      this.disableBtn = true;\n      handleServe(params)\n        .then(res => {\n          this.disableBtn = false;\n          if (handleType == 1) {\n            this.$toast('订单完成');\n          }\n          this.$router.push({\n            path: '/handle'\n          });\n        })\n        .catch(e => {\n          this.disableBtn = false;\n        });\n    },\n    getEvaluateList() {\n      getEvaluateList(this.serveId).then(res => {\n        const data = res.data || {};\n        this.serveEvaluateObj = data;\n        this.serveEvaluateTag =\n          data.evaluateLabel && data.evaluateLabel.split(',');\n      });\n    },\n    delServe() {\n      this.$router.push({\n        path: '/fill/delete',\n        query: {\n          serveId: this.serveId\n        }\n      });\n      console.log('del');\n    },\n    revokeServe() {\n      this.$router.push({\n        path: '/fill/revoke',\n        query: {\n          serveId: this.serveId\n        }\n      });\n      console.log('revoke');\n    },\n    evaluateServe() {\n      this.$router.push({\n        path: '/fill/evaluate',\n        query: {\n          serveId: this.serveId\n        }\n      });\n    },\n    getItemTypeImage(item) {\n      const name = item.fileName || item.name;\n      const imageList = ['jpg', 'png', 'gif', 'jpeg'];\n      const type = this.getFileType(name);\n      return imageList.includes(type.toLowerCase());\n    },\n    getFileType(name) {\n      if (!name) return 'png';\n      return name.slice(name.lastIndexOf('.') + 1);\n    },\n    getImg(listData) {\n      ImagePreview({\n        images: listData,\n        showIndex: false,\n        loop: false,\n        startPosition: 0\n      });\n    },\n    previewPc(item) {\n      dd.biz.util.downloadFile({\n        url: item.url, //要下载的文件的url\n        name: item.fileName, //定义下载文件名字\n        onProgress: function(msg) {\n          // 文件下载进度回调\n        },\n        onSuccess: function(result) {\n          /*\n              true\n            */\n        },\n        onFail: function() {}\n      });\n    },\n    previewIphone(item) {\n      // dd.biz.util.downloadFile({\n      //   url: item.url, //要下载的文件的url\n      //   name: item.fileName, //定义下载文件名字\n      //   onProgress: function(msg) {\n      //     // 文件下载进度回调\n      //   },\n      //   onSuccess: function(result) {\n      //     /*\n      //         true\n      //       */\n      //   },\n      //   onFail: function() {}\n      // });\n\n      dd.biz.util.openLink({\n        // url: item.url, //要打开链接的地址\n        // url: item.url,\n        url: `${config.OPEN_FILE_PREFIX}/serve/serve/file/download/${item.id}`,\n        onSuccess: function(result) {\n          /**/\n        },\n        onFail: function(err) {}\n      });\n    },\n    previewAndroid(item) {\n      dd.biz.util.openLink({\n        url: `${config.OPEN_FILE_PREFIX}/serve/serve/file/download/${item.id}`, //要打开链接的地址\n        // url: `http://172.17.15.42:8505/serve/serve/file/download/${item.id}`,\n        onSuccess: function(result) {\n          /**/\n        },\n        onFail: function(err) {}\n      });\n    },\n    formatDate(date) {\n      return date.replace(/-/g, '/');\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.fill-detail-top {\n  height: 1.8rem;\n  background: linear-gradient(270deg, #3eb0e1 0%, #5e8df5 100%);\n  color: #fff;\n  box-sizing: border-box;\n  padding: 0.3rem;\n  &-title {\n    font-size: 0.34rem;\n    margin-top: 0.15rem;\n    line-height: 0.5rem;\n  }\n  &-apply {\n    line-height: 0.5rem;\n    margin-top: 0.1rem;\n    &-item {\n      margin-right: 0.2rem;\n      color: rgba(255, 255, 255, 0.8);\n      &-icon {\n        position: relative;\n        wid
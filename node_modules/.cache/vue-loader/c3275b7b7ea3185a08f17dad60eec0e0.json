{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/catering-h5/views/ManualLogin.vue?vue&type=script&lang=js&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/catering-h5/views/ManualLogin.vue","mtime":1645176309703},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ManualLogin.vue"],"names":[],"mappings":";;;;;;;;;;;;;AAaA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ManualLogin.vue","sourceRoot":"src/pages/catering-h5/views","sourcesContent":["<template>\n  <div>\n    <input v-model=\"staffCode\" type=\"text\" placeholder=\"工号\" />\n    <button type=\"button\" @click=\"goTo\">登录</button>\n    <p>\n      <button v-if=\"$store.state.token\" type=\"button\" @click=\"logout\">\n        清除当前登录状态\n      </button>\n    </p>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'manual-login',\n  props: {\n    /**\n     * 登录成功重定向地址\n     */\n    redirect: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      staffCode: ''\n    };\n  },\n  created() {\n    console.log(this.redirect);\n  },\n  methods: {\n    goTo() {\n      if (this.staffCode === '') return this.$toast('请求输入工号');\n      // 显示登录中\n      const loading = this.$toast.loading({\n        message: '登录中...',\n        forbidClick: true,\n        loadingType: 'spinner'\n      });\n      this.$store\n        .dispatch('user/manualLogin', {\n          staffCode: this.staffCode\n        })\n        .then(() => {\n          //不同权限跳转不同页面，多重身份初始化先选择身份\n          const roleType = this.$store.getters['user/roleType'];\n          console.log(this.$store.getters['user/rolePath']);\n          console.log('roleType', roleType, this.$store.getters);\n          if (!roleType.length) {\n            this.$dialog.alert({\n              message: '暂无权限'\n            });\n          }\n          if (roleType.length > 1 && !this.redirect) {\n            this.$router.push({\n              path: '/choose-identity'\n            });\n          } else {\n            this.$router.push({\n              path: this.$store.getters['user/rolePath'][roleType[0]]\n            });\n          }\n        })\n        .finally(() => {\n          loading.clear();\n        });\n    },\n    /**\n     * 退出当前登录\n     */\n    logout() {\n      this.$store.dispatch('logout').then(() => {\n        this.$toast('成功清除当前登录状态');\n      });\n    }\n  }\n};\n</script>\n"]}]}
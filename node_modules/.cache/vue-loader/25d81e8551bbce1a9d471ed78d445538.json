column|ranking\" slot-scope=\"{ row }\">\n      <span :class=\"{ red: row.warning == 1 ? true : false }\">{{\n        row.realizedIncomeRanking\n      }}</span>\n    </template>\n  </c-table>\n</template>\n\n<script>\nimport CTable from '@/components/common/CTable';\nexport default {\n  name: 'RankingPanel',\n  components: { CTable },\n  props: {\n    /**\n     * 获取表格数据\n     */\n    data: {\n      type: Array\n    }\n  },\n  data() {\n    function formatterNum(value) {\n      if (typeof value === 'number' && !isNaN(value)) {\n        return value.toLocaleString();\n      } else {\n        return '-';\n      }\n    }\n    return {\n      headers: [\n        {\n          label: '绩效排名',\n          prop: 'performanceRanking',\n          width: 50\n        },\n        {\n          label: '姓名',\n          prop: 'teacherName'\n        },\n        {\n          label: '工号',\n          prop: 'teacherNum',\n          width: 90\n        },\n        {\n          label: '年薪',\n          prop: 'salary',\n          formatter: row => {\n            return formatterNum(row.salary);\n          }\n        },\n        {\n          label: '已发年薪内收入',\n          prop: 'plannedIncome',\n          formatter: row => {\n            return formatterNum(row.plannedIncome);\n          }\n        },\n        {\n          label: '已发年薪外收入',\n          prop: 'unplannedIncome',\n          formatter: row => {\n            return formatterNum(row.unplannedIncome);\n          }\n        },\n        {\n          label: '分配奖金',\n          prop: 'bonus',\n          formatter: row => {\n            return formatterNum(row.bonus);\n          }\n        },\n        {\n          label: '实际年薪内收入(已发年薪内+奖金)',\n          width: 140,\n          prop: 'realizedIncome',\n          formatter: row => {\n            return formatterNum(row.realizedIncome);\n          }\n        },\n        {\n          label: '实际年薪内收入排名',\n          prop: 'realizedIncomeRanking',\n          slotName: 'ranking',\n          colType: 'column',\n          width: 100\n        },\n        {\n          label: '备注',\n          prop: 'note',\n          'show-overflow-tooltip': true\n        }\n      ]\n    };\n  },\n  computed: {\n    cHeight() {\n      let h =\n        document.documentElement.clientHeight || document.body.clientHeight; // 窗口高度\n      let minH = 552 + 268; // 外层最小高度school-main + 头部底部高度\n      return h > minH ? h : minH;\n    }\n  },\n  watch: {},\n  created() {\n    console.log();\n  },\n  mounted() {},\n  methods: {}\n};\n</script>\n\n<style lang=\"less\" scoped>\n.m-table {\n  height: 100%;\n  /deep/ .el-table {\n    height: 100%;\n    max-height: 500px;\n    overflow-x: auto;\n  }\n}\n.red {\n  color: #ff442a;\n}\n</style>\n"]}]}
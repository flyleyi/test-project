g src=\"../../../assets/images/no-data.png\" alt=\"\" />\r\n      <p>暂无数据</p>\r\n    </div>\r\n    <div v-else>\r\n      <dd\r\n        class=\"lists\"\r\n        @click=\"details(item.id)\"\r\n        v-for=\"item in teacherTableData\"\r\n        :key=\"item.id\"\r\n      >\r\n        <p v-if=\"active === '课题'\">\r\n          <span>{{ item.topicThesisName }}</span>\r\n          <span>{{ item.leaderAuthorName }}</span>\r\n          <span :class=\"computedClassName(item.statusName)\">{{\r\n            item.statusName\r\n          }}</span>\r\n        </p>\r\n        <p v-else>\r\n          <span>{{ item.topicThesisName }}</span>\r\n          <span>{{ item.leaderAuthorName }}</span>\r\n          <span>{{ item.topicThesisLeveName }}</span>\r\n        </p>\r\n      </dd>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CourseResearchList',\r\n  props: {\r\n    active: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    teacherTableData: {\r\n      type: Array,\r\n      default: () => []\r\n    },\r\n    finished: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  methods: {\r\n    computedClassName(name) {\r\n      let className;\r\n      switch (name) {\r\n        case '结题':\r\n          className = 'red';\r\n          break;\r\n        case '开题':\r\n          className = 'green';\r\n          break;\r\n        case '结题延期':\r\n          className = 'blue';\r\n          break;\r\n        default:\r\n          className = '';\r\n          break;\r\n      }\r\n      return className;\r\n    },\r\n    /**\r\n     * 点击 查看详情\r\n     */\r\n    details(id) {\r\n      let name;\r\n      if (this.active === '课题') {\r\n        name = 'courseResearchTask';\r\n      } else if (this.active === '论文') {\r\n        name = 'courseResearchThesis';\r\n      }\r\n      this.$router.push({ name, query: { id } });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.no-data-list {\r\n  text-align: center;\r\n  p {\r\n    color: rgba(51, 51, 51, 0.45);\r\n    font-size: 0.28rem;\r\n  }\r\n  img {\r\n    width: 4rem;\r\n  }\r\n}\r\n.lists {\r\n  padding: 0.3rem 0;\r\n  border-bottom: 0.02rem solid rgba(25, 31, 37, 0.08);\r\n  > p {\r\n    display: flex;\r\n    flex-wrap: nowrap;\r\n    align-items: center;\r\n    span {\r\n      display: inline-block;\r\n      width: 22.5%;\r\n      height: 0.4rem;\r\n      overflow: hidden;\r\n      text-overflow: ellipsis;\r\n      white-space: nowrap;\r\n      position: relative;\r\n    }\r\n    span:first-child {\r\n      width: 55%;\r\n    }\r\n    span:last-child {\r\n      text-indent: 0.16rem;\r\n      &::before {\r\n        position: absolute;\r\n        content: '';\r\n        width: 1px;\r\n        height: 1px;\r\n        border-radius: 50%;\r\n        transform: scale(6);\r\n        top: calc(50% - 0.01rem);\r\n        left: 0.06rem;\r\n      }\r\n    }\r\n    .green {\r\n      &::before {\r\n        background: #52c41a;\r\n      }\r\n    }\r\n    .red {\r\n      &::before {\r\n        background: #f25643;\r\n      }\r\n    }\r\n    .blue {\r\n      &::before {\r\n        background: #489ef3;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}
{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/laborcost/views/Finance/index.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/laborcost/views/Finance/index.vue","mtime":1627979214313},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4ZA;AACA;AACA","file":"index.vue","sourceRoot":"src/pages/laborcost/views/Finance","sourcesContent":["<template>\n  <div class=\"finance-wrapper\">\n    <el-form :inline=\"true\" :model=\"formInline\" class=\"demo-form-inline\">\n      <el-form-item label=\"单位\">\n        <el-select\n          v-model=\"formInline.financeCostCenter\"\n          @change=\"changeFinance\"\n          clearable\n          placeholder=\"请选择单位\"\n        >\n          <el-option\n            v-for=\"item in financeList\"\n            :key=\"item\"\n            :label=\"item\"\n            :value=\"item\"\n          ></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"日期\">\n        <el-date-picker\n          v-model=\"formInline.dateRange\"\n          type=\"month\"\n          format=\"yyyyMM\"\n          value-format=\"yyyyMM\"\n          :picker-options=\"pickerOptions\"\n          placeholder=\"选择日期\"\n        >\n        </el-date-picker>\n      </el-form-item>\n      <!-- <el-form-item label=\"营收金额/元\">\n        <el-input\n          v-model=\"formInline.revenueAmount\"\n          placeholder=\"营收金额/元\"\n        ></el-input>\n      </el-form-item> -->\n      <el-form-item label=\"操作时间\">\n        <el-date-picker\n          v-model=\"formInline.operaDate\"\n          type=\"date\"\n          :picker-options=\"pickerOptions\"\n          format=\"yyyy-MM-dd\"\n          value-format=\"yyyy-MM-dd\"\n          placeholder=\"选择日期\"\n        >\n        </el-date-picker>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onQuery\">查询</el-button>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"addItem\">新增</el-button>\n      </el-form-item>\n      <el-form-item>\n        <el-upload\n          class=\"upload-demo\"\n          :action=\"importUrl\"\n          :headers=\"{\n            Authorization: token\n          }\"\n          :on-progress=\"handleProgress\"\n          :on-success=\"handleSuccess\"\n          :on-error=\"handleError\"\n          accept=\".xls,.xlsx\"\n          :show-file-list=\"false\"\n        >\n          <el-button type=\"primary\" @click=\"onImport\">导入</el-button>\n        </el-upload>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"onExport\">导出</el-button>\n      </el-form-item>\n    </el-form>\n    <div>\n      <el-table border :data=\"tableData\">\n        <el-table-column label=\"单位\" prop=\"financeCostCenter\">\n        </el-table-column>\n        <el-table-column label=\"营收金额/元\" prop=\"revenueAmount\">\n        </el-table-column>\n        <el-table-column label=\"所有成本/元\" prop=\"costAmount\">\n        </el-table-column>\n        <el-table-column label=\"人力成本/元\" prop=\"hrCostAmount\">\n        </el-table-column>\n        <el-table-column label=\"净利润/元\" prop=\"netProfitAmount\">\n        </el-table-column>\n        <el-table-column label=\"原始预算/元\" prop=\"originalBudgetAmount\">\n        </el-table-column>\n        <el-table-column label=\"时间\" prop=\"dateRange\"> </el-table-column>\n        <el-table-column label=\"操作\">\n          <template slot-scope=\"scope\">\n            <!-- <el-button @click=\"addItem(scope.row)\" type=\"text\" size=\"small\"\n              >新增</el-button\n            > -->\n            <el-button @click=\"editItem(scope.row)\" type=\"text\" size=\"small\"\n              >编辑</el-button\n            >\n            <el-button\n              @click=\"confirmDialog(scope.row)\"\n              type=\"text\"\n              size=\"small\"\n              >删除</el-button\n            >\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作时间\" prop=\"updateTime\" width=\"180\">\n          <template slot-scope=\"scope\">\n            {{ formatTime(scope.row.updateTime) }}\n          </template>\n        </el-table-column>\n      </el-table>\n    </div>\n    <div>\n      <el-pagination\n        @size-change=\"handleSizeChange\"\n        @current-change=\"handleCurrentChange\"\n        :current-page=\"currentPage\"\n        :page-sizes=\"[10, 50, 100]\"\n        :page-size=\"currentSize\"\n        layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\"\n      >\n      </el-pagination>\n    </div>\n    <el-dialog\n      :title=\"dialogTitle\"\n      @close=\"dialogCancel\"\n      center\n      :visible.sync=\"dialogFormVisible\"\n    >\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"roleForm\">\n        <el-form-item label=\"单位\" prop=\"financeCostCenter\" label-width=\"160px\">\n          <el-input\n            v-model=\"form.financeCostCenter\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"营收金额/元\"\n          prop=\"revenueAmount\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.revenueAmount\"\n            type=\"number\"\n            maxlength=\"24\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"所有成本/元\" prop=\"costAmount\" label-width=\"160px\">\n          <el-input\n            v-model=\"form.costAmount\"\n            maxlength=\"24\"\n            type=\"number\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"人力成本/元\"\n          prop=\"hrCostAmount\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.hrCostAmount\"\n            maxlength=\"24\"\n            type=\"number\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"净利润/元\"\n          prop=\"netProfitAmount\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.netProfitAmount\"\n            maxlength=\"24\"\n            type=\"number\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item\n          label=\"原始预算/元\"\n          prop=\"originalBudgetAmount\"\n          label-width=\"160px\"\n        >\n          <el-input\n            v-model=\"form.originalBudgetAmount\"\n            maxlength=\"24\"\n            type=\"number\"\n            autocomplete=\"off\"\n          ></el-input>\n        </el-form-item>\n        <el-form-item label=\"日期\" prop=\"dateRange\" label-width=\"160px\">\n          <el-date-picker\n            v-model=\"form.dateRange\"\n            :picker-options=\"pickerOptions\"\n            type=\"month\"\n            format=\"yyyyMM\"\n            value-format=\"yyyyMM\"\n            placeholder=\"选择日期\"\n          >\n          </el-date-picker>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogCancel\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"dialogConfirm('roleForm')\"\n          >确 定</el-button\n        >\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport {\n  getFinanceCenterList,\n  getFinanceList,\n  getFinanceItem,\n  saveFinanceItem,\n  deleteFinanceItem,\n  exportFinanceData\n} from './../../api/data.js';\nimport { getToken } from '@/plugins/core/auth';\nimport API_PREFIX from '@/plugins/common/api.js';\nimport laborcostMixin from '../../assets/mixins/laborcostMixin';\nconst checkNumber = (rule, value, callback) => {\n  const reg = /^(-?\\d+)(\\.\\d{1,2})?$/;\n  if (value && !reg.test(value)) {\n    callback(new Error('金额最多两位小数'));\n  }\n  callback();\n};\nexport default {\n  name: 'finance',\n  mixins: [laborcostMixin],\n  data() {\n    return {\n      importUrl: API_PREFIX['laborcost'] + '/financeData/import',\n      formInline: {\n        financeCostCenter: '',\n        revenueAmount: '',\n        operaDate: '',\n        dateRange: ''\n      },\n      form: {\n        financeCostCenter: '',\n        revenueAmount: '',\n        costAmount: '',\n        hrCostAmount: '',\n        netProfitAmount: '',\n        originalBudgetAmount: '',\n        dateRange: ''\n      },\n      dialogTitle: '新增',\n      currentPage: 1,\n      currentSize: 10,\n      token: getToken(),\n      total: 0,\n      importTypeList: [],\n      financeList: [],\n      dialogFormVisible: false,\n      tableData: [],\n      rules: {\n        financeCostCenter: [\n          {\n            required: true,\n            message: '请输入单位',\n            trigger: 'blur'\n          }\n        ],\n        revenueAmount: [{ validator: checkNumber, trigger: 'blur' }],\n        costAmount: [{ validator: checkNumber, trigger: 'blur' }],\n        hrCostAmount: [{ validator: checkNumber, trigger: 'blur' }],\n        netProfitAmount: [{ validator: checkNumber, trigger: 'blur' }],\n        originalBudgetAmount: [{ validator: checkNumber, trigger: 'blur' }],\n        dateRange: [\n          { required: true, message: '请输入财务成本中心', trigger: 'blur' }\n        ]\n      },\n      pickerOptions: {\n        disabledDate(time) {\n          return time.getTime() > Date.now();\n        }\n      }\n    };\n  },\n  created() {\n    this.getFinanceCenterList();\n    this.getFinanceList();\n  },\n  methods: {\n    changeImportType() {},\n    changeFinance() {},\n    handleSizeChange(val) {\n      this.currentPage = 1;\n      this.currentSize = val;\n      this.getFinanceList();\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val;\n      this.getFinanceList();\n    },\n    getFinanceCenterList() {\n      getFinanceCenterList().then(res => {\n        this.financeList = res.data || [];\n      });\n    },\n    addItem() {\n      this.dialogFormVisible = true;\n    },\n    onQuery() {\n      this.getFinanceList();\n    },\n    onImport() {},\n    onExport() {\n      exportFinanceData({}).then(res => {\n        const content = res;\n        this.$util.downLoadBlob(content, '财务导入数据表');\n      });\n    },\n    editItem(row) {\n      this.dialogFormVisible = true;\n      this.$nextTick(function() {\n        this.form = Object.assign({}, row);\n      });\n    },\n    getFinanceList() {\n      const {\n        financeCostCenter,\n        revenueAmount,\n        dateRange,\n        operaDate\n      } = this.formInline;\n      let params = {\n        current: this.currentPage,\n        size: this.currentSize,\n        financeCostCenter: financeCostCenter,\n        revenueAmount: revenueAmount,\n        dateRange: dateRange,\n        // hrCostCenterCode: null,\n        updateTimeStr: operaDate\n      };\n      getFinanceList(params).then(res => {\n        this.tableData = res.data.records || [];\n        this.total = res.data.total;\n        if (this.currentPage != res.data.current) {\n          this.currentPage = res.data.current;\n        }\n      });\n    },\n    confirmDialog(row) {\n      this.$confirm('是否确认删除本行？', '', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        center: true\n      })\n        .then(() => {\n          this.deleteItem(row);\n        })\n        .catch(() => {});\n    },\n    dialogConfirm(formName) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.saveFinanceItem();\n        } else {\n          return false;\n        }\n      });\n    },\n    saveFinanceItem() {\n      let params = Object.assign({ id: null }, this.form);\n      saveFinanceItem(params)\n        .then(res => {\n          this.dialogFormVisible = false;\n          if (this.form.id) {\n            this.warning('修改成功', 'success');\n          } else {\n            this.warning('新增成功', 'success');\n          }\n          this.getFinanceList();\n        })\n        .catch(e => {\n          if (e.status && e.status == 511) {\n            this.$confirm('该条记录已存在！', '', {\n              confirmButtonText: '确定',\n              cancelButtonText: '取消',\n              center: true\n            })\n              .then(() => {})\n              .catch(() => {\n                // cancelFun && cancelFun();\n              });\n          }\n        });\n    },\n    dialogCancel() {\n      this.dialogFormVisible = false;\n      this.$refs['roleForm'].resetFields();\n    },\n    deleteItem(row) {\n      deleteFinanceItem({ id: row.id }).then(res => {\n        this.getFinanceList();\n        this.warning('删除成功', 'success');\n      });\n    }\n  }\n};\n</script>\n\n<style>\n.finance-wrapper {\n  padding: 20px;\n}\n</style>\n"]}]}
y-foundation/views/student/StudentInfo","sourcesContent":["<template>\n  <div class=\"StudentInfo\" v-loading=\"loading\">\n    <el-card class=\"infobox\">\n      <div class=\"infoRow\">\n        <div class=\"info\">\n          <p class=\"label\">姓名:</p>\n          <p class=\"txt\">{{ tableData.name ? tableData.name : '--' }}</p>\n        </div>\n        <div class=\"info\">\n          <p class=\"label\">身份证号:</p>\n          <p class=\"txt\">\n            {{ tableData.cardCode ? tableData.cardCode : '--' }}\n          </p>\n        </div>\n        <div class=\"info\">\n          <p class=\"label\">健康状况:</p>\n          <p class=\"txt\">\n            {{ tableData.healthStatusName ? tableData.healthStatusName : '--' }}\n          </p>\n        </div>\n        <div class=\"info rowInfobox\">\n          <div class=\"info\">\n            <p class=\"label\">性别:</p>\n            <p class=\"txt\">\n              {{ tableData.sexName ? tableData.sexName : '--' }}\n            </p>\n          </div>\n          <div class=\"info\">\n            <p class=\"label\">民族:</p>\n            <p class=\"txt\">\n              {{ tableData.nationName ? tableData.nationName : '--' }}\n            </p>\n          </div>\n          <div class=\"info FamilySituation\">\n            <p class=\"label\">家庭情况:</p>\n            <p\n              class=\"txt\"\n              :title=\"tableData.familyDetail ? tableData.familyDetail : ''\"\n            >\n              {{ tableData.familyDetail ? tableData.familyDetail : '--' }}\n            </p>\n          </div>\n        </div>\n      </div>\n      <div class=\"infoRow\">\n        <div class=\"info\">\n          <p class=\"label\">监护人:</p>\n          <p class=\"txt\">\n            {{ tableData.guardianName ? tableData.guardianName : '--' }}\n          </p>\n        </div>\n        <div class=\"info\">\n          <p class=\"label\">监护人联系方式:</p>\n          <p class=\"txt\">\n            {{ tableData.guardianPhone ? tableData.guardianPhone : '--' }}\n          </p>\n        </div>\n        <div class=\"info\" v-if=\"tableData.projectType !== 23\">\n          <p class=\"label\">爱心妈妈姓名:</p>\n          <p class=\"txt\">\n            {{ tableData.helpMotherName ? tableData.helpMotherName : '--' }}\n          </p>\n        </div>\n        <div class=\"info\" v-if=\"tableData.projectType !== 23\">\n          <p class=\"label\">爱心妈妈联系方式:</p>\n          <p class=\"txt\">\n            {{ tableData.helpMotherPhone ? tableData.helpMotherPhone : '--' }}\n          </p>\n        </div>\n      </div>\n      <div class=\"infoRow\" v-if=\"tableData.projectType !== 23\">\n        <div class=\"info\">\n          <p class=\"label\">进入幸福院时间:</p>\n          <p class=\"txt\">\n            {{ tableData.comeTime | format }}\n          </p>\n        </div>\n        <div class=\"info\" v-if=\"tableData.projectType !== 23\">\n          <p class=\"label\">离开幸福院时间:</p>\n          <p class=\"txt\">\n            {{ tableData.leaveTime | format }}\n          </p>\n        </div>\n      </div>\n    </el-card>\n    <el-card class=\"tab-card\">\n      <div class=\"tabBox\">\n        <div\n          :class=\"actived === 1 ? 'actived' : ''\"\n          @click=\"actived = 1\"\n          v-auth=\"['agreementInformation']\"\n        >\n          协议资料\n        </div>\n        <div\n          :class=\"actived === 2 ? 'actived' : ''\"\n          @click=\"actived = 2\"\n          v-if=\"\n            tableData.studentStatus === '0' || tableData.studentStatus === '1'\n          \"\n          v-auth=\"['studyWork']\"\n        >\n          学习就业\n        </div>\n        <div\n          :class=\"actived === 3 ? 'actived' : ''\"\n          @click=\"actived = 3\"\n          v-auth=\"['feeManager']\"\n        >\n          费用管理\n        </div>\n        <div\n          :class=\"actived === 4 ? 'actived' : ''\"\n          @click=\"actived = 4\"\n          v-auth=\"['growRecord']\"\n        >\n          成长记录\n        </div>\n      </div>\n      <div\n        class=\"GrowthRecord\"\n        v-if=\"actived === 1 && Object.keys(tableData).length > 0\"\n      >\n        <Agreement :studentData=\"tableData\"></Agreement>\n      </div>\n      <div\n        class=\"GrowthRecord\"\n        v-if=\"actived === 2 && Object.keys(tableData).length > 0\"\n      >\n        <study-employment :studentData=\"tableData\"></study-employment>\n      </div>\n      <div\n        class=\"GrowthRecord\"\n        v-if=\"actived === 3 && Object.keys(tableData).length > 0\"\n      >\n        <cost-management :studentData=\"tableData\"></cost-management>\n      </div>\n      <div\n        class=\"GrowthRecord\"\n        v-if=\"actived === 4 && Object.keys(tableData).length > 0\"\n      >\n        <growth-record :studentData=\"tableData\"></growth-record>\n      </div>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport { Card } from 'element-ui';\nimport GrowthRecord from '../components/GrowthRecord/index';\nimport Agreement from '../components/Agreement/index';\nimport StudyEmployment from '../components/StudyEmployment/index';\nimport CostManagement from '../components/CostManagement/index';\nimport { getStudentDetail } from '../../../api/sutdent/index';\nimport { mapGetters } from 'vuex';\nimport utils from '@/plugins/common/utils';\nexport default {\n  name: 'StudentInfo',\n  components: {\n    [Card.name]: Card,\n    GrowthRecord,\n    Agreement,\n    StudyEmployment,\n    CostManagement\n  },\n  data() {\n    return {\n      tableData: {},\n      loading: false,\n      actived: 1\n    };\n  },\n  computed: {\n    ...mapGetters(['authList'])\n  },\n  filters: {\n    format(val) {\n      let healthStatus = val ? utils.dateConverter('yyyy-MM-dd', val) : '--';\n      return healthStatus;\n    }\n  },\n  watch: {\n    '$route.query.id': {\n      handler() {\n        if (this.$route.query.id) {\n          if (this.$has(['agreementInformation'])) {\n            this.actived = 1;\n          } else if (this.$has(['studyWork'])) {\n            this.actived = 2;\n          } else if (this.$has(['feeManager'])) {\n            this.actived = 3;\n          } else {\n            this.actived = 4;\n          }\n          this.initData();\n        }\n      },\n      deep: true,\n      immediate: true\n    }\n  },\n  mounted() {\n    if (this.$has(['agreementInformation'])) {\n      this.actived = 1;\n    } else if (this.$has(['studyWork'])) {\n      this.actived = 2;\n    } else if (this.$has(['feeManager'])) {\n      this.actived = 3;\n    } else {\n      this.actived = 4;\n    }\n    this.initData();\n  },\n  methods: {\n    initData() {\n      this.loading = true;\n      this.tableData = {};\n      getStudentDetail({ id: this.$route.query.id }).then(res => {\n        this.tableData = res.data;\n        this.loading = false;\n      });\n    },\n    /**\n     * 修改新增\n     * @param {Number}} type 1 新增 2修改\n     */\n    editRow(type, item) {\n      item['isedit'] = type === 1 ? false : true;\n      this.$refs.bImportData.show(item);\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.StudentInfo {\n  .infobox /deep/.el-card__body {\n    .infoRow {\n      display: flex;\n      align-items: center;\n      margin-bottom: 20px;\n      > .info {\n        width: 20%;\n        display: flex;\n        .label {\n          color: rgba(0, 0, 0, 0.65);\n          font-weight: 400;\n        }\n        .txt {\n          color: rgba(0, 0, 0, 0.85);\n          font-weight: 400;\n          margin-left: 10px;\n        }\n      }\n      .rowInfobox {\n        display: flex;\n        width: 40%;\n        .info {\n          display: flex;\n          margin-right: 20px;\n          min-width: 75px;\n        }\n        .FamilySituation {\n          flex: 1;\n          .txt {\n            flex: 1;\n            overflow: hidden;\n            overflow: hidden;\n            white-space: nowrap;\n            text-overflow: ellipsis;\n          }\n        }\n      }\n    }\n  }\n  .tab-card {\n    margin-top: 20px;\n    .tabBox {\n      display: flex;\n      align-items: center;\n      > div {\n        margin-right: 20px;\n        width: 80px;\n        height: 32px;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        color: rgba(0, 0, 0, 0.45);\n        font-weight: 400;\n        cursor: pointer;\n      }\n      > div.actived {\n        background: rgba(63, 127, 241, 0.1);\n        border-radius: 2px;\n        font-weight: 600;\n        color: #3f7ff1;\n      }\n    }\n    .GrowthRecord {\n      margin-top: 20px;\n    }\n  }\n}\n</style>\n"]}]}
n              car.joinLicence ? '车辆可进' : '车辆不可进'\n            }}</span>\n          </li>\n        </ul>\n      </div>\n    </div>\n\n    <check-panel\n      :item=\"item\"\n      :type=\"type\"\n      ref=\"checkPanel\"\n      @onRefresh=\"onRefresh\"\n      @onSearchClose=\"onSearchClose\"\n    ></check-panel>\n\n    <sign-in\n      :item=\"item\"\n      :type=\"type\"\n      ref=\"checkPanel\"\n      @onRefresh=\"onRefresh\"\n      @onSearchClose=\"onSearchClose\"\n    ></sign-in>\n\n    <sign-out\n      :item=\"item\"\n      :type=\"type\"\n      ref=\"checkPanel\"\n      @onRefresh=\"onRefresh\"\n      @onSearchClose=\"onSearchClose\"\n    ></sign-out>\n\n    <div class=\"item-bottom\" v-if=\"type == 'outward'\">\n      <div class=\"checklist\" @click.stop=\"gotoOutward(item)\">\n        <div class=\"left\" ref=\"outwardBtn\">\n          <van-icon name=\"orders-o\" />\n          外运物资清单\n        </div>\n        <div class=\"right\">\n          <span>装车核销</span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { formateDate } from '@/pages/visitor-h5/plugins/common/utils';\nimport checkPanel from './CheckPanel.vue';\nimport signIn from './SignIn.vue';\nimport signOut from './SignOut.vue';\n\nexport default {\n  props: ['item', 'type'],\n  components: { checkPanel, signIn, signOut },\n  data() {\n    return {\n      result: [],\n      checked: false\n    };\n  },\n  methods: {\n    formateDate,\n    clickCard(event, item) {\n      this.$router.push({\n        path: '/verify-detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n    gotoOutward(item) {\n      this.$router.push({\n        path: '/verify-outward-list',\n        query: {\n          id: item.id\n        }\n      });\n    },\n    onRefresh() {\n      this.$emit('onRefresh');\n    },\n    onSearchClose() {\n      this.$emit('onSearchClose');\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n/deep/ .van-checkbox__label {\n  word-break
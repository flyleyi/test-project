{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/catering-h5/views/receive-goods/DetailInfo.vue?vue&type=style&index=0&id=19c8ee7b&lang=less&scoped=true&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/catering-h5/views/receive-goods/DetailInfo.vue","mtime":1645178321123},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/less-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/style-resources-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["DetailInfo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwTA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"DetailInfo.vue","sourceRoot":"src/pages/catering-h5/views/receive-goods","sourcesContent":["<template>\n  <div class=\"app-wrapper\" v-if=\"loaded\">\n    <div class=\"buy-info\">\n      <div class=\"title\">采购信息</div>\n      <div class=\"operation-tab\" :class=\"classObj(orderInfo.status)\">\n        {{ statusObj[orderInfo.status + ''] }}\n      </div>\n\n      <div class=\"content\">\n        <div>\n          <div class=\"item-head\">\n            <div class=\"name\">\n              <span>{{ orderInfo.ingredientName }}</span>\n            </div>\n          </div>\n          <ul class=\"item-content\">\n            <li>\n              <span class=\"item-label\">净菜重量(kg):</span>\n              <div class=\"item-info\">{{ orderInfo.cleanVegetableWeight }}</div>\n            </li>\n            <li>\n              <span class=\"item-label\">毛菜重量(kg):</span>\n              <div class=\"item-info\">\n                {{ orderInfo.originalVegetableWeight }}\n              </div>\n            </li>\n            <li>\n              <span class=\"item-label\">总重量(kg):</span>\n              <div class=\"item-info\">{{ orderInfo.quantity }}</div>\n            </li>\n            <li>\n              <span class=\"item-label\">数量({{ orderInfo.packUnit }}):</span>\n              <div class=\"item-info\">\n                {{ orderInfo.packNum }}\n              </div>\n            </li>\n            <li>\n              <span class=\"item-label\">成本参考价(元):</span>\n              <div class=\"item-info\">\n                {{ orderInfo.costPrice }}\n              </div>\n            </li>\n            <li>\n              <span class=\"item-label\">配送时间:</span>\n              <div class=\"item-info\">\n                {{ orderInfo.arrivalTime }}\n              </div>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n    <!-- 待收货 -->\n    <div class=\"receive-info\" v-if=\"status == 2020100\">\n      <div class=\"title\">收货信息</div>\n      <div class=\"content\">\n        <van-form ref=\"form\">\n          <van-field required name=\"radio\" label=\"收货类型\">\n            <template #input>\n              <van-radio-group\n                v-model=\"formData.actualType\"\n                direction=\"horizontal\"\n              >\n                <van-radio :name=\"1\">毛菜</van-radio>\n                <van-radio :name=\"2\">净菜 </van-radio>\n              </van-radio-group>\n            </template>\n          </van-field>\n          <van-field\n            required\n            v-model=\"formData.actualQuantity\"\n            name=\"实收数量\"\n            label=\"实收数量\"\n            placeholder=\"请输入实收数量\"\n            :rules=\"[\n              { required: true },\n              {\n                pattern: actualQuantityPattern,\n                message: '输入值不等于0且不大于9999.99，保留2位小数'\n              }\n            ]\"\n          >\n            <template #button>\n              <span>{{ packUnit }}</span>\n            </template></van-field\n          >\n          <van-field\n            v-model=\"formData.remark\"\n            rows=\"2\"\n            autosize\n            label=\"备注\"\n            type=\"textarea\"\n            maxlength=\"200\"\n            placeholder=\"请输入备注\"\n            show-word-limit\n          />\n        </van-form>\n      </div>\n    </div>\n    <!-- 已收货 -->\n    <div class=\"receive-info\" v-else>\n      <div class=\"title\">收货信息</div>\n      <div class=\"content\">\n        <ul>\n          <li>\n            <span class=\"item-label\">收货类型:</span>\n            <div class=\"item-info\">\n              {{ orderInfo.actualType == 1 ? '毛菜' : '净菜' }}\n            </div>\n          </li>\n          <li>\n            <span class=\"item-label\">实收数量:</span>\n            <div class=\"item-info\">\n              {{ orderInfo.actualQuantity }}{{ orderInfo.packUnit }}\n            </div>\n          </li>\n          <li>\n            <span class=\"item-label\">实收成本:</span>\n            <div class=\"item-info\">\n              {{ orderInfo.actualCost ? orderInfo.actualCost + '元' : '' }}\n            </div>\n          </li>\n          <li>\n            <span class=\"item-label\">备注:</span>\n            <div class=\"item-info\">{{ orderInfo.remarks }}</div>\n          </li>\n        </ul>\n      </div>\n    </div>\n    <div class=\"operation-info\" v-if=\"status != 2020100\">\n      <div class=\"title\">操作记录</div>\n      <div class=\"operation-content\">\n        <div\n          v-for=\"(item, index) in operationList\"\n          :key=\"item.orderOperationRecordId\"\n          class=\"operation-item  active\"\n        >\n          <div :class=\"['progress-icon', index == 0 ? 'active' : '']\">\n            <div class=\"icon-inner\"></div>\n          </div>\n          <div\n            :class=\"['icon-dot', index == 0 ? 'active' : '']\"\n            v-if=\"index < operationList.length - 1\"\n          >\n            <div class=\"dot\"></div>\n            <div class=\"dot\"></div>\n          </div>\n          <div class=\"operation-item-div1\">\n            <span class=\"item-label\">操作人:</span>\n            <div\n              class=\"item-info\"\n              @mouseenter=\"enter($event, item.creatorName)\"\n              @mouseleave=\"leave($event)\"\n            >\n              {{\n                item.creatorName.length > 5\n                  ? item.creatorName.slice(0, 5) + '...'\n                  : item.creatorName\n              }}\n            </div>\n          </div>\n          <div class=\"operation-item-div2\">\n            <span class=\"item-label\">操作时间:</span>\n            <div class=\"item-info\">\n              {{ formateDate(item.updateTime, 2) }}\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div v-if=\"status == 2020100 || (status == 2030100 && actualType == 2)\">\n      <button class=\"app-btn\" @click=\"onSubmit\">\n        确认收货\n      </button>\n    </div>\n    <div id=\"mouse\" v-if=\"true\" class=\"opeartion-tip\"></div>\n  </div>\n</template>\n\n<script>\nconst statusObj = {\n  '2010000': '待发货',\n  '2020100': '待收货',\n  '2030100': '收货待确认',\n  '2040100': '已收货',\n  '-2010000': '已退单'\n};\nimport { formateDate } from '../../plugins/common/utils.js';\nimport {\n  appApproveOrderDetail,\n  getReceivingGoodsOrderDetail\n} from '../../api/ReceiveGoods/index.js';\nexport default {\n  data() {\n    return {\n      loaded: false, //数据加载完成展示页面\n      orderId: '', //列表id\n      orderContentDetailId: '',\n      actualQuantityPattern: /^([1-9]\\d{0,3})([.]\\d{1,2})?$|^0[.]([1-9]\\d{0,1}|[0-9][1-9])$/,\n      statusObj,\n      orderInfo: {},\n      formData: {\n        actualType: 1,\n        actualQuantity: '',\n        remark: ''\n      },\n      operationList: [],\n      packUnit: 'kg',\n      status: ''\n    };\n  },\n  computed: {\n    actualType() {\n      return this.$store.getters['user/roleType'].includes(8) ? 1 : 2;\n    }\n  },\n  methods: {\n    formateDate,\n    getReceivingGoodsOrderDetail() {\n      let loading = this.$toast.loading();\n      getReceivingGoodsOrderDetail({\n        orderContentDetailId: this.orderContentDetailId\n      })\n        .then(res => {\n          this.loaded = true;\n          loading.close();\n          this.orderInfo = res.data;\n          this.formData.actualQuantity = res.data.packNum;\n          this.operationList = res.data.orderOperationRecordRespDtoList;\n          this.packUnit = this.orderInfo.packUnit;\n        })\n        .catch(e => {\n          loading.close();\n        });\n    },\n    classObj(status) {\n      /* eslint-disable */\n      let cls =\n        status == 2020100\n          ? 'orange1'\n          : status == 2030100\n          ? 'orange2'\n          : status == 2040100\n          ? 'green'\n          : 'gray';\n      return cls;\n    },\n    receiveGoods() {\n      this.$dialog\n        .confirm({\n          message: '确认收货'\n        })\n        .then(() => {\n          let loading = this.$toast.loading();\n          appApproveOrderDetail({\n            ...this.formData,\n            id: this.orderInfo.orderId,\n            detailIds: [this.orderInfo.orderContentDetailId],\n            code:\n              this.status == '2020100' && this.actualType == 1\n                ? '2030100'\n                : '2040100'\n          })\n            .then(res => {\n              loading.close();\n              this.$router.replace({\n                path: '/receive-goods-detail-list',\n                query: { id: this.orderId }\n              });\n            })\n            .catch(e => {\n              this.$toast.fail(e.message);\n              loading.close();\n            });\n        })\n        .catch(() => {});\n    },\n    onSubmit() {\n      if (this.$refs.form) {\n        this.$refs.form.validate().then(res => {\n          this.receiveGoods();\n        });\n      } else {\n        this.formData.remark = this.orderInfo.remarks;\n        this.receiveGoods();\n      }\n    },\n    enter(event, data) {\n      var showDiv = document.getElementById('mouse');\n      showDiv.style.left = event.pageX + 'px';\n      showDiv.style.top = event.pageY + 20 + 'px';\n      showDiv.style.display = 'block';\n      showDiv.innerHTML = data;\n    },\n    leave($event) {\n      var showDiv = document.getElementById('mouse');\n      showDiv.style.display = 'none';\n      showDiv.innerHTML = '';\n    }\n  },\n  created() {\n    let queryObj = this.$route.query;\n    this.status = queryObj.status;\n    this.orderContentDetailId = queryObj.id;\n    this.orderId = queryObj.orderId;\n    this.getReceivingGoodsOrderDetail();\n    this.formData.actualType = this.actualType;\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n/deep/ .van-field__label {\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: rgba(0, 0, 0, 0.65);\n}\n/deep/ .van-cell {\n  // line-height: 12px;\n}\n/deep/ .van-radio__icon--checked .van-icon {\n  background-color: #51d0bc;\n  border-color: #51d0bc;\n}\n.app-wrapper {\n  background: #f5f5f5;\n}\n.title {\n  line-height: 48px;\n  font-size: 15px;\n  font-family: PingFangSC-Medium, PingFang SC;\n  font-weight: 500;\n  color: rgba(0, 0, 0, 0.85);\n  position: relative;\n  padding-left: 18px;\n  border-bottom: 1px solid #dedede;\n  &::after {\n    position: absolute;\n    content: '';\n    width: 2px;\n    height: 11px;\n    border-radius: 1px;\n    background: #51d0bc;\n    left: 12px;\n    top: 19px;\n  }\n}\n.buy-info,\n.receive-info,\n.operation-info {\n  background: #fff;\n  margin-bottom: 10px;\n  .content {\n    padding: 12px;\n    ul {\n      li {\n        line-height: 18px;\n        margin-bottom: 12px;\n      }\n    }\n  }\n  .operation-item {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    padding-left: 20px;\n    // padding-right: 5px;\n    line-height: 40px;\n    position: relative;\n    box-sizing: border-box;\n    .progress-icon {\n      width: 12px;\n      height: 12px;\n      border-radius: 50%;\n      background: #d4d4d4;\n      position: absolute;\n      left: 5px;\n      top: 14px;\n      &.active {\n        background: #51d0bc;\n      }\n      .icon-inner {\n        width: 7px;\n        height: 7px;\n        border-radius: 50%;\n        background: #fff;\n        position: absolute;\n        top: 2.5px;\n        left: 2.5px;\n      }\n    }\n    .icon-dot {\n      position: absolute;\n      bottom: -8px;\n      left: 8.5px;\n      .dot {\n        width: 4px;\n        height: 4px;\n        border-radius: 50%;\n        background: #d4d4d4;\n        margin-bottom: 4px;\n      }\n      &.active {\n        .dot {\n          background: #51d0bc;\n        }\n      }\n    }\n    .operation-item-div1 {\n      width: 130px;\n      overflow: hidden;\n      white-space: nowrap;\n      text-overflow: ellipsis;\n    }\n    .operation-item-div2 {\n      padding-right: 5px;\n    }\n  }\n}\n\n.item-head {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 15px;\n  .name {\n    line-height: 35px;\n\n    span {\n      font-size: 14px;\n      // margin-left: 10px;\n      font-family: PingFangSC-Medium, PingFang SC;\n      font-weight: 500;\n      color: rgba(0, 0, 0, 0.85);\n    }\n  }\n}\n.item-content {\n  display: flex;\n  justify-content: space-between;\n  flex-wrap: wrap;\n  li {\n    width: 49%;\n    margin-bottom: 12px;\n    font-size: 14px;\n    color: rgba(0, 0, 0, 0.65);\n    span {\n      margin-right: 6px;\n    }\n    div {\n      display: inline-block;\n\n      font-weight: 400;\n      color: rgba(0, 0, 0, 0.85);\n    }\n  }\n}\n\n.item-label {\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: rgba(0, 0, 0, 0.65);\n  margin-right: 6px;\n}\n.item-info {\n  font-size: 14px;\n  display: inline-block;\n  word-wrap: break-word;\n  table-layout: fixed;\n  word-break: break-all;\n}\n\n.app-btn {\n  width: 320px;\n  height: 49px;\n  margin: 16px 27.5px;\n  background: #51d0bc;\n  border-radius: 3px;\n  color: #fff;\n  font-size: 17px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n}\n.operation-tab {\n  position: absolute;\n  right: 0;\n  top: 13px;\n  width: 44px;\n  height: 19.5px;\n  line-height: 19.5px;\n  background-repeat: no-repeat;\n  background-position: center left;\n  background-size: 100% 100%;\n  color: #fff;\n  font-size: 11px;\n  text-align: center;\n  &.green {\n    background-image: url('../../assets/images/operation-green.png');\n  }\n  &.orange1 {\n    background-image: url('../../assets/images/operation-orange1.png');\n  }\n  &.orange2 {\n    width: 66px;\n    background-image: url('../../assets/images/operation-orange2.png');\n  }\n  &.gray {\n    background-image: url('../../assets/images/operation-gray.png');\n  }\n}\n\n.opeartion-tip {\n  position: absolute;\n  padding: 5px 10px;\n  background-color: #000;\n  color: #fff;\n  display: none;\n}\n</style>\n"]}]}
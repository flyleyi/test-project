ePopup: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    statusPopup: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  components: {\r\n    [Icon.name]: Icon\r\n  },\r\n  watch: {\r\n    schoolPopup(val) {\r\n      if (val) {\r\n        this.$refs.school.classList.add('choosed');\r\n      } else {\r\n        this.$refs.school.classList.remove('choosed');\r\n      }\r\n    },\r\n    subjectPopup(val) {\r\n      if (val) {\r\n        this.$refs.subject.classList.add('choosed');\r\n      } else {\r\n        this.$refs.subject.classList.remove('choosed');\r\n      }\r\n    },\r\n    topicThesisLevePopup(val) {\r\n      if (val) {\r\n        this.$refs.topicThesisLeve.classList.add('choosed');\r\n      } else {\r\n        this.$refs.topicThesisLeve.classList.remove('choosed');\r\n      }\r\n    },\r\n    topicThesisTypePopup(val) {\r\n      if (val) {\r\n        this.$refs.topicThesisType.classList.add('choosed');\r\n      } else {\r\n        this.$refs.topicThesisType.classList.remove('choosed');\r\n      }\r\n    },\r\n    statusPopup(val) {\r\n      if (val) {\r\n        this.$refs.status.classList.add('choosed');\r\n      } else {\r\n        this.$refs.status.classList.remove('choosed');\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    /**\r\n     * 日期区间\r\n     */\r\n    showDate() {\r\n      this.$emit('startDateShow', true);\r\n    },\r\n    showDate2() {\r\n      this.$emit('startDateShow2', true);\r\n    },\r\n    showSchool() {\r\n      this.$emit('schoolShow', true);\r\n    },\r\n    showSubject() {\r\n      this.$emit('subjectShow', true);\r\n    },\r\n    showTopicThesisLeve() {\r\n      this.$emit('topicThesisLeveShow', true);\r\n    },\r\n    showTopicThesisType() {\r\n      this.$emit('topicThesisTypeShow', true);\r\n    },\r\n    showStatus() {\r\n      this.$emit('statusShow', true);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.filter-page {\r\n  .filter-wrap {\r\n    overflow: hid
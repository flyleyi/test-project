          label: '操作',\n          colType: 'btns',\n          width: '50px',\n          btns: [\n            {\n              label: '查看',\n              type: 'text',\n              size: 'mini',\n              click: this.onLook\n            }\n          ]\n        }\n      ],\n      //表单查询项\n      formItems: [\n        {\n          type: 'input',\n          label: '需求编号',\n          prop: 'demandCode',\n          attrs: {\n            placeholder: '请输入需求编号',\n            clearable: true\n          }\n        },\n        {\n          type: 'input',\n          label: '工号',\n          prop: 'staffCode',\n          attrs: {\n            placeholder: '请输入工号',\n            clearable: true\n          }\n        },\n        {\n          type: 'input',\n          label: '联系方式',\n          prop: 'mobile',\n          attrs: {\n            placeholder: '请输入手机号',\n            clearable: true\n          }\n        },\n        {\n          type: 'select',\n          label: '归属',\n          prop: 'belongTo',\n          options: [\n            {\n              label: '需求方',\n              value: 1\n            },\n            {\n              label: '资源方',\n              value: 2\n            }\n          ]\n        },\n        {\n          type: 'select',\n          label: '任务类型',\n          prop: 'demandType',\n          options: [\n            {\n              label: '资源需求',\n              value: 0\n            },\n            {\n              label: '任务需求',\n              value: 1\n            }\n          ]\n        }\n      ],\n      feedKeyName: '',\n      commentKeyName: '',\n      demandInfo: [],\n      demandType: ''\n    };\n  },\n  methods: {\n    getSelectDemandList,\n    changeFeedKeyName(item) {\n      switch (item) {\n        case '0':\n          this.feedKeyName = '解决对方需求';\n          break;\n        case '1':\n          this.feedKeyName = '反馈快';\n          break;\n        case '2':\n          this.feedKeyName = '沟通顺畅';\n          break;\n        case '3':\n          this.feedKeyName = '目标资源一致';\n          break;\n      }\n    },\n    changeCommendKeyName(item) {\n      switch (item) {\n        case '0':\n          this.feedKeyName = '资源质量好';\n          break;\n        case '1':\n          this.feedKeyName = '反馈快';\n          break;\n        case '2':\n          this.feedKeyName = '沟通顺畅';\n          break;\n        case '3':\n          this.feedKeyName = '是您需要的目标资源';\n          break;\n      }\n    },\n    //点击查看按钮\n    onLook(row) {\n      this.demandType = row.demandType;\n      if (row.demandType === 0) this.drawerTitle = '需求单详情';\n      else this.drawerTitle = '任务单详情';\n      getDemandInfo(row.id)\n        .then(res => {\n          if (res.data == '') {\n            this.demandInfo = [];\n          } else {\n            this.demandInfo = res.data;\n          }\n          if (!this.demandInfo.commentKey) {\n            this.demandInfo.commentKey = [];\n          }\n          if (!this.demandInfo.feedbackKey) {\n            this.demandInfo.feedbackKey = [];\n          }\n          this.drawer = true;\n        })\n        .catch(res => {\n          this.demandInfo = [];\n          this.$message({\n            type: 'error',\n            message: res.error\n          });\n        });\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.drawer-content {\n  min-width: 470px;\n  margin-bottom: 10px;\n}\n.el-row {\n  //padding: 10px 10px 0 10px;\n  &:last-child {\n    margin-bottom: 0;\n  }\n}\n.el-col {\n  border-radius: 4px;\n}\n.bg-purple-dark {\n  background: #fff;\n}\n.bg-purple {\n  background: #fff;\n}\n.bg-purple-light {\n  background: #e5e9f2;\n}\n.grid-content {\n  border-radius: 4px;\n  min-height: 36px;\n}\n.row-bg {\n  padding: 10px 0;\n  background-color: #f9fafc;\n}\n.box-card {\n  display: flex;\n  justify-content: space-between;\n  margin: 0;\n  width: 100%;\n  text-align: left;\n}\n.box-card-item {\n  width: 100%;\n}\n.box-card-content {\n  display: flex;\n  justify-content: space-around;\n  flex-wrap: w
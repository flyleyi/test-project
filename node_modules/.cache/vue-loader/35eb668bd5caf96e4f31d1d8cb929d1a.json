{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/teaching-points-h5/views/normal-examine/index.vue?vue&type=style&index=0&id=0df9e183&lang=less&scoped=true&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/teaching-points-h5/views/normal-examine/index.vue","mtime":1627979214413},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/less-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/style-resources-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6lBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/pages/teaching-points-h5/views/normal-examine","sourcesContent":["<template>\r\n  <div class=\"normal-examine\">\r\n    <van-tabs\r\n      v-model=\"active\"\r\n      color=\"#1DAACC\"\r\n      title-active-color=\"#489EF3\"\r\n      title-inactive-color=\"rgba(0,0,0,0.65)\"\r\n      animated\r\n      @change=\"changeTab\"\r\n    >\r\n      <van-tab v-for=\"type in titleData\" :key=\"type\" :title=\"type\">\r\n        <!-- <button @click=\"newPage\">新建</button> -->\r\n        <!-- <button @click=\"searchPage\">搜索</button> -->\r\n        <filter-component\r\n          :schoolPopup=\"show\"\r\n          :subjectPopup=\"show2\"\r\n          :checkDate=\"checkDate\"\r\n          :schoolNameList=\"schoolNameList\"\r\n          :subjectNameList=\"subjectNameList\"\r\n          @startDateShow=\"startDateShow\"\r\n          @schoolShow=\"schoolShow\"\r\n          @subjectShow=\"subjectShow\"\r\n        />\r\n      </van-tab>\r\n    </van-tabs>\r\n    <div class=\"list-data\">\r\n      <van-pull-refresh v-model=\"refreshing\" @refresh=\"onRefresh\">\r\n        <van-list\r\n          v-model=\"loading\"\r\n          :finished=\"finished\"\r\n          :offset=\"20\"\r\n          :error.sync=\"error\"\r\n          error-text=\"请求失败，点击重新加载\"\r\n          :finished-text=\"finishedText\"\r\n          @load=\"onLoad\"\r\n        >\r\n          <dl>\r\n            <list\r\n              :finished=\"finished\"\r\n              :active=\"titleData[active]\"\r\n              :teacherTableData=\"teacherTableData\"\r\n            ></list>\r\n          </dl>\r\n        </van-list>\r\n      </van-pull-refresh>\r\n    </div>\r\n\r\n    <van-calendar\r\n      ref=\"calendar\"\r\n      class=\"timePicker\"\r\n      v-model=\"visibleStartTime\"\r\n      :allow-same-day=\"true\"\r\n      type=\"range\"\r\n      color=\"#489EF3\"\r\n      :min-date=\"minDate\"\r\n      :max-date=\"maxDate\"\r\n      @select=\"seleceDate\"\r\n      title=\"检查日期\"\r\n      get-container=\"#app\"\r\n    >\r\n      <template v-slot:footer>\r\n        <van-button round @click=\"resetDate\" plain type=\"info\">重置</van-button>\r\n        <van-button round @click=\"handleDate\" :disabled=\"isDisabled\" type=\"info\"\r\n          >确定</van-button\r\n        >\r\n      </template>\r\n    </van-calendar>\r\n    <van-overlay :lock-scroll=\"false\" :show=\"show\" @click=\"show = false\">\r\n      <div class=\"wrapper\">\r\n        <div class=\"block\">\r\n          <van-checkbox-group v-model=\"schoolCodeList\">\r\n            <van-cell-group>\r\n              <van-cell\r\n                v-for=\"(item, index) in getSchoolOptions\"\r\n                clickable\r\n                :key=\"item.dictionaryKey\"\r\n                :title=\"item.dictionaryText\"\r\n                @click.stop=\"toggle(index)\"\r\n              >\r\n                <template #right-icon>\r\n                  <van-checkbox :name=\"item.dictionaryKey\" ref=\"checkboxes\">\r\n                    <template #icon=\"props\">\r\n                      <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                    </template>\r\n                  </van-checkbox>\r\n                </template>\r\n              </van-cell>\r\n            </van-cell-group>\r\n          </van-checkbox-group>\r\n          <div class=\"fixed-btn-group\">\r\n            <van-button\r\n              @click.stop=\"resetSchool\"\r\n              icon=\"replay\"\r\n              plain\r\n              type=\"info\"\r\n              >重置</van-button\r\n            >\r\n            <van-button @click=\"confirmSchool\" type=\"info\">确定</van-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </van-overlay>\r\n    <van-overlay :lock-scroll=\"false\" :show=\"show2\" @click=\"show2 = false\">\r\n      <div class=\"wrapper\">\r\n        <div class=\"block\">\r\n          <van-checkbox-group v-model=\"subjectCodeList\">\r\n            <van-cell-group>\r\n              <van-cell\r\n                v-for=\"(item, index) in getSubjectOptions\"\r\n                clickable\r\n                :key=\"item.dictionaryKey\"\r\n                :title=\"item.dictionaryText\"\r\n                @click.stop=\"toggle2(index)\"\r\n              >\r\n                <template #right-icon>\r\n                  <van-checkbox :name=\"item.dictionaryKey\" ref=\"checkboxes2\">\r\n                    <template #icon=\"props\">\r\n                      <van-icon :name=\"props.checked ? 'success' : ''\" />\r\n                    </template>\r\n                  </van-checkbox>\r\n                </template>\r\n              </van-cell>\r\n            </van-cell-group>\r\n          </van-checkbox-group>\r\n          <div class=\"fixed-btn-group\">\r\n            <van-button\r\n              @click.stop=\"resetSubject\"\r\n              icon=\"replay\"\r\n              plain\r\n              type=\"info\"\r\n              >重置</van-button\r\n            >\r\n            <van-button @click=\"confirmSubject\" type=\"info\">确定</van-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </van-overlay>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  Tab,\r\n  Tabs,\r\n  Overlay,\r\n  Checkbox,\r\n  CheckboxGroup,\r\n  Cell,\r\n  CellGroup,\r\n  Icon,\r\n  Button,\r\n  Toast,\r\n  List,\r\n  PullRefresh,\r\n  Grid,\r\n  GridItem,\r\n  Calendar\r\n} from 'vant';\r\nimport { getTableData, getPrepareList } from '../../api/base/types-all';\r\nimport list from './components/list';\r\nimport filterComponent from './components/filter';\r\nimport fMixin from './components/mixin';\r\nimport { compareAuth } from '../../plugins/common/auth';\r\nimport utils from '@/plugins/common/utils';\r\nexport default {\r\n  name: 'normalExamine',\r\n  data() {\r\n    return {\r\n      active: 0,\r\n      currentStartTime: '',\r\n      currentEndTime: '',\r\n      currentCheckDate: '',\r\n      visibleStartTime: false,\r\n      visibleEndTime: false,\r\n      checkDate: '',\r\n      show: false,\r\n      show2: false,\r\n      schoolNameList: '',\r\n      subjectNameList: '',\r\n\r\n      titleData: [],\r\n\r\n      formData: {\r\n        // 查询参数\r\n        teacherName: '',\r\n        startTime: '',\r\n        endTime: '',\r\n        schoolCodeList: [],\r\n        subjectCodeList: []\r\n      },\r\n      schoolCodeList: [],\r\n      subjectCodeList: [],\r\n      params: {},\r\n      pageInfo: {\r\n        //填报信息总页数\r\n        total: 0\r\n      },\r\n      pagingParams: {\r\n        //填报分页参数\r\n        limit: 8,\r\n        offset: 0\r\n      },\r\n      load: true,\r\n      teacherTableData: [],\r\n\r\n      loading: false,\r\n      finished: false,\r\n      refreshing: false,\r\n      error: false,\r\n      showDatePicker: false,\r\n      iconAdd: require('../../assets/images/icon/icon-add.png'),\r\n      iconSearch: require('../../assets/images/icon/icon-search.png'),\r\n      minDate: new Date(new Date().getFullYear() - 5, 0, 1),\r\n      maxDate: new Date(new Date().getFullYear() + 5, 12, 31),\r\n      defaultDate: new Date(),\r\n      isDisabled: false\r\n    };\r\n  },\r\n  mixins: [fMixin],\r\n  components: {\r\n    [Tab.name]: Tab,\r\n    [Tabs.name]: Tabs,\r\n    [Overlay.name]: Overlay,\r\n    [Checkbox.name]: Checkbox,\r\n    [CheckboxGroup.name]: CheckboxGroup,\r\n    [Cell.name]: Cell,\r\n    [CellGroup.name]: CellGroup,\r\n    [Icon.name]: Icon,\r\n    [Button.name]: Button,\r\n    [Toast.name]: Toast,\r\n    [PullRefresh.name]: PullRefresh,\r\n    [List.name]: List,\r\n    [Grid.name]: Grid,\r\n    [GridItem.name]: GridItem,\r\n    [Calendar.name]: Calendar,\r\n    filterComponent,\r\n    list\r\n  },\r\n  computed: {\r\n    showSum() {\r\n      return (\r\n        this.show || this.show2 || this.visibleStartTime || this.visibleEndTime\r\n      );\r\n    }\r\n  },\r\n  watch: {\r\n    showSum(val) {\r\n      if (val) {\r\n        document.querySelector('body').style.overflow = 'hidden';\r\n      } else {\r\n        document.querySelector('body').style.overflow = 'auto';\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    if (sessionStorage.getItem('active')) {\r\n      this.active = Number(sessionStorage.getItem('active'));\r\n    }\r\n    this.init();\r\n  },\r\n  methods: {\r\n    init() {\r\n      let _self = this;\r\n      let datas = [];\r\n      _self.$store.state.user.menus.forEach(item => {\r\n        if (item.url === '/normal-examine/types-all') {\r\n          datas.push({ name: '统计汇总', num: 1 });\r\n        }\r\n        if (item.url === '/normal-examine/prepare-course') {\r\n          datas.push({ name: '备课', num: 2 });\r\n        }\r\n        if (item.url === '/normal-examine/modify-lecture') {\r\n          datas.push({ name: '改作', num: 3 });\r\n        }\r\n        if (item.url === '/normal-examine/attend-class') {\r\n          datas.push({ name: '听课记录', num: 4 });\r\n        }\r\n      });\r\n      datas = datas.sort((a, b) => {\r\n        return a.num - b.num;\r\n      });\r\n      datas.forEach(item => {\r\n        _self.titleData.push(item.name);\r\n      });\r\n      let item = [\r\n        {\r\n          id: '1',\r\n          iconId: 'search',\r\n          text: '搜索',\r\n          url: _self.iconSearch\r\n        }\r\n      ];\r\n      if (_self.titleData[_self.active] !== '统计汇总') {\r\n        if (_self.titleData[_self.active] === '备课') {\r\n          let isAdd = compareAuth('prepare-course-add');\r\n          if (isAdd) {\r\n            item.push({\r\n              id: '2',\r\n              iconId: 'add',\r\n              text: '新建',\r\n              url: _self.iconAdd\r\n            });\r\n          }\r\n        }\r\n        if (_self.titleData[_self.active] === '改作') {\r\n          let isAdd = compareAuth('modify-lecture-add');\r\n          if (isAdd) {\r\n            item.push({\r\n              id: '2',\r\n              iconId: 'add',\r\n              text: '新建',\r\n              url: _self.iconAdd\r\n            });\r\n          }\r\n        }\r\n        if (_self.titleData[_self.active] === '听课记录') {\r\n          let isAdd = compareAuth('attend-class-add');\r\n          if (isAdd) {\r\n            item.push({\r\n              id: '2',\r\n              iconId: 'add',\r\n              text: '新建',\r\n              url: _self.iconAdd\r\n            });\r\n          }\r\n        }\r\n      }\r\n      _self.$dd.biz.navigation.setMenu({\r\n        backgroundColor: '#20D1CD',\r\n        textColor: '#ADD8E611',\r\n        items: item,\r\n        onSuccess: function(res) {\r\n          // 调用成功时回调\r\n          if (res.id === '1') {\r\n            _self.searchPage();\r\n          } else if (res.id === '2') {\r\n            _self.newPage();\r\n          }\r\n        },\r\n        onFail: function(err) {\r\n          // 调用失败时回调\r\n          console.log(err);\r\n        }\r\n      });\r\n    },\r\n    /**\r\n     * 点击搜索icon 进入搜索页面\r\n     */\r\n    searchPage() {\r\n      this.$router.push({\r\n        name: 'normalExamineSearch',\r\n        query: {\r\n          active: this.titleData[this.active]\r\n        }\r\n      });\r\n    },\r\n    /**\r\n     * 重置学校下拉框\r\n     */\r\n    resetSchool() {\r\n      this.schoolNameList = '';\r\n      this.schoolCodeList = this.formData.schoolCodeList = [];\r\n      this.show = false;\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 确定学校下拉框\r\n     */\r\n    confirmSchool() {\r\n      let names = [];\r\n      this.schoolNameList = '';\r\n      this.getSchoolOptions.map(item => {\r\n        this.schoolCodeList.map(i => {\r\n          if (item.dictionaryKey == i) {\r\n            names.push(item.dictionaryText);\r\n          }\r\n        });\r\n      });\r\n      this.schoolNameList = names.join(',');\r\n      this.onRefresh();\r\n    },\r\n    /**\r\n     * 重置学科下拉框\r\n     */\r\n    resetSubject() {\r\n      this.subjectNameList = '';\r\n      this.subjectCodeList = this.formData.subjectCodeList = [];\r\n      this.show2 = false;\r\n      this.onRefresh();\r\n    },\r\n    // 日期确定\r\n    handleDate() {\r\n      this.visibleStartTime = false;\r\n      let nextDay = new Date(this.defaultDate.getTime() + 24 * 60 * 60 * 1000);\r\n      if (this.currentStartTime === '' && this.defaultDate) {\r\n        this.currentStartTime = `${utils.dateConverter(\r\n          'yyyy-MM-dd',\r\n          this.defaultDate\r\n        )}`;\r\n        this.currentEndTime = `${utils.dateConverter('yyyy-MM-dd', nextDay)}`;\r\n        this.currentCheckDate = `${utils.dateConverter(\r\n          'yyyy.MM.dd',\r\n          this.defaultDate\r\n        )} - ${utils.dateConverter('yyyy.MM.dd', nextDay)}`;\r\n      }\r\n      this.formData.startTime = this.currentStartTime;\r\n      this.formData.endTime = this.currentEndTime;\r\n      this.checkDate = this.currentCheckDate;\r\n      this.onRefresh();\r\n    },\r\n    // 日期重置\r\n    resetDate() {\r\n      this.$refs.calendar.reset();\r\n      this.currentStartTime = this.currentEndTime = this.currentCheckDate = this.formData.startTime = this.formData.endTime = this.checkDate =\r\n        '';\r\n      this.visibleStartTime = false;\r\n      this.onRefresh();\r\n    },\r\n    // 日期选择事件\r\n    seleceDate(date) {\r\n      const [start, end] = date;\r\n      if (!end) {\r\n        this.isDisabled = true;\r\n      } else {\r\n        this.isDisabled = false;\r\n      }\r\n      this.currentStartTime = `${utils.dateConverter('yyyy-MM-dd', start)}`;\r\n      this.currentEndTime = `${utils.dateConverter('yyyy-MM-dd', end)}`;\r\n      this.currentCheckDate = `${utils.dateConverter(\r\n        'yyyy.MM.dd',\r\n        start\r\n      )} - ${utils.dateConverter('yyyy.MM.dd', end)}`;\r\n    },\r\n    /**\r\n     * 确定学科下拉框\r\n     */\r\n    confirmSubject() {\r\n      let names = [];\r\n      this.subjectNameList = '';\r\n      this.getSubjectOptions.map(item => {\r\n        this.subjectCodeList.map(i => {\r\n          if (item.dictionaryKey == i) {\r\n            names.push(item.dictionaryText);\r\n          }\r\n        });\r\n      });\r\n      this.subjectNameList = names.join(',');\r\n      this.onRefresh();\r\n    },\r\n    queryItem() {\r\n      //查询\r\n      var params = {};\r\n      this.formData.schoolCodeList = this.schoolCodeList;\r\n      this.formData.subjectCodeList = this.subjectCodeList;\r\n      for (let key in this.formData) {\r\n        if (key == 'teacherName' || key == 'startTime' || key == 'endTime') {\r\n          params[key] = this.formData[key];\r\n        } else if (key == 'schoolCodeList' || key == 'subjectCodeList') {\r\n          if (\r\n            Object.prototype.toString.call(this.formData[key]) ==\r\n            '[object Array]'\r\n          ) {\r\n            params[key] = this.formData[key].join(',');\r\n          } else {\r\n            params[key] = this.formData[key];\r\n          }\r\n        }\r\n      }\r\n      for (let item in params) {\r\n        if (params[item] == '') {\r\n          delete params[item];\r\n        }\r\n      }\r\n      this.params = params;\r\n      this.getData();\r\n    },\r\n    /**\r\n     * 获取列表数据\r\n     */\r\n    getData() {\r\n      if (this.pagingParams.offset > this.pageInfo.total) return;\r\n      this.load = true;\r\n      let promiseData;\r\n      if (this.titleData[this.active] === '统计汇总') {\r\n        promiseData = getTableData({ ...this.params, ...this.pagingParams });\r\n      } else {\r\n        let typeName = this.titleData[this.active];\r\n        let type;\r\n        switch (typeName) {\r\n          case '备课':\r\n            type = 1;\r\n            break;\r\n          case '改作':\r\n            type = 2;\r\n            break;\r\n          case '听课记录':\r\n            type = 3;\r\n            break;\r\n          default:\r\n            break;\r\n        }\r\n        promiseData = getPrepareList({\r\n          ...this.params,\r\n          ...this.pagingParams,\r\n          type\r\n        });\r\n      }\r\n      promiseData\r\n        .then(({ data }) => {\r\n          if (data.current == 1 && data.records.length == 0) {\r\n            this.teacherTableData = [];\r\n            this.finished = true;\r\n          }\r\n          this.teacherTableData = this.teacherTableData.concat(data.records);\r\n          this.pageInfo.total = data.total || 0;\r\n          if (this.pageInfo.total <= data.current * data.size) {\r\n            this.finished = true;\r\n          } else {\r\n            this.pagingParams.offset =\r\n              (Math.floor(this.pagingParams.offset / this.pagingParams.limit) +\r\n                1) *\r\n              this.pagingParams.limit;\r\n          }\r\n          this.loading = false;\r\n          setTimeout(() => {\r\n            this.load = false;\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.teacherTableData = [];\r\n          this.finished = true;\r\n        });\r\n    },\r\n    schoolShow(status) {\r\n      if (this.show2) {\r\n        this.subjectCodeList = this.formData.subjectCodeList;\r\n      }\r\n      this.show2 = this.visibleStartTime = false;\r\n      this.show = status;\r\n    },\r\n    subjectShow(status) {\r\n      if (this.show) {\r\n        this.schoolCodeList = this.formData.schoolCodeList;\r\n      }\r\n      this.show = this.visibleStartTime = false;\r\n      this.show2 = status;\r\n    },\r\n    startDateShow(status) {\r\n      if (this.show) {\r\n        this.schoolCodeList = this.formData.schoolCodeList;\r\n      }\r\n      if (this.show2) {\r\n        this.subjectCodeList = this.formData.subjectCodeList;\r\n      }\r\n      this.show = this.show2 = false;\r\n      this.visibleStartTime = status;\r\n    },\r\n    toggle(index) {\r\n      this.$refs.checkboxes[index].toggle();\r\n    },\r\n    toggle2(index) {\r\n      this.$refs.checkboxes2[index].toggle();\r\n    },\r\n    onLoad() {\r\n      if (this.refreshing) {\r\n        this.teacherTableData = [];\r\n        this.refreshing = false;\r\n      }\r\n      this.loading = true;\r\n      this.queryItem();\r\n    },\r\n    onRefresh() {\r\n      this.teacherTableData = [];\r\n      this.finished = false;\r\n      this.loading = true;\r\n      this.pageInfo.total = 0;\r\n      this.pagingParams.offset = 0;\r\n      this.onLoad();\r\n    },\r\n    changeTab(active) {\r\n      Object.assign(this.$data, this.$options.data.call(this));\r\n      sessionStorage.setItem('active', active);\r\n      this.active = active;\r\n      let nextDay = new Date(this.defaultDate.getTime() + 24 * 60 * 60 * 1000);\r\n      let arr = [this.defaultDate, nextDay];\r\n      this.$refs.calendar.reset(arr);\r\n      this.init();\r\n      this.onRefresh();\r\n    },\r\n    newPage() {\r\n      let name;\r\n      if (this.titleData[this.active] === '备课') {\r\n        name = 'bkEdit';\r\n      } else if (this.titleData[this.active] === '改作') {\r\n        name = 'gzEdit';\r\n      } else if (this.titleData[this.active] === '听课记录') {\r\n        name = 'tkEdit';\r\n      }\r\n      this.$router.push({ name });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.normal-examine {\r\n  width: 100%;\r\n  height: calc(100% - 1rem);\r\n  background: #f6f6f6;\r\n  /deep/ .van-tab {\r\n    font-size: 0.28rem;\r\n  }\r\n  /deep/ .van-tab--active {\r\n    font-size: 0.36rem;\r\n  }\r\n  /deep/ .van-tabs__line {\r\n    width: 0.72rem;\r\n    heigh
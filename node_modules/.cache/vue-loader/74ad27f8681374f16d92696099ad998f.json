{"remainingRequest":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/jx-questionnaire-h5/views/survey-submit/index.vue?vue&type=style&index=0&id=a42d6880&lang=less&scoped=true&","dependencies":[{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/src/pages/jx-questionnaire-h5/views/survey-submit/index.vue","mtime":1645178321143},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/less-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/style-resources-loader/lib/index.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/fuyueyi/Desktop/practice/mutilwap/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAymBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/pages/jx-questionnaire-h5/views/survey-submit","sourcesContent":["<!--  -->\n<template>\n  <div class=\"app\">\n    <div class=\"base-info\">\n      <div class=\"time\">\n        <img\n          src=\"@/pages/jx-questionnaire-h5/assets/images/icons/time_icon.png\"\n        />\n        <div>\n          填表时间：{{\n            baseInfo.startTime +\n              (baseInfo.endTime ? '~' + baseInfo.endTime : '')\n          }}\n        </div>\n      </div>\n      <div class=\"title\">{{ baseInfo.title }}</div>\n      <div class=\"remark\" v-if=\"baseInfo.description\">\n        {{ baseInfo.description }}\n      </div>\n      <!-- 文件列表 -->\n      <div class=\"file-list\">\n        <div\n          class=\"file-item\"\n          v-for=\"(item, index) in baseInfo.fileList\"\n          :key=\"index\"\n          @click=\"previewFile(item.url)\"\n        >\n          <div class=\"file-name\">\n            <img\n              class=\"file-icon\"\n              src=\"@/pages/jx-questionnaire-h5/assets/images/icons/file3_icon.png\"\n            />\n            <div>{{ item.name }}</div>\n            <img\n              class=\"arrow-icon\"\n              src=\"@/pages/jx-questionnaire-h5/assets/images/icons/right_arrow2_icon.png\"\n            />\n          </div>\n        </div>\n      </div>\n      <!-- 图片列表 -->\n      <div class=\"img-list\">\n        <div\n          class=\"img-item\"\n          v-for=\"(item, index) in baseInfo.imgList\"\n          :key=\"index\"\n        >\n          <img class=\"img\" :src=\"item.url\" @click=\"previewImg(item.url)\" />\n        </div>\n      </div>\n    </div>\n    <template\n      v-if=\"\n        surveyStatus == 0 &&\n          isMain == 1 &&\n          (baseInfo.status == 0 || baseInfo.status == 1)\n      \"\n    >\n      <!-- 选项列表 -->\n      <div class=\"question-list\">\n        <div v-for=\"(item, index) in questions\" :key=\"index\">\n          <!-- 单选 -->\n          <div class=\"item-content\" v-if=\"item.type == '1'\">\n            <div class=\"item-title\">\n              <div class=\"item-num\">\n                <div>{{ index + 1 >= 10 ? index + 1 : '0' + (index + 1) }}</div>\n                <div>{{ item.subject }}</div>\n              </div>\n            </div>\n            <div class=\"options-list\">\n              <div\n                class=\"item-option\"\n                v-for=\"(item2, index2) in item.options\"\n                :key=\"index2\"\n                @click=\"handleRadioChoose(index, index2)\"\n              >\n                <img\n                  :src=\"\n                    item2.isChecked\n                      ? require('@/pages/jx-questionnaire-h5/assets/images/icons/checked4_icon.png')\n                      : require('@/pages/jx-questionnaire-h5/assets/images/icons/unchecked2_icon.png')\n                  \"\n                />\n                <div>{{ item2.value }}</div>\n              </div>\n            </div>\n          </div>\n          <!-- 多选 -->\n          <div class=\"item-content\" v-else-if=\"item.type == '2'\">\n            <div class=\"item-title\">\n              <div class=\"item-num\">\n                <div>{{ index + 1 >= 10 ? index + 1 : '0' + (index + 1) }}</div>\n                <div>{{ item.subject }}</div>\n              </div>\n            </div>\n            <div class=\"options-list\">\n              <div\n                class=\"item-option\"\n                v-for=\"(item2, index2) in item.options\"\n                :key=\"index2\"\n                @click=\"handleMultipleChoose(index, index2, item.optionsCnt)\"\n              >\n                <img\n                  :src=\"\n                    item2.isChecked\n                      ? require('@/pages/jx-questionnaire-h5/assets/images/icons/checked3_icon.png')\n                      : require('@/pages/jx-questionnaire-h5/assets/images/icons/unchecked3_icon.png')\n                  \"\n                />\n                <div>{{ item2.value }}</div>\n              </div>\n            </div>\n          </div>\n          <!-- 填空 -->\n          <div class=\"item-content\" v-else-if=\"item.type == '3'\">\n            <div class=\"item-title\">\n              <div class=\"item-num\">\n                <div>{{ index + 1 >= 10 ? index + 1 : '0' + (index + 1) }}</div>\n                <div>{{ item.subject }}</div>\n              </div>\n            </div>\n            <div class=\"cloze-option\">\n              <input\n                type=\"text\"\n                placeholder=\"待填写\"\n                maxlength=\"50\"\n                v-model=\"item.answerCont\"\n              />\n            </div>\n          </div>\n          <!-- 图片 -->\n          <div class=\"item-content\" v-else-if=\"item.type == '4'\">\n            <div class=\"item-title\">\n              <div class=\"item-num\">\n                <div>{{ index + 1 >= 10 ? index + 1 : '0' + (index + 1) }}</div>\n                <div>{{ item.subject }}</div>\n              </div>\n            </div>\n            <div class=\"img-option\">\n              <!-- 图片列表 -->\n              <div\n                class=\"img-upload\"\n                v-for=\"(item2, index2) in item.answerCont\"\n                :key=\"index2\"\n              >\n                <img\n                  class=\"upload-img\"\n                  :src=\"item2.url\"\n                  @click=\"previewImg(item2.url)\"\n                />\n                <div class=\"delete-btn\" @click=\"handleImgDelete(index, index2)\">\n                  <img\n                    src=\"@/pages/jx-questionnaire-h5/assets/images/icons/delete2_icon.png\"\n                  />\n                </div>\n              </div>\n              <!-- 图片上传 -->\n              <van-uploader\n                :before-read=\"beforeRead\"\n                :name=\"index\"\n                :after-read=\"afterRead\"\n                v-if=\"item.answerCont.length < item.optionsCnt\"\n              >\n                <div\n                  class=\"img-upload\"\n                  :style=\"\n                    item.answerCont.length > 2 ? 'margin: 0.37rem 0 0 0' : ''\n                  \"\n                >\n                  <img\n                    class=\"upload-icon\"\n                    src=\"@/pages/jx-questionnaire-h5/assets/images/icons/img2_icon.png\"\n                  />\n                  <div class=\"tip\">点击上传图片</div>\n                </div>\n              </van-uploader>\n            </div>\n          </div>\n          <!-- 文件 -->\n          <div class=\"item-content\" v-else-if=\"item.type == '5'\">\n            <div class=\"item-title\">\n              <div class=\"item-num\">\n                <div>{{ index + 1 >= 10 ? index + 1 : '0' + (index + 1) }}</div>\n                <div>{{ item.subject }}</div>\n              </div>\n            </div>\n            <div class=\"file-option\">\n              <!-- 附件列表 -->\n              <div class=\"file-list\">\n                <div\n                  class=\"item\"\n                  v-for=\"(item2, index2) in item.answerCont\"\n                  :key=\"index2\"\n                >\n                  <div class=\"file-name\" @click=\"previewFile(item2.url)\">\n                    <img\n                      class=\"file-icon\"\n                      src=\"@/pages/jx-questionnaire-h5/assets/images/icons/file3_icon.png\"\n                    />\n                    <div>{{ item2.name }}</div>\n                    <img\n                      class=\"arrow-icon\"\n                      src=\"@/pages/jx-questionnaire-h5/assets/images/icons/right_arrow2_icon.png\"\n                    />\n                  </div>\n                  <img\n                    class=\"close-icon\"\n                    src=\"@/pages/jx-questionnaire-h5/assets/images/icons/close_icon.png\"\n                    @click=\"handleDeleteFile(index, index2)\"\n                  />\n                </div>\n              </div>\n              <!-- 附件上传 -->\n              <van-uploader\n                accept=\".doc,.docx,application/msword,application/pdf\"\n                :before-read=\"beforeReadFile\"\n                :name=\"index\"\n                :after-read=\"afterReadFile\"\n                v-if=\"item.answerCont.length < item.optionsCnt\"\n              >\n                <div class=\"file-upload\">\n                  <img\n                    src=\"@/pages/jx-questionnaire-h5/assets/images/icons/file2_icon.png\"\n                  />\n                  <div>点击选择文件（10M以内）</div>\n                </div>\n              </van-uploader>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- 备注 -->\n      <div class=\"other-info\">\n        <div class=\"remark\">\n          <div>备注：</div>\n          <input\n            type=\"text\"\n            placeholder=\"选填\"\n            maxlength=\"20\"\n            v-model=\"baseInfo.remark\"\n          />\n        </div>\n        <div class=\"name\" v-if=\"baseInfo.isAnonymous\">\n          <div>匿名提交</div>\n          <van-switch\n            v-model=\"baseInfo.isAnonymousSubmit\"\n            size=\"25\"\n            active-color=\"#4DD865\"\n          />\n        </div>\n      </div>\n    </template>\n    <div class=\"blank\"></div>\n    <!-- 主家长展示提示按钮提交按钮 -->\n    <div\n      class=\"bottom-btn\"\n      v-if=\"\n        surveyStatus == 0 &&\n          isMain == 1 &&\n          (baseInfo.status == 0 || baseInfo.status == 1)\n      \"\n    >\n      <div @click=\"handleSubmit\">提交</div>\n    </div>\n  </div>\n</template>\n<script>\nimport { mapState } from 'vuex';\nimport { OssUpload } from '../../plugins/common/ossUpload.js';\nimport { surveyDetail, writeSurvey } from '../../api/survey';\nimport { Uploader, ImagePreview, Switch } from 'vant';\nexport default {\n  name: 'SurveySubmit',\n  components: {\n    [Uploader.name]: Uploader,\n    [Switch.name]: Switch\n  },\n  data() {\n    return {\n      surveyStatus: null, // 0 未提交 1 已提交\n      isMain: null, // 0 次家长 1 主家长\n      canEdit: 3, // 是否能编辑 0为班主任解绑情况 3为家长身份进入情况\n      baseInfo: {\n        studentIds: '',\n        title: '', // 问卷标题\n        description: '', // 问卷描述\n        imgList: [], // 图片附件\n        fileList: [], // 文件附件\n        startTime: '', // 开始日期\n        endTime: '', // 截止日期\n        collectionMaxCnt: 0, // 最大收集数\n        submitCount: 0, // 已收集份数\n        isAnonymous: 0, // 是否展示匿名开关\n        remark: '', // 备注\n        isAnonymousSubmit: 0, // 是否匿名\n        status: null // 当前问卷状态\n      },\n      collectNum: 0, // 收集份数\n      questions: []\n    };\n  },\n  computed: {\n    ...mapState('user', ['wechatUserId', 'classId', 'role'])\n  },\n  created() {\n    this.surveyStatus = this.$route.query.type;\n    this.isMain = this.$route.query.isMain;\n    this.canEdit = this.$route.query.canEdit ? this.$route.query.canEdit : 3;\n    if (this.canEdit == 3) {\n      this.getSurveyDetail();\n    } else {\n      this.getSurveyDetailTeacher();\n    }\n  },\n  methods: {\n    // 获取问卷详情(家长身份正常进入)\n    getSurveyDetail() {\n      let params = {\n        wechatUserId: this.wechatUserId,\n        role: this.role,\n        surveyId: this.$route.query.surveyId,\n        studentId: this.$route.query.studentId\n      };\n      surveyDetail(params)\n        .then(res => {\n          this.baseInfo.status = res.data.status;\n          this.baseInfo.startTime = res.data.startTime;\n          this.baseInfo.endTime = res.data.endTime;\n          this.baseInfo.title = res.data.title;\n          this.baseInfo.description = res.data.description;\n          this.collectNum = res.data.collectionMaxCnt;\n          this.baseInfo.collectionMaxCnt = res.data.collectionMaxCnt;\n          this.baseInfo.submitCount = res.data.submitCount;\n          this.baseInfo.isAnonymous = res.data.isAnonymous;\n          this.baseInfo.remark = res.data.remark;\n          res.data.attachmentList.forEach(item => {\n            if (item.type == 'img') {\n              this.baseInfo.imgList.push({ url: item.url, id: item.id });\n            } else if (item.type == 'file') {\n              this.baseInfo.fileList.push({\n                url: item.url,\n                name: item.name,\n                id: item.id\n              });\n            }\n          });\n          res.data.questionList.forEach(item => {\n            if (item.type == 2) {\n              item.checkedNum = 0;\n              item.answerCont = [];\n            } else if (item.type == '4' || item.type == '5') {\n              item.answerCont = [];\n              item.answerList = [];\n            } else {\n              item.answerCont = '';\n            }\n          });\n          this.questions = res.data.questionList;\n        })\n        .catch(err => {\n          this.$toast(err.msg);\n        });\n    },\n    // 获取问卷详情(班主任无权限修改时进入)\n    getSurveyDetailTeacher() {\n      let params = {\n        wechatUserId: this.wechatUserId,\n        role: this.role,\n        surveyId: this.$route.query.surveyId\n      };\n      surveyDetail(params)\n        .then(res => {\n          this.baseInfo.startTime = res.data.startTime;\n          this.baseInfo.endTime = res.data.endTime;\n          this.baseInfo.title = res.data.title;\n          this.baseInfo.description = res.data.description;\n          this.collectNum = res.data.collectionMaxCnt;\n          this.baseInfo.collectionMaxCnt = res.data.collectionMaxCnt;\n          this.baseInfo.submitCount = res.data.submitCount;\n          this.baseInfo.isAnonymous = res.data.isAnonymous;\n          this.baseInfo.remark = res.data.remark;\n          res.data.attachmentList.forEach(item => {\n            if (item.type == 'img') {\n              this.baseInfo.imgList.push({ url: item.url, id: item.id });\n            } else if (item.type == 'file') {\n              this.baseInfo.fileList.push({\n                url: item.url,\n                name: item.name,\n                id: item.id\n              });\n            }\n          });\n          res.data.questionList.forEach(item => {\n            if (item.type == 2) {\n              item.checkedNum = 0;\n              item.answerCont = [];\n            } else if (item.type == '4' || item.type == '5') {\n              item.answerCont = [];\n              item.answerList = [];\n            } else {\n              item.answerCont = '';\n            }\n          });\n          this.questions = res.data.questionList;\n        })\n        .catch(err => {\n          this.$toast(err.msg);\n        });\n    },\n    // 图片预览\n    previewImg(url) {\n      ImagePreview([url]);\n    },\n    // 文件预览\n    previewFile(url) {\n      // window.location.href = url;\n      if (typeof wx == 'object') {\n        let fileUrl = encodeURIComponent(url);\n        window.wx.miniProgram.navigateTo({\n          url: `/h5/file/index?url=${fileUrl}`\n        });\n      } else {\n        this.$toast('预览失败，请重试');\n      }\n    },\n    // 单选选择\n    handleRadioChoose(index, index2) {\n      this.questions[index].options.forEach((item, idx) => {\n        if (idx == index2) {\n          item.isChecked = true;\n          this.questions[index].answerCont = '' + item.id;\n        } else {\n          item.isChecked = false;\n        }\n      });\n    },\n    // 多选选择\n    handleMultipleChoose(index, index2, limit) {\n      this.questions[index].options.forEach((item, idx) => {\n        if (idx == index2) {\n          // 当已选个数=限制个数，且点击为未选中状态按钮时 return；\n          if (this.questions[index].checkedNum >= limit && !item.isChecked)\n            return;\n          item.isChecked = !item.isChecked;\n          if (item.isChecked) {\n            this.questions[index].checkedNum++;\n            this.questions[index].answerCont.push(item.id);\n          } else {\n            this.questions[index].checkedNum--;\n            this.questions[index].answerCont.splice(index2, 1);\n          }\n        }\n      });\n    },\n    // 上传附件前置处理\n    beforeRead(file, detail) {\n      let type = file.type;\n      if (!type.match(/(jpg|png|jpeg?)$/i)) {\n        this.$toast('仅允许上传图片文件类型');\n        return false;\n      }\n      let item = this.questions[detail.name];\n      if (item.answerCont.length >= item.optionsCnt) {\n        this.$toast(`附件最大上传数量为${item.optionsCnt}`);\n        return false;\n      }\n      // 上传附件单个需小于10M\n      if (file.size > 10 * 1024 * 1024) {\n        this.$toast('上传单个附件需小于10M');\n        return false;\n      }\n      return true;\n    },\n    // 文件上传完成，此时可以自行将文件上传至服务器\n    afterRead(file, detail) {\n      let item = this.questions[detail.name];\n      this.$toast.loading({\n        duration: 0, // 持续展示 toast\n        forbidClick: true,\n        message: '上传中'\n      });\n      OssUpload(file)\n        .then(res => {\n          item.answerCont = item.answerCont.concat(res.ids);\n          item.answerList = item.answerList.concat(res.uarr);\n          this.$toast.clear();\n        })\n        .catch(err => {\n          this.$toast.clear();\n          this.$toast.fail(err.msg);\n        });\n    },\n    // 删除图片\n    handleImgDelete(index, index2) {\n      this.questions[index].answerCont.splice(index2, 1);\n      this.questions[index].answerList.splice(index2, 1);\n    },\n    // 上传附件前置处理\n    beforeReadFile(file, detail) {\n      console.log(file);\n      // var type = file.type.split('/')[1] || file.name.match(/\\.([^.]+?)$/)[1];\n      var type = file.name.match(/\\.([^.]+?)$/)[1];\n      // 仅允许上传PDF和DOC\n      if (!type.match(/(pdf|docx?)$/i)) {\n        this.$toast('仅允许上传PDF和DOC文件类型');\n        return false;\n      }\n      let item = this.questions[detail.name];\n      if (item.answerCont.length >= item.optionsCnt) {\n        this.$toast(`附件最大上传数量为${item.optionsCnt}`);\n        return false;\n      }\n      // 上传附件单个需小于10M\n      if (file.size > 10 * 1024 * 1024) {\n        this.$toast('上传单个附件需小于10M');\n        return false;\n      }\n      return true;\n    },\n    afterReadFile(file, detail) {\n      let item = this.questions[detail.name];\n      this.$toast.loading({\n        duration: 0, // 持续展示 toast\n        forbidClick: true,\n        message: '上传中'\n      });\n      OssUpload(file)\n        .then(res => {\n          res.ids.forEach(item => {\n            item.name = this.getCaption(item.name);\n          });\n          item.answerCont = item.answerCont.concat(res.ids);\n          item.answerList = item.answerList.concat(res.uarr);\n          this.$toast.clear();\n        })\n        .catch(err => {\n          this.$toast.clear();\n          this.$toast.fail(err.msg);\n        });\n    },\n    // 获取指定字符后面的所有字符内容\n    getCaption(obj) {\n      var index = obj.lastIndexOf('-');\n      obj = obj.substring(index + 1, obj.length);\n      return obj;\n    },\n    // 删除文件\n    handleDeleteFile(index, index2) {\n      this.questions[index].answerCont.splice(index2, 1);\n      this.questions[index].answerList.splice(index2, 1);\n    },\n    // 提交\n    handleSubmit() {\n      if (\n        !this.questions.every(item => {\n          return item.answerCont.length > 0;\n        })\n      ) {\n        console.log(\n          this.questions.every(item => {\n            return item.answerCont.length > 0;\n          })\n        );\n        this.$toast('请完善问卷');\n        return false;\n      }\n      this.$toast.loading({\n        duration: 0, // 持续展示 toast\n        forbidClick: true,\n        message: '发布中'\n      });\n      this.questions.forEach(item => {\n        if (item.type == 1 || item.type == 3) {\n          item.answer = item.answerCont;\n        } else if (item.type == 2) {\n          item.answer = item.answerCont.join(',');\n        } else if (item.type == 4 || item.type == 5) {\n          item.answer = item.answerList.join(',');\n        }\n      });\n      let params = {\n        wechatUserId: this.wechatUserId,\n        surveyId: this.$route.query.surveyId,\n        studentId: this.$route.query.studentId,\n        answerList: this.questions,\n        remark: this.baseInfo.remark,\n        isAnonymousSubmit: this.baseInfo.isAnonymousSubmit ? 1 : 0\n      };\n      writeSurvey(params)\n        .then(res => {\n          // 手动清除 Toast\n          this.$toast.clear();\n          let surveyId = this.$route.query.surveyId,\n            studentId = this.$route.query.studentId;\n          this.$router.replace({\n            path: `/survey-submit-done?surveyId=${surveyId}&studentId=${studentId}`\n          });\n        })\n        .catch(err => {\n          // 手动清除 Toast\n          this.$toast.clear();\n          this.$toast(err.msg);\n        });\n    },\n\n    // 次家长和已提交状态下查看提交选项详情\n    handleCheckOptionDetail() {\n      this.$store.commit('survey/UPDATA_QUESTION', this.questions);\n      let remark = this.baseInfo.remark ? this.baseInfo.remark : '';\n      this.$router.push({\n        path: `/option-detail?remark=${remark}&isMain=${this.isMain}`\n      });\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n.app {\n  // 顶部基础信息\n  .base-info {\n    background-color: #ffffff;\n    padding: 0 0.24rem;\n    .time {\n      padding: 0.4rem 0 0.24rem 0;\n      display: flex;\n      align-items: center;\n      position: relative;\n      //伪元素绝对定位\n      &::after {\n        content: ' ';\n        position: absolute;\n        left: 0;\n        bottom: 0;\n        right: 0;\n        height: 1px;\n        background: rgba(0, 0, 0, 0.08);\n        transform-origin: 0 0;\n        transform: scaleY(0.5);\n      }\n      & > img {\n        height: 0.32rem;\n      }\n      & > div {\n        font-size: 0.28rem;\n        font-family: PingFangSC-Regular, PingFang SC;\n        color: rgba(0, 0, 0, 0.45);\n        padding-left: 0.1rem;\n      }\n    }\n    .title {\n      font-size: 0.32rem;\n      font-family: PingFangSC-Medium, PingFang SC;\n      font-weight: 500;\n      color: rgba(0, 0, 0, 0.85);\n      padding: 0.24rem 0;\n    }\n    .remark {\n      font-size: 0.28rem;\n      font-family: PingFangSC-Regular, PingFang SC;\n      font-weight: 400;\n      color: rgba(0, 0, 0, 0.85);\n      padding-bottom: 0.24rem;\n    }\n    .file-list {\n      .file-item {\n        padding-bottom: 0.24rem;\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n        .file-name {\n          display: flex;\n          align-items: center;\n          .file-icon {\n            height: 0.28rem;\n          }\n          & > div {\n            max-width: 5rem;\n            overflow: hidden;\n            white-space: nowrap;\n            text-overflow: ellipsis;\n            font-size: 0.24rem;\n            font-family: PingFangSC-Regular, PingFang SC;\n            color: #5ecfa8;\n            padding: 0 0.1rem;\n          }\n          .arrow-icon {\n            height: 0.22rem;\n          }\n        }\n      }\n    }\n    .img-list {\n      display: flex;\n      flex-wrap: wrap;\n      .img-item {\n        font-size: 0;\n        position: relative;\n        .img {\n          width: 1.57rem;\n          height: 1.57rem;\n          object-fit: cover;\n          margin: 0 0 0.24rem 0.24rem;\n          border-radius: 0.06rem;\n        }\n      }\n      .img-item:first-child {\n        .img {\n          margin: 0 0 0.24rem 0;\n        }\n      }\n      .img-item:nth-child(5) {\n        .img {\n          margin: 0 0 0.24rem 0;\n        }\n      }\n    }\n  }\n  // 问题列表\n  .question-list {\n    padding: 0.2rem 0;\n    .item-content {\n      background-color: #ffffff;\n      margin: 0 0.24rem 0.2rem 0.24rem;\n      padding-bottom: 0.24rem;\n      box-shadow: 0 0 0.1rem 0 rgba(0, 0, 0, 0.1);\n      border-radius: 0.06rem;\n      .item-title {\n        padding: 0 0.2rem;\n        display: flex;\n        justify-content: space-between;\n        .item-num {\n          display: flex;\n          & > div:first-child {\n            width: 0.4rem;\n            height: 0.4rem;\n            border-radius: 50%;\n            background: rgba(0, 0, 0, 0.08);\n            font-size: 0.22rem;\n            font-family: PingFangSC-Medium, PingFang SC;\n            font-weight: 500;\n            color: #5ecfa8;\n            margin-top: 0.24rem;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n          }\n          & > div:nth-child(2) {\n            font-size: 0.28rem;\n            font-family: PingFangSC-Regular, PingFang SC;\n            color: rgba(0, 0, 0, 0.85);\n            padding: 0 0.12rem;\n            margin-top: 0.26rem;\n          }\n        }\n        & > img {\n          height: 0.32rem;\n          padding: 0.2rem 0 0.12rem 0;\n        }\n      }\n      // 单选，多选\n      .options-list {\n        .item-option {\n          padding: 0.24rem 0.2rem 0 0.2rem;\n          display: flex;\n          & > img {\n            height: 0.4rem;\n          }\n          & > div {\n            font-size: 0.28rem;\n            font-family: PingFangSC-Regular, PingFang SC;\n            color: rgba(0, 0, 0, 0.85);\n            padding-left: 0.12rem;\n          }\n        }\n      }\n      // 填空\n      .cloze-option {\n        padding: 0.24rem 0.24rem 0 0.24rem;\n        & > input {\n          width: 6.22rem;\n          height: 0.68rem;\n          border-radius: 0.02rem;\n          border: 0.01rem solid rgba(0, 0, 0, 0.08);\n          padding: 0 0.16rem;\n        }\n      }\n      // 图片\n      .img-option {\n        padding: 0.24rem 0 0 0.72rem;\n        display: flex;\n        flex-wrap: wrap;\n        .img-upload {\n          width: 1.6rem;\n          height: 1.6rem;\n          background: #ededed;\n          border-radius: 0.06rem;\n          margin: 0 0.37rem 0 0;\n          display: flex;\n          flex-direction: column;\n          align-items: center;\n          justify-content: center;\n          position: relative;\n          .upload-img {\n            width: 1.6rem;\n            height: 1.6rem;\n            border-radius: 0.06rem;\n            object-fit: cover;\n          }\n          .delete-btn {\n            width: 1.6rem;\n            height: 0.44rem;\n            background: rgba(0, 0, 0, 0.65);\n            border-radius: 0 0 0.06rem 0.06rem;\n            position: absolute;\n            bottom: 0;\n            left: 0;\n            z-index: 1;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            & > img {\n              height: 0.21rem;\n            }\n          }\n          .upload-icon {\n            height: 0.52rem;\n          }\n          .tip {\n            font-size: 0.24rem;\n            font-family: PingFangSC-Regular, PingFang SC;\n            color: rgba(0, 0, 0, 0.25);\n          }\n        }\n        .img-upload:nth-child(n + 4) {\n          margin: 0.37rem 0.37rem 0 0;\n        }\n      }\n      // 文件\n      .file-option {\n        padding: 0.24rem 0.24rem 0 0.24rem;\n        .file-list {\n          .item {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            padding-bottom: 0.24rem;\n            .file-name {\n              display: flex;\n              align-items: center;\n              .file-icon {\n                height: 0.28rem;\n              }\n              & > div {\n                max-width: 5rem;\n                overflow: hidden;\n                white-space: nowrap;\n                text-overflow: ellipsis;\n                font-size: 0.24rem;\n                font-family: PingFangSC-Regular, PingFang SC;\n                color: #5ecfa8;\n                padding: 0 0.1rem;\n              }\n              .arrow-icon {\n                height: 0.22rem;\n              }\n            }\n            .close-icon {\n              height: 0.32rem;\n            }\n          }\n        }\n        .file-upload {\n          width: 6.54rem;\n          height: 1.6rem;\n          border-radius: 0.06rem;\n          border: 0.02rem dashed rgba(0, 0, 0, 0.2);\n          display: flex;\n          flex-direction: column;\n          justify-content: center;\n          align-items: center;\n          & > img {\n            height: 0.52rem;\n          }\n          & > div {\n            font-size: 0.24rem;\n            font-family: PingFangSC-Regular, PingFang SC;\n            color: rgba(0, 0, 0, 0.25);\n          }\n        }\n      }\n    }\n  }\n  // 其他选填信息\n  .other-info {\n    padding: 0.24rem;\n    background: #ffffff;\n    .remark {\n      & > div {\n        padding-bottom: 0.24rem;\n        font-size: 0.28rem;\n        font-family: PingFangSC-Regular, PingFang SC;\n        color: rgba(0, 0, 0, 0.85);\n      }\n      & > input {\n        width: 6.7rem;\n        padding: 0 0.16rem;\n        height: 0.68rem;\n        border-radius: 0.02rem;\n        border: 0.01rem solid rgba(0, 0, 0, 0.08);\n      }\n    }\n    .name {\n      display: flex;\n      align-items: center;\n      justify-content: space-between;\n      padding-top: 0.42rem;\n      font-size: 0.28rem;\n      font-family: PingFangSC-Regular, PingFang SC;\n      color: rgba(0, 0, 0, 0.65);\n    }\n  }\n  .blank {\n    height: 2rem;\n  }\n  // 底部提交按钮\n  .bottom-btn {\n    width: 100%;\n    padding: 0.48rem 0;\n    background-color: #f5f5f5;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    position: fixed;\n    bottom: 0;\n    left: 0;\n    z-index: 10;\n    & > div {\n      width: 7.02rem;\n      height: 0.98rem;\n      background: #51c49c;\n      border-radius: 0.06rem;\n      font-size: 0.34rem;\n      font-family: PingFangSC-Regular, PingFang SC;\n      color: #ffffff;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n    }\n  }\n}\n</style>\n"]}]}
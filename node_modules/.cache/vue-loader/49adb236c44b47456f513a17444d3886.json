/div>\n        </div>\n\n        <div class=\"right select-wrapper\">\n          <search-select\n            class=\"search-select\"\n            :actions=\"optionTime\"\n            :value=\"searchParam.cameTime\"\n            @changeValue=\"changeTime\"\n            isLarge\n          ></search-select>\n          <search-select\n            class=\"search-select\"\n            :actions=\"option\"\n            :value=\"searchParam.doorId\"\n            @changeValue=\"changeValue\"\n            isLarge\n          ></search-select>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {\n  getVerifyList,\n  getVisitorTotal,\n  getDoorListBySafetyUser\n} from '@/pages/visitor-h5/api/verify';\nimport { formateDate } from '@/pages/visitor-h5/plugins/common/utils';\nimport searchSelect from '@/pages/visitor-h5/components/SearchSelect.vue';\n\nexport default {\n  components: { searchSelect },\n  props: ['arrivedNumber', 'totalNumber'],\n  data() {\n    return {\n      getVerifyList,\n      option: [],\n      optionTime: [\n        { text: '到访时间', value: '' },\n        { text: '01时', value: 1 },\n        { text: '02时', value: 2 },\n        { text: '03时', value: 3 },\n        { text: '04时', value: 4 },\n        { text: '05时', value: 5 },\n        { text: '06时', value: 6 },\n        { text: '07时', value: 7 },\n        { text: '08时', value: 8 },\n        { text: '09时', value: 9 },\n        { text: '10时', value: 10 },\n        { text: '11时', value: 11 },\n        { text: '12时', value: 12 },\n        { text: '13时', value: 13 },\n        { text: '14时', value: 14 },\n        { text: '15时', value: 15 },\n        { text: '16时', value: 16 },\n        { text: '17时', value: 17 },\n        { text: '18时', value: 18 },\n        { text: '19时', value: 19 },\n        { text: '20时', value: 20 },\n        { text: '21时', value: 21 },\n        { text: '22时', value: 22 },\n        { text: '23时', value: 23 }\n      ],\n      show: false,\n      date: formateDate(new Date(), 4),\n      searchParam: {\n        doorId: '',\n        cameTime: ''\n      }\n    };\n  },\n  methods: {\n    clickCard(item) {\n      this.$router.push({\n        path: '/verify-detail',\n        query: {\n          id: item.id\n        }\n      });\n    },\n    changeValue(item) {\n      this.searchParam.doorId = item.value;\n      this.$emit('refreshData', this.searchParam);\n    },\n    changeTime(item) {\n      this.searchParam.cameTime = item.value;\n      this.$emit('refreshData', this.searchParam);\n    },\n    onClickDate() {\n      this.show = true;\n    },\n    onConfirm(date) {\n      this.show = false;\n      this.date = formateDate(date, 4);\n    },\n\n    getDoorListBySafetyUser() {\n      getDoorListBySafetyUser().then(res => {\n        this.option = res.data.map(item => {\n          return {\n            text: item.door,\n            value: item.id\n          };\n        });\n        this.option.unshift({\n          text: '访客入口',\n          value: ''\n        });\n      });\n    }\n  },\n  mounted() {\n    this.getDoorListBySafetyUser();\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.verify-search {\n  line-height: 52.5px;\n  box-sizing: border-box;\n  .verify-search-inner {\n    height: 100%;\n    .verify-search-top {\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      height: 36px;\n      background: #fdfcec;\n      color: #f86e21;\n      padding: 0 27px;\n      margin-bottom: 12px;\n      font-size: 14px;\n    }\n  }\n  .left {\n    flex: 1;\n    display: flex;\n    width: 100%;\n    height: 100%;\n    div {\n      flex: 1;\n      font-size: 12px;\n      font-family: PingFangSC-Regular, PingFang SC;\n      font-weight: 400;\n      color: rgba(0, 0, 0, 0.45);\n      line-height: 15px;\n      span {\n        display: block;\n        line-height: 30px;\n        font-size: 15px;\n        color: rgba(0, 0, 0, 0.85);\n      }\n    }\n    .total {\n      position: relative;\n      &::after {\n        display: block;\n        content: '';\n        width: 1px;\n        height: 11px;\n        background: rgba(0, 0, 0, 0.45);\n        border-radius: 1.5px;\n        position: absolute;\n        top: 25px;\n        left: -18px;\n      }\n    }\n  }\n  .select-wrapper {\n    flex: 2;\n    display: flex;\n    justify-content: space-between;\n    padding: 0 12px;\n    .search-select {\n      z-index: 1;\n      margin-right: 10px;\n      background: #fff;\n      width: 165px;\n      height: 44px;\n      border-radius: 4px;\n      font-size: 13px;\n      font-family: PingFangSC-Regular, PingFang SC;\n      font-weight: 400;\n      color: rgba(0, 0, 0, 0.65);\n      option {\n        border: none;\n      }\n    }\n  }\n}\n</style>\n"]}]}
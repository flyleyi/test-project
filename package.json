{
  "name": "mutilwap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "startCom": "cross-env NODE_ENV=production PORT=8991 BASE_URL=/wap/ nodemon server/index.js",
    "start": "cross-env NODE_ENV=production PORT=8990 BASE_URL=/wap/ nodemon server/index.js",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "build": "cross-env mode=prd node config/tools/exec-tasks.js",
    "buildSit": "cross-env mode=sit node config/tools/exec-tasks.js",
    "buildCom": "cross-env mode=com node config/tools/exec-tasks.js",
    "precommit": "node config/tools/commit.js",
    "commitmsgcheck": "node config/tools/commit-msg.js",
    "jest": "node config/tools/jest.js",
    "cypress": "./node_modules/.bin/cypress open",
    "dll": "webpack -p --progress --config ./config/dll/webpack.dll.conf.js"
  },

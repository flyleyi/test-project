    "qrcodejs2": "^0.0.2",
    "sortablejs": "^1.13.0",
    "svg-sprite-loader": "^5.0.0",
    "svgaplayerweb": "^2.3.1",
    "uuid": "^8.3.2",
    "v-hl-preview-file": "^1.0.1",
    "vant": "^2.12.0",
    "vue": "2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-count-to": "^1.0.13",
